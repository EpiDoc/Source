        </optional>
        <optional>
          <group>
            <group>
              <ref name="TEI_model.divGenLike"/>
            </group>
            <zeroOrMore>
              <choice>
                <ref name="TEI_model.global"/>
                <ref name="TEI_model.divGenLike"/>
              </choice>
            </zeroOrMore>
          </group>
        </optional>
        <group>
          <choice>
