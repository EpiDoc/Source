              <ref name="TEI_model.divTop"/>
            </group>
            <group>
              <ref name="TEI_model.global"/>
            </group>
          </choice>
        </zeroOrMore>
        <choice>
          <oneOrMore>
            <ref name="TEI_item"/>
            <zeroOrMore>
              <ref name="TEI_model.global"/>
            </zeroOrMore>
          </oneOrMore>
          <group>
            <optional>
              <ref name="TEI_headLabel"/>
            </optional>
            <optional>
              <ref name="TEI_headItem"/>
            </optional>
            <oneOrMore>
              <ref name="TEI_label"/>
              <zeroOrMore>
                <ref name="TEI_model.global"/>
              </zeroOrMore>
              <ref name="TEI_item"/>
              <zeroOrMore>
                <ref name="TEI_model.global"/>
              </zeroOrMore>
            </oneOrMore>
          </group>
        </choice>
