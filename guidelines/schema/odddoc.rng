        <zeroOrMore>
          <group>
            <group>
              <ref name="TEI_model.divBottom"/>
            </group>
            <zeroOrMore>
              <ref name="TEI_model.global"/>
            </zeroOrMore>
          </group>
        </zeroOrMore>
      </group>
      <ref name="att.global.attribute.xmlid"/>
      <ref name="att.global.attribute.n"/>
      <ref name="att.global.attribute.xmllang"/>
