          <group>
            <group>
              <ref name="TEI_model.divBottom"/>
            </group>
            <zeroOrMore>
              <ref name="TEI_model.global"/>
            </zeroOrMore>
          </group>
        </zeroOrMore>
      </group>
      <rng:optional>
        <rng:attribute name="type">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          <rng:choice>
            <rng:value>unordered</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> (UnNumbered list) </a:documentation>
            <rng:value>bulleted</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> (UnNumbered list) </a:documentation>
            <rng:value>ordered</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> (Numbered list) </a:documentation>
            <rng:value>simple</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> (Plain list) </a:documentation>
            <rng:value>gloss</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> (Glossary list) </a:documentation>
            <rng:value>inline</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> (Inline list) </a:documentation>
