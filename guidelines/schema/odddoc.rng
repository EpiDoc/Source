    <notAllowed/>
  </define>
  <define xmlns="http://relaxng.org/ns/structure/1.0" name="TEI_model.encodingPart">
    <rng:choice>
      <ref name="TEI_projectDesc"/>
      <ref name="TEI_samplingDecl"/>
      <ref name="TEI_editorialDecl"/>
      <ref name="TEI_tagsDecl"/>
