          <choice>
            <ref name="TEI_text"/>
            <ref name="TEI_group"/>
          </choice>
          <zeroOrMore>
            <choice>
              <ref name="TEI_text"/>
              <ref name="TEI_group"/>
              <ref name="TEI_model.global"/>
            </choice>
          </zeroOrMore>
        </group>
        <zeroOrMore>
          <ref name="TEI_model.divBottom"/>
        </zeroOrMore>
