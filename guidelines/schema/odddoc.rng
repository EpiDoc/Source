            </oneOrMore>
            <oneOrMore>
              <group>
                <group>
                  <ref name="TEI_model.div1Like"/>
                </group>
                <zeroOrMore>
                  <choice>
                    <ref name="TEI_model.global"/>
                    <ref name="TEI_model.divGenLike"/>
                  </choice>
                </zeroOrMore>
              </group>
            </oneOrMore>
            <group>
              <oneOrMore>
                <group>
                  <ref name="TEI_model.common"/>
                </group>
                <zeroOrMore>
                  <ref name="TEI_model.global"/>
                </zeroOrMore>
              </oneOrMore>
              <optional>
                <choice>
                  <oneOrMore>
                    <group>
                      <group>
                        <ref name="TEI_model.divLike"/>
                      </group>
                      <zeroOrMore>
                        <choice>
                          <ref name="TEI_model.global"/>
                          <ref name="TEI_model.divGenLike"/>
                        </choice>
                      </zeroOrMore>
                    </group>
                  </oneOrMore>
                  <oneOrMore>
                    <group>
                      <group>
                        <ref name="TEI_model.div1Like"/>
                      </group>
                      <zeroOrMore>
                        <choice>
                          <ref name="TEI_model.global"/>
                          <ref name="TEI_model.divGenLike"/>
                        </choice>
                      </zeroOrMore>
                    </group>
                  </oneOrMore>
                </choice>
              </optional>
            </group>
