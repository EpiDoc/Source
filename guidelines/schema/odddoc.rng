            <rng:value>silent</rng:value>
            <a:documentation>normalization made silently
  </a:documentation>
            <rng:value>markup</rng:value>
            <a:documentation>normalization represented using markup
   </a:documentation>
          </rng:choice>
        </rng:attribute>
      </rng:optional>
      <empty/>
