<div xmlns="http://www.tei-c.org/ns/1.0" xmlns:tei="http://www.tei-c.org/ns/1.0"
	xmlns:xi="http://www.w3.org/2001/XInclude" id="deletion">
	<head>Deletion of text in antiquity</head>
	<div id="deletion-typographic" type="gl-typographic" org="uniform" part="N" sample="complete">
		<head>Traditional typographic representation</head>
		<list type="simple">
			<item><bibl default="NO"><title level="a" type="abbreviated">
						<xref href="bibliography.xml#panciera" evaluate="all" from="ROOT" targOrder="Y"
							to="DITTO">Panciera</xref>
					</title>, <abbr expan="page">p.</abbr><biblScope type="pages">18-19</biblScope></bibl>;
					<bibl default="NO"><title level="a" type="abbreviated">
						<xref href="bibliography.xml#dohnicht" evaluate="all" from="ROOT" targOrder="Y"
							to="DITTO">Dohnicht</xref>
					</title>, <abbr expan="page">p.</abbr><biblScope type="pages">4</biblScope></bibl>: <list
					lang="lat" type="simple">
					<item id="TypoEx-1">et [[Getae Caesari]] ob rem publicam</item>
					<item>et [[G̣ẹṭạẹ Caesari]] ob rem publicam</item>
					<item>et [[ + c.11 + ]] ob rem publicam</item>
					<item>et [[ [· c.11 · ] ]] ob rem publicam</item>
					<item>et [[ [ - - - - - - - - - - - ] ]] ob rem publicam</item>
				</list></item>
		</list>
	</div>
	<div id="deletion-regex" type="gl-regextest">
		<head>Regular Expression Test</head>
		<list>
			<item id="Regex-1"><![CDATA[\[\[\s*((\w|\s)+)\s*\]\]  =   <del rend="erasure" status="unremarkable">$1</del>]]></item>
		</list>
		<link id="TestLink-1" type="regextest" targets="TypoEx-1 EncEx-1 Regex-1"/>
	</div>
	<div id="deletion-edition" type="gl-edition" lang="lat" org="uniform" part="N" sample="complete">
		<head lang="en">EpiDoc appearance with standard XSLT</head>
		<list type="simple">
			<item id="EncEx-1">et <del rend="erasure" status="unremarkable">Getae Caesari</del> ob rem
				publicam</item>
			<item>et <del rend="erasure" status="unremarkable"><unclear reason="damage"
						cert="high">Getae</unclear> Caesari</del> ob rem publicam</item>
			<item>et <del rend="erasure" status="unremarkable">
					<gap id="gap20" reason="illegible" extent="11" unit="character"/>
					<certainty target="gap20" locus="extent" degree="low"/>
				</del> ob rem publicam</item>
			<item>et <del rend="erasure" status="unremarkable">
					<gap id="gap21" reason="lost" extent="11" unit="character"/>
					<certainty target="gap21" locus="extent" degree="low"/>
				</del> ob rem publicam</item>
			<item>et <del rend="erasure" status="unremarkable">
					<gap id="gap22" reason="lost" extent="11" unit="character"/>
					<certainty target="gap22" locus="unit" degree="low"/>
				</del> ob rem publicam</item>
		</list>
	</div>
