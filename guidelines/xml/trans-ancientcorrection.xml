<?oxygen NVDLSchema="../schema/epidoc.nvdl"?>

<div xmlns="http://www.tei-c.org/ns/1.0" xml:id="trans-ancientcorrection" xml:lang="en">
    <head>Ancient Corrections (Where Both Old and New Text Can Be Read)</head>
    <p><!--gloss--></p>
    <p><specList>
        <specDesc key="subst"/>
        <specDesc key="del"/>
        <specDesc key="add"/>
    </specList>
        <!--<listRef>
            <ptr target="a"/>
            </listRef>--></p> 
    
    <p><!--More prose explanation--></p>
    
    <cit>
        <quote><!-- conventional rendering --></quote>
        <bibl corresp="#panciera">
            <biblScope>VI.1</biblScope>
        </bibl>
        <bibl corresp="#ddbdp"/>
        <!-- etc. -->
    </cit>
    
    <egXML xmlns="http://www.tei-c.org/ns/Examples" corresp="#panciera">
        <subst><del rend="corrected">αβ</del> <add place="overstrike">γδ</add></subst>
    </egXML>
    
    <!-- No IRT examples of this -->
    <!-- No iAph examples of this -->
    
    <egXML xmlns="http://www.tei-c.org/ns/Examples" rend="ddbdp" corresp="http://papyri.info/ddbdp/bgu;1;183"> 
        <subst><add place="inline">τέσσαρας</add><del rend="corrected">τεσσαρες</del></subst>
    </egXML>
    
    <div xml:id="trans-ancientcorrection-responsibility">
    
        <head>Responsibility for this section</head>
        <listBibl>
            <bibl><respStmt>
                <resp>author</resp>
                <name>Charlotte Tupman</name>
            </respStmt></bibl>
        </listBibl>
    </div>
</div>
