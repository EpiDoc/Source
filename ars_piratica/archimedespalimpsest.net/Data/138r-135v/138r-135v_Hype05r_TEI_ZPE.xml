    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="138r-135v" type="folio">
                    <seg n="01" type="line" facs="z138r-135v_01">τοῦ μὲν εὑρίσκοντος ἐν τῶι
                        δικαστηρίωι μὴ ἔλαττον ἦι</seg>
                    <seg n="02" type="line" facs="z138r-135v_02">τοῖς παισίν· ἐὰν δὲ πλείω
                            περιπο<unclear>ι</unclear>ή<unclear>σω</unclear>σι<unclear>ν</unclear>
                        τοῖς <w part="I">παι</w></seg>
                    <seg n="03" type="line" facs="z138r-135v_03"><w part="F">σίν,</w> τούτων εἴη
                            <seg type="word">
                            <choice>
                                <abbr>φιλοτιμί</abbr>
                                <expan>φιλοτιμία</expan>
                            </choice>
                        </seg>. αὑτοῖς δὲ τοῦς <w part="I">ἐπιτρό</w></seg>
                    <seg n="04" type="line" facs="z138r-135v_04"><w part="F">πους</w> ἀπαγορεύουσιν
                        οἱ νόμοι μὴ ἐξεῖναι τὸν οἶκον</seg>
                    <seg n="05" type="line" facs="z138r-135v_05">μισθώσασθαι· ἔξεστι <choice>
                            <sic>δὲ</sic>
                            <corr>δ’</corr>
                        </choice> ἐν τῶι δικαστηρίωι <w part="I">ἀμφισ</w></seg>
                    <seg n="06" type="line" facs="z138r-135v_06"><w part="F">βητῆσαι</w> μὴ ἄμεινον
                        εἶναι τὸν οἶκον μισθῶσαι <seg type="word">
                            <choice>
                                <abbr>τῶ</abbr>
                                <expan>τῶν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="07" type="line" facs="z138r-135v_07">παίδων, ὑμῶν δὲ τοὺς λαχόντας
                        δικάζειν <w part="I">ἀκού</w></seg>
                    <seg n="08" type="line" facs="z138r-135v_08"><w part="F">σαντας</w> ψηφίσασθαι ἃ
                        ἂν δοκῆι βέλτιστα εἶναι τῶι</seg>
                    <seg n="09" type="line" facs="z138r-135v_09">παιδί. καί μοι λέγε τούτους τοὺς
                        νόμους. ΝΟΜΟΙ</seg>
                    <seg n="10" type="line" facs="z138r-135v_10">τούτων τοίνυν <seg type="word">
                            <choice>
                                <abbr>οὗτ</abbr>
                                <expan>οὗτος</expan>
                            </choice>
                        </seg> οὐδὲν ἐποίησεν οὐδ’ ὅλως</seg>
                    <seg n="11" type="line" facs="z138r-135v_11">ἀπέγραψεν τὸν οἶκον πρὸς τὸν <seg
                            type="word">
                            <choice>
                                <abbr>ἄρχον</abbr>
                                <expan>ἄρχοντα</expan>
                            </choice>
                        </seg>. καί μοι <w part="I">λα</w></seg>
                    <seg n="12" type="line" facs="z138r-135v_12"><w part="F">βὲ</w> τὴν μαρτυρίαν.
                        ΜΑΡΤΥΡΙΑ</seg>
                    <seg n="13" type="line" facs="z138r-135v_13">ὅτι μὲν τοίνυν
                        <unclear>οὐ</unclear> κατὰ τοὺς νόμους τὴν οὐσίαν τὴν</seg>
                    <seg n="14" type="line" facs="z138r-135v_14">Ἀκαδήμου τουτουὶ διεχείρισε <seg
                            type="word">
                            <choice>
                                <abbr>Τίμανδρ</abbr>
                                <expan>Τίμανδρος</expan>
                            </choice>
                        </seg> οὑτοσί, <w part="I">ἀκη</w></seg>
                    <seg n="15" type="line" facs="z138r-135v_15"><w part="F">κόατε</w> τῶν νόμων,
                        καὶ τῶν μαρτύρων ὅτι οὔτε <w part="I">ἐ</w></seg>
                    <seg n="16" type="line" facs="z138r-135v_16"><w part="F">μίσθωσε</w> τὸν οἶκον
                        ἑτέρου <choice>
                            <sic/>
                            <corr>τε</corr>
                        </choice>
                        <seg type="word">
                            <choice>
                                <abbr>φήναντ</abbr>
                                <expan>φήναντος</expan>
                            </choice>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr>ἵν</abbr>
                                <expan>ἵνα</expan>
                            </choice>
                        </seg>
                        <w part="I">μισθω</w></seg>
                    <seg n="17" type="line" facs="z138r-135v_17"><w part="F">θῆι</w> ἐκώλυσεν·
                            <unclear>ὅ</unclear>τι δὲ <seg type="word">
                            <choice>
                                <abbr>ταῦ</abbr>
                                <expan><unclear>τα</unclear>ῦτα</expan>
                            </choice>
                        </seg> ἵνα δ<unclear>ια</unclear>φ<unclear>ορ</unclear>ήσηι τὰ <seg
                            type="clearword">χρή</seg></seg>
                    <seg n="18" type="line" facs="z138r-135v_18"><seg type="wordend">
                            <choice>
                                <abbr>μα</abbr>
                                <expan>ματα</expan>
                            </choice>
                        </seg> οὑτωσὶ ἐποίησεν ἤ<unclear>δη</unclear> ἀ<gap extent="5"/> δείξω. καὶ
                        γὰρ</seg>
                    <seg n="19" type="line" facs="z138r-135v_19">διὰ τὰ <seg type="word">
                            <choice>
                                <abbr>χρήμα</abbr>
                                <expan>χρήματα</expan>
                            </choice>
                        </seg> καὶ εἰς τὴν ἀδελφὴν τουτουὶ <w part="I">θα</w></seg>
                    <seg n="20" type="line" facs="z138r-135v_20"><w part="F">νάτου</w>
                        <seg type="word">
                            <choice>
                                <abbr>ἄξι</abbr>
                                <expan>ἄξια</expan>
                            </choice>
                        </seg> ἠδίκηκεν· καταλειφθέν<unclear>τω</unclear>ν γὰρ <w part="I">του</w></seg>
                    <seg n="21" type="line" facs="z138r-135v_21"><w part="F">τωνὶ</w> δυοῖν ἀδελφοῖν
                        καὶ ἀδελφαῖν δυοῖν <seg type="unclearword">ὀρ<unclear>φα</unclear></seg></seg>
                    <seg n="22" type="line" facs="z138r-135v_22"><seg type="wordend">ναῖν</seg> καὶ
                        μητρὸς καὶ <seg type="word">
                            <choice>
                                <abbr>πρς</abbr>
                                <expan>πατρὸς</expan>
                            </choice>
                        </seg> καὶ παιδαρίων</seg>
                    <seg n="23" type="line" facs="z138r-135v_23">πάντων ὄντων –
                        ἴ<unclear>σως</unclear> γὰρ ὁ <seg type="word">
                            <choice>
                                <abbr>πρεσβύτατ</abbr>
                                <expan>πρεσβύτατος</expan>
                            </choice>
                        </seg>
                        <seg type="unclearword">ἀδ<unclear>ελ</unclear></seg></seg>
                    <seg n="24" type="line" facs="z138r-135v_24"><seg type="wordend"
                            >φ<unclear>ὸς</unclear></seg> Ἀντίφιλος ὁ τελευτήσας
                        <unclear>ἦ</unclear>ν δέκα ἐτῶν – </seg>
                    <seg n="25" type="line" facs="z138r-135v_25">τὴν νεωτέραν αὐτῶν ἀδελφὴν
                            ἀποσπάσα<unclear>ς</unclear>
                        <seg type="unclearword">ο<unclear>ὑ</unclear></seg></seg>
                    <seg n="26" type="line" facs="z138r-135v_26"><seg type="wordend">τοσὶ</seg>
                        Τίμανδρος ἔτρεφε παρ’ αὐτῶι <seg type="word">
                            <choice>
                                <abbr>ἀποκομίσ</abbr>
                                <expan>ἀπο<unclear>κομίσας</unclear></expan>
                            </choice>
                        </seg></seg>
                    <seg n="27" type="line" facs="z138r-135v_27">εἰς Λῆμνον ἴσως οὖσαν ἑπτὰ ἐτῶν.
                            κ<unclear>α</unclear>ί<unclear>τοι</unclear>
                        <w part="I">τοῦ</w></seg>
                    <seg n="28" type="line" facs="z138r-135v_28"><w part="F">το</w> μὴ ὅτι <seg
                            type="word">
                            <choice>
                                <abbr>ἐπίτροπ</abbr>
                                <expan>ἐπίτροπος</expan>
                            </choice>
                        </seg> ἢ εὔνους <choice>
                            <sic/>
                            <corr>ἂν</corr>
                        </choice>
                        <seg type="word">
                            <choice>
                                <abbr>ἄνος</abbr>
                                <expan>ἄνθρωπος</expan>
                            </choice>
                        </seg> ποιήσαι, <w part="I">ἀλ</w></seg>
                    <seg n="29" type="line" facs="z138r-135v_29"><w part="F">λ’</w> οὐδ’ οἱ κατὰ
                        πόλεμον ἐγκρατεῖς γιγνόμενοι <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τῶν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="30" type="line" facs="z138r-135v_30">σωμάτων, ἀλλὰ καὶ κατ’ οἰκίαν
                        πωλοῦσιν ὅτι</seg>
                    <seg n="31" type="line" facs="z138r-135v_31">μάλιστα. οἱ τοίνυν <seg type="word">
                            <choice>
                                <abbr>ἀνδραποδοκάπηλ</abbr>
                                <expan>ἀνδραποδοκάπηλοι</expan>
                            </choice>
                        </seg> καὶ <w part="I">ἔμ</w></seg>
                    <seg n="32" type="line" facs="z138r-135v_32"><w part="F">ποροι</w> κέρδους ἕνεκα
                        πᾶν πράττοντες <unclear>ἀσελγές</unclear>,</seg>
                </seg>
            </ab>
        </body>
    </text>
