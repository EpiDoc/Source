    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="69r1" type="folio">
                    <seg n="1" type="line" facs="z69r1_01">Τὸ ὀρθὸν τμῆμα τοῦ <w part="I"
                        >ὀρθογωνί</w></seg>
                    <seg n="2" type="line" facs="z69r1_02"><w part="F">ου</w> κωνοειδοῦς, ὅταν τὸ
                        ὑγρὸν <seg type="clearword">
                            <choice>
                                <abbr>κ</abbr>
                                <expan>κου</expan>
                            </choice>
                        </seg></seg>
                    <seg n="3" type="line" facs="z69r1_03"><seg type="wordend">φότερον</seg> ἦ καὶ
                        τὸν ἄξονα ἔχη</seg>
                    <seg n="4" type="line" facs="z69r1_04">μείζονα ἢ ἐλάσσονα δὲ ἢ ὥστε </seg>
                    <seg n="5" type="line" facs="z69r1_05">λόγον ἔχειν πρὸς τὴν μέχρι τοῦ</seg>
                    <seg n="6" type="line" facs="z69r1_06">ἄξονος ἢ ἡμιόλιον τῆς μέχρι τοῦ </seg>
                    <seg n="7" type="line" facs="z69r1_07">ἄξονος, ὃν τὰ <num>ΡΕ</num>
                        <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice>
                        <num>ΔΑ,</num> ἀφεθὲν ὲς</seg>
                    <seg n="8" type="line" facs="z69r1_08">τὸ ὑγρὸν οὕτως, ὥστε τὴν βάσιν <w
                            part="I">ὅ</w></seg>
                    <seg n="9" type="line" facs="z69r1_09"><w part="F">λην</w> εἶναι ἐν τῶι ὑγρῶι,
                        οὐδέποτε</seg>
                    <seg n="10" type="line" facs="z69r1_10">καταστήσεται οὕτως, ὥστε τὴν <w part="I"
                            >βά</w></seg>
                    <seg n="11" type="line" facs="z69r1_11"><w part="F">σιν</w> αὐτοῦ ἅπτεσθαι τῆς
                        τοῦ ὑγροῦ</seg>
                    <seg n="12" type="line" facs="z69r1_12">ἐπιφανείας.</seg>
                </seg>
                <seg n="69r1" type="folio">
                    <seg n="12" type="line" facs="z69r1_12">ἔστω τμῆμα,</seg>
                    <seg n="13" type="line" facs="z69r1_13">οἷον εἴρηται, καὶ ἀφεθὲν ἐς τὸ <w
                            part="I">ὑ</w></seg>
                    <seg n="14" type="line" facs="z69r1_14"><w part="F">γρὸν</w> καθάπερ ἐρρέθη, <w
                            part="I">καθε</w></seg>
                    <seg n="15" type="line" facs="z69r1_15"><w part="F">στηκέτω</w> οὕτως, ὥστε τὴν
                        βάσιν <w part="I">αὐ</w></seg>
                    <seg n="16" type="line" facs="z69r1_16"><w part="F">τοῦ</w>
                        <seg type="word">μ<supplied reason="lost">ὴ</supplied></seg>
                        <seg type="word"><supplied reason="lost">κ</supplied>αθ’</seg> ἓν ἅπτεσθαι
                        τῆς τοῦ </seg>
                    <seg n="17" type="line" facs="z69r1_17">ὑγροῦ ἐπιφανείας.</seg>
                </seg>
                <seg n="69r1" type="folio">
                    <seg n="17" type="line" facs="z69r1_17">τμηθέντος</seg>
                    <seg n="18" type="line" facs="z69r1_18">γὰρ αὐτοῦ ἐπιπέδωι ὀρθῶι <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice></seg>
                </seg>
                <seg n="68v1" type="folio">
                    <seg n="1" type="line" facs="z68v1_01">τὴν τοῦ ὑγροῦ ἐπιφάνειαν τομὴ</seg>
                    <seg n="2" type="line" facs="z68v1_02">ἔστω ἡ ΑΠ ΟΛ ὀρθογωνίου <choice>
                            <abbr>κων</abbr>
                            <expan>κώνου</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z68v1_03">τομὴς, ἔστω δὲ καὶ τῆς τοῦ ὑγροῦ <w
                            part="I">ἐπι</w></seg>
                    <seg n="4" type="line" facs="z68v1_04"><w part="F">φανείας</w> τομὴ ἡ ΣΑ, ἄξων
                        δ’ <w part="I">ἔ</w></seg>
                    <seg n="5" type="line" facs="z68v1_05"><w part="F">στω</w> τοῦ τμήματος καὶ
                        διάμετρος</seg>
                    <seg n="6" type="line" facs="z68v1_06">ἡ ΠΦ, <seg type="word">π<supplied
                                reason="lost">ά</supplied>λιν</seg> δὲ <sic>τεμήσθω</sic> ἡ ΠΦ <choice>
                            <abbr>κα</abbr>
                            <expan>κατὰ</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z68v1_07">μὲν τὸ Ρ, ὥστε διπλασίαν εἶναι</seg>
                    <seg n="8" type="line" facs="z68v1_08">τὴν <supplied reason="lost">Ρ</supplied>Π
                        τῆς ΡΦ, κατὰ δὲ τὸ Ω, ὥστε</seg>
                    <seg n="9" type="line" facs="z68v1_09">τὴν Π<unclear>Φ</unclear> πρὸς τὴν ΡΩ
                        λόγον <choice>
                            <abbr>εχει</abbr>
                            <expan>ἔχειν</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z68v1_10">ὃν τὰ <num>ΙΕ</num> πρὸς <num>Δ,</num>
                        καὶ ἡ ΩΚ ὀρθὴ</seg>
                    <seg n="11" type="line" facs="z68v1_11">ἤχθω τῆι ΠΦ· ἔσται δ’ ἐλάσσων</seg>
                    <seg n="12" type="line" facs="z68v1_12">ἡ ΡΩ τῆς μέχρι τοῦ ἄξονος.</seg>
                    <seg n="13" type="line" facs="z68v1_13"><seg type="word"
                                >ἀπ<unclear>ει</unclear>λ<unclear>ή</unclear>φθω</seg> οὖν τῆ μέχρι
                        τοῦ</seg>
                    <seg n="14" type="line" facs="z68v1_14">ἄξονος ἴση ἡ ΡΗ, καὶ ἡ μὲν ΤΟ</seg>
                    <seg n="15" type="line" facs="z68v1_15">ἤχθω ἐφαπτομένη τῆς τομῆς</seg>
                    <seg n="16" type="line" facs="z68v1_16">κατὰ τὸ Ο παράλληλος οὖσα τᾶι</seg>
                    <seg n="17" type="line" facs="z68v1_17"><unclear>Λ</unclear>Σ, ἡ δὲ ΝΟ τῆι ΠΦ,
                        τεμνέτω δὴ</seg>
                </seg>
                <seg n="69r2" type="folio">
                    <seg n="1" type="line" facs="z69r2_01">ἡ ΝΟ τὴν ΚΩ πρότερον <seg type="word"
                                ><unclear>κα</unclear>τὰ</seg> τὸ Ι.</seg>
                    <seg n="2" type="line" facs="z69r2_02">ὁμοίως δὴ τῶ πρὸ <seg type="word"
                                >τούτο<unclear>υ</unclear></seg>
                        <seg type="word">
                            <choice>
                                <abbr>δειχθήσετ</abbr>
                                <expan><unclear>δ</unclear>ειχθήσεται</expan>
                            </choice>,</seg></seg>
                    <seg n="3" type="line" facs="z69r2_03"><expan>ὅτι</expan> ἡ ΝΟ ἤτοι ἡ ἡμιολία
                        τῆς <supplied reason="lost">ΟΙ</supplied> ἢ <w part="I">μεί</w></seg>
                    <seg n="4" type="line" facs="z69r2_04"><w part="F">ζον</w> ἡμιολία· γίνεται ἡ δὲ
                            <unclear>Ο</unclear>Θ τῆς</seg>
                    <seg n="5" type="line" facs="z69r2_05">ΘΝ ἐλάσσον ἢ διπλασία τῆς
                        Β<unclear>Ν</unclear>, <expan>καὶ</expan></seg>
                    <seg n="6" type="line" facs="z69r2_06">κατεσκευάσθω τὰ αὐτά· ὁμοίως οὖν</seg>
                    <seg n="7" type="line" facs="z69r2_07">δειχθήσεται ἡ ΡΘ ὀρθὰς γωνίας</seg>
                    <seg n="8" type="line" facs="z69r2_08">ποιοῦσα πρὸς τὴν ΤΟ καὶ πρὸς τὴν</seg>
                    <seg n="9" type="line" facs="z69r2_09">τοῦ ὑγροῦ ἐπιφάνειαν, καὶ ἀπὸ <seg
                            type="word">τ<unclear>ῶ</unclear>ν</seg></seg>
                    <seg n="10" type="line" facs="z69r2_10">ΒΓ ἀχθεῖσαν παρὰ τὴν ΡΟ κάθετοι</seg>
                    <seg n="11" type="line" facs="z69r2_11">ἔσονται ἐπὶ τὴν τοῦ ὑγροῦ <choice>
                            <abbr>επιφανει</abbr>
                            <expan>ἐπιφάνειαν.</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z69r2_12">κατενεχθήσεται οὖν τὸ μὲν ἐκτὸς</seg>
                    <seg n="13" type="line" facs="z69r2_13">τοῦ ὑγροῦ τμῆμα εἰς τὸ ὑγρὸν κατὰ</seg>
                    <seg n="14" type="line" facs="z69r2_14">τὴν διὰ τοῦ Β κάθετον, τὸ δ’ ἐν τῶι</seg>
                    <seg n="15" type="line" facs="z69r2_15">ὑγρῶι ἀνενεχθήσεται κατὰ τὴν</seg>
                    <seg n="16" type="line" facs="z69r2_16">Γ· φανερὸν οὖν, <expan>ὅτι</expan>
                        ἐπικλιθήσεται τὸ</seg>
                    <seg n="17" type="line" facs="z69r2_17">στερεόν, <seg type="word"><supplied
                                reason="lost">ὥ</supplied>στε</seg> τὴν βάσιν αὐτοῦ <w part="I"
                        >μη</w></seg>
                    <seg n="18" type="line" facs="z69r2_18"><w part="F">δὲ</w> καθ’ ἓν ἅπτεσθαι τῆς
                        τοῦ ὑγροῦ <w part="I">ἐ</w></seg>
                    <seg n="19" type="line" facs="z69r2_19"><w part="F">πιφανείας,</w> ἐπειδὴ νῦν
                        καθ’ ἓν <w part="I">ση</w></seg>
                    <seg n="20" type="line" facs="z69r2_20"><w part="F">μεῖον</w>
                        <seg type="word">ἁπ<supplied reason="lost">το</supplied>μένη</seg> εἰς τὰ
                        κάτω <w part="I">φέρε</w></seg>
                </seg>
                <seg n="68v2" type="folio">
                    <seg n="1" type="line" facs="z68v2_01"><w part="F">ται</w> ἐπὶ τὰ αὐτὰ τῶ
                    Α.</seg>
                </seg>
                <seg n="68v2" type="folio">
                    <seg n="1" type="line" facs="z68v2_01">φανερὸν δὲ,</seg>
                    <seg n="2" type="line" facs="z68v2_02"><expan>ὅτι,</expan> κἂν ἡ ΟΝ μὴ τέμνη τὴν
                        ΩΚ,</seg>
                    <seg n="3" type="line" facs="z68v2_03">ταὐτὰ δειχθήσεται.</seg>
                    <figure n="2.7.1">
                        <figDesc xml:lang="eng">Figure 2.7.1</figDesc>
                    </figure>
                </seg>
                <seg n="68v2" type="folio">
                    <seg n="4" type="line" facs="z68v2_04">Τὸ ὀρθὸν τμῆμα τοῦ <choice>
                            <abbr>ορθογωνι</abbr>
                            <expan>ὀρθογωνίου</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z68v2_05">κωνοειδοῦς, ὅταν τὸν ἄξονα</seg>
                    <seg n="6" type="line" facs="z68v2_06">ἔχη μεῖζον ἡμιόλιον τῆς μέχρι</seg>
                    <seg n="7" type="line" facs="z68v2_07">τοῦ ἄξονος, ἐλάσσονα δὲ τὴν, ὥστε</seg>
                    <seg n="8" type="line" facs="z68v2_08">πρὸς τὴν μέχρι τοῦ ἄξονος τοῦτον</seg>
                    <seg n="9" type="line" facs="z68v2_09">ἔχειν τὸν λόγον, ὃν ἔχει τὰ <num>ΙΕ</num>
                        ἡ <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z68v2_10">τὰ <num>Δ,</num> ἐὰν τὸ <seg type="word"
                                >βάρο<supplied reason="lost">ς</supplied></seg> πρὸς τὸ ὑγρὸν</seg>
                    <seg n="11" type="line" facs="z68v2_11">ἐλάσσονα λόγον ἔχη τοῦ, ὃν ἔχει</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
