    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="127v1" type="folio">
                    <seg n="1" type="line" facs="z127v1_01">τῆς Χ· μείζων ἢ διπλασία ΔΙ ἡ</seg>
                    <seg n="2" type="line" facs="z127v1_02">ΠΗ τῆς ΗΜ. ἔστω οὖν διπλῆ ἡ ΠΖ</seg>
                    <seg n="3" type="line" facs="z127v1_03">τῆς ΖΜ, καὶ ἐπιζευχθεῖσα ἡ ΖΘ</seg>
                    <seg n="4" type="line" facs="z127v1_04"><seg type="word"
                                ><unclear>ἐ</unclear><supplied reason="lost"
                                >κβεβ</supplied>λή<supplied reason="lost">σ</supplied>θω</seg> ἐπὶ
                        τὸ Γ· ἔσται οὖν τοῦ </seg>
                    <seg n="5" type="line" facs="z127v1_05"><supplied reason="lost">μὲν</supplied>
                        <seg type="word">ὅλο<unclear>υ</unclear></seg> τμήματος <seg type="word"
                                ><unclear>κ</unclear>έντρον</seg> τοῦ <seg type="suppliedword"
                                ><supplied reason="lost">β</supplied>ά</seg></seg>
                    <seg n="6" type="line" facs="z127v1_06"><seg type="wordend">ρεος</seg> τὸ Θ, τοῦ
                        δ’ ἐκτὸς τοῦ ὑγροῦ τὸ</seg>
                    <seg n="7" type="line" facs="z127v1_07">Ζ<unclear>Π,</unclear> τοῦ δ’ ἐντὸς ἐπὶ
                        τῆς ΘΓ· ἔστω δὲ</seg>
                    <seg n="8" type="line" facs="z127v1_08">τὸ Γ. <seg type="word"
                            ><unclear>δ</unclear>ειχθήσεται</seg> δὴ ὁμοίως <seg type="word">
                            <choice>
                                <abbr>το</abbr>
                                <expan>το<supplied reason="lost">ῖς</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="9" type="line" facs="z127v1_09">πρότερον ἥ τε ΘΚ κάθετος ἐπὶ <choice>
                            <abbr>τη</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z127v1_10">τοῦ ὑγροῦ ἐπιφάνειαν καὶ διὰ τῶν </seg>
                    <seg n="11" type="line" facs="z127v1_11"><supplied reason="lost">Ζ</supplied>Γ
                        παρὰ <seg type="word">τὴ<supplied reason="lost">ν</supplied></seg> ΘΗ
                        ἀγόμεναι <gap extent="1"/>Α <w part="I">κά</w></seg>
                    <seg n="12" type="line" facs="z127v1_12"><w part="F">θετοι</w> καὶ αὐταὶ ἐπὶ τὴν
                        τοῦ ὑγροῦ <w part="I">ἐ</w></seg>
                    <seg n="13" type="line" facs="z127v1_13"><w part="F">πιφάνειαν.</w>
                        κατενεχθήσεται <expan>ἄρα</expan></seg>
                    <seg n="14" type="line" facs="z127v1_14"><seg type="word"><supplied
                                reason="lost">τ</supplied>ὸ</seg> μὲν ἐκτὸς τοῦ ὑγροῦ τμῆμα</seg>
                    <seg n="15" type="line" facs="z127v1_15">ἐς τὸ κάτω διὰ τοῦ Ζ, τὸ δ’ ἐντὸς</seg>
                    <seg n="16" type="line" facs="z127v1_16">κατὰ τὴν διὰ τοῦ Γ <w part="I"
                        >ἀνενεχθή</w></seg>
                    <seg n="17" type="line" facs="z127v1_17"><w part="F">σεται·</w> οὐ μενεῖ ἄρα τὸ
                        ὅλον <w part="I">τμῆ</w></seg>
                    <seg n="18" type="line" facs="z127v1_18"><w part="F">μα</w> ἀκλινές οὐδὲ μὴν <w
                            part="I">καταστρα</w></seg>
                    <seg n="19" type="line" facs="z127v1_19"><w part="F">φήσεται,</w> ὥστε κατὰ
                        κάθετον</seg>
                    <seg n="20" type="line" facs="z127v1_20">εἶναι τὸν ἄξονα ἐπὶ τὴν τοῦ <w part="I"
                            >ὑ</w></seg>
                    <seg n="21" type="line" facs="z127v1_21"><w part="F">γροῦ</w> ἐπιφάνειαν, ἐπειδὴ
                        τὰ ἐπὶ</seg>
                </seg>
                <seg n="130r1" type="folio">
                    <seg n="1" type="line" facs="z130r1_01">τὰ αὐτὰ τῶι Λ ἐς <seg type="word"
                                >τ<supplied reason="lost">ὸ</supplied></seg>
                        <seg type="word"><supplied reason="lost">ἄ</supplied>νω</seg> φέρεται,</seg>
                    <seg n="2" type="line" facs="z130r1_02">διὰ τὰν ἀνάλογον τοῖς <w part="I"
                        >λεγομέ</w></seg>
                    <seg n="3" type="line" facs="z130r1_03"><w part="F">νοις</w> ἐπὶ τοῦ πρὸ
                    αὐτοῦ.</seg>
                </seg>
                <seg n="130r1" type="folio">
                    <seg n="3" type="line" facs="z130r1_03">ἐὰν δὲ</seg>
                    <seg n="4" type="line" facs="z130r1_04">ὁ ἄξων πρὸς τὸ ὑγρὸν ποιῆι <w part="I"
                            >γωνί</w></seg>
                    <seg n="5" type="line" facs="z130r1_05"><w part="F">αν</w> ἔλασσον τῆς Β, ὁμοίως
                        τοῖς</seg>
                    <seg n="6" type="line" facs="z130r1_06">πρότερον δειχθήσεται ὅτι οὐ <w part="I"
                            >με</w></seg>
                    <seg n="7" type="line" facs="z130r1_07"><w part="F">νεῖ</w> τὸ τμῆμα, ἀλλὰ
                        κλιθήσεται,</seg>
                    <seg n="8" type="line" facs="z130r1_08">ἕως ἂν ὁ ἄξων <seg type="word"
                                >πο<supplied reason="lost">ι</supplied>ῆι</seg> γωνίαν</seg>
                    <seg n="9" type="line" facs="z130r1_09">πρὸς τὴν ἐπιφάνειαν τοῦ <choice>
                            <abbr>υγρ</abbr>
                            <expan>ὑγροῦ</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z130r1_10">ἴσην τῆι Β.</seg>
                    <figure n="2.9.1">
                        <figDesc xml:lang="eng">Figure 2.9.1</figDesc>
                    </figure>
                </seg>
                <seg n="127v2" type="folio">
                    <seg n="1" type="line" facs="z127v2_01">Τὸ ὀρθὸν τμῆμα τοῦ <w part="I"
                        >ὀρθογωνί</w></seg>
                    <seg n="2" type="line" facs="z127v2_02"><w part="F">ου</w> κωνοειδοῦς, ὅταν <w
                            part="I">κουφότε</w></seg>
                    <seg n="3" type="line" facs="z127v2_03"><w part="F">ρον</w> ὂν τοῦ ὑγροῦ τὸν
                        ἄξονα <w part="I">ἔ</w></seg>
                    <seg n="4" type="line" facs="z127v2_04"><w part="F">χη</w> μείζονα ὥστε λόγον
                        ἔχειν</seg>
                    <seg n="5" type="line" facs="z127v2_05">πρὸς τὴν μέχρι τοῦ ἄξονος
                        <sic>τοῦ</sic>,</seg>
                    <seg n="6" type="line" facs="z127v2_06">ὃν ἔχει τὰ <num>ΙΕ</num> πρὸς τὰ
                        <num>Δ</num>, <choice>
                            <abbr>αφεθ</abbr>
                            <expan>ἀφεθὲν</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z127v2_07">ἐς τὸ ὑγρὸν οὕτως, ὥστε τὴν βάσιν</seg>
                    <seg n="8" type="line" facs="z127v2_08">αὐτοῦ μὴ <seg type="word"
                            >ἅπ<unclear>τ</unclear>εσθαι</seg> τοῦ ὑγροῦ, <seg type="word"
                                ><unclear>ὁ</unclear>τὲ</seg></seg>
                    <seg n="9" type="line" facs="z127v2_09">μὲν ὀρθὸν καταστησεῖται, ὁτὲ</seg>
                    <seg n="10" type="line" facs="z127v2_10">δὲ κεκλιμένον, <expan>καὶ</expan> ποτὲ
                        μὲν <w part="I">οὕ</w></seg>
                    <seg n="11" type="line" facs="z127v2_11"><w part="F">τω</w> κεκλιμένον, ὥστε τὴν <choice>
                            <abbr>βασι</abbr>
                            <expan>βάσιν</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z127v2_12">αὐτοῦ καθ’ ἓν σημεῖον ἅπτεσθαι</seg>
                    <seg n="13" type="line" facs="z127v2_13">τῆς τοῦ ὑγροῦ ἐπιφανείας,
                        <expan>καὶ</expan></seg>
                    <seg n="14" type="line" facs="z127v2_14">τοῦτο ἐν δισσοῖς <seg type="word"
                                ><supplied reason="lost">κ</supplied>λίμασι</seg>
                        <w part="I">ποιή</w></seg>
                    <seg n="15" type="line" facs="z127v2_15"><w part="F">σει,</w> ποτὲ δὲ οὕτως <choice>
                            <abbr>κεκλιμενο</abbr>
                            <expan>κεκλιμένον</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z127v2_16">καταστησεῖται, ὥστε τὴν βάσιν</seg>
                    <seg n="17" type="line" facs="z127v2_17">αὐτοῦ κατὰ πλείονα τόπον</seg>
                    <seg n="18" type="line" facs="z127v2_18">βρέχεσθαι, <seg type="word">π<supplied
                                reason="lost">ο</supplied>τὲ</seg> δ’ οὕτως, ὥστε</seg>
                    <seg n="19" type="line" facs="z127v2_19">τὴν βάσιν αὐτοῦ μηδὲ καθ’ ἓν</seg>
                    <seg n="20" type="line" facs="z127v2_20">ἅπτεσθαι τῆς τοῦ ὑγροῦ <seg
                            type="suppliedword">ἐπ<supplied reason="lost">ι</supplied>φα</seg></seg>
                </seg>
                <seg n="130r2" type="folio">
                    <seg n="1" type="line" facs="z130r2_01"><seg type="wordend">νείας·</seg> τίνα δὲ
                        λόγον <seg type="word">ἔχοντ<unclear>α</unclear></seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ῶ</unclear>
                        </seg></seg>
                    <seg n="2" type="line" facs="z130r2_02">βάρει πρὸς τὸ ὑγρὸν ἕκαστα <w part="I"
                            >τού</w></seg>
                    <seg n="3" type="line" facs="z130r2_03"><w part="F">των</w> ἐσται, νῦν
                        δηλωθήσεται.</seg>
                </seg>
                <seg n="130r2" type="folio">
                    <seg n="4" type="line" facs="z130r2_04">ἔστω τμῆμα, οἷον εἴρηται, καὶ</seg>
                    <seg n="5" type="line" facs="z130r2_05"><seg type="word"
                            >τμ<unclear>η</unclear>θέντος</seg> αὐτοῦ <seg type="word">ἐπ<supplied
                                reason="lost">ι</supplied>πέδωι</seg></seg>
                    <seg n="6" type="line" facs="z130r2_06">ὀρθῶι πρὸς τὴν ἐπιφάνειαν</seg>
                    <seg n="7" type="line" facs="z130r2_07">τοῦ ὑγροῦ τομὴ ἔστω ἐν τῆ <w part="I"
                            >ἐπιφα</w></seg>
                    <seg n="8" type="line" facs="z130r2_08"><w part="F">νείαι</w>
                        Α<unclear>Π</unclear>ΟΛ ὀρθογωνίου <choice>
                            <abbr>κων</abbr>
                            <expan>κώνου</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z130r2_09">τομή, ἄξων δ’ ἔστω καὶ <choice>
                            <abbr>διαμετρ</abbr>
                            <expan>διάμετρος</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z130r2_10">τῆς τομῆς ἡ ΒΔ, τετμήσθω δὲ</seg>
                    <seg n="11" type="line" facs="z130r2_11">ἡ ΒΔ κατὰ τὸ Κ, <seg type="word"
                                >ὥ<unclear>στ</unclear>ε</seg> διπλῶς</seg>
                    <seg n="12" type="line" facs="z130r2_12">εἶναι τὴν ΒΚ τῆς ΚΔ, κατὰ δὲ</seg>
                    <seg n="13" type="line" facs="z130r2_13">τὸ Τ, ὥστε τὴν ΔΒ πρὸς τὴν ΚΤ</seg>
                    <seg n="14" type="line" facs="z130r2_14">λόγον ἔχειν, ὃν τὰ <num>ΙΕ</num>
                        <expan>πρὸς</expan>
                        <num>Δ</num>· δῆλον</seg>
                    <seg n="15" type="line" facs="z130r2_15">οὖν, <expan>ὅτι</expan> ἡ ΚΤ μείζων
                        ἐστὶ τῆς <w part="I">μέ</w></seg>
                    <seg n="16" type="line" facs="z130r2_16"><w part="F">χρι</w> τοῦ ἄξονος. ἔστω
                        οὖν ἡ ΚΡ</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
