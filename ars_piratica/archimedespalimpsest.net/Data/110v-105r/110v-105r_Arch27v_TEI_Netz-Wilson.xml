    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="110v1" type="folio">
                    <seg n="1" type="line" facs="z110v1_01">ΕΗ, <expan>ὅτι</expan> ἔσται ὡς τὸ
                        τρίγωνον τὸ <w part="I">γε</w></seg>
                    <seg n="2" type="line" facs="z110v1_02"><w part="F">νόμενον</w>
                        <seg type="word">ἐ<supplied reason="lost">ν</supplied></seg> τῶι πρίσματι
                            <expan>πρὸς</expan> τὸ</seg>
                    <seg n="3" type="line" facs="z110v1_03"><seg type="word">τρ<supplied
                                reason="lost">ίγω</supplied><unclear>νον</unclear></seg>
                        <unclear>τὸ</unclear>
                        <unclear>ἐν</unclear> τῶι <choice>
                            <abbr>ἀποτμήμα</abbr>
                            <expan>ἀποτμήματι</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z110v1_04">τὸ ἐν τῶι ἀπὸ τοῦ κυλίνδρου,
                            <expan>οὕτως</expan></seg>
                    <seg n="5" type="line" facs="z110v1_05">ἡ ἀχθεῖσα ἐν <unclear>τῶι</unclear>
                            Δ<unclear>Η</unclear>
                        <w part="I">παραλλη</w></seg>
                    <seg n="6" type="line" facs="z110v1_06"><w part="F">λογράμμωι</w> παράλληλος
                        οὖσα τῆ</seg>
                    <seg n="7" type="line" facs="z110v1_07">Κ<unclear>Ζ</unclear>
                        <expan>πρὸς</expan> τὴν ἀποληφθεῖσαν ἀπὸ</seg>
                    <seg n="8" type="line" facs="z110v1_08">τῆς ΗΖ τοῦ ὀρθογωνίου κώνου</seg>
                    <seg n="9" type="line" facs="z110v1_09">τομῆς καὶ τῆς ΕΗ διαμέτρου. <w part="I"
                            >συμ</w></seg>
                    <seg n="10" type="line" facs="z110v1_10"><w part="F">πληρωθέντος</w> οὖν τοῦ τε
                        ΔΗ <seg type="unclearword">πα</seg></seg>
                    <seg n="11" type="line" facs="z110v1_11"><seg type="wordend"
                            >ραλληλο<unclear>γ</unclear>ράμμου</seg> ὑπὸ τῶν <w part="I">ἀγομέ</w></seg>
                    <seg n="12" type="line" facs="z110v1_12"><w part="F">νων</w> παρὰ τὴν ΚΖ καὶ τοῦ
                            <w part="I">τμή</w></seg>
                    <seg n="13" type="line" facs="z110v1_13"><w part="F">ματος</w> τοῦ περιεχομένου
                        ὑπό</seg>
                    <seg n="14" type="line" facs="z110v1_14">τε τῆς τοῦ ὀρθογωνίου κώνου <seg
                            type="word">το<supplied reason="lost">μῆς</supplied></seg></seg>
                    <seg n="15" type="line" facs="z110v1_15">καὶ τῆς ΕΗ <choice>
                            <abbr>μέτρου</abbr>
                            <expan>διαμέτρου</expan>
                        </choice> ὑπὸ <seg type="word"><unclear>τ</unclear>ῶν</seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἀ</supplied>
                        </seg></seg>
                    <seg n="16" type="line" facs="z110v1_16"><seg type="wordend"
                        >πολαμβανομένων</seg> ἐν <unclear>τῶ</unclear>
                        <seg type="suppliedword">
                            <choice>
                                <abbr>
                                    <supplied reason="lost">τμ</supplied>
                                </abbr>
                                <expan><supplied reason="lost">τμ</supplied>ή</expan>
                            </choice>
                        </seg></seg>
                    <seg n="17" type="line" facs="z110v1_17"><seg type="wordend">ματι</seg>· <seg
                            type="word">συμπληρωθέντο<unclear>ς</unclear></seg>
                        <seg type="word"><unclear>δ</unclear>ὲ</seg> καὶ</seg>
                    <seg n="18" type="line" facs="z110v1_18">τοῦ πρίσματος ὑπὸ τῶν <seg
                            type="unclearword">τ<unclear>ρ</unclear>ιγώ</seg></seg>
                </seg>
                <seg n="105r1" type="folio">
                    <seg n="1" type="line" facs="z105r1_01"><seg type="wordend"
                            ><unclear>ν</unclear>ων</seg> τῶν <seg type="word"><supplied
                                reason="lost"
                                >γ</supplied><unclear>ε</unclear>νομ<unclear>έ</unclear>ν<unclear>ων</unclear></seg>
                        <unclear>ἐν</unclear>
                        <seg type="word"><supplied reason="lost">αὐ</supplied>τῶι</seg>,</seg>
                    <seg n="2" type="line" facs="z105r1_02"><expan>καὶ</expan> τοῦ τμήματος τοῦ <w
                            part="I">ἀποτμη</w></seg>
                    <seg n="3" type="line" facs="z105r1_03"><w part="F">θέντος</w> ἀπὸ τοῦ <seg
                            type="word">κ<unclear>υ</unclear><supplied reason="lost"
                            >λίνδρου</supplied></seg>· <seg type="word"
                        >ἔστ<unclear>αι</unclear></seg>
                    </seg>
                    <seg n="4" type="line" facs="z105r1_04">τινὰ <seg type="word"
                            >μεγέ<unclear>θ</unclear>η</seg>
                        <seg type="word"><unclear>ἴσ</unclear>α</seg>
                        <seg type="word">ἀλλήλο<supplied reason="lost">ις,</supplied></seg>
                        <supplied reason="lost">τὰ</supplied>
                        <seg type="suppliedword">τ<supplied reason="lost">ρί</supplied></seg></seg>
                    <seg n="5" type="line" facs="z105r1_05"><seg type="wordend"
                            >γω<unclear>ν</unclear>α</seg> τὰ ἐν τῶι <seg type="word"
                                >πρίσμα<unclear>τι</unclear></seg>, <expan>καὶ</expan>
                        <w part="I">ἕ</w></seg>
                    <seg n="6" type="line" facs="z105r1_06"><w part="F">τερα</w> μεγέθη, αἵ εἰσιν
                            <seg type="word">εὐθεῖ<unclear>αι</unclear></seg>
                        <supplied reason="lost">ἐν</supplied></seg>
                    <seg n="7" type="line" facs="z105r1_07">τῶ ΔΗ <seg type="word">παραλλη<supplied
                                reason="lost">λογ</supplied>ράμμ<supplied reason="lost">ω</supplied></seg>
                        <w part="I">πα</w></seg>
                    <seg n="8" type="line" facs="z105r1_08"><w part="F">ράλληλοι</w> οὖσαι <seg
                            type="word">
                            <unclear>τ</unclear>
                            <supplied reason="lost">ῆ</supplied>
                            <unclear>ι</unclear>
                        </seg> ΖΚ <seg type="word">ἴ<supplied reason="lost"
                            >σ</supplied>α<unclear>ι</unclear></seg>
                        <seg type="unclearword">ἀ<unclear>λ</unclear></seg></seg>
                    <seg n="9" type="line" facs="z105r1_09"><seg type="wordend"
                            >λήλ<unclear>οις</unclear></seg>
                        <seg type="word"><supplied reason="lost">κα</supplied>ὶ</seg> ἔτι <seg
                            type="word"><unclear>τῶ</unclear>ι</seg> πλήθει ἴσα</seg>
                    <seg n="10" type="line" facs="z105r1_10"><seg type="word"
                            >το<unclear>ῖ</unclear>ς</seg> ἐν τῶ πρίσματι <seg type="word">
                            <choice>
                                <abbr>τρι<unclear>γ</unclear>ών</abbr>
                                <expan>τρι<unclear>γ</unclear>ών<supplied reason="lost"
                                    >οις</supplied></expan>
                            </choice>
                        </seg>·</seg>
                    <seg n="11" type="line" facs="z105r1_11">ἔσται δὲ καὶ <seg type="word"
                                >ἕτ<unclear>ε</unclear>ρα</seg>
                        <seg type="word">τρ<supplied reason="lost">ί</supplied>γωνα</seg> τὰ</seg>
                    <seg n="12" type="line" facs="z105r1_12"><seg type="word"
                            ><unclear>γ</unclear>ενόμενα</seg> ἐν τῶι <seg type="unclearword"
                            >ἀποτμηθέν</seg></seg>
                    <seg n="13" type="line" facs="z105r1_13"><seg type="wordend">
                            <unclear>τι</unclear>
                        </seg> ἴσα τῶι πλήθει <seg type="word">το<unclear>ῖς</unclear></seg>
                        <seg type="suppliedword">γενομ<unclear>έ</unclear><supplied reason="lost"
                            >ν</supplied></seg></seg>
                    <seg n="14" type="line" facs="z105r1_14"><seg type="word">οις</seg> ἐν τῶι
                        πρίσματι <seg type="suppliedword">τρι</seg></seg>
                    <seg n="15" type="line" facs="z105r1_15"><seg type="wordend">γώνοι<supplied
                                reason="lost">ς</supplied></seg>· καὶ αἱ <seg type="word"
                                >ἕτε<supplied reason="lost">ραι</supplied></seg>
                        <seg type="word">
                            <supplied reason="lost">εὐθ</supplied>
                            <unclear>εῖ</unclear>
                            <supplied reason="lost">αι</supplied>
                        </seg>
                    </seg>
                    <seg n="16" type="line" facs="z105r1_16">ἀπολαμβανόμεναι <seg type="word">
                            <unclear>ἀ</unclear>
                            <supplied reason="lost">πὸ</supplied>
                        </seg> τῶν</seg>
                </seg>
                <seg n="110v2" type="folio">
                    <seg n="1" type="line" facs="z110v2_01">ἀγομένων <choice>
                            <abbr>π</abbr>
                            <expan>παρὰ</expan>
                        </choice> τὴν Κ<supplied reason="lost">Ζ</supplied>
                        <seg type="word"><supplied reason="lost"
                            >μετ</supplied>α<unclear>ξ</unclear>ὺ</seg>
                    </seg>
                    <seg n="2" type="line" facs="z110v2_02">τῆς τοῦ <seg type="word"
                                >ὀρθογωνί<unclear>ο</unclear><supplied reason="lost">υ</supplied></seg>
                        <seg type="word"><supplied reason="lost">κώ</supplied>νου</seg></seg>
                    <seg n="3" type="line" facs="z110v2_03">τομῆς <seg type="word"
                            >κα<unclear>ὶ</unclear></seg> τῆς ΕΗ <seg type="word"
                                >ἴ<unclear>σ</unclear><supplied reason="lost">αι</supplied></seg>
                        <seg type="word"><unclear>τ</unclear>ῶ<unclear>ι</unclear></seg></seg>
                    <seg n="4" type="line" facs="z110v2_04">πλήθει ταῖς ἐν τῶι ΔΗ <seg
                            type="suppliedword">π<supplied reason="lost">αραλ</supplied></seg></seg>
                    <seg n="5" type="line" facs="z110v2_05">
                        <seg type="wordend">ληλογράμμωι</seg> ἠγμέναις <seg type="word">
                            <choice>
                                <abbr>π</abbr>
                                <expan>π<supplied reason="lost">αρὰ</supplied></expan>
                            </choice>
                        </seg>
                    </seg>
                    <seg n="6" type="line" facs="z110v2_06">τὴν ΚΖ· καὶ ἔσται πάντα τὰ</seg>
                    <seg n="7" type="line" facs="z110v2_07">τρίγωνα τὰ ἐν τῶι πρίσματι</seg>
                    <seg n="8" type="line" facs="z110v2_08">πρὸς πάντα τὰ τρίγωνα τὰ</seg>
                    <seg n="9" type="line" facs="z110v2_09">ἐν τῶι ἀποτμηθέντι τῶι ἀπὸ</seg>
                    <seg n="10" type="line" facs="z110v2_10">τοῦ κυλίνδρου ἀφηρημένα,</seg>
                    <seg n="11" type="line" facs="z110v2_11">οὕτως πᾶσαι αἱ εὐθεῖαι αἱ ἐν</seg>
                    <seg n="12" type="line" facs="z110v2_12">τῶι ΔΗ παραλληλογράμμωι
                        <expan>πρὸς</expan></seg>
                    <seg n="13" type="line" facs="z110v2_13">πάσας τὰς εὐθείας τὰς <w part="I"
                        >μετα</w></seg>
                    <seg n="14" type="line" facs="z110v2_14"><w part="F">ξὺ</w> τῆς τοῦ ὀρθογωνίου
                        κώνου</seg>
                    <seg n="15" type="line" facs="z110v2_15">τομῆς καὶ τῆς ΕΗ εὐθείας.
                        <expan>καὶ</expan></seg>
                    <seg n="16" type="line" facs="z110v2_16">ἐκ μὲν τῶν ἐν τῶι πρίσματι <w part="I"
                            >τρι</w></seg>
                    <seg n="17" type="line" facs="z110v2_17"><w part="F">γώνων</w> σύγκειται τὸ
                        πρίσμα, ἐκ</seg>
                    <seg n="18" type="line" facs="z110v2_18">δὲ τῶν ἐν τῶι ἀποτμήματι τῶι</seg>
                    <seg n="19" type="line" facs="z110v2_19"><seg type="word"
                        >ἀπ<unclear>ὸ</unclear></seg> τοῦ <seg type="word">κυ<supplied reason="lost"
                                >λ</supplied><unclear>ί</unclear>ν<supplied reason="lost"
                            >δ</supplied>ρου</seg> τὸ <seg type="suppliedword">ἀ<supplied
                                reason="lost"
                        >π</supplied><unclear>ό</unclear>τμ<unclear>η</unclear></seg></seg>
                </seg>
                <seg n="105r2" type="folio">
                    <seg n="1" type="line" facs="z105r2_01"><seg type="wordend">μα</seg>, ἐκ δὲ τῶν
                        εὐθειῶν τῶν ἐν</seg>
                    <seg n="2" type="line" facs="z105r2_02">τῶι ΔΗ παραλληλογράμμωι
                        <expan>τῶν</expan></seg>
                    <seg n="3" type="line" facs="z105r2_03">παρὰ τὴν ΚΖ τὸ ΔΗ <w part="I"
                        >παραλλη</w></seg>
                    <seg n="4" type="line" facs="z105r2_04"><w part="F">λόγραμμον</w>, ἐκ δὲ τῶν
                            <seg type="word"><supplied reason="lost">εὐ</supplied>θειῶν</seg>
                    </seg>
                    <seg n="5" type="line" facs="z105r2_05">μεταξὺ τῆς τοῦ ὀρθογωνίου <w part="I"
                        >κώ</w></seg>
                    <seg n="6" type="line" facs="z105r2_06"><w part="F">νου</w> τομῆς καὶ τῆς ΕΗ τὸ
                            <seg type="unclearword">τμῆ</seg></seg>
                    <seg n="7" type="line" facs="z105r2_07"><seg type="wordend"
                            ><unclear>μ</unclear>α</seg> τῆς παραβολῆς· ὡς <expan>ἄρα</expan> τὸ
                            <seg type="expandedword">
                            <choice>
                                <abbr>πρ</abbr>
                                <expan>πρίσ</expan>
                            </choice>
                        </seg></seg>
                    <seg n="8" type="line" facs="z105r2_08"><seg type="wordend">μα</seg>
                        <expan>πρὸς</expan> τὸ ἀπότμημα <sic>
                            <choice>
                                <abbr>το</abbr>
                                <expan>τοοῦ</expan>
                            </choice>
                        </sic> ἀπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z105r2_09"><seg type="word"
                                ><unclear>κ</unclear>υ<unclear>λ</unclear>ίνδρου</seg>, οὕτως τὸ ΔΗ
                            <seg type="suppliedword">πα<supplied reason="lost">ρ</supplied>αλ</seg></seg>
                    <seg n="10" type="line" facs="z105r2_10"><seg type="wordend">ληλόγραμμον</seg>
                        <expan>πρὸς</expan> τὸ ΕΖΗ τμῆμα</seg>
                    <seg n="11" type="line" facs="z105r2_11">τὸ περιεχόμενον ὑπὸ τῆς τοῦ</seg>
                    <seg n="12" type="line" facs="z105r2_12">ὀρθογωνίου <seg type="word"
                                >κώνο<unclear>υ</unclear></seg> τομῆς καὶ</seg>
                    <seg n="13" type="line" facs="z105r2_13">τῆς Ε<unclear>Η</unclear>
                        <seg type="word">εὐ<unclear>θεία</unclear>ς</seg>· <seg type="word"
                                ><unclear>ἡ</unclear>μιόλιον</seg> δέ <expan>ἐστι</expan></seg>
                    <seg n="14" type="line" facs="z105r2_14">τὸ ΔΗ <seg type="word"
                                >πα<unclear>ρ</unclear><supplied reason="lost"
                                >α</supplied>λλ<supplied reason="lost"
                                >ηλό</supplied>γραμμο<unclear>ν</unclear></seg> τοῦ</seg>
                    <seg n="15" type="line" facs="z105r2_15"><seg type="word"
                            >τμήματ<unclear>ος</unclear></seg> τοῦ <seg type="word">
                            <choice>
                                <abbr>π<supplied reason="lost"
                                        >ε</supplied><unclear>ρ</unclear><supplied reason="lost"
                                    >ι</supplied>εχομέν</abbr>
                                <expan>π<supplied reason="lost"
                                        >ε</supplied><unclear>ρ</unclear><supplied reason="lost"
                                    >ι</supplied>εχομένου</expan>
                            </choice>
                        </seg></seg>
                    <seg n="16" type="line" facs="z105r2_16">ὑπὸ τῆς τοῦ ὀρθογωνίου <seg type="word"
                                >κ<unclear>ώ</unclear><supplied reason="lost">νου</supplied></seg></seg>
                    <seg n="17" type="line" facs="z105r2_17"><seg type="word"><supplied
                                reason="lost">το</supplied><unclear>μ</unclear>ῆς</seg> καὶ <seg
                            type="word">τῆ<supplied reason="lost">ς</supplied></seg> ΕΗ <seg
                            type="word">εὐ<unclear>θ</unclear>εία<unclear>ς</unclear></seg>· <w
                            part="I">δέ</w></seg>
                </seg>
            </ab>
        </body>
    </text>
