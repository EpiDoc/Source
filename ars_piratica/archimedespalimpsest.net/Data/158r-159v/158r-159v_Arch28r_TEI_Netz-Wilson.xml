    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="158r1" type="folio">
                    <seg n="1" type="line" facs="z158r1_01"><w part="F">δεικται</w> γὰρ τοῦτο ἐν
                        τοῖς <choice>
                            <abbr>πρότερο</abbr>
                            <expan>πρότερον</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z158r1_02"><seg type="word"
                                >ἐκδ<unclear>ε</unclear>δο<supplied reason="lost"
                        >μ</supplied>ένοις</seg>· ἡμιόλιον ἄρα <expan>ἐστὶ</expan></seg>
                    <seg n="3" type="line" facs="z158r1_03">
                        <seg type="word">
                            <unclear>κα</unclear>
                            <supplied reason="lost">ὶ</supplied>
                        </seg>
                        <supplied reason="lost">τὸ</supplied>
                        <seg type="word">
                            <supplied reason="lost">πρίσ</supplied>
                            <unclear>μα</unclear>
                        </seg>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice>
                        <choice>
                            <abbr>ἀποτμήματ</abbr>
                            <expan>ἀποτμήματος</expan>
                        </choice>
                    </seg>
                    <seg n="4" type="line" facs="z158r1_04">τοῦ <seg type="word"><supplied
                                reason="lost">ἀ</supplied>φηρημένου</seg> ἀπὸ τοῦ <seg
                            type="unclearword">κυ<unclear>λίν</unclear></seg></seg>
                    <seg n="5" type="line" facs="z158r1_05"><seg type="wordend">δρου</seg>· οἵων
                            <expan>ἄρα</expan> ἐστὶ τὸ ἀπότμημα</seg>
                    <seg n="6" type="line" facs="z158r1_06"><seg type="word">το<supplied
                                reason="lost">ῦ</supplied></seg>
                        <seg type="word"><supplied reason="lost">κυ</supplied>λίνδρου</seg> δύο,
                        τοιούτων <expan>ἔσται</expan> τὸ</seg>
                    <seg n="7" type="line" facs="z158r1_07">πρίσμα <seg type="word"
                            >τρ<unclear>ι</unclear>ῶν</seg> τοιούτων ἐστὶν τὸ</seg>
                    <seg n="8" type="line" facs="z158r1_08">ὅλον πρίσμα τὸ περὶ ὅλον τὸν</seg>
                    <seg n="9" type="line" facs="z158r1_09">κύλινδρον <num>ΙΒ</num>
                        <expan>διὰ</expan> τὸ <num>Δ</num>
                        <expan>εἶναι</expan> τὸ ἕτερον</seg>
                    <seg n="10" type="line" facs="z158r1_10">τοῦ ἑτέρου· οἵων <expan>ἄρα</expan> τὸ
                        ἀπότμημα </seg>
                    <seg n="11" type="line" facs="z158r1_11">τοῦ <seg type="word">κ<supplied
                                reason="lost">υ</supplied>λίνδρου</seg> δύο, τοιούτων <choice>
                            <abbr>ἐστὶ</abbr>
                            <expan>ἐστὶν</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z158r1_12">τὸ ὅλον πρίσμα <num>ΙΒ</num>· ὥστε τὸ
                            <w part="I">τμῆ</w></seg>
                    <seg n="13" type="line" facs="z158r1_13"><w part="F">μα</w> τὸ ἀποτμηθὲν ἀπὸ τοῦ <choice>
                            <abbr>ὅλ</abbr>
                            <expan>ὅλου</expan>
                        </choice>
                    </seg>
                    <seg n="14" type="line" facs="z158r1_14">κυλίνδρου ἕκτον μέρος <unclear>
                            <expan>ἐστὶ</expan>
                        </unclear>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice>
                        <seg type="word"><supplied reason="lost"
                                >ὅ</supplied><unclear>λ</unclear>ο<supplied reason="lost"
                            >υ</supplied></seg></seg>
                    <seg n="15" type="line" facs="z158r1_15">πρίσματος.</seg>
                    <figure n="14.1">
                        <figDesc xml:lang="eng">Figure 14.1</figDesc>
                    </figure>
                </seg>
                <seg n="159v1" type="folio">
                    <seg n="1" type="line" facs="z159v1_01">ἜΣΤω πρίσμα <seg type="word"><supplied
                                reason="lost">ὀ</supplied>ρθὸν</seg>
                        <seg type="word">τετραγώνο<supplied reason="lost">υ</supplied>ς</seg></seg>
                    <seg n="2" type="line" facs="z159v1_02">ἔχον βάσεις, ὧν μία ἔστω τὸ ΑΒ ΓΔ</seg>
                    <seg n="3" type="line" facs="z159v1_03">τετράγωνον, καὶ ἀπειλήφθω εἰς</seg>
                    <seg n="4" type="line" facs="z159v1_04">τὸ πρίσμα ὁ κύλινδρος, οὗ <seg
                            type="word">
                            <choice>
                                <abbr>βάσ</abbr>
                                <expan>βάσ<supplied reason="lost">ις</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="5" type="line" facs="z159v1_05">ἔστω ὁ ΕΖ ΗΘ κύκλος· ἐφάψεται</seg>
                    <seg n="6" type="line" facs="z159v1_06">δὴ οὗτος τῶν τοῦ <seg type="word"
                                >τετραγώνο<unclear>υ</unclear></seg>
                        <w part="I">πλευ</w></seg>
                    <seg n="7" type="line" facs="z159v1_07"><w part="F">ρῶν</w> κατὰ τὰ ΕΖ ΗΘ
                        σημεῖα· <w part="I">κέν</w></seg>
                    <seg n="8" type="line" facs="z159v1_08"><w part="F">τρον</w> δὲ αὐτοῦ ἔστω τὸ Κ,
                        διὰ δὲ τῆς</seg>
                    <seg n="9" type="line" facs="z159v1_09">ΕΗ <seg type="word"
                            >δια<unclear>μ</unclear>έτρου</seg> καὶ τῆς <seg type="word"
                                >πλ<unclear>ε</unclear>υρᾶ<unclear>ς</unclear></seg></seg>
                    <seg n="10" type="line" facs="z159v1_10">τοῦ <seg type="word"
                            ><unclear>τ</unclear>ετραγώνου</seg> τοῦ ἐν <seg type="word">τῆ<supplied
                                reason="lost">ι</supplied></seg>
                        <seg type="suppliedword">ἑτ<supplied reason="lost">έ</supplied></seg></seg>
                    <seg n="11" type="line" facs="z159v1_11"><seg type="wordend"
                            >ρα<unclear>ι</unclear></seg>
                        <seg type="word"><supplied reason="lost">βά</supplied><unclear>σ</unclear>ει</seg>
                        <supplied reason="lost">τοῦ</supplied>
                        <seg type="word"><unclear>πρ</unclear>ίσμα<unclear>τ</unclear><supplied
                                reason="lost">ος</supplied></seg> τοῦ</seg>
                    <seg n="12" type="line" facs="z159v1_12"><seg type="word">κα<supplied
                                reason="lost">τὰ</supplied></seg>
                        <seg type="word"><unclear>τ</unclear>ὴν</seg> ΓΔ <seg type="word"
                                >ἐπί<supplied reason="lost">πε</supplied>δο<supplied reason="lost"
                            >ν</supplied></seg>
                        <seg type="word"><supplied reason="lost">ἤχ</supplied>θω</seg>·</seg>
                </seg>
                <seg n="158r2" type="folio">
                    <seg n="1" type="line" facs="z158r2_01">τοῦτο δὴ τὸ ἐπίπεδον ἀποτέμνει</seg>
                    <seg n="2" type="line" facs="z158r2_02"><seg type="word"
                                >πρ<unclear>ίσ</unclear><supplied reason="lost">μα</supplied></seg>
                        <seg type="word"><supplied reason="lost"
                        >ἀ</supplied><unclear>π</unclear>ὸ</seg> τοῦ <seg type="word"
                                >ὅ<unclear>λ</unclear><supplied reason="lost">ου</supplied></seg>
                        <seg type="word"><supplied reason="lost">πρί</supplied>σματος</seg>
                        <supplied reason="lost">καὶ</supplied>
                    </seg>
                    <seg n="3" type="line" facs="z158r2_03">ἀπὸ <seg type="word">τῆ<supplied
                                reason="lost">ς</supplied></seg>
                        <seg type="word">
                            <choice>
                                <abbr>
                                    <supplied reason="lost">βάσε</supplied>
                                </abbr>
                                <expan>
                                    <supplied reason="lost">βάσεως</supplied>
                                </expan>
                            </choice>
                        </seg> καὶ <seg type="word">ἀπ<supplied reason="lost">ὸ</supplied></seg>
                        <supplied reason="lost">τοῦ</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">κυ</supplied>
                        </seg></seg>
                    <seg n="4" type="line" facs="z158r2_04"><seg type="wordend"
                                >λ<unclear>ί</unclear>νδρ<unclear>ου</unclear></seg>. <seg
                            type="word"><supplied reason="lost"
                                >τ</supplied>μ<unclear>ῆ</unclear><supplied reason="lost"
                            >μα</supplied></seg>
                        <seg type="word"><unclear>τ</unclear>οῦτο</seg>
                        <seg type="word"><unclear>ἔ</unclear>στ<unclear>αι</unclear></seg>
                        <supplied reason="lost">δὲ</supplied>
                        <seg type="word"><unclear>τ</unclear>ὸ</seg></seg>
                    <seg n="5" type="line" facs="z158r2_05">τμῆμα τὸ ἀποτμηθὲν ἀπὸ</seg>
                    <seg n="6" type="line" facs="z158r2_06">τοῦ κυλίνδρου <seg type="word"><supplied
