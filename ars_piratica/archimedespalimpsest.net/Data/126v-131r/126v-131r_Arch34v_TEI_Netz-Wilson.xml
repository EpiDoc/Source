    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="126v1" type="folio">
                    <seg n="1" type="line" facs="z126v1_01"><seg type="word"
                            >περι<unclear>φ</unclear>ερείας </seg> φερόμενον τὰν</seg>
                    <seg n="2" type="line" facs="z126v1_02">ΘΚΖ <seg type="word">
                            <supplied reason="lost">πε</supplied>ριφέρειαν</seg>, τὸ δὲ Α τὰν ΑΕ</seg>
                    <seg n="3" type="line" facs="z126v1_03">εὐθεῖαν, <expan>καὶ</expan> πάλιν τό τε
                        Α σημεῖον</seg>
                    <seg n="4" type="line" facs="z126v1_04">τὰν ΑΔ γραμμὰν καὶ τὸ Θ τὰν</seg>
                    <seg n="5" type="line" facs="z126v1_05">Θ<unclear>ΚΑ</unclear> περιφέρειαν,
                        ἑκάτερον <seg type="unclearword">ἰ</seg></seg>
                    <seg n="6" type="line" facs="z126v1_06"><seg type="wordend"
                            >σο<unclear>τ</unclear>αχέως</seg> αὐτὸ ἑωυτῶ <seg type="clearword"
                            >φερόμε</seg></seg>
                    <seg n="7" type="line" facs="z126v1_07"><seg type="wordend">νον·</seg> δῆλον οὖν
                            <expan>ὅτι</expan> τὸν αὐτὸν ἔχοντι</seg>
                    <seg n="8" type="line" facs="z126v1_08">λόγον ἁ ΑΕ ποτὶ τὰν
                        <unclear>Α</unclear>Δ, ὃν ἁ ΘΚ<supplied reason="lost">Ζ</supplied></seg>
                    <seg n="9" type="line" facs="z126v1_09">περιφέρεια ποτὶ τὰν ΘΚΗ <seg
                            type="clearword">περι</seg></seg>
                    <seg n="10" type="line" facs="z126v1_10"><seg type="wordend">φέρειαν</seg>
                        δέδεικται γὰρ τοῦτο ἔξω</seg>
                    <seg n="11" type="line" facs="z126v1_11">ἐν τοῖς πρώτοις.</seg>
                </seg>
                <seg n="126v1" type="folio">
                    <seg n="11" type="line" facs="z126v1_11"><seg type="word">
                            <supplied reason="lost">ὁ</supplied>μοίως</seg> δὲ <seg type="clearword"
                            >δειχθή</seg></seg>
                    <seg n="12" type="line" facs="z126v1_12"><seg type="wordend">σεται</seg>, καὶ εἴ
                        κα ἑτέρα τᾶν <seg type="clearword">
                            <choice>
                                <abbr>ποτιπιπτ</abbr>
                                <expan>ποτιπιπτου</expan>
                            </choice>
                        </seg></seg>
                    <seg n="13" type="line" facs="z126v1_13"><seg type="wordend">σᾶν</seg> ἐπὶ τὸ
                        πέρας τᾶς ἕλικος <seg type="clearword">πο</seg></seg>
                    <seg n="14" type="line" facs="z126v1_14"><seg type="wordend">τιπίπτη</seg>, τὸ
                        αὐτὸ συμβαίνειν.</seg>
                    <figure n="14.1">
                        <figDesc>Figure 14.1</figDesc>
                    </figure>
                </seg>
                <seg n="131r1" type="folio">
                    <seg n="1" type="line" facs="z131r1_01">εἰ κα ποτὶ τὰν ἐν τᾶι δευτέραι <seg
                            type="clearword">
                            <expan>περι</expan>
                        </seg></seg>
                    <seg n="2" type="line" facs="z131r1_02"><seg type="wordend">φορᾶι</seg>
                        γεγραμμέναν ἕλικα <seg type="clearword">πο</seg></seg>
                    <seg n="3" type="line" facs="z131r1_03"><seg type="wordend">τιπίπτοντι</seg>
                        εὐθεῖαι ἀπὸ τᾶς <seg type="clearword">ἀρ</seg></seg>
                    <seg n="4" type="line" facs="z131r1_04"><seg type="wordend">χᾶς</seg> τᾶς
                        ἕλικος, τὸν αὐτὸν <seg type="clearword">ἑξοῦν</seg></seg>
                    <seg n="5" type="line" facs="z131r1_05"><seg type="wordend">τι</seg> λόγον αἱ
                        εὐθεῖαι ποτ’ ἀλλάλαν,</seg>
                    <seg n="6" type="line" facs="z131r1_06">ὧν αἱ εἰρημέναι περιφέρειαι</seg>
                    <seg n="7" type="line" facs="z131r1_07">μεθ’ ὅλας τᾶς τοῦ κύκλου <seg
                            type="clearword">περιφε</seg></seg>
                    <seg n="8" type="line" facs="z131r1_08"><seg type="wordend">ρείας</seg>
                        λαμβανομένας.</seg>
                </seg>
                <seg n="131r1" type="folio">
                    <seg n="8" type="line" facs="z131r1_08">ἔστω</seg>
                    <seg n="9" type="line" facs="z131r1_09">ἕλιξ, ἐφ’ ἇς αἱ ΑΒ ΓΔΘ,
                        <unclear>ἁ</unclear> μὲν ΑΒΓ</seg>
                    <seg n="10" type="line" facs="z131r1_10">ΔΘ ἐν τᾶι πρώται <seg type="word">
                            <unclear>π</unclear>εριφορᾶι</seg></seg>
                    <seg n="11" type="line" facs="z131r1_11">γεγραμμέναι, ἁ <seg type="word">
                            <unclear>δ</unclear>ὲ</seg> ΘΛ <unclear>Ε</unclear>Μ ἐν τᾶ</seg>
                    <seg n="12" type="line" facs="z131r1_12">δευτέραι, καὶ <seg type="word">
                            <unclear>πο</unclear>τιπίπτοντι</seg>
                        <seg type="clearword">εὐ</seg></seg>
                    <seg n="13" type="line" facs="z131r1_13"><seg type="wordend">θεῖαι</seg> αἱ ΑΕ
                            Α<supplied reason="lost">Λ</supplied>. <seg type="word">δει<supplied
                                reason="lost">κ</supplied>τέον</seg>, ὅτι <seg type="word">
                            <choice>
                                <abbr>τὸ</abbr>
                                <expan><unclear>τὸ</unclear>ν</expan>
                            </choice>
                        </seg></seg>
                </seg>
                <seg n="126v2" type="folio">
                    <seg n="1" type="line" facs="z126v2_01">αὐτὸν ἔχοντι λόγον ἁ ΑΛ ποτὶ <choice>
                            <abbr>τὰ</abbr>
                            <expan>τὰν</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z126v2_02">ΑΕ, ὃν ἁ ΘΚΖ περιφέρεια μεθ’ <choice>
                            <abbr>ὅλ</abbr>
                            <expan>ὅλας</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z126v2_03">τᾶς τοῦ <seg type="word">
                            <supplied reason="lost">κύκλ</supplied>ου</seg> περιφερείας
                        <expan>πρὸς</expan></seg>
                    <seg n="4" type="line" facs="z126v2_04">ΘΚΗ <seg type="word">με<supplied
                                reason="lost">θ’</supplied>
                        </seg> ὅλας τᾶς τοῦ κύκλου <seg type="clearword">περι</seg></seg>
                    <seg n="5" type="line" facs="z126v2_05">
                        <seg type="wordend">φερείας</seg>. </seg>
                </seg>
                <seg n="126v2" type="folio">
                    <seg n="5" type="line" facs="z126v2_05">ἐν ὅσω γὰρ χρόνωι τὸ</seg>
                    <seg n="6" type="line" facs="z126v2_06">Α σαμεῖον κατὰ τᾶς εὐθείας <seg
                            type="clearword">φε</seg></seg>
                    <seg n="7" type="line" facs="z126v2_07"><seg type="wordend">ρόμενον</seg> τὰν ΑΛ
                        γραμμὰν <seg type="clearword">δια</seg></seg>
                    <seg n="8" type="line" facs="z126v2_08"><seg type="wordend">πορεύεται</seg>, καὶ
                        τὸ Θ σαμεῖον κατὰ</seg>
                    <seg n="9" type="line" facs="z126v2_09">τᾶς τοῦ κύκλου περιφερείας <seg
                            type="clearword">φε</seg></seg>
                    <seg n="10" type="line" facs="z126v2_10"><seg type="wordend">ρόμενον</seg> ὅλαν
                        τε τὰν τοῦ κύκλου</seg>
                    <seg n="11" type="line" facs="z126v2_11">περιφέρειαν καὶ ἐπὶ τὰν ΘΚΖ</seg>
                    <seg n="12" type="line" facs="z126v2_12">περιφέρειαν διαπορεύεται,
                        <expan>καὶ</expan></seg>
                    <seg n="13" type="line" facs="z126v2_13">πάλιν τὸ Α σαμεῖον τὰν ΑΕ <choice>
                            <abbr>εὐθεῖ</abbr>
                            <expan>εὐθεῖαν</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z126v2_14">κατὰ τὸ Ε ὅλαν τε τὰν τοῦ κύκλου</seg>
                    <seg n="15" type="line" facs="z126v2_15">περιφέρειαν καὶ ἐπὶ τὰν ΘΚΗ,</seg>
                    <seg n="16" type="line" facs="z126v2_16">ἑκάτερον ἰσοταχέως αὐτὸ <seg
                            type="clearword">ἑω</seg></seg>
                    <seg n="17" type="line" facs="z126v2_17"><seg type="wordend">υτῶι</seg>
                        φερόμενον· δῆλον οὖν <expan>ὅτι</expan></seg>
                    <seg n="18" type="line" facs="z126v2_18">τὸν αὐτὸν ἔχοντι λόγον ἁ ΑΛ</seg>
                    <seg n="19" type="line" facs="z126v2_19"><seg type="word">γρ<supplied
                                reason="lost">α</supplied>μμὰ</seg> ποτὶ τὰν ΑΕ, ὃν <supplied
                            reason="lost">ἁ</supplied> ΘΚ<supplied reason="lost">Ζ</supplied></seg>
                </seg>
                <seg n="131r2" type="folio">
                    <seg n="1" type="line" facs="z131r2_01">περιφέρεια μεθ’ ὅλας τᾶς τοῦ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλου</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z131r2_02">περιφερείας ποτὶ τὰν ΘΚΗ <seg
                            type="clearword">πε</seg></seg>
                    <seg n="3" type="line" facs="z131r2_03"><seg type="wordend">ριφέρειαν</seg> μεθ’
                        ὅλας τᾶς τοῦ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλου</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z131r2_04">περιφερείας.</seg>
                </seg>
                <seg n="131r2" type="folio">
                    <seg n="4" type="line" facs="z131r2_04">τὸν αὐτὸν δὲ <seg type="clearword"
                        >τρό</seg></seg>
                    <seg n="5" type="line" facs="z131r2_05"><seg type="wordend">πον</seg>
                        δειχθήσεται, καὶ εἴ κα ποτὶ</seg>
                    <seg n="6" type="line" facs="z131r2_06">τὰν ἐν <seg type="word">τᾶ<supplied
                                reason="lost">ι</supplied>
                        </seg> τρίται περιφορᾶι</seg>
                    <seg n="7" type="line" facs="z131r2_07">γεγραμμέναν ἕλικα <seg type="clearword"
                            >ποτιπε</seg></seg>
                    <seg n="8" type="line" facs="z131r2_08"><seg type="wordend">σέωντι</seg>
                        εὐθεῖαι, τὸν αὐτὸν <choice>
                            <abbr>λόγο</abbr>
                            <expan>λόγον</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z131r2_09">ἑξοῦντι ποτ’ ἀλλάλας, ὃν ἁ <seg
                            type="clearword">εἰ</seg></seg>
                    <seg n="10" type="line" facs="z131r2_10"><seg type="wordend">ρημένα</seg>
                        περιφέρεια μεθ’ <choice>
                            <abbr>ὅλ</abbr>
                            <expan>ὅλας</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z131r2_11">τᾶς τοῦ κύκλου περιφερείας δὶς</seg>
                    <seg n="12" type="line" facs="z131r2_12">λαμβανομένας· ὁμοίως δὲ καὶ</seg>
                    <seg n="13" type="line" facs="z131r2_13">αἱ ποτὶ τὰς ἄλλας <seg type="word"
                                >ἕλι<supplied reason="lost">κ</supplied>ας</seg>
                        <seg type="suppliedword">πο</seg></seg>
                    <seg n="14" type="line" facs="z131r2_14"><seg type="wordend">τιπίπτ<supplied
                                reason="lost">ου</supplied>σαι</seg> δείκνυται <expan>ὅτι</expan>
                        τὸν</seg>
                    <seg n="15" type="line" facs="z131r2_15">αὐτὸν ἔχοντι λόγον, ὃν αἱ <seg
                            type="clearword">εἰρη</seg></seg>
                    <seg n="16" type="line" facs="z131r2_16"><seg type="wordend">μέναι</seg>
                        περιφέρειαι μεθ’ ὅλας τᾶς</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
