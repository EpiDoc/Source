    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="59v1" type="folio">
                    <seg n="1" type="line" facs="z59v1_01">τᾶν ΑΒ ΓΔ ΕΖ ΗΘ τετραγώνοις·</seg>
                    <seg n="2" type="line" facs="z59v1_02">λοιπὸν δὲ ἐπιδείξομεν ὅτι τὰ <seg
                            type="clearword">δι</seg></seg>
                    <seg n="3" type="line" facs="z59v1_03"><seg type="wordend">πλάσια</seg>
                        <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice> περιεχομένων <seg type="clearword">ὑ</seg></seg>
                    <seg n="4" type="line" facs="z59v1_04"><seg type="wordend">πὸ</seg> τῶν τμαμάτων
                        ἐν ε <seg type="word">κα<unclear>ι</unclear>
                        </seg> τε</seg>
                    <seg n="5" type="line" facs="z59v1_05">γραμμᾶι τᾶν ἰσᾶν τᾶι Α <seg
                            type="clearword">ποτι</seg></seg>
                    <seg n="6" type="line" facs="z59v1_06"><seg type="wordend">λαβόντα</seg> τὸ
                        περιεχόμενον ὑπό</seg>
                    <seg n="7" type="line" facs="z59v1_07">τε τᾶς Θ καὶ τᾶς ἴσας <seg type="word">
                                πάσαι<supplied reason="lost">ς</supplied>
                        </seg></seg>
                    <seg n="8" type="line" facs="z59v1_08">ταῖς ΑΒ ΓΔ ΕΖ ΗΘ ἴσα ἐντὶ τοῖς</seg>
                    <seg n="9" type="line" facs="z59v1_09">ἀπὸ τῶν ΑΒ ΓΔ ΕΖ ΗΘ. καὶ ἐπὶ δύο</seg>
                    <seg n="10" type="line" facs="z59v1_10">μὲν τὰ ὑπὸ ΒΙ περιεχόμενα ἴσα</seg>
                    <seg n="11" type="line" facs="z59v1_11">δυσὶ τοῖς ὑπὸ τῶν ΒΘ <seg
                            type="clearword">περιεχο</seg></seg>
                    <seg n="12" type="line" facs="z59v1_12"><seg type="wordend">μένοις</seg>, δύο δὲ
                        τὰ ὑπὸ τῶν ΚΓ <seg type="clearword">ἴ</seg></seg>
                    <seg n="13" type="line" facs="z59v1_13"><seg type="wordend">σα</seg> τῶι
                        περιεχομένω ὑπό <seg type="word">τ<unclear>ε</unclear>
                        </seg>
                        <seg type="word">
                            <unclear>τ</unclear>ᾶς</seg></seg>
                    <seg n="14" type="line" facs="z59v1_14">Θ καὶ τᾶς τετραπλασία <supplied
                            reason="lost">τᾶς</supplied></seg>
                    <seg n="15" type="line" facs="z59v1_15">Γ διὰ τὸ τὰν Κ διπλασίονα <seg
                            type="clearword">εἶ</seg></seg>
                    <seg n="16" type="line" facs="z59v1_16"><seg type="wordend">μεν</seg> τᾶς Θ, δύο
                        δὲ τὰ ὑπὸ τῶν Δ<supplied reason="lost">Λ</supplied></seg>
                    <seg n="17" type="line" facs="z59v1_17">ἴσα τῶι ὑπὸ τᾶς Θ καὶ τᾶς <seg
                            type="clearword">ἑξα</seg></seg>
                    <seg n="18" type="line" facs="z59v1_18">
                        <seg type="wordend">πλασίας</seg> Δ <expan>διὰ</expan> τὰν Λ <seg
                            type="clearword">τριπλα</seg>
                    </seg>
                </seg>
                <seg n="62r1" type="folio">
                    <seg n="1" type="line" facs="z62r1_01">σίαν εἶμεν τᾶς Θ, ὁμοίως δὲ
                        <expan>καὶ</expan></seg>
                    <seg n="2" type="line" facs="z62r1_02">τἄλλα τὰ διπλάσια τὰ <seg
                            type="clearword">περιε</seg></seg>
                    <seg n="3" type="line" facs="z62r1_03"><seg type="wordend">χόμενα</seg> ὑπὸ τᾶν <choice>
                            <abbr>τμαμάτω</abbr>
                            <expan>τμαμάτων</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z62r1_04"><seg type="word">
                            <supplied reason="lost">ἴσ</supplied>α</seg> ἐντὶ τῶι περιεχομένωι <seg
                            type="clearword">ὑ</seg></seg>
                    <seg n="5" type="line" facs="z62r1_05"><seg type="wordend">πό</seg> τε τᾶς Θ καὶ
                        τᾶς <seg type="clearword">πολλαπλα</seg></seg>
                    <seg n="6" type="line" facs="z62r1_06"><seg type="wordend">σίας</seg>
                        <seg type="word">
                            <unclear>ἀ</unclear>εὶ</seg> κατὰ τοὺς ἑξῆς <seg type="clearword"
                        >ἀριθ</seg></seg>
                    <seg n="7" type="line" facs="z62r1_07"><seg type="wordend">μοὺς</seg>
                        <seg type="word">ἀρτίο<unclear>υ</unclear>ς</seg> τᾶς <seg type="word"
                                >ἑπομέν<unclear>α</unclear>ς</seg></seg>
                    <seg n="8" type="line" facs="z62r1_08"><seg type="word">
                            <supplied reason="lost">γ</supplied>ραμμᾶς</seg>, τὰ οὖν σύμπαντα <seg
                            type="clearword">πο</seg></seg>
                    <seg n="9" type="line" facs="z62r1_09"><seg type="wordend">τιλαβόντα</seg> τὸ
                        περιεχόμενον <seg type="clearword">ὑ</seg></seg>
                    <seg n="10" type="line" facs="z62r1_10"><seg type="wordend">πό</seg> τε τᾶς Θ
                        καὶ τᾶς <seg type="word">ἴσα<unclear>ς</unclear>
                        </seg>
                        <seg type="clearword">πά</seg></seg>
                    <seg n="11" type="line" facs="z62r1_11"><seg type="wordend">σαις</seg> τᾶν ΑΒ ΓΔ
                        ΕΖ ΗΘ <seg type="word">
                            <choice>
                                <abbr>ἐσοῦντ</abbr>
                                <expan>ἐσοῦν<unclear>τ</unclear>αι</expan>
                            </choice>
                        </seg></seg>
                    <seg n="12" type="line" facs="z62r1_12">ἴσα τῶι <seg type="word">
                            <unclear>π</unclear><supplied reason="lost"
                                >ερ</supplied><unclear>ιεχ</unclear><supplied reason="lost"
                            >ομ</supplied>ένωι</seg> ὑπό <seg type="word"> τ<unclear>ε</unclear>
                        </seg> τᾶς</seg>
                    <seg n="13" type="line" facs="z62r1_13">Θ καὶ τᾶς ἴσας <seg type="word">
                            <unclear>π</unclear>
                            <supplied reason="lost">άσ</supplied>
                            <unclear>α</unclear>
                            <supplied reason="lost">ι</supplied>
                            <unclear>ς</unclear>
                        </seg> τᾶ τε Α</seg>
                    <seg n="14" type="line" facs="z62r1_14">καὶ τᾶ <seg type="word">τρι<supplied
                                reason="lost">π</supplied>λασία</seg> τᾶς Γ καὶ</seg>
                    <seg n="15" type="line" facs="z62r1_15">τᾶ <seg type="word">περ<supplied
                                reason="lost">ι</supplied><unclear>σσ</unclear>ᾶ</seg> κατὰ τοὺς
                        ἑξῆς</seg>
                    <seg n="16" type="line" facs="z62r1_16">
                        <seg type="word">
                            <supplied reason="lost">ἀ</supplied>ριθμοὺς</seg>
                        <seg type="word">π<unclear>ε</unclear>ρισσοὺς</seg>
                        <seg type="clearword">πολλα<supplied reason="lost">πλα</supplied></seg>
                    </seg>
                </seg>
                <seg n="59v2" type="folio">
                    <seg n="1" type="line" facs="z59v2_01"><seg type="wordend">σίους</seg> τᾶς <seg
                            type="word">ἑ<supplied reason="lost">πο</supplied>μένας</seg>
                        <choice>
                            <abbr>γραμμ</abbr>
                            <expan>γραμμᾶς</expan>
                        </choice>.</seg>
                    <seg n="2" type="line" facs="z59v2_02">ἐντὶ δὲ καὶ τὰ ἀπὸ τῶν ΑΒ ΓΔ</seg>
                    <seg n="3" type="line" facs="z59v2_03">ΕΖ ΗΘ τετράγωνα ἴσα τῶι</seg>
                    <seg n="4" type="line" facs="z59v2_04">περιεχομένωι ὑπὸ τᾶν αὐτᾶν <seg
                            type="clearword">γραμ</seg></seg>
                    <seg n="5" type="line" facs="z59v2_05"><seg type="wordend">μᾶν</seg>. ἔστι γὰρ
                        τὸ ἀπὸ τᾶς Α <seg type="clearword">τε</seg></seg>
                    <seg n="6" type="line" facs="z59v2_06"><seg type="wordend">τράγωνον</seg> ἴσον
                        τῶι <seg type="clearword">περιεχομέ</seg></seg>
                    <seg n="7" type="line" facs="z59v2_07"><seg type="wordend">νωι</seg> ὑπό τε τᾶς
                        Θ καὶ τᾶς ἴσας</seg>
                    <seg n="8" type="line" facs="z59v2_08">πάσαις τᾶ τε Α καὶ τᾶ ἴσα</seg>
                    <seg n="9" type="line" facs="z59v2_09">ταῖς λοιπαῖς, ὧν ἑκάστα ἴσα</seg>
                    <seg n="10" type="line" facs="z59v2_10">τῶ Α· ἰσάκις <expan>γὰρ</expan>
                        <sic>μετρια</sic> τε Θ τὰν</seg>
                    <seg n="11" type="line" facs="z59v2_11">Α καὶ ἁ Α τὰς ἴσας αὐτᾶ <seg
                            type="clearword">πά</seg></seg>
                    <seg n="12" type="line" facs="z59v2_12"><seg type="wordend">σας</seg> ἐν τᾶι Α·
                        ὥστε ἴσον ἐστὶ τὸ</seg>
                    <seg n="13" type="line" facs="z59v2_13">ἀπὸ Α τετράγωνον τῶι <seg
                            type="clearword">περιεχο</seg></seg>
                    <seg n="14" type="line" facs="z59v2_14"><seg type="wordend">μένωι</seg> ὑπό τε
                        τᾶς Θ καὶ τᾶς <seg type="clearword">ἴ</seg></seg>
                    <seg n="15" type="line" facs="z59v2_15"><seg type="wordend">σας</seg> τᾶ Α καὶ
                        τᾶ διπλασία τῶν</seg>
                    <seg n="16" type="line" facs="z59v2_16">ΑΒ ΓΔ ΕΖ ΗΘ· αἱ γὰρ ἴσαι τᾶ Α</seg>
                    <seg n="17" type="line" facs="z59v2_17">πᾶσαι χωρὶς τᾶς Α διπλάσιαί</seg>
                    <seg n="18" type="line" facs="z59v2_18">ἐντι τᾶν ΑΒ ΓΔ ΕΖ ΗΘ. ὁμοίως δὲ</seg>
                </seg>
                <seg n="62r2" type="folio">
                    <seg n="1" type="line" facs="z62r2_01">καὶ <supplied reason="lost">τὸ</supplied>
                        ἀπὸ τᾶς Α <seg type="word">τετ<unclear>ρ</unclear>άγων<supplied
                                reason="lost">ον</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">ἴ</supplied>σο<supplied reason="lost"
                            >ν</supplied>
                        </seg></seg>
                    <seg n="2" type="line" facs="z62r2_02">ἐντὶ τῶι περιεχομένωι ὑπό τε</seg>
                    <seg n="3" type="line" facs="z62r2_03">τᾶς Θ καὶ τᾶς ἴσας τᾶι τε Β
                        <expan>καὶ</expan></seg>
                    <seg n="4" type="line" facs="z62r2_04">τᾶ διπλασία τᾶν ΔΕ Ζ ΗΘ, <seg
                            type="clearword">ὁμοί</seg></seg>
                    <seg n="5" type="line" facs="z62r2_05"><seg type="wordend">ως</seg> δὲ καὶ τὰ
                        ἀπὸ τᾶν ἀλλᾶν <seg type="clearword">τε</seg></seg>
                    <seg n="6" type="line" facs="z62r2_06"><seg type="wordend">
                            <supplied reason="lost">τράγωνα</supplied>
                        </seg> ἴσα ἐντὶ τοῖς <seg type="clearword"><unclear>π</unclear>εριε</seg></seg>
                    <seg n="7" type="line" facs="z62r2_07"><seg type="wordend">χομένοις</seg> ὑπό τε
                            <supplied reason="lost">τᾶς</supplied> Θ καὶ τᾶς</seg>
                    <seg n="8" type="line" facs="z62r2_08">ἴσας αὐτᾶ <unclear>τε</unclear> καὶ τᾶ
                        διπλασία</seg>
                    <seg n="9" type="line" facs="z62r2_09">τῶν λοιπῶν. δῆλον οὖν, <supplied
                            reason="lost">
                            <expan>ὅτι</expan>
                        </supplied> τὰ <seg type="clearword">ἀ</seg></seg>
                    <seg n="10" type="line" facs="z62r2_10"><seg type="wordend">
                            <unclear>πὸ</unclear>
                        </seg>
                        <unclear>πασᾶν</unclear> τετράγωνα ἴσα <seg type="clearword">ἐν</seg></seg>
                    <seg n="11" type="line" facs="z62r2_11"><seg type="wordend">τὶ</seg> τῶι <seg
                            type="word">π<unclear>ε</unclear>ριεχομένωι</seg> ὑπό τε <choice>
                            <abbr>τ</abbr>
                            <expan>τᾶς</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z62r2_12">Θ <seg type="word"
                            >κα<unclear>ὶ</unclear>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ᾶς</unclear>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">ἴσ</supplied>ας</seg>
                        <seg type="word">πάσ<supplied reason="lost">α</supplied>ι<supplied
                                reason="lost">ς</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ᾶ</seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ε</seg></seg>
                    <seg n="13" type="line" facs="z62r2_13"><unclear>Α</unclear>
                        <seg type="word">
                            <unclear>κ</unclear>αὶ</seg> τᾶ <seg type="word"> τριπλα<supplied
                                reason="lost">σ</supplied>ία</seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ᾶς</seg> Β <expan>καὶ</expan></seg>
                    <seg n="14" type="line" facs="z62r2_14"><supplied reason="lost">τᾶ</supplied>
                        πενταπλασία <seg type="word">
                            <supplied reason="lost">τ</supplied>ᾶς</seg> Γ <expan>καὶ</expan>
                        <seg type="word">
                            <unclear>τ</unclear>
                            <supplied reason="lost">ᾶ</supplied>
                        </seg></seg>
                    <seg n="15" type="line" facs="z62r2_15">κατὰ τοὺς ἑξῆς <seg type="word">
                            <supplied reason="lost">ἀ</supplied>ριθμοὺς</seg>
                        <seg type="clearword">πε</seg></seg>
                    <seg n="16" type="line" facs="z62r2_16"><seg type="wordend">ρισσοὺς</seg>
                        πολλαπλασία τᾶς</seg>
                    <seg n="17" type="line" facs="z62r2_17"><seg type="word">
                            <supplied reason="lost">ἑπ</supplied>ομένας</seg>.</seg>
                    <figure n="10.2">
                        <figDesc>Figure 10.2</figDesc>
                    </figure>
                </seg>
            </ab>
