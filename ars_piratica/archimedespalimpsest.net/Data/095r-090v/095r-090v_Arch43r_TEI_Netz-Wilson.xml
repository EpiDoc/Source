    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="95r1" type="folio">
                    <seg n="1" type="line" facs="z95r1_01">τῶν εἰρημένων τᾶς ἐκ τοῦ <seg
                            type="clearword">
                            <choice>
                                <abbr>κέ</abbr>
                                <expan>κέν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="2" type="line" facs="z95r1_02"><seg type="wordend">τρου</seg> τοῦ
                        ἐλάσσονος κύκλου <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z95r1_03">εἰρημένων, ποτὶ τὸ <seg type="word">
                            <choice>
                                <abbr>τετράγων</abbr>
                                <expan>τετράγ<supplied reason="lost">ω</supplied>νον</expan>
                            </choice>
                        </seg></seg>
                    <seg n="4" type="line" facs="z95r1_04">τὸ ἀπὸ τᾶς ἐκ τοῦ κέντρου τοῦ</seg>
                    <seg n="5" type="line" facs="z95r1_05">μείζονος κύκλου τῶν εἰρημένων.</seg>
                </seg>
                <seg n="95r1" type="folio">
                    <seg n="6" type="line" facs="z95r1_06">τὸ περιεχόμενον χωρίον</seg>
                    <seg n="7" type="line" facs="z95r1_07">ὑπό τε τᾶς ἕλικος, <supplied
                            reason="lost">ἅ</supplied>
                        <seg type="word">
                            <supplied reason="lost">ἐστι</supplied>ν</seg>
                        <choice>
                            <abbr>ἐλάσσ</abbr>
                            <expan>ἐλάσσων</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z95r1_08">τᾶς ἐν μιᾶι περιφορᾶι <seg
                            type="clearword">γεγραμ</seg></seg>
                    <seg n="9" type="line" facs="z95r1_09"><seg type="wordend">μένας</seg>, οὐκ
                        ἐχούσας πέρας τὰν</seg>
                    <seg n="10" type="line" facs="z95r1_10">ἀρχὰν τᾶς ἕλικος, καὶ τᾶν <seg
                            type="clearword">εὐ</seg></seg>
                    <seg n="11" type="line" facs="z95r1_11"><seg type="wordend">θειᾶν</seg> τᾶν ἀπὸ
                        τῶν <choice>
                            <abbr>περάτω</abbr>
                            <expan>περάτων</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z95r1_12">αὐτᾶς ἐπὶ τὰν ἀρχὰν τᾶς <seg
                            type="clearword">ἕλι</seg></seg>
                    <seg n="13" type="line" facs="z95r1_13"><seg type="wordend">κος</seg> ἀγμενᾶν
                        ποτὶ τὸν τομέα</seg>
                    <seg n="14" type="line" facs="z95r1_14">τὸν ἔχοντα τὰν μὲν ἐκ τοῦ <seg
                            type="clearword">
                            <choice>
                                <abbr>κέ</abbr>
                                <expan>κέν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="15" type="line" facs="z95r1_15"><seg type="wordend">τρου</seg> ἴσαν τᾶ
                        μείζονι τᾶν ἀπὸ</seg>
                    <seg n="16" type="line" facs="z95r1_16">τοῦ πέρατος ἐπὶ τὰν ἀρχὰν <choice>
                            <abbr>τ</abbr>
                            <expan>τᾶς</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z95r1_17">ἕλικος ἀγμενᾶν, τὰν δὲ <seg
                            type="clearword">περι</seg></seg>
                    <seg n="18" type="line" facs="z95r1_18"><seg type="wordend">φέρειαν</seg>, ἅ
                        ἐστι τᾶ μεταξὺ τᾶν</seg>
                    <seg n="19" type="line" facs="z95r1_19"><seg type="word">εἰ<supplied
                                reason="lost">ρ</supplied>ημενᾶν</seg> εὐθειᾶν ἐπὶ τὰ αὐτὰ</seg>
                </seg>
                <seg n="90v1" type="folio">
                    <seg n="1" type="line" facs="z90v1_01">τᾶ ἕλικι, τοῦτον ἔχει τὸν λόγον,</seg>
                    <seg n="2" type="line" facs="z90v1_02">ὃν ἔχει <seg type="word">
                                συναμφό<unclear>τ</unclear>ερα</seg> τό τε <seg type="clearword"
                        >πε</seg></seg>
                    <seg n="3" type="line" facs="z90v1_03"><seg type="wordend">ριεχόμενον</seg> ἀπὸ
                        τᾶν ὑπὸ <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z90v1_04">περάτων ἐπὶ τὰν ἀρχὰν τᾶς</seg>
                    <seg n="5" type="line" facs="z90v1_05">ἕλικος ἀγομέναν καὶ τὸ <choice>
                            <abbr>τρίτ</abbr>
                            <expan>τρίτον</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z90v1_06">μέρος <seg type="word">
                            <supplied reason="lost">το</supplied>ῦ</seg> τετραγώνου τοῦ ἀπὸ</seg>
                    <seg n="7" type="line" facs="z90v1_07">τᾶς ὑπεροχᾶς, ἇ ὑπερέχει ἁ <seg
                            type="unclearword">μεί</seg></seg>
                    <seg n="8" type="line" facs="z90v1_08"><seg type="wordend"
                            ><unclear>ζ</unclear>ων</seg> τῶν εἰρημενῶν εὐθειῶν</seg>
                    <seg n="9" type="line" facs="z90v1_09">τᾶς ἐλάσσονος, ποτὶ τὸ <seg
                            type="clearword">τετρά</seg></seg>
                    <seg n="10" type="line" facs="z90v1_10"><seg type="wordend">γωνον</seg> τὸ ἀπὸ
                        τᾶς μείζονος</seg>
                    <seg n="11" type="line" facs="z90v1_11">τᾶν <seg type="word">
                            <unclear>ἀπ</unclear>ὸ</seg> τῶν <seg type="word">
                            π<unclear>ε</unclear>ράτων</seg> ἐπὶ <choice>
                            <abbr>τὰ</abbr>
                            <expan>τὰν</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z90v1_12">ἀρχὰν τᾶς ἕλικος <seg type="word">
                            <choice>
                                <abbr>ἐπιζευχθεισ</abbr>
                                <expan>ἐ<supplied reason="lost">πι</supplied>ζευχθεισᾶν</expan>
                            </choice>
                        </seg>.</seg>
                </seg>
                <seg n="90v1" type="folio">
                    <seg n="13" type="line" facs="z90v1_13">ἔστω ἕλιξ, ἐφ’ ἇς ἁ ΑΒΓΔΕ, <choice>
                            <abbr>ἐλάσσω</abbr>
                            <expan>ἐλάσσων</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z90v1_14">τᾶς ἐν μιᾶι περιφορᾶι <seg
                            type="clearword">γεγραμ</seg></seg>
                    <seg n="15" type="line" facs="z90v1_15"><seg type="wordend">μένας</seg>, πέρατα
                        δὲ αὐτᾶς ἔστω</seg>
                    <seg n="16" type="line" facs="z90v1_16">τὰ αΕ, ἔστω δὲ ἀρχὰ τᾶς ἕλικος</seg>
                </seg>
                <seg n="95r2" type="folio">
                    <seg n="1" type="line" facs="z95r2_01">τὸ Θ σαμεῖον, καὶ <seg type="word">
                                κέντρ<supplied reason="lost">ωι</supplied>
                        </seg> μὲν</seg>
                    <seg n="2" type="line" facs="z95r2_02"><seg type="word">
                            <supplied reason="lost">τῶ</supplied>ι</seg> Θ, διαστάματι δὲ τῶι
                            Θ<unclear>Α</unclear>
                        <seg type="suppliedword">κύ</seg></seg>
                    <seg n="3" type="line" facs="z95r2_03"><seg type="wordend"><supplied
                                reason="lost">κ</supplied>λος</seg> γεγράφθω, καὶ <seg type="word">
                                συμπ<supplied reason="lost">ι</supplied>πτέτω</seg></seg>
                    <seg n="4" type="line" facs="z95r2_04">τᾶι περιφερείαι αὐτοῦ ἁ ΘΕ <choice>
                            <abbr>κα</abbr>
                            <expan>κατὰ</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z95r2_05">τὸ Ζ. δεικτέον <expan>ὅτι</expan> τὸ
                        περιεχόμενον</seg>
                    <seg n="6" type="line" facs="z95r2_06">χωρίον ὑπό τε τᾶς ΑΒΓΔΕ <choice>
                            <abbr>ἕλικ</abbr>
                            <expan>ἕλικος</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z95r2_07">καὶ τᾶν εὐθειᾶν τᾶν ΑΘ ΘΕ ποτὶ</seg>
                    <seg n="8" type="line" facs="z95r2_08">τὸν τομέα τὸν ΑΘΖ τοῦτον ἔχει <choice>
                            <abbr>τὸ</abbr>
                            <expan>τὸν</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z95r2_09">λόγον, ὃν ἔχει συναμφότερα τό τε</seg>
                    <seg n="10" type="line" facs="z95r2_10">ὑπὸ τᾶν ΑΘ ΘΕ καὶ τὸ τρίτον <seg
                            type="clearword">μέ</seg></seg>
                    <seg n="11" type="line" facs="z95r2_11"><seg type="wordend">ρος</seg> τοῦ ἀπὸ
                        τᾶς ΕΖ ποτὶ τὸ <seg type="suppliedword"><supplied reason="lost"
                            >τετ</supplied>ρά</seg></seg>
                    <seg n="12" type="line" facs="z95r2_12"><seg type="wordend">γωνον</seg> τὸ ἀπὸ
                        τᾶς ΘΑ.</seg>
                </seg>
                <seg n="95r2" type="folio">
                    <seg n="12" type="line" facs="z95r2_12">ἔστω δὴ <seg type="clearword">κύ</seg></seg>
                    <seg n="13" type="line" facs="z95r2_13"><seg type="wordend">
                            <choice>
                                <abbr>κλ</abbr>
                                <expan>κλος</expan>
                            </choice>
                        </seg>, ἐν ὧ ϞΧ, τὰν ἐκ τοῦ κέντρου <seg type="clearword">ἔ</seg></seg>
                    <seg n="14" type="line" facs="z95r2_14"><seg type="wordend">χων</seg> ἴσαν
                        δύναμιν τῶι τε ὑπὸ</seg>
                    <seg n="15" type="line" facs="z95r2_15">τᾶν ΑΘ ΘΕ καὶ τῶι τρίτωι μέρει <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z95r2_16">ἀπὸ τᾶς ΕΖ, ποτὶ δὲ τῶι κέντρωι</seg>
                    <seg n="17" type="line" facs="z95r2_17">αὐτοῦ γωνία ἴσα τᾶ πρὸς τῶι Θ·</seg>
                    <seg n="18" type="line" facs="z95r2_18">ὁ δὴ τομεὺς ὁ ϞΧ ποτὶ τὸν τομέα</seg>
                </seg>
                <seg n="90v2" type="folio">
                    <seg n="1" type="line" facs="z90v2_01">τὸν ΘΑΖ τὸν αὐτὸν ἔχει λόγον, <seg
                            type="word">
                            <supplied reason="lost">ὃ</supplied>ν</seg></seg>
                    <seg n="2" type="line" facs="z90v2_02">ἔχει τὸ ὑπὸ τᾶν ΑΘ ΘΕ καὶ τὸ <seg
                            type="clearword">τρί</seg></seg>
                    <seg n="3" type="line" facs="z90v2_03"><seg type="wordend">τον</seg> μέρος τοῦ
                        ἀπὸ τᾶς ΕΖ <seg type="clearword">τετρα</seg></seg>
                    <seg n="4" type="line" facs="z90v2_04"><seg type="wordend">γώνου</seg> ποτὶ τὸ
                        ἀπὸ τᾶς ΘΑ <seg type="clearword">τετρά</seg></seg>
                    <seg n="5" type="line" facs="z90v2_05"><seg type="wordend">γωνον·</seg> αἱ γὰρ
                        ἐκ τῶν κέντρων</seg>
                    <seg n="6" type="line" facs="z90v2_06">τοῦτον ἔχοντι τὸν λόγον δυνάμει</seg>
                    <seg n="7" type="line" facs="z90v2_07">ποτ’ ἀλλάλας. δειχθήσεται δὴ ὁ</seg>
                    <seg n="8" type="line" facs="z90v2_08">ΧϞ τομεὺς ἴσος ὢν τῶι χωρίωι</seg>
                    <seg n="9" type="line" facs="z90v2_09">τῶ περιεχομένω ὑπό τε τᾶς</seg>
                    <seg n="10" type="line" facs="z90v2_10">ΑΒΓΔΕ ἕλικος καὶ τᾶν ΑΘ ΘΕ <seg
                            type="clearword">εὐ</seg></seg>
                    <seg n="11" type="line" facs="z90v2_11">
                        <seg type="wordend">θειᾶν</seg>. </seg>
                </seg>
                <seg n="90v2" type="folio">
                    <seg n="11" type="line" facs="z90v2_11">εἰ γὰρ μή, ἤτοι μείζων <supplied
                            reason="lost">ἢ</supplied>
                        <seg type="clearword">ἐ</seg></seg>
                    <seg n="12" type="line" facs="z90v2_12"><seg type="wordend">λάττων</seg>. ἔστω
                        πρότερον, <seg type="word">δυνα<supplied reason="lost"
                                >τ</supplied><unclear>ό</unclear>ν</seg>,</seg>
                    <seg n="13" type="line" facs="z90v2_13">μείζων. δυνατὸν οὖν <seg type="word">
                                ἐστ<supplied reason="lost">ιν</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">π</supplied>ερὶ</seg></seg>
                    <seg n="14" type="line" facs="z90v2_14"><seg type="word">τ<supplied
                                reason="lost">ὸ</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">εἰρ</supplied>ημένον</seg> χωρίον <choice>
                            <abbr>περιγράψ</abbr>
                            <expan>περιγράψαι</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z90v2_15"><supplied reason="lost">σχᾶμα</supplied>
                        <seg type="word">ἐπίπε<supplied reason="lost">δ</supplied>ον</seg> ἐξ ὁμοίων
                            <seg type="suppliedword">τομέ</seg></seg>
                    <seg n="16" type="line" facs="z90v2_16"><seg type="wordend">
                            <supplied reason="lost">ων</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">συγκεί</supplied>μ<supplied reason="lost"
                            >ενο</supplied>ν</seg>, ὥστε τὸ <seg type="suppliedword">περιγρα</seg></seg>
                    <seg n="17" type="line" facs="z90v2_17"><seg type="wordend"><supplied
                                reason="lost">φόμε</supplied>νον</seg> σχᾶμα μείζονι μὲν τοῦ</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
