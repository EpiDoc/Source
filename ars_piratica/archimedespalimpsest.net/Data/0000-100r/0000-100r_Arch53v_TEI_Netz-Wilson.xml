    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="100r1" type="folio">
                    <seg n="1" type="line" facs="z100r1_01">τοῦτο ἴσος <abbr>ἐστὶν</abbr> ὁ ΜΝΞ <seg
                            type="word"><unclear>κῶ</unclear>νος</seg>
                        <unclear>τῶι</unclear> Α</seg>
                    <seg n="2" type="line" facs="z100r1_02">ΒΓ <seg type="word"
                            >κών<unclear>ω</unclear>ι</seg>· ἐὰν γὰρ ὦσι δύο κῶνοι</seg>
                    <seg n="3" type="line" facs="z100r1_03">ἰσοσκελεῖς, ἡ δὲ τοῦ ἑτέρου <seg
                            type="word">κώ<unclear>ν</unclear><supplied reason="lost"
                        >ου</supplied></seg></seg>
                    <seg n="4" type="line" facs="z100r1_04">ἐπιφάνεια ἴση ἦ τῆι τοῦ ἑτέρου</seg>
                    <seg n="5" type="line" facs="z100r1_05">βάσει, ἔτι δὲ ἡ ἀπὸ τοῦ κέντρου</seg>
                    <seg n="6" type="line" facs="z100r1_06">τῆς βάσεως ἐπὶ τὴν πλευρὰν</seg>
                    <seg n="7" type="line" facs="z100r1_07">τοῦ κώνου λεγομένη κάθετος</seg>
                    <seg n="8" type="line" facs="z100r1_08">τῶι ὕψει ἴση, ἴσοι ἔσονται οἱ κῶνοι,</seg>
                    <seg n="9" type="line" facs="z100r1_09">τὴν δὲ τοῦ ΟΠΡ κώνου βάσιν ἴσην</seg>
                    <seg n="10" type="line" facs="z100r1_10">εἶναι τῆι ἐπιφανείαι τοῦ ΔΒΕ <w
                            part="I">κώ</w></seg>
                    <seg n="11" type="line" facs="z100r1_11"><w part="F">νου</w>, <seg type="word"
                                >ὕ<unclear>ψ</unclear>ος</seg> δὲ τῆι ΖΗ διὰ δὴ τούτοις </seg>
                    <seg n="12" type="line" facs="z100r1_12">ἴσος <expan>ἐστὶν</expan> ὁ
                            ΟΠ<unclear>Ρ</unclear> κῶνος τῶι ΒΔ ΖΕ <w part="I">ῥόμ</w></seg>
                    <seg n="13" type="line" facs="z100r1_13"><w part="F">βωι</w>· τοῦτο γὰρ
                        προαπεδείχθη. ἐπεὶ</seg>
                    <seg n="14" type="line" facs="z100r1_14">δὲ ἡ τοῦ Α<supplied reason="lost"
                        >Β</supplied>Γ κώνου ἐπιφάνεια</seg>
                    <seg n="15" type="line" facs="z100r1_15">σύγκειται ἔκ τε τῆς τοῦ ΔΒΕ <w part="I"
                            >ἐπι</w></seg>
                    <seg n="16" type="line" facs="z100r1_16"><w part="F">φανείας</w> καὶ τῆς μεταξὺ
                        τῶν</seg>
                    <seg n="17" type="line" facs="z100r1_17">ΔΕ ΑΓ, ἀλλ’ ἡ μὲν τοῦ ΑΒΓ κώνου</seg>
                    <seg n="18" type="line" facs="z100r1_18">ἐπιφάνεια ἴση <expan>ἐστὶ</expan> τῆι
                        βάσει τοῦ</seg>
                    <gap/>
                </seg>
                <seg n="100r2" type="folio">
                    <num value="21">ΚΑ</num>
                </seg>
                <seg n="100r2" type="folio">
                    <seg n="1" type="line" facs="z100r2_01">ἐὰν ῥόμβου ἐξ ἰσοσκελῶν <choice>
                            <abbr>κωνω</abbr>
                            <expan>κώνων</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z100r2_02">συγκειμένου ὁ ἕτερος κῶνος</seg>
                    <seg n="3" type="line" facs="z100r2_03">ἐπιπέδῶι <seg type="word"
                                >τμηθῆ<unclear>ι</unclear></seg> παραλλήλῶι</seg>
                    <seg n="4" type="line" facs="z100r2_04">τῆι βάσει, ἀπὸ δὲ τοῦ γενομένου</seg>
                    <seg n="5" type="line" facs="z100r2_05">κύκλου κῶνος ἀναγραφῆι <w part="I"
                        >κο</w></seg>
                    <seg n="6" type="line" facs="z100r2_06"><w part="F">ρυφὴν</w> ἔχων τὴν αὐτὴν τῶι
                            <seg type="unclearword">ἑ</seg></seg>
                    <seg n="7" type="line" facs="z100r2_07"><seg type="wordend"
                            ><unclear>τ</unclear>έρωι</seg> κώνωι, ἀπὸ δὲ τοῦ ὅλου <w part="I"
                        >ῥόμ</w></seg>
                    <seg n="8" type="line" facs="z100r2_08"><w part="F">βου</w> ὁ γενόμενος ῥόμβος
                            <w part="I">ἀφαι</w></seg>
                    <seg n="9" type="line" facs="z100r2_09"><w part="F">ρεθῆ</w>, <seg type="word">
                            <supplied reason="lost">τῶι</supplied>
                        </seg>
                        <seg type="word"><supplied reason="lost">π</supplied>εριλείμματι</seg> ἴσος
                            <expan>ἔσται</expan></seg>
                    <seg n="10" type="line" facs="z100r2_10">ὁ κῶνος ὁ βάσιν μὲν ἔχων <seg
                            type="word">ἴσ<expan>ην</expan></seg></seg>
                    <seg n="11" type="line" facs="z100r2_11"><seg type="word"
                                ><unclear>τ</unclear>ῆ<supplied reason="lost">ι</supplied></seg>
                        <seg type="word">ἐπ<supplied reason="lost"
                            >ι</supplied><unclear>φ</unclear>ανείαι</seg> τοῦ κώνου τῆι </seg>
                    <seg n="12" type="line" facs="z100r2_12"><seg type="word">
                            <supplied reason="lost">μετα</supplied>
                            <unclear>ξ</unclear>
                            <supplied reason="lost">ὺ</supplied>
                        </seg> τῶν παραλλήλων <seg type="suppliedword">ἐπι</seg></seg>
                    <seg n="13" type="line" facs="z100r2_13"><seg type="wordend">
                            <supplied reason="lost">πέδων</supplied>
                        </seg>, <supplied reason="lost">ὕψος</supplied> δὲ <seg type="word"
                                >ἴ<unclear>σ</unclear>ον</seg> τὸ ἀπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice></seg>
                </seg>
            </ab>
