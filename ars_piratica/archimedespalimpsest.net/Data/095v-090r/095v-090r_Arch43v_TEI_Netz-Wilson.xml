    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="95v1" type="folio">
                    <seg n="1" type="line" facs="z95v1_01">εἰρημένου χωρίου <seg type="word">
                                ἐλάσ<unclear>σ</unclear>ονι</seg> ἢ <seg type="clearword">ἁ</seg></seg>
                    <seg n="2" type="line" facs="z95v1_02"><seg type="wordend">λίκωι</seg> ὑπερέχει
                        ὁ ϞΧ τομεὺς τοῦ</seg>
                    <seg n="3" type="line" facs="z95v1_03">εἰρημένου χωρίου. <seg type="clearword"
                            >περιγεγρά</seg></seg>
                    <seg n="4" type="line" facs="z95v1_04"><seg type="wordend">φθω</seg> δή, καὶ
                        ἔστω τῶν τομῶν, ἐξ ὧν</seg>
                    <seg n="5" type="line" facs="z95v1_05">σύγκειται τὸ περιγεγραμμένον <seg
                            type="clearword">σχᾶ</seg></seg>
                    <seg n="6" type="line" facs="z95v1_06"><seg type="wordend">μα</seg>, μείζων μὲν
                        ὁ ΘΑΚ, ἐλάσσων</seg>
                    <seg n="7" type="line" facs="z95v1_07">δὲ ὁ ΘΟΔ· δῆλον, <expan>ὅτι</expan> τὸ
                            <seg type="clearword">περιγεγραμ</seg></seg>
                    <seg n="8" type="line" facs="z95v1_08"><seg type="wordend">μένον</seg> σχᾶμα
                        ἔλασσόν ἐστι τοῦ</seg>
                    <seg n="9" type="line" facs="z95v1_09">ΧϞ τομέως. διήχθωσαν δὴ <seg
                            type="clearword">εὐ</seg></seg>
                    <seg n="10" type="line" facs="z95v1_10"><seg type="wordend">θεῖαι</seg> αἱ
                        ποιοῦσαι τὰς ἴσας <seg type="clearword">γω</seg></seg>
                    <seg n="11" type="line" facs="z95v1_11"><seg type="wordend">νίας</seg> ποτὶ τὸ
                        Θ, ἔστ’ ἂν ποτὶ τὰν</seg>
                    <seg n="12" type="line" facs="z95v1_12">περιφέρειαν τοῦ ΘΑΖ τομέως</seg>
                    <seg n="13" type="line" facs="z95v1_13">πέσωντι. ἐντὶ δή τινες εὐθεῖαι τῶι</seg>
                    <seg n="14" type="line" facs="z95v1_14">ἴσωι ἀλλαλᾶν ὑπερέχουσαι, αἱ <seg
                            type="clearword">ἀ</seg></seg>
                    <seg n="15" type="line" facs="z95v1_15"><seg type="wordend">πὸ</seg> τοῦ Θ ποτὶ
                        τὰν ἕλικα <seg type="clearword">ποτι</seg></seg>
                    <seg n="16" type="line" facs="z95v1_16"><seg type="wordend">πίπτουσαι</seg>, ὧν
                        ἐστι μεγίστα μὲν ἁ</seg>
                    <seg n="17" type="line" facs="z95v1_17">ΘΑ, ἐλαχίστα δὲ ἁ ΘΕ, ἐντὶ δὲ καὶ <seg
                            type="clearword">ἄλ</seg></seg>
                    <seg n="18" type="line" facs="z95v1_18"><seg type="wordend">λαι</seg> εὐθεῖαι ἐν
                        μὲν τῶι πλήθει μιᾶι</seg>
                    <seg n="19" type="line" facs="z95v1_19"><seg type="word">
                            <supplied reason="lost">ἐ</supplied><unclear>λ</unclear>άσσων </seg>
                        ταυτᾶν, τῶι δὲ μεγέθει</seg>
                </seg>
                <seg n="90r1" type="folio">
                    <seg n="1" type="line" facs="z90r1_01">ἴσαι ἀλλήλαις τε καὶ τᾶ μεγίστα,</seg>
                    <seg n="2" type="line" facs="z90r1_02">αἱ ἀπὸ τοῦ Θ ποτὶ τὰν τοῦ ΑΘΖ</seg>
                    <seg n="3" type="line" facs="z90r1_03">τομέως <seg type="word">περιφέ<supplied
                                reason="lost">ρεια</supplied><unclear>ν</unclear>
                        </seg>
                        <seg type="suppliedword">πο<supplied reason="lost">τιπ</supplied>ί</seg></seg>
                    <seg n="4" type="line" facs="z90r1_04"><seg type="wordend">πτουσαι</seg> χωρὶς
                        τᾶς ΘΖ, καὶ <seg type="clearword">ἀνα</seg></seg>
                    <seg n="5" type="line" facs="z90r1_05"><seg type="wordend">γεγράφαται</seg>
                        ὁμοῖοι τομέες <seg type="clearword">ἀ</seg></seg>
                    <seg n="6" type="line" facs="z90r1_06"><seg type="wordend">πὸ</seg> πασᾶν, ἀπό
                        τε τᾶν ἰσᾶν <seg type="clearword">ἀλ</seg></seg>
                    <seg n="7" type="line" facs="z90r1_07"><seg type="wordend">λήλαις</seg> τε καὶ
                        τᾶ μεγίστα καὶ <seg type="clearword">ἀ</seg></seg>
                    <seg n="8" type="line" facs="z90r1_08"><seg type="wordend">πὸ</seg> τᾶν τῶι ἴσωι
                        ἀλλαλᾶν <seg type="clearword">ὑπε</seg></seg>
                    <seg n="9" type="line" facs="z90r1_09"><seg type="wordend">ρεχουσᾶν</seg>, ἀπὸ
                        δὲ τᾶς ΘΕ οὐκ <seg type="clearword">ἀ</seg></seg>
                    <seg n="10" type="line" facs="z90r1_10"><seg type="wordend">ναγέγραπται·</seg>
                        τομέες οὖν οἱ ἀπὸ</seg>
                    <seg n="11" type="line" facs="z90r1_11">τᾶν ἰσᾶν ἀλλάλαις τε καὶ τᾶ <seg
                            type="clearword">με</seg></seg>
                    <seg n="12" type="line" facs="z90r1_12"><seg type="wordend">γίστα</seg> ποτὶ
                        τοὺς τομέας τοὺς <seg type="clearword">ἀ</seg></seg>
                    <seg n="13" type="line" facs="z90r1_13"><seg type="wordend">πὸ</seg> τᾶν τῶι
                        ἴσωι ἀλλαλᾶν <seg type="clearword">ὑπε</seg></seg>
                    <seg n="14" type="line" facs="z90r1_14"><seg type="wordend">ρεχουσᾶν</seg> χωρὶς
                            <seg type="word">
                            <unclear>τ</unclear>οῦ</seg> ἀπὸ <seg type="word">τᾶ<supplied
                                reason="lost">ς</supplied>
                        </seg>
                        <seg type="clearword">ἐ</seg></seg>
                    <seg n="15" type="line" facs="z90r1_15"><seg type="wordend">λαχίστας</seg>
                        τομέως <seg type="word">ἐλάσσον<supplied reason="lost">α</supplied>
                        </seg></seg>
                    <seg n="16" type="line" facs="z90r1_16">λόγον ἔχοντι ἢ τὸ <seg type="word">
                                ἀπ<unclear>ὸ</unclear>
                        </seg>
                        <seg type="word">
                            <unclear>τ</unclear>
                            <supplied reason="lost">ᾶς</supplied>
                        </seg>
                        <supplied reason="lost">Θ</supplied>Α</seg>
                </seg>
                <seg n="95v2" type="folio">
                    <seg n="1" type="line" facs="z95v2_01">ποτὶ τὰ συναμφότερα τά τε</seg>
                    <seg n="2" type="line" facs="z95v2_02"><seg type="word">
                            <supplied reason="lost">ἀ</supplied>πὸ</seg> τᾶν ΑΘ ΘΕ καὶ τὸ τρίτον
                            <seg type="clearword">μέ</seg></seg>
                    <seg n="3" type="line" facs="z95v2_03"><seg type="wordend">ρος</seg> τοῦ ἀπὸ τᾶς
                        ΕΖ <choice>
                            <abbr>τετραγών</abbr>
                            <expan>τετραγώνου</expan>
                        </choice>.</seg>
                    <seg n="4" type="line" facs="z95v2_04">ἔστι δὲ τοῖς μὲν τομεῦσι τοῖς ἀπὸ</seg>
                    <seg n="5" type="line" facs="z95v2_05">τᾶν ἰσᾶν ἀλλήλαις τε καὶ τᾶ</seg>
                    <seg n="6" type="line" facs="z95v2_06">μεγίστα ἴσος ὁ ΘΑΖ τομεύς, <seg
                            type="word"> το<add>Ι<unclear>Σ</unclear></add>ς</seg></seg>
                    <seg n="7" type="line" facs="z95v2_07">δὲ ἀπὸ τᾶν τῶι ἴσωι ἀλλαλᾶν <seg
                            type="clearword">ὑ</seg></seg>
                    <seg n="8" type="line" facs="z95v2_08"><seg type="wordend">περεχουσᾶν</seg> τὸ
                            <seg type="clearword">περιγεγραμμέ</seg></seg>
                    <seg n="9" type="line" facs="z95v2_09"><seg type="wordend">νον·</seg> ἐλάσσονα
                        οὖν λόγον ἔχει</seg>
                    <seg n="10" type="line" facs="z95v2_10">ὁ ΘΑΖ τομεὺς ποτὶ τὸ <seg
                            type="clearword">περιγε</seg></seg>
                    <seg n="11" type="line" facs="z95v2_11"><seg type="wordend">γραμμένον</seg>
                        σχᾶμα ἢ τὸ <seg type="clearword">τετρά</seg></seg>
                    <seg n="12" type="line" facs="z95v2_12"><seg type="wordend">γωνον</seg> τὸ ἀπὸ
                        τᾶς ΘΑ ποτὶ τὰ</seg>
                    <seg n="13" type="line" facs="z95v2_13">συναμφότερα τό τε ὑπὸ τᾶν ΘΑ</seg>
                    <seg n="14" type="line" facs="z95v2_14">ΘΕ <expan>καὶ</expan> τὸ τρίτον μέρος
                        τοῦ ἀπὸ</seg>
                    <seg n="15" type="line" facs="z95v2_15">τᾶς ΖΕ. ὃν δὲ λόγον ἔχει τὸ <seg
                            type="clearword">ἀ</seg></seg>
                    <seg n="16" type="line" facs="z95v2_16"><seg type="wordend">πὸ</seg> τᾶς ΘΑ ποτὶ
                        τὰ εἰρημένα,</seg>
                    <seg n="17" type="line" facs="z95v2_17">τοῦτον τὸν λόγον ἔχει ὁ ΘΑΖ <seg
                            type="clearword">το</seg></seg>
                    <seg n="18" type="line" facs="z95v2_18"><seg type="wordend">μεὺς</seg> ποτὶ τὸν
                        Χ τομέα· ὥστε</seg>
                </seg>
                <seg n="90r2" type="folio">
                    <seg n="1" type="line" facs="z90r2_01">ἐλάσσων ἐστὶν ὁ Χ <seg type="word">
                                τομ<unclear>ε</unclear><supplied reason="lost">ὺ</supplied>ς </seg>
                        τοῦ</seg>
                    <seg n="2" type="line" facs="z90r2_02">περιγεγραμμένου σχάματος.</seg>
                    <seg n="3" type="line" facs="z90r2_03">οὐκ <expan>ἔστι</expan> δέ, ἀλλὰ μείζων·
                        οὐκ <expan>ἄρα</expan></seg>
                    <seg n="4" type="line" facs="z90r2_04">ἔσται ὁ Χ τομεὺς μείζων τοῦ <seg
                            type="clearword">
                            <expan>περι</expan>
                        </seg></seg>
                    <seg n="5" type="line" facs="z90r2_05"><seg type="wordend">εχομένου</seg> χωρίου
                        ὑπό τε τᾶς</seg>
                    <seg n="6" type="line" facs="z90r2_06">ΑΒΓΔΕ ἕλικος καὶ τᾶν ΑΘ ΘΕ</seg>
                    <seg n="7" type="line" facs="z90r2_07">εὐθειᾶν.</seg>
                </seg>
                <seg n="90r2" type="folio">
                    <seg n="8" type="line" facs="z90r2_08">ΟΥδὲ τοίνυν <expan>ἐλάσσων</expan>.
                            <expan>ἔστω</expan>
                        <expan>γὰρ</expan> ἐλάσσων, <expan>καὶ</expan></seg>
                    <seg n="9" type="line" facs="z90r2_09">τὰ ἄλλα τὰ αὐτὰ <sic>
                            <seg type="clearword">κατασκευ</seg>
                        </sic></seg>
                    <seg n="10" type="line" facs="z90r2_10"><sic>
                            <seg type="wordend">άσθω</seg>
                        </sic>. πάλιν δὴ δυνατόν <choice>
                            <abbr>ἐστι</abbr>
                            <expan>ἐστιν</expan>
                        </choice></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
