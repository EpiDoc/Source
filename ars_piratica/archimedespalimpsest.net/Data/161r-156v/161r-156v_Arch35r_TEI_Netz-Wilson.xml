    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="161r1" type="folio">
                    <seg n="1" type="line" facs="z161r1_01">τοῦ κύκλου <seg type="word">
                                περι<unclear>φερεί</unclear><supplied reason="lost">ας</supplied>
                        </seg>
                        <seg type="clearword">τοσαύ</seg></seg>
                    <seg n="2" type="line" facs="z161r1_02"><seg type="wordend">τας</seg>
                        λαμβανομένας, <seg type="word">
                            <supplied reason="lost">ὅ</supplied><unclear>σ</unclear>ος</seg>
                        <choice>
                            <abbr>ἐστὶ</abbr>
                            <expan>ἐστὶν</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z161r1_03">
                        <supplied reason="lost">ὁ</supplied>
                        <seg type="word">ε<supplied reason="lost">ν</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">ἐ</supplied>λάσσων</seg>
                        <seg type="word">ἀριθμὸ<unclear>ς</unclear>
                        </seg>
                        <seg type="word">τᾶ<supplied reason="lost">ν</supplied>
                        </seg>
                    </seg>
                    <seg n="4" type="line" facs="z161r1_04"><seg type="word">
                            <supplied reason="lost">πε</supplied>ρι<supplied reason="lost"
                                >φ</supplied>ο<supplied reason="lost">ρᾶν</supplied>
                        </seg>, <seg type="word">
                            <unclear>κ</unclear>
                            <supplied reason="lost">αὶ</supplied>
                        </seg> εἴ κα ἁ <seg type="suppliedword">ποτι<unclear>πί</unclear></seg></seg>
                    <seg n="5" type="line" facs="z161r1_05">
                        <seg type="wordend">
                            <supplied reason="lost">πτουσα</supplied>
                        </seg>
                        <supplied reason="lost">ἁ</supplied>
                        <seg type="word">
                            <supplied reason="lost">ἑκ</supplied>
                            <unclear>ατ</unclear>
                            <supplied reason="lost">έρα</supplied>
                        </seg>
                        <unclear>ποτὶ</unclear>
                        <seg type="word">
                            <unclear>τ</unclear>
                            <supplied reason="lost">ὸ</supplied>
                        </seg>
                        <seg type="word">
                            <unclear>π</unclear>έρας</seg>
                    </seg>
                    <seg n="6" type="line" facs="z161r1_06"><supplied reason="lost">τᾶς</supplied>
                        <seg type="word">
                            <supplied reason="lost">ἕ</supplied>λικος</seg>
                        <seg type="word">πί<supplied reason="lost">π</supplied>τηι </seg>.</seg>
                    <figure n="15.1">
                        <figDesc>Figure 15.1</figDesc>
                    </figure>
                </seg>
                <seg n="161r1" type="folio">
                    <seg n="7" type="line" facs="z161r1_07">εἴ κα τᾶς ἕλικος τᾶς ἐν τᾶι <seg
                            type="unclearword">
                            <unclear>πρώ</unclear>
                        </seg></seg>
                    <seg n="8" type="line" facs="z161r1_08">
                        <seg type="wordend">τα</seg>
                        <seg type="word">
                            <unclear>πε</unclear>ριφορᾶ</seg>
                        <seg type="word">γεγ<supplied reason="lost">ρα</supplied>μμένας </seg>
                    </seg>
                    <seg n="9" type="line" facs="z161r1_09">εὐθεῖα γραμμὰ <seg type="word">
                                ἐ<unclear>π</unclear><supplied reason="lost"
                            >ι</supplied><unclear>ψ</unclear>αύηι</seg>, καὶ</seg>
                    <seg n="10" type="line" facs="z161r1_10">ἀπὸ <seg type="word">
                            <supplied reason="lost">τ</supplied>ᾶς</seg> ἁφᾶς <seg type="word">
                            <supplied reason="lost">εὐ</supplied>θεῖα</seg>
                        <seg type="clearword">γραμ</seg></seg>
                </seg>
                <seg n="156v1" type="folio">
                    <seg n="1" type="line" facs="z156v1_01">
                        <seg type="wordend">μὰ</seg> ἐπιζευχθῆ <seg type="word">
                            <supplied reason="lost">ἐ</supplied>πὶ</seg> τὸ σαμεῖον, ὅ
                        <expan>ἐστιν</expan></seg>
                    <seg n="2" type="line" facs="z156v1_02">ἀρχὰ τᾶς ἕλικος, ἃς ποιεῖ <seg
                            type="clearword">γω</seg></seg>
                    <seg n="3" type="line" facs="z156v1_03"><seg type="wordend">νίας</seg> ἁ
                        ἐφαπτομένα ποτὶ <choice>
                            <abbr>τὰ</abbr>
                            <expan>τὰν</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z156v1_04">ἐπιζευχθεῖσαν, ἀνίσοι <choice>
                            <abbr>ἐσσοῦντ</abbr>
                            <expan>ἐσσοῦνται</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z156v1_05">καὶ ἁ μὲν ἐν τοῖς <seg type="clearword"
                            >προαγουμέ</seg></seg>
                    <seg n="6" type="line" facs="z156v1_06"><seg type="wordend">νοις</seg> ἀμβλεῖα,
                        ἁ δὲ ἐν τοῖς <seg type="clearword">ἑ</seg></seg>
                    <seg n="7" type="line" facs="z156v1_07"><seg type="wordend">πομένοις</seg>
                        ὀξεῖα.</seg>
                </seg>
                <seg n="156v1" type="folio">
                    <seg n="7" type="line" facs="z156v1_07">ἔστω ἕλιξ,</seg>
                    <seg n="8" type="line" facs="z156v1_08">ἐφ’ ἇς τὰ ΑΒ ΓΔΘ, ἐν τᾶι <seg
                            type="clearword">πρώ</seg></seg>
                    <seg n="9" type="line" facs="z156v1_09"><seg type="wordend">ται</seg> περιφορᾶι
                            <seg type="word">γε<supplied reason="lost"
                            >γρ</supplied><unclear>α</unclear>μμένα</seg>,</seg>
                    <seg n="10" type="line" facs="z156v1_10">καὶ ἔστω τὸ μὲν <supplied reason="lost"
                            >Α</supplied> σαμεῖον ἀρχὰ</seg>
                    <seg n="11" type="line" facs="z156v1_11">τᾶς ἕλικος, ἁ δὲ <unclear>Α</unclear>Θ
                            <seg type="word">εὐθε<unclear>ῖ</unclear>α</seg>
                        <seg type="clearword">ἀρ</seg></seg>
                    <seg n="12" type="line" facs="z156v1_12"><seg type="wordend">χὰ</seg> τᾶς <seg
                            type="word">πε<supplied reason="lost">ρι</supplied>φορᾶς </seg>,
                            <supplied reason="lost">ὅ</supplied>
                        <supplied reason="lost">τε</supplied>
                        <choice>
                            <abbr>πρῶτ</abbr>
                            <expan>πρῶτος</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z156v1_13"><seg type="word">κ<supplied
                                reason="lost">ύ</supplied>κλος</seg> ὁ ΘΚΗ, ἐπιψαυέτω δέ</seg>
                    <seg n="14" type="line" facs="z156v1_14">τις εὐθεῖα γραμμὰ τᾶς <choice>
                            <abbr>ἕλικ</abbr>
                            <expan>ἕλικος</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z156v1_15">ἁ Δ<unclear>Ε</unclear>Ζ κατὰ τὸ
                            <supplied reason="lost">Δ,</supplied>
                        <seg type="word">
                            <supplied reason="lost">κ</supplied>αὶ</seg> ἀπὸ τοῦ</seg>
                    <seg n="16" type="line" facs="z156v1_16">Α <supplied reason="lost"
                        >ἐπὶ</supplied> τὸ Α <seg type="word">ἐπεζεύχ<supplied reason="lost"
                            >θω</supplied>
                        </seg> ἁ ΔΑ. <seg type="suppliedword">δει</seg></seg>
                </seg>
                <seg n="161r2" type="folio">
                    <seg n="1" type="line" facs="z161r2_01"><seg type="wordend">κτ<supplied
                                reason="lost">έον</supplied></seg>
                        <expan>ὅτι</expan> ἁ ΔΖ ποτὶ τὰν ΑΔ <seg type="clearword">ἀμβλεῖ</seg></seg>
                    <seg n="2" type="line" facs="z161r2_02"><seg type="wordend">αν</seg> ποιεῖ
                        γωνίαν.</seg>
                </seg>
                <seg n="161r2" type="folio">
                    <seg n="2" type="line" facs="z161r2_02">
                        <seg type="word">γεγρά<supplied reason="lost">φ</supplied>θ<supplied
                                reason="lost">ω</supplied>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr>κύκλ</abbr>
                                <expan><supplied reason="lost">κύκ</supplied>λος</expan>
                            </choice>
                        </seg>
                    </seg>
                    <seg n="3" type="line" facs="z161r2_03">ὁ ΔΤΝ <seg type="word">
                            κ<unclear>έ</unclear>ντρωι</seg> μὲν τῶι Α, <seg type="suppliedword"
                                >δι<supplied reason="lost">α</supplied><unclear>σ</unclear>τά</seg></seg>
                    <seg n="4" type="line" facs="z161r2_04"><seg type="wordend">ματι</seg> δὲ <seg
                            type="word">
                            <unclear>τ</unclear>ῶι</seg> ΑΔ· <seg type="word">
                            ἀν<unclear>α</unclear>γκαῖον</seg>
                        <seg type="word">δ<unclear>ὴ</unclear>
                        </seg>
                        <choice>
                            <abbr>τούτ</abbr>
                            <expan>τούτου</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z161r2_05">τοῦ <seg type="word">
                            <supplied reason="lost">κ</supplied>ύκλου</seg> τὰ μὲν ἐν τοῖς <seg
                            type="suppliedword">πρ<supplied reason="lost">οα</supplied></seg></seg>
                    <seg n="6" type="line" facs="z161r2_06"><seg type="wordend">γευμένοις</seg>
                        περιφέρειαν ἐντὸς</seg>
                    <seg n="7" type="line" facs="z161r2_07"><seg type="word"
                            >πίπ<unclear>τ</unclear>ειν </seg> τᾶς ἕλικος, τὰν δὲ ἐν τοῖς</seg>
                    <seg n="8" type="line" facs="z161r2_08">ἑπομένοις ἐκτὸς διὰ τὸ τᾶν <seg
                            type="suppliedword">ἀ</seg></seg>
                    <seg n="9" type="line" facs="z161r2_09"><seg type="wordend">
                            <supplied reason="lost">πὸ</supplied>
                        </seg> τοῦ Α ποτὶ τὰν ἕλικα <seg type="clearword">ποτιπι</seg></seg>
