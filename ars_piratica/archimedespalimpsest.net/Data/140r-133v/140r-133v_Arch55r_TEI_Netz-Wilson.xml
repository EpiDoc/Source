    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="140r1" type="folio">
                    <seg n="1" type="line" facs="z140r1_01"><expan>πρὸς</expan> ΒΞ οὕτως ἡ ΔΖ
                            <expan>πρὸς</expan> ΖΒ.</seg>
                </seg>
                <seg n="140r1" type="folio">
                    <seg n="1" type="line" facs="z140r1_01">πάλιν γὰρ <w part="I">ὁμοί</w></seg>
                    <seg n="2" type="line" facs="z140r1_02"><w part="F">ως</w>
                        <seg type="word">ἐπ<unclear>ε</unclear>ζεύχθωσαν</seg> αἱ ΗΕ ΑΘ· <seg
                            type="unclearword">παρ<unclear>ά</unclear>λ</seg></seg>
                    <seg n="3" type="line" facs="z140r1_03"><seg type="wordend">ληλοι</seg>
                        <expan>ἄρα</expan>
                        <expan>εἰσὶν</expan> τῆι ΒΖ· διὰ δὴ ταὐτά ἐστιν</seg>
                    <seg n="4" type="line" facs="z140r1_04">ὡς ἡ ΚΖ <expan>πρὸς</expan> ΚΒ ἥ τε ΗΚ
                            <expan>πρὸς</expan> ΚΛ καὶ ἡ ΕΜ</seg>
                    <seg n="5" type="line" facs="z140r1_05"><expan>πρὸς</expan> ΜΛ
                        <expan>καὶ</expan> ἡ ΗΘ πρὸς ΜΝ καὶ ἡ ΞΑ <expan>πρὸς</expan> ΞΝ</seg>
                    <seg n="6" type="line" facs="z140r1_06">καὶ ὡς <expan>ἄρα</expan> πάντα
                            <expan>πρὸς</expan> πάντα εἷς τὸν <expan>λογὸν</expan></seg>
                    <seg n="7" type="line" facs="z140r1_07">πρὸς ἕνα· ὡς <expan>ἄρα</expan> αἱ ΖΗ
                            <unclear>Ε</unclear>Θ ΑΞ <expan>πρὸς</expan> ΒΞ <expan>οὕτως</expan></seg>
                    <seg n="8" type="line" facs="z140r1_08">ἡ ΖΚ <expan>πρὸς</expan> ΚΒ. ὡς δὲ ἡ ΖΚ
                            <expan>πρὸς</expan> ΚΒ <expan>οὕτως</expan> ἡ ΔΖ</seg>
                    <seg n="9" type="line" facs="z140r1_09"><expan>πρὸς</expan> ΖΒ· <expan>ὡς</expan>
                        <expan>ἄρα</expan> ἡ ΔΖ <expan>πρὸς</expan> ΖΒ <expan>οὕτως</expan> αἱ ΖΗ ΕΘ
                        ΑΞ</seg>
                    <seg n="10" type="line" facs="z140r1_10"><expan>πρὸς</expan> ΞΒ. <choice>
                            <abbr>ἑξ</abbr>
                            <expan>ἑξῆς</expan>
                        </choice> τὸ ΣΧΗΜΑ.</seg>
                </seg>
                <seg n="133v1" type="folio">
                    <seg n="1" type="line" facs="z133v1_01">ἔστω ἐν σφαίραι μέγιστος κύκλος ὁ</seg>
                    <seg n="2" type="line" facs="z133v1_02">ΑΒΓΔ, καὶ ἐγγεγράφθω εἰς αὐτὸν</seg>
                    <seg n="3" type="line" facs="z133v1_03">πολύγωνον ἰσόπλευρον, τὸ δὲ <w part="I"
                            >πλῆ</w></seg>
                    <seg n="4" type="line" facs="z133v1_04"><w part="F">θος</w> τῶν πλευρῶν αὐτοῦ <w
                            part="I">μετρείσ</w></seg>
                    <seg n="5" type="line" facs="z133v1_05"><w part="F">θω</w> ὑπὸ τετράδος, αἱ δὲ
                        ΑΓ, ΔΒ</seg>
                    <seg n="6" type="line" facs="z133v1_06">διάμετροι ἔστωσαν ἐὰν δὴ <w part="I"
                            >μενού</w></seg>
                    <seg n="7" type="line" facs="z133v1_07"><w part="F">σης</w> τῆς ΑΓ διαμέτρου <w
                            part="I">περιενε</w></seg>
                    <seg n="8" type="line" facs="z133v1_08"><w part="F">χθῆ</w> ὁ ΑΒ ΓΔ κύκλος ἔχων
                        τὸ <w part="I">πο</w></seg>
                    <seg n="9" type="line" facs="z133v1_09"><w part="F">λύγωνον</w>, δῆλον
                            <expan>ὅτι</expan> ἡ μὲν <w part="I">περιφέ</w></seg>
                    <seg n="10" type="line" facs="z133v1_10"><w part="F">ρεια</w> αὐτοῦ κατὰ τῆς
                        ἐπιφανείας</seg>
                    <seg n="11" type="line" facs="z133v1_11">τῆς σφαίρας ἐνεχθήσεται, αἱ</seg>
                    <seg n="12" type="line" facs="z133v1_12">δὲ τοῦ πολυγώνου γωνίαι χωρὶς <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z133v1_13"><expan>πρὸς</expan> τοῖς ΑΓ σημείοις
                        κατὰ <choice>
                            <abbr>κύκλω</abbr>
                            <expan>κύκλων</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z133v1_14">περιφέρειαν ἐνεχθήσονται ἐν</seg>
                    <seg n="15" type="line" facs="z133v1_15">τῆι ἐπιφανείαι τῆς σφαίρας</seg>
                </seg>
                <seg n="140r2" type="folio">
                    <seg n="1" type="line" facs="z140r2_01"><seg type="word">γεγραμμέ<supplied
                                reason="lost">ν</supplied>ων</seg> ὀρθῶν <expan>πρὸς</expan>
                        <choice>
                            <abbr>τὸ</abbr>
                            <expan>τὸν</expan>
                        </choice> ΑΒΓΔ</seg>
                    <seg n="2" type="line" facs="z140r2_02">κύκλον· διάμετροι δὲ αὐτῶν <choice>
                            <abbr>εσν</abbr>
                            <expan>ἔσονται</expan>
                        </choice> αἱ</seg>
                    <seg n="3" type="line" facs="z140r2_03">ἐπιζευγνοῦσαι τὰς γωνίας τοῦ <w part="I"
                            >πο</w></seg>
                    <seg n="4" type="line" facs="z140r2_04">λυγώνου <choice>
                            <abbr>π</abbr>
                            <expan>παρὰ</expan>
                        </choice> τὴν ΒΔ οὖσαι. αἱ δὲ <seg type="word">το<supplied reason="lost"
                            >ῦ</supplied></seg></seg>
                    <seg n="5" type="line" facs="z140r2_05">πολυγώνου πλευραὶ κατά τινων</seg>
                    <seg n="6" type="line" facs="z140r2_06">κώνων ἐνεχθήσονται, αἱ μὲν ΑΖ,</seg>
                    <seg n="7" type="line" facs="z140r2_07">ΑΝ κατ᾽ ἐπιφανείας <seg type="word"
                                >κών<unclear>ο</unclear>υ</seg>, <seg type="word"><supplied
                                reason="lost">ο</supplied>ὗ</seg>
                        <seg type="unclearword"><unclear>β</unclear>ά</seg></seg>
                    <seg n="8" type="line" facs="z140r2_08"><seg type="wordend">σις</seg> μὲν ὁ
                        κύκλος ὁ περὶ <choice>
                            <abbr>διάμετρο</abbr>
                            <expan>διάμετρον</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z140r2_09">τὴν ΖΝ, κορυφὴ δὲ τὸ Α σημεῖον, αἱ</seg>
                    <seg n="10" type="line" facs="z140r2_10">δὲ ΖΗ, ΜΝ κατά τινος κωνικῆς</seg>
                    <seg n="11" type="line" facs="z140r2_11">ἐπιφανείας οἰσθήσονται, ἧς <w part="I"
                            >βά</w></seg>
                    <seg n="12" type="line" facs="z140r2_12"><w part="F">σις</w>
                        <expan>μὲν</expan> ὁ κύκλος ὁ περὶ διάμετρον</seg>
                    <seg n="13" type="line" facs="z140r2_13">τὴν ΗΝ, κορυφὴ δὲ τὸ σημεῖον, <w
                            part="I">κα</w></seg>
                    <seg n="14" type="line" facs="z140r2_14"><w part="F">θ᾽</w> ὃ <choice>
                            <abbr>συμβάλλουσι</abbr>
                            <expan>συμβάλλο<unclear>υ</unclear>σιν</expan>
                        </choice> ἐκβαλλόμεναι</seg>
                    <seg n="15" type="line" facs="z140r2_15">αἱ <seg type="word"
                            >Ζ<unclear>Η</unclear></seg>, <seg type="word"
                        ><unclear>Μ</unclear>Ν</seg> ἀλλήλαις τε καὶ τῆι ΑΓ,</seg>
                    <seg n="16" type="line" facs="z140r2_16">αἱ δὲ ΒΗ, ΜΔ πλευραὶ κατὰ <w part="I"
                            >κωνι</w></seg>
                    <seg n="17" type="line" facs="z140r2_17"><w part="F">κῆς</w> ἐπιφανείας
                        οἰσθήσονται,</seg>
                    <seg n="18" type="line" facs="z140r2_18">ἧς <seg type="word">βάσ<supplied
                                reason="lost">ις</supplied></seg>
                        <expan>μέν</expan>
                        <expan>ἐστιν</expan> ὁ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice> ὁ περὶ τὴν </seg>
                </seg>
                <seg n="133v2" type="folio">
                    <seg n="1" type="line" facs="z133v2_01">διάμετρον <seg type="word"
                            >τὴ<unclear>ν</unclear></seg> ΒΔ ὀρθὸς <expan>πρὸς</expan> τὸν <seg
                            type="word"><unclear>Α</unclear>Β</seg></seg>
                    <seg n="2" type="line" facs="z133v2_02">ΓΔ κύκλον, κορυφὴ δὲ τὸ σημεῖον,</seg>
                    <seg n="3" type="line" facs="z133v2_03">καθ᾽ ὃ <choice>
                            <abbr>συμβάλλουσι</abbr>
                            <expan>συμβάλλουσιν</expan>
                        </choice>
                        <w part="I">ἐκβαλλόμε</w></seg>
                    <seg n="4" type="line" facs="z133v2_04"><w part="F">ναι</w> αἱ ΒΗ, ΔΜ ἀλλήλαις
                        τε <expan>καὶ</expan> τῆι ΓΑ·</seg>
                    <seg n="5" type="line" facs="z133v2_05">ὁμοίως <seg type="word"><supplied
                                reason="lost">δ</supplied>ὲ</seg> καὶ ἐν τῶι ἑτέρωι <w part="I"
                            >ἡμικυ</w></seg>
                    <seg n="6" type="line" facs="z133v2_06"><w part="F">κλίωι</w> πλευραὶ κατὰ
                        κωνικῶν</seg>
                    <seg n="7" type="line" facs="z133v2_07">ἐπιφανειῶν οἰσθήσονται <choice>
                            <abbr>πάλι</abbr>
                            <expan>πάλιν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z133v2_08">ὁμοίων ταύταις. ἔσται δή τι <w part="I"
                            >σχῆ</w></seg>
                    <seg n="9" type="line" facs="z133v2_09"><w part="F">μα</w> ἐγγεγραμμένη ἐν τῆι <choice>
                            <abbr>σφαιρ</abbr>
                            <expan>σφαίρα</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z133v2_10">ὑπὸ κωνικῶν ἐπιφανειῶν <seg
                            type="expandedword">
                            <abbr>περι</abbr>
                        </seg></seg>
                    <seg n="11" type="line" facs="z133v2_11"><seg type="wordend">
                            <choice>
                                <abbr>εχόμεν</abbr>
                                <expan>περιεχόμενον</expan>
                            </choice>
                        </seg> τῶν προειρημένων, οὗ ἡ</seg>
                    <seg n="12" type="line" facs="z133v2_12">ἐπιφάνεια ἐλάσσων <choice>
                            <abbr>ἔστ</abbr>
                            <expan>ἔσται</expan>
                        </choice> τῆς <w part="I">ἐπι</w></seg>
                    <seg n="13" type="line" facs="z133v2_13"><w part="F">φανείας</w> τῆς
                    σφαίρας.</seg>
                </seg>
                <seg n="133v2" type="folio">
                    <seg n="13" type="line" facs="z133v2_13">
                        <w part="I">διαιρε</w>
                    </seg>
                    <seg n="14" type="line" facs="z133v2_14"><w part="F">θείσης</w> γὰρ τῆς σφαίρας
                        ὑπὸ τοῦ</seg>
                    <seg n="15" type="line" facs="z133v2_15">ἐπιπέδου τοῦ <choice>
                            <abbr>κα</abbr>
                            <expan>κατὰ</expan>
                        </choice> τὴν ΒΔ ὀρθοῦ <expan>πρὸς</expan></seg>
                    <seg n="16" type="line" facs="z133v2_16">τὸν ΑΒ ΓΔ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλον</expan>
                        </choice> ἡ ἐπιφάνεια τοῦ</seg>
                    <seg n="17" type="line" facs="z133v2_17">ἑτέρου ἡμισφαιρίου <expan>καὶ</expan> ἡ
                        ἐπιφάνεια</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
