    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="110r1" type="folio">
                    <seg n="1" type="line" facs="z110r1_01">ἔστω πρίσμα ὀρθὸν <sic>τετραγών</sic></seg>
                    <seg n="2" type="line" facs="z110r1_02">ἔχον <seg type="word">
                            <choice>
                                <abbr>βάσ</abbr>
                                <expan>βάσ<supplied reason="lost">εις</supplied></expan>
                            </choice>
                        </seg>, καὶ ἔστω αὐτοῦ μία <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z110r1_03"><seg type="word"
                            ><unclear>β</unclear>άσεων</seg> τὸ Α<unclear>Β</unclear>ΓΔ <seg
                            type="word">τε<unclear>τ</unclear>ράγωνον</seg>,</seg>
                    <seg n="4" type="line" facs="z110r1_04"><supplied reason="lost">καὶ</supplied>
                        ἐγγεγράφθω εἰς τὸ πρίσμα <seg type="unclearword">κύ</seg></seg>
                    <seg n="5" type="line" facs="z110r1_05"><seg type="wordend"
                            >λι<unclear>ν</unclear>δρος</seg>, καὶ ἔστω τοῦ κυλίνδρου</seg>
                    <seg n="6" type="line" facs="z110r1_06">βάσις ὁ ΕΖ ΗΘ κύκλος <w part="I"
                        >ἐφαπτό</w></seg>
                    <seg n="7" type="line" facs="z110r1_07"><w part="F">μενος</w> τῆς τοῦ τετραγώνου
                            <w part="I">πλευ</w></seg>
                    <seg n="8" type="line" facs="z110r1_08"><w part="F">ρᾶς</w> τῆς Ε <seg n="word"
                                >κατεν<unclear>α</unclear>ντ<unclear>ί</unclear>ον</seg>
                        <seg type="unclearword">ἐπ<unclear>ι</unclear>πέ</seg></seg>
                    <seg n="9" type="line" facs="z110r1_09"><seg type="wordend">δωι</seg> τοῦ ΑΒ ΓΔ
                        τῆς κατὰ τὴν ΓΔ</seg>
                    <seg n="10" type="line" facs="z110r1_10">ἐπίπεδον ἤχθω· ἀποτεμεῖ <sic>
                            <seg type="clearword">ἀ</seg>
                        </sic></seg>
                    <seg n="11" type="line" facs="z110r1_11"><sic>
                            <seg type="wordend">ποτεμεῖ</seg>
                        </sic> δὴ τοῦτο ἀπὸ τοῦ ὅλου</seg>
                    <seg n="12" type="line" facs="z110r1_12">πρίσματος ἔσται <seg type="word"
                                >τέ<unclear>τ</unclear>αρτον</seg>
                        <choice>
                            <abbr>μέρ</abbr>
                            <expan>μέρος</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z110r1_13">τοῦ ὅλου πρίσματος. αὐτὸ δὲ <seg
                            type="word">το<unclear>ῦ</unclear>το</seg></seg>
                    <seg n="14" type="line" facs="z110r1_14">ἔσται περιεχόμενον ὑπὸ τριῶν</seg>
                    <seg n="15" type="line" facs="z110r1_15"><seg type="word"
                            >π<unclear>α</unclear>ραλληλογράμμων</seg> καὶ δύο <w part="I">τρι</w></seg>
                    <seg n="16" type="line" facs="z110r1_16"><w part="F">γώνων</w> κατεναντίον <seg
                            type="word">ἀλλήλ<unclear>ο</unclear>ις</seg>.</seg>
                    <seg n="17" type="line" facs="z110r1_17">γεγράφθω δὴ ἐν τῶι
                        Ε<unclear>Ζ</unclear>Η <w part="I">ἡμικυ</w></seg>
                    <seg n="18" type="line" facs="z110r1_18"><w part="F">κλίωι</w> ὀρθογωνίου κώνου
                            <seg type="word">τομ<unclear>ή</unclear></seg>,</seg>
                    <seg n="19" type="line" facs="z110r1_19"><seg type="word">διά<supplied
                                reason="lost">μ</supplied>ε<supplied reason="lost"
                        >τ</supplied>ρος</seg> δὲ αὐτῆς ἔστω ἡ ΖΚ,</seg>
                </seg>
                <seg n="105v1" type="folio">
                    <seg n="1" type="line" facs="z105v1_01">ἔστω δὲ καὶ <seg type="word"
                                >πα<unclear>ρ</unclear></seg>’ ἣν δύνανται <supplied reason="lost"
                            >αἱ</supplied></seg>
                    <seg n="2" type="line" facs="z105v1_02">καταγόμεναι ἐν τῆι τομῆι <seg
                            type="unclearword">
                            <unclear>αὐ</unclear>
                        </seg></seg>
                    <seg n="3" type="line" facs="z105v1_03"><seg type="wordend">τὴ</seg> ἡ ΖΚ καὶ
                        ἤχθω τις ἐν τῶ</seg>
                    <seg n="4" type="line" facs="z105v1_04">ΔΗ παραλληλογράμμωι ἡ ΜΝ</seg>
                    <seg n="5" type="line" facs="z105v1_05"><seg type="word">
                            <choice>
                                <abbr><supplied reason="lost">πα</supplied>ράλληλ</abbr>
                                <expan><supplied reason="lost">πα</supplied>ράλληλος</expan>
                            </choice>
                        </seg> οὖσα τῆι ΚΖ· τεμεῖ</seg>
                    <seg n="6" type="line" facs="z105v1_06">δὴ αὕτη τὴν μὲν τοῦ <seg type="word">
                            <choice>
                                <abbr>ἡμικυκλί</abbr>
                                <expan>ἡμικυκλί<supplied reason="lost">ου</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="7" type="line" facs="z105v1_07">περιφέρειαν κατὰ τὸ
                        <unclear>Σ</unclear>, τὴν δὲ <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">οῦ</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="8" type="line" facs="z105v1_08">κώνου τομὴν κατὰ τὸ Λ. καί <seg
                            type="word">
                            <choice>
                                <abbr>ἔστ</abbr>
                                <expan>ἔστ<supplied reason="lost">αι</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="9" type="line" facs="z105v1_09">ἴσον τὸ ὑπὸ ΜΝΛ τῶι ἀπὸ <seg type="word"
                                >τῆ<supplied reason="lost">ς</supplied></seg></seg>
                    <seg n="10" type="line" facs="z105v1_10">Ν<unclear>Σ</unclear>· τοῦτο γάρ ἐστι
                            <seg type="word">σαφ<unclear>έ</unclear>ς</seg>· <expan>διὰ</expan>
                        <w part="I">τοῦ</w></seg>
                    <seg n="11" type="line" facs="z105v1_11"><w part="F">το</w> δὴ <choice>
                            <abbr>ἔστ</abbr>
                            <expan>ἔσται</expan>
                        </choice>, ὡς ἡ ΜΝ <expan>πρὸς</expan> ΝΛ, <choice>
                            <abbr>οὕτ</abbr>
                            <expan>οὕτως</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z105v1_12">τὸ ἀπὸ ΜΝ <expan>πρὸς</expan> τὸ ἀπὸ
                        ΝΣ. καὶ <w part="I">ἀ</w></seg>
                    <seg n="13" type="line" facs="z105v1_13"><w part="F">πὸ</w> τῆς ΜΝ ἐπίπεδον <w
                            part="I">ἀνεστά</w></seg>
                    <seg n="14" type="line" facs="z105v1_14"><w part="F">τω</w> ὀρθὸν
                        <expan>πρὸς</expan> τὴν ΕΗ· ποιήσει δὴ</seg>
