    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="39r1" type="folio">
                    <seg n="1" type="line" facs="z39r1_01">ἴση <expan>ἐστὶ</expan> κύκλωι, οὗ ἡ ἐκ
                        τοῦ <choice>
                            <abbr>κεντρ</abbr>
                            <expan>κέντρου</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z39r1_02">ἴση ἔσται τῆι ἀπὸ τῆς κορυφῆς</seg>
                    <seg n="3" type="line" facs="z39r1_03">ἐπὶ τὴν περιφέρειαν ἠγμένηι τοῦ</seg>
                    <seg n="4" type="line" facs="z39r1_04">
                        <seg type="word">κύκλο<supplied reason="lost">υ</supplied></seg>, <seg
                            type="word"><supplied reason="lost">ὅ</supplied>ς</seg> ἐστι βάσις τοῦ <choice>
                            <abbr>τηματ</abbr>
                            <expan>τμήματος</expan>
                        </choice>.</seg>
                </seg>
                <seg n="39r1" type="folio">
                    <seg n="5" type="line" facs="z39r1_05">ἔστω <expan>γὰρ</expan> σφαῖρα καὶ ἐν
                        αὐτῆι <seg type="expandedword">
                            <choice>
                                <abbr>μεγισ</abbr>
                                <expan>μέγιστος</expan>
                            </choice>
                        </seg></seg>
                    <seg n="6" type="line" facs="z39r1_06"><seg type="wordend">
                            <abbr>τος</abbr>
                        </seg>
                        <expan>κύκλος</expan>, καὶ <seg type="word"><supplied reason="lost"
                            >νο</supplied>είσθω</seg> τετμημένη <w part="I">ἐπιπέ</w></seg>
                    <seg n="7" type="line" facs="z39r1_07"><w part="F">δωι</w> ὀρθῶι τῶι κατὰ τὴν
                        ΑΔ, <expan>καὶ</expan></seg>
                    <seg n="8" type="line" facs="z39r1_08">τὸ ΑΒΔ ἔλασσον ἔστω ἡμισφαιρίου,</seg>
                    <seg n="9" type="line" facs="z39r1_09"><expan>καὶ</expan> διάμετρος ἡ ΒΓ
                            <expan>πρὸς</expan> ὀρθὰς τῆι ΑΔ,</seg>
                    <seg n="10" type="line" facs="z39r1_10">
                        <expan>καὶ</expan> ἀπὸ τῶν ΒΓ ἐπὶ τὸ Α <w part="I">ἐπεζεύ</w></seg>
                    <seg n="11" type="line" facs="z39r1_11"><w part="F">χθωσαν</w> αἱ ΒΑ ΑΓ, καὶ
                        ἔστω ὁ μὲν</seg>
                    <seg n="12" type="line" facs="z39r1_12">Ε κύκλος, οὗ ἡ ἐκ τοῦ κέντρου ἴση</seg>
                    <seg n="13" type="line" facs="z39r1_13"><expan>ἐστὶ</expan> τῆι ΑΒΓ, ὁ δὲ Ζ
                        κύκλος, οὗ ἡ ἐκ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z39r1_14">κέντρου ἴση <expan>ἐστὶ</expan> τῆι
                            Α<del>Β</del>Γ, ὁ δὲ Η <choice>
                            <abbr>κυκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice>,</seg>
                    <seg n="15" type="line" facs="z39r1_15">οὗ ἡ ἐκ τοῦ κέντρου ἴση
                        <expan>ἐστὶ</expan> τῆι ΒΓ· <expan>καὶ</expan></seg>
                    <seg n="16" type="line" facs="z39r1_16">ὁ Η κύκλος <expan>ἄρα</expan>
                        <choice>
                            <abbr>ισ</abbr>
                            <expan>ἴσος</expan>
                        </choice>
                        <expan>ἐστὶ</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῖς</expan>
                        </choice> δυσὶ κύκλοις</seg>
                    <seg n="17" type="line" facs="z39r1_17">τοῖς ΕΖ. ὁ δὲ Η κύκλος ἴσος
                        <expan>ἐστὶν</expan> ὅληι</seg>
                    <seg n="18" type="line" facs="z39r1_18">τῆι ἐπιφανείαι τῆς σφαίρας</seg>
                    <seg n="19" type="line" facs="z39r1_19">ἐπειδήπερ ἑκατέρα <w part="I"
                        >τετραπλα</w></seg>
                    <seg n="20" type="line" facs="z39r1_20"><w part="F">σία</w> ἐστὶ τοῦ περὶ <choice>
                            <abbr>μετρον</abbr>
                            <expan>διάμετρον</expan>
                        </choice> τὴν ΒΓ </seg>
                    <seg n="21" type="line" facs="z39r1_21">κύκλου, ὁ δὲ Ε κύκλος ἴσος
                        <expan>ἐστὶ</expan> τῆι</seg>
                    <seg n="22" type="line" facs="z39r1_22">ἐπιφανείαι τοῦ ΑΒΔ τμήματος</seg>
                    <seg n="23" type="line" facs="z39r1_23">
                        <supplied reason="lost">δέδεικται <expan>γὰρ</expan> τοῦτο</supplied>
                        <seg type="word">
                            <unclear>ἐ</unclear>
                            <supplied reason="lost">πὶ</supplied>
                        </seg>
                        <supplied reason="lost">τοῦ</supplied>
                        <seg type="word"><supplied reason="lost">ἐλάσσον</supplied>ος</seg>
                    </seg>
                    <gap/>
                </seg>
                <seg n="39r2" type="folio">
                    <gap/>
                    <seg n="1" type="line" facs="z39r2_01">ἔστω σφαῖρα καὶ ἐν αὐτῆι <seg type="word"
                                >μ<supplied reason="lost">έ</supplied>γιστος</seg></seg>
                    <seg n="2" type="line" facs="z39r2_02">κύκλος ὁ ΑΒΔ καὶ <seg type="word"
                                ><supplied reason="lost">κέ</supplied>ντρον</seg> τὸ Γ
                        <expan>καὶ</expan></seg>
                    <seg n="3" type="line" facs="z39r2_03">κῶνος ὁ βάσιν μὲν ἔχων <supplied
                            reason="lost">τὴν</supplied>
                        <w part="I">κύ</w></seg>
                    <seg n="4" type="line" facs="z39r2_04"><w part="F">κλον</w> τὸν ἴσον τῆι κατὰ
                        τὴν <supplied reason="lost">ΑΒΔ</supplied></seg>
                    <seg n="5" type="line" facs="z39r2_05">περιφέρειαν <seg type="word"
                            ><unclear>ἐ</unclear>πιφανείαι</seg>, ὕψος δὲ</seg>
                    <seg n="6" type="line" facs="z39r2_06">ἴσον τῆι ΒΓ· <seg type="word"
                            ><unclear>δ</unclear>εικτέον</seg>
                        <expan>ὅτι</expan> ὁ τομεὺς ὁ</seg>
                    <seg n="7" type="line" facs="z39r2_07">ΑΒΓΔ ἴσος ἐστὶ τῶι εἰρημένωι κώνωι.</seg>
                </seg>
                <seg n="39r2" type="folio">
                    <seg n="8" type="line" facs="z39r2_08">εἰ γὰρ μή, ἔστω μείζων ὁ τομεὺς τοῦ <w
                            part="I">κώ</w></seg>
                    <seg n="9" type="line" facs="z39r2_09"><w part="F">νου</w>, καὶ κείσθω ὁ Θ
                        κῶνος, οἷος <w part="I">εἴρη</w></seg>
                    <seg n="10" type="line" facs="z39r2_10"><w part="F">ται</w>· δύο δὴ μεγεθῶν
                        ἀνίσων <choice>
                            <abbr>οντω</abbr>
                            <expan>ὄντων</expan>
                        </choice>,</seg>
                    <seg n="11" type="line" facs="z39r2_11">τοῦ τομέως καὶ τοῦ Θ κώνου, <w part="I"
                            >εὑρήσ</w></seg>
                    <seg n="12" type="line" facs="z39r2_12"><w part="F">θωσαν</w> δύο γραμμαὶ αἱ ΔΕ,
                        μείζων</seg>
                    <seg n="13" type="line" facs="z39r2_13">δὲ ἡ Δ τῆς Ε, καὶ ἐλάσσονα λόγον</seg>
                    <seg n="14" type="line" facs="z39r2_14">ἐχέτω ἡ Δ <expan>πρὸς</expan> Ε ἤπερ ὁ
                        τομεὺς <expan>πρὸς</expan> τὸν <w part="I">κῶ</w></seg>
                    <seg n="15" type="line" facs="z39r2_15"><w part="F">νον</w>, καὶ εἰλήφθωσαν δύο <choice>
                            <abbr>γραμμ</abbr>
                            <expan>γραμμαὶ</expan>
                        </choice>
                    </seg>
                    <seg n="16" type="line" facs="z39r2_16">αἱ ΖΗ, ὅπως τῶι ἴσωι ὑπερέχει ἡ Δ</seg>
                    <seg n="17" type="line" facs="z39r2_17">τῆς Ζ καὶ ἡ Ζ τῆς Η <expan>καὶ</expan> ἡ
                        Η τῆς Ε,</seg>
                    <seg n="18" type="line" facs="z39r2_18">καὶ περὶ τὸν ἐπίπεδον τομέα τοῦ </seg>
                    <seg n="19" type="line" facs="z39r2_19">κύκλου περιγεγράφθω <choice>
                            <abbr>πολυγων</abbr>
                            <expan>πολύγωνον</expan>
                        </choice></seg>
                    <seg n="20" type="line" facs="z39r2_20">ἰσόπλευρον καὶ ἀρτιογώνιον,
                        <expan>καὶ</expan></seg>
                    <seg n="21" type="line" facs="z39r2_21">τούτωι ὅμοιον ἐγγεγράφθω, ὅπως</seg>
                    <seg n="22" type="line" facs="z39r2_22">ἡ τοῦ περιγεγραμμένου <seg type="word"
                                ><unclear>π</unclear>λ<unclear>ευ</unclear><supplied reason="lost"
                            >ρ</supplied>ὰ</seg>
                    </seg>
                    <seg n="23" type="line" facs="z39r2_23">ἐλάσσονα λόγον ἔχη <expan>πρὸς</expan>
                        τὴν τοῦ <sic>ἐγ</sic></seg>
                    <gap/>
                </seg>
            </ab>
