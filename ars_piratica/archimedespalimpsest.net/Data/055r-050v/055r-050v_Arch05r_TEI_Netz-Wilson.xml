    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="55r1" type="folio">
                    <seg n="1" type="line" facs="z55r1_01">ὕστερον πυραμίδι ἴσον τὸ κατὰ</seg>
                    <seg n="2" type="line" facs="z55r1_02">τὸ Ζ στερεὸν, τὰ δὲ μέρεα τοῦ <w part="I"
                            >ὑ</w></seg>
                    <seg n="3" type="line" facs="z55r1_03"><w part="F">γροῦ</w> τοῦ ἐν τᾶι πρώται <w
                            part="I">πυρα</w></seg>
                    <seg n="4" type="line" facs="z55r1_04"><w part="F">μίδι</w> τὰ ὑπὸ τὰν
                        ἐπιφάνειαν <choice>
                            <abbr>τα</abbr>
                            <expan>τὰν</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z55r1_05">κατὰ τὰ ΞΟ περιφέρειαν καὶ τὸ</seg>
                    <seg n="6" type="line" facs="z55r1_06">ἐν τᾶι δευτέραι τῶν ὑπὸ τὰν <w part="I"
                            >ἐπι</w></seg>
                    <seg n="7" type="line" facs="z55r1_07"><w part="F">φάνειαν</w> τὰν κατὰ τὸ ΝΟΠ
                            <w part="I">περι</w></seg>
                    <seg n="8" type="line" facs="z55r1_08"><w part="F">φέρειαν</w> ἐξ ἴσου τέ ἐντι
                        κείμενα</seg>
                    <seg n="9" type="line" facs="z55r1_09">καὶ συνεχέα ἀλλήλοις. οὐχ ὁμοίως</seg>
                    <seg n="10" type="line" facs="z55r1_10">δὲ θλίβονται· τὸ μὲν γὰρ ἐν τᾶι <w
                            part="I">πρώ</w></seg>
                    <seg n="11" type="line" facs="z55r1_11"><w part="F">ται</w> πυραμίδι θλίβεται
                        τῶι κατὰ</seg>
                    <seg n="12" type="line" facs="z55r1_12">τὸ Ζ στερεῶι μεγέθει καὶ τῶι <w part="I"
                            >περιέ</w></seg>
                    <seg n="13" type="line" facs="z55r1_13"><w part="F">χοντι</w> ὑγρῶι αὐτὸ καὶ
                        ἐόντι ἐν τῶι</seg>
                    <seg n="14" type="line" facs="z55r1_14">τόπωι τᾶς πυραμίδος τῶι κατὰ</seg>
                    <seg n="15" type="line" facs="z55r1_15">τὸ ΑΒ ΟΞ, τὸ δ᾽ ἐν τᾶι <seg type="word"
                                >ἑ<supplied reason="lost">τ</supplied>έραι</seg>
                        <w part="I">πυρα</w></seg>
                    <seg n="16" type="line" facs="z55r1_16"><w part="F">μίδι</w> θλίβεται τῶι ὑγρῶι
                        τῶι <w part="I">πε</w></seg>
                    <seg n="17" type="line" facs="z55r1_17"><w part="F">ριέχοντι</w> αὐτὸ <seg
                            type="word"><supplied reason="lost"
                        >κ</supplied><unclear>α</unclear>ὶ</seg> ἐόντι τᾶς <w part="I">πυρα</w></seg>
                    <seg n="18" type="line" facs="z55r1_18"><w part="F">μίδος</w> ἐν τῶι τόπωι τῶι
                        κατὰ</seg>
                    <seg n="19" type="line" facs="z55r1_19">τὸ ΠΟ ΒΓ, ἔστι τὸ βάρος τὸ κατὰ</seg>
                </seg>
                <seg n="50v1" type="folio">
                    <seg n="1" type="line" facs="z50v1_01">τὸ Ζ<unclear>Η</unclear> τὸν τοῦ ὑγροῦ
                        τοῦ κατὰ τὸ</seg>
                    <seg n="2" type="line" facs="z50v1_02">ΖΗ, ἐπειδὴ τῶι μὲν μεγέθει ἴσον</seg>
                    <seg n="3" type="line" facs="z50v1_03">ἐστίν, κουφότερον δὲ ὑπόκειται</seg>
                    <seg n="4" type="line" facs="z50v1_04">τὸ στερεὸν μέγεθος εἶμεν τοῦ <seg
                            type="unclearword">ὑ</seg></seg>
                    <seg n="5" type="line" facs="z50v1_05"><seg type="wordend"
                            ><unclear>γ</unclear>ροῦ,</seg>
                        <seg type="word">τ<unclear>ὰ</unclear></seg> δὲ περιέχοντος ὑγροῦ τὰ</seg>
                    <seg n="6" type="line" facs="z50v1_06"><supplied reason="lost">Ζ</supplied>Η
                        μεγέθη ἑκατέρα τῶν <seg type="unclearword">πυρα</seg></seg>
                    <seg n="7" type="line" facs="z50v1_07"><seg type="wordend"
                            ><unclear>μί</unclear>δων</seg> ἴσα· μᾶλλον οὖν <w part="I">θλιβή</w></seg>
                    <seg n="8" type="line" facs="z50v1_08"><w part="F">σεται</w> τὸ μέρος τοῦ ὑγροῦ
                        τὸ ὑπὸ</seg>
                    <seg n="9" type="line" facs="z50v1_09">τὴν ἐπιφάνειαν τὰν κατὰ τὰν</seg>
                    <seg n="10" type="line" facs="z50v1_10">ΟΠ περιφέρειαν· ἐξωθήσοι οὖν</seg>
                    <seg n="11" type="line" facs="z50v1_11"><seg type="word">τ<supplied
                                reason="lost">ὸ</supplied></seg>
                        <sic>
                            <seg type="word">ἶσ<add rend="superscript">σ</add>ον</seg>
                        </sic> θλιβόμενον, καὶ οὐ <w part="I">με</w></seg>
                    <seg n="12" type="line" facs="z50v1_12"><w part="F">νεῖ</w> τὸ ὑγρὸν ἀκίνητον. <sic>
                            <seg type="suppliedword">ὑπόκει</seg>
                        </sic></seg>
                    <seg n="13" type="line" facs="z50v1_13"><sic>
                            <seg type="wordend">τ <supplied reason="lost">ο</supplied></seg>
                        </sic> δέ· οὐκ ἄρα καταδύσεται <choice>
                            <abbr>ολο</abbr>
                            <expan>ὅλο<supplied reason="lost">ν</supplied></expan>
                        </choice>,</seg>
                    <seg n="14" type="line" facs="z50v1_14">ἀλλ᾽ ἔσσεταί τι</seg>
                    <figure n="1.4.1">
                        <figDesc xml:lang="eng">Figure 1.4.1</figDesc>
                    </figure>
                    <seg n="15" type="line" facs="z50v1_15"><seg type="word"
                            >αὐ<unclear>τ</unclear>οῦ</seg> ἐκτὸς</seg>
                    <seg n="16" type="line" facs="z50v1_16">τᾶς <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice>
                        <w part="I">ὑ</w></seg>
                    <seg n="17" type="line" facs="z50v1_17">
                        <w part="F">γροῦ</w>
                        <w part="I">ἐπι</w>
                    </seg>
                    <seg n="18" type="line" facs="z50v1_18">
                        <w part="F">φανείας.</w>
                    </seg>
                </seg>
                <seg n="55r2" type="folio">
                    <seg n="1" type="line" facs="z55r2_01">τῶν στερεῶν μεγεθέων ὅ κα <seg
                            n="κουφότερον" type="suppliedword">
                            <supplied reason="lost">
                                <choice>
                                    <abbr>κ</abbr>
                                    <expan>κου</expan>
                                </choice>
                            </supplied>
                        </seg>
                    </seg>
                    <seg n="2" type="line" facs="z55r2_02"><seg type="wordend">φότερον</seg> τοῦ
                        ὑγροῦ, ἀφεθὲν εἰς τὸ <w part="I">ὑ</w></seg>
                    <seg n="3" type="line" facs="z55r2_03"><w part="F">γρὸν</w> τοσοῦτο <seg
                            type="word">καταδύ<unclear>σ</unclear>εται,</seg> ὡς τὸν</seg>
                    <seg n="4" type="line" facs="z55r2_04">ταλικοῦτον ὄγκον τοῦ ὑγροῦ, <choice>
                            <abbr>ἡλίκ</abbr>
                            <expan>ἡλίκος</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z55r2_05">ἐστὶν ὁ <seg type="word"
                            >το<unclear>ῦ</unclear></seg>
                        <seg type="word">κα<unclear>τα</unclear>δεδυκότος</seg> ὄγκος, </seg>
                    <seg n="6" type="line" facs="z55r2_06">ἴσον βάρος ἔχειν ὅλωι τῶι μεγέθει.</seg>
                </seg>
                <seg n="55r2" type="folio">
                    <seg n="7" type="line" facs="z55r2_07"><sic>κατασκευάσθω</sic> ταὐτὰ τοῖς <w
                            part="I">πρότε</w></seg>
                    <seg n="8" type="line" facs="z55r2_08"><w part="F">ρον,</w> καὶ ἔστω τὸ ὑγρὸν
                        ἀκίνητον,</seg>
                    <seg n="9" type="line" facs="z55r2_09">ἔστω δὲ κουφότερον τοῦ ὑγροῦ τὸ ΕΖ</seg>
                    <seg n="10" type="line" facs="z55r2_10">ΗΘ μέγεθος. ἐπεὶ οὖν ἀκίνητόν <choice>
                            <abbr>ἐστι</abbr>
                            <expan>ἐστιν</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z55r2_11">τὸ ὑγρόν, ὁμοίως θλιβήσεται τὰ</seg>
                    <seg n="12" type="line" facs="z55r2_12">μέρη <seg type="word"
                            >αὐτο<unclear>ῦ</unclear></seg> τὰ ἐξ ἴσου κείμενα·</seg>
                    <seg n="13" type="line" facs="z55r2_13">ὁμοίως ἄρα θλιβήσεται τὸ ὑγρὸν</seg>
                    <seg n="14" type="line" facs="z55r2_14">τὸ ὑπὸ τὰν ἐπιφάνειαν τὰν <w part="I"
                        >κα</w></seg>
                    <seg n="15" type="line" facs="z55r2_15"><w part="F">τὰ</w> ΝΞΟ καὶ ΠΟ
                        περιφέρειαν· <w part="I">ὥσ</w></seg>
                    <seg n="16" type="line" facs="z55r2_16"><w part="F">τε</w> ἴσον ἐστὶ τὸ βάρος,
                        ὧι <w part="I">θλίβον</w></seg>
                    <seg n="17" type="line" facs="z55r2_17"><w part="F">ται.</w> ἔστι δὲ καὶ τοῦ
                        ὑγροῦ τὸ βάρος</seg>
                    <seg n="18" type="line" facs="z55r2_18">τὸ ἐν τᾶι πρώτα πυραμίδι χωρὶς</seg>
                    <seg n="19" type="line" facs="z55r2_19">τοῦ ΒΗΘ στερεοῦ <seg type="word"
                                >ἴ<supplied reason="lost">σ</supplied><unclear>ο</unclear>ν</seg>
                        τῶι βάρει τῶι</seg>
                </seg>
                <seg n="50v2" type="folio">
                    <seg n="1" type="line" facs="z50v2_01">ὑγρῶι <seg type="word">το<supplied
                                reason="lost">ῦ</supplied></seg>
                        <seg type="word"><supplied reason="lost">ἐ</supplied>ν</seg> τᾶι <seg
                            type="word"><supplied reason="lost">ἑτ</supplied>έραι</seg>
                        <seg type="word">πυραμί<supplied reason="lost">δι</supplied></seg></seg>
                    <seg n="2" type="line" facs="z50v2_02">χωρὶς <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> ΡΣ ΤΥ ὑγροῦ· δῆλον οὖν, <expan>ὅτι</expan></seg>
                    <seg n="3" type="line" facs="z50v2_03">τὸ τοῦ ΕΖ ΗΘ μεγέθους βάρος ἴσον</seg>
                    <seg n="4" type="line" facs="z50v2_04">ἐστὶ τῶι <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> ΡΣ ΤΥ ὑγροῦ βάρει. <w part="I">φα</w></seg>
                    <seg n="5" type="line" facs="z50v2_05"><w part="F">νερὸν</w> οὖν,
                        <expan>ὅτι</expan> ταλικοῦτος ὄγκος <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z50v2_06">ὑγροῦ, ἁλίκον ἐστὶ τὸ δεδυκὸς τοῦ <w
                            part="I">στε</w></seg>
                    <seg n="7" type="line" facs="z50v2_07"><w part="F">ρεοῦ</w> μεγέθεος, ἴσον βάρος
                        ἔχει</seg>
                    <seg n="8" type="line" facs="z50v2_08">ὅλωι τῶι μεγέθει.</seg>
                </seg>
                <seg n="50v2" type="folio">
                    <seg n="9" type="line" facs="z50v2_09">τὰ κουφότερα</seg>
                    <seg n="10" type="line" facs="z50v2_10">στερεὰ τοῦ <w part="I">ὑ</w></seg>
                    <seg n="11" type="line" facs="z50v2_11">
                        <w part="F">γροῦ</w>
                        <sic>
                            <w part="I">βιαθέν</w>
                        </sic>
                    </seg>
                    <seg n="12" type="line" facs="z50v2_12"><sic>
                            <w part="F">τα</w>
                        </sic> εἰς τὸ <choice>
                            <abbr>υγρ</abbr>
                            <expan>ὑγρὸν</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z50v2_13">ἀναφέρεται </seg>
                    <seg n="14" type="line" facs="z50v2_14">τοσαύτηι βίαι</seg>
                    <seg n="15" type="line" facs="z50v2_15">ἐς τὸ ἄνω, <choice>
                            <abbr>οσο</abbr>
                            <expan>ὅσον</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z50v2_16">ἐστὶ τὸ βάρος, ὃ βαρύτερόν ἐστι τοῦ</seg>
                    <seg n="17" type="line" facs="z50v2_17">μεγέθεος τὸ ὑγρὸν τὸ ἴσον ὄγκον</seg>
                    <seg n="18" type="line" facs="z50v2_18">ἔχον τῶι μεγέθει. </seg>
                </seg>
                <seg n="50v2" type="folio">
                    <seg n="18" type="line" facs="z50v2_18">ἔστω τι μέγεθος</seg>
                    <seg n="19" type="line" facs="z50v2_19">τὸ Α κουφότερον τοῦ ὑγροῦ, ἔστω</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
