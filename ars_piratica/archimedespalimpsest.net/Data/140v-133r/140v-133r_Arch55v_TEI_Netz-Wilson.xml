    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="140v1" type="folio">
                    <seg n="1" type="line" facs="z140v1_01"><supplied reason="lost">τοῦ</supplied>
                        <seg type="word"><unclear>σ</unclear>χήματος</seg> τοῦ ἐν αὐτῶι <seg
                            type="unclearword"><unclear>ἐ</unclear>γγε</seg></seg>
                    <seg n="2" type="line" facs="z140v1_02"><seg type="wordend"
                            >γραμμέν<unclear>ο</unclear>υ</seg> τὰ αὐτὰ <seg type="word"
                                >πέρατ<supplied reason="lost">α</supplied></seg>
                        <seg type="expandedword">
                            <choice>
                                <abbr>
                                    <supplied reason="lost">εχ</supplied>
                                </abbr>
                                <expan>ἔχουσιν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="3" type="line" facs="z140v1_03"><seg type="wordend">
                            <abbr>σιν</abbr>
                        </seg> ἐν <seg type="word">ἑν<supplied reason="lost">ὶ</supplied></seg>
                        <seg type="word">ἐπι<unclear>πέ</unclear><supplied reason="lost"
                            >δωι</supplied></seg>· <seg type="word"><supplied reason="lost"
                                >ἀμφ</supplied>οτ<unclear>έ</unclear>ρων</seg></seg>
                    <seg n="4" type="line" facs="z140v1_04">γὰρ τῶν ἐπιφανειῶν πέρας ἐστὶ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z140v1_05"><seg type="word"><supplied reason="lost"
                                >κ</supplied><unclear>ύ</unclear>κ<supplied reason="lost"
                            >λο</supplied>υ</seg> ἡ περιφέρεια τοῦ περὶ <w part="I">διά</w></seg>
                    <seg n="6" type="line" facs="z140v1_06"><w part="F">μετρον</w> τὴν ΒΔ ὀρθοῦ πρὸς
                        τὸν ΑΒΓΔ</seg>
                    <seg n="7" type="line" facs="z140v1_07"><seg type="word">κ<supplied
                                reason="lost">ύκλο</supplied><unclear>ν</unclear></seg>· καί
                            <expan>ἐστὶν</expan>
                        <seg type="word">ἀμφό<supplied reason="lost">τε</supplied>ραι</seg> ἐπὶ τὰ
                            <w part="I">αὐ</w></seg>
                    <seg n="8" type="line" facs="z140v1_08"><w part="F">τὰ</w>
                        <seg type="word">κο<unclear>ῖ</unclear>λαι</seg>, <expan>καὶ</expan>
                        περιλαμβάνεται <choice>
                            <abbr>αὐτ</abbr>
                            <expan>αὐτῶν</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z140v1_09">ἡ ἑτέρα ὑπὸ τῆς ἑτέρας <seg
                            type="unclearword">ἐπιφα</seg></seg>
                    <seg n="10" type="line" facs="z140v1_10"><seg type="wordend"
                            >νεί<unclear>α</unclear>ς</seg> καὶ τῆς ἐπιπέδου τῆς τὰ <w part="I"
                        >αὐ</w></seg>
                    <seg n="11" type="line" facs="z140v1_11"><w part="F">τὰ</w> πέρατα ἐχούσης
                        αὐτῆι. <w part="I">ὁμοί</w></seg>
                    <seg n="12" type="line" facs="z140v1_12"><w part="F">ως</w>
                        <seg type="word"><unclear>δ</unclear>ὲ</seg> καὶ τοῦ <seg type="word"
                                >ἐ<supplied reason="lost">ν</supplied></seg> τῶι ἑτέρωι <w part="I"
                            >ἡμισφαι</w></seg>
                    <seg n="13" type="line" facs="z140v1_13"><w part="F">ρίωι</w> σχήματος <seg
                            type="word">ἐπ<supplied reason="lost"
                            >ιφ</supplied>ά<unclear>ν</unclear>εια</seg>
                        <w part="I">ἐλάσ</w></seg>
                    <seg n="14" type="line" facs="z140v1_14"><w part="F">σων</w> ἐστὶν τῆς τοῦ <seg
                            type="word"><unclear>ἡμ</unclear>ι<supplied reason="lost"
                                >σφ</supplied>αιρίο<unclear>υ</unclear></seg>
                        <w part="I">ἐπι</w></seg>
                    <seg n="15" type="line" facs="z140v1_15"><w part="F">φανείας</w>·
                        <expan>καὶ</expan> ὅλη οὖν ἡ ἐπιφάνεια</seg>
                    <seg n="16" type="line" facs="z140v1_16">τοῦ σχήματος <seg type="word"
                                >τ<unclear>ο</unclear>ῦ</seg> ἐν τῆι σφαίραι</seg>
                    <seg n="17" type="line" facs="z140v1_17">ἐλάσσων ἐστὶν τῆς ἐπιφανείας τῆς</seg>
                    <seg n="18" type="line" facs="z140v1_18"><supplied reason="lost"
                        >σφαίρας</supplied>. </seg>
                </seg>
                <seg n="133r1" type="folio">
                    <num>ΚΔ</num>
                </seg>
                <seg n="133r1" type="folio">
                    <seg n="1" type="line" facs="z133r1_01">ἡ τοῦ ἐγγραφομένου σχήματος εἰς <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z133r1_02">σφαῖραν ἐπιφάνεια ἴση
                        <expan>ἐστὶν</expan> κύκλω,</seg>
                    <seg n="3" type="line" facs="z133r1_03">οὗ ἡ ἐκ τοῦ κέντρου δύναται τὸ <seg
                            type="suppliedword">π<supplied reason="lost">ε</supplied></seg></seg>
                    <seg n="4" type="line" facs="z133r1_04"><seg type="wordend">ριεχ<supplied
                                reason="lost">ό</supplied>μενον</seg> ὑπό τε τῆς πλευρᾶς <choice>
                            <abbr>τ</abbr>
                            <expan>τ<unclear>οῦ</unclear></expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z133r1_05">σχήματος <expan>καὶ</expan> τῆς ἴσης
                        πάσαις <choice>
                            <abbr>τς</abbr>
                            <expan>ταῖς</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z133r1_06">ἐπιζευγνυούσαις τὰς πλευρὰς</seg>
                    <seg n="7" type="line" facs="z133r1_07">τοῦ <seg type="word"
                            >πολ<unclear>υγ</unclear>ώνου</seg>
                        <expan>παραλλήλας</expan> οὔσας τῆι <w part="I">ὑ</w></seg>
                    <seg n="8" type="line" facs="z133r1_08"><w part="F">πὸ</w>
                        <seg type="word">δύ<supplied reason="lost">ο</supplied></seg> πλευρὰς <seg
                            type="word">το<unclear>ῦ</unclear></seg>
                        <choice>
                            <abbr>πολυγών</abbr>
                            <expan>πολυγώνου</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z133r1_09"><supplied reason="lost">ὑποτεινούσα</supplied>
                        <seg type="word"><unclear>εὐθεί</unclear>α</seg>.</seg>
                </seg>
                <seg n="133r1" type="folio">
                    <seg n="9" type="line" facs="z133r1_09">ἔστω</seg>
                </seg>
                <seg n="140v2" type="folio">
                    <seg n="1" type="line" facs="z140v2_01">ἐν <seg type="word"
                            >σφαίρ<unclear>α</unclear>ι</seg> μέγιστος <seg type="word">κ<supplied
                                reason="lost">ύ</supplied>κλος</seg> ὁ ΑΒΓΔ,</seg>
                    <seg n="2" type="line" facs="z140v2_02">καὶ ἐν αὐτῶι πολύγωνον <w part="I"
                            >ἐγγεγρά</w></seg>
                    <seg n="3" type="line" facs="z140v2_03"><w part="F">φθω</w> ἰσόπλευρον, οὗ αἱ
                        πλευραὶ</seg>
                    <seg n="4" type="line" facs="z140v2_04">ὑπὸ τετράδος μετροῦνται, καὶ ἀπὸ</seg>
                    <seg n="5" type="line" facs="z140v2_05">τοῦ πολυγώνου τοῦ ἐγγεγραμμένου</seg>
                    <seg n="6" type="line" facs="z140v2_06">νοείσθω τι εἰς τὴν σφαῖραν <w part="I"
                            >ἐγγρα</w></seg>
                    <seg n="7" type="line" facs="z140v2_07"><w part="F">φὲν</w> σχῆμα, καὶ
                        ἐπεζεύχθωσαν αἱ</seg>
                    <seg n="8" type="line" facs="z140v2_08">ΕΖ, ΗΘ, ΓΔ, ΚΛ, ΜΝ παράλληλοι <w
                            part="I">οὖ</w></seg>
                    <seg n="9" type="line" facs="z140v2_09"><w part="F">σαι</w> τῆι ὑπὸ δύο πλευρὰς
                            <seg type="expandedword">
                            <choice>
                                <abbr>ὑποτειν</abbr>
                                <expan>ὑποτεινούσηι</expan>
                            </choice>
                        </seg></seg>
                    <seg n="10" type="line" facs="z140v2_10"><seg type="wordend">
                            <abbr>σηι</abbr>
                        </seg> εὐθείαι, κύκλος δέ τις <seg type="suppliedword">ἐκκείσ</seg></seg>
                    <seg n="11" type="line" facs="z140v2_11">
                        <seg type="wordend">
                            <supplied reason="lost">θω</supplied>
                        </seg>
                        <supplied reason="lost">ὁ Ξ, οὗ ἡ ἐκ τοῦ κέντρου</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">δυνά</supplied>
                        </seg>
                    </seg>
                </seg>
                <seg n="133r2" type="folio">
                    <seg n="1" type="line" facs="z133r2_01">
                        <seg type="wordend">
                            <supplied reason="lost">σθω</supplied>
                        </seg>
                        <supplied reason="lost">τὸ περιεχόμενον ὑπό τε τῆς ΑΕ</supplied>
                    </seg>
                    <seg n="2" type="line" facs="z133r2_02"><supplied reason="lost">καὶ</supplied>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ῆ</unclear>
                            <supplied reason="lost">ς</supplied>
                        </seg> ἴσης ταῖς ΕΖ, ΗΘ, ΓΔ, ΚΛ,</seg>
                    <seg n="3" type="line" facs="z133r2_03"><supplied reason="lost">ΜΝ</supplied>·
                            <seg type="word"><unclear>λ</unclear>έγω</seg>
                        <expan>ὅτι</expan> ὁ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice> οὗτος <expan>ἴσος</expan>
                        <expan>ἐστὶ</expan> τῆι <seg type="unclearword">ἐ</seg></seg>
                    <seg n="4" type="line" facs="z133r2_04"><seg type="wordend"
                                >π<unclear>ι</unclear>φανεία<unclear>ι</unclear></seg> τοῦ εἰς τὴν
                        σφαῖραν <seg type="suppliedword">ἐγ</seg></seg>
                    <seg n="5" type="line" facs="z133r2_05"><seg type="wordend"><supplied
                                reason="lost">γ</supplied>ραφομένο<unclear>υ</unclear></seg>
                        σχήματος.</seg>
                </seg>
                <seg n="133r2" type="folio">
                    <seg n="5" type="line" facs="z133r2_05">
                        <w part="I">ἐκκείσθω</w>
                    </seg>
                    <seg n="6" type="line" facs="z133r2_06"><w part="F">σαν</w> γὰρ κύκλοι οἱ Ο, Π,
                        Ρ, Σ, Τ, Υ, <expan>καὶ</expan> τοῦ μὲν</seg>
                    <seg n="7" type="line" facs="z133r2_07">Ο <unclear>ἡ</unclear> ἐκ τοῦ κέντρου
                        δυνάσθω τὸ <w part="I">πε</w></seg>
                    <seg n="8" type="line" facs="z133r2_08"><w part="F">ριεχόμενον</w> ὑπό τε τῆς ΕΑ
                        καὶ τῆς</seg>
                    <seg n="9" type="line" facs="z133r2_09">ἡμισείας τῆς ΕΖ, ἡ δὲ ἐκ τοῦ <choice>
                            <abbr>κέντρ</abbr>
                            <expan>κέντρου</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z133r2_10">τοῦ Π δυνάσθω τὸ <choice>
                            <abbr>περιεχόμ</abbr>
                            <expan>περιεχόμενον</expan>
                        </choice> ὑπό τε</seg>
                    <seg n="11" type="line" facs="z133r2_11">τῆς ΕΑ <expan>καὶ</expan> τῆς <seg
                            type="word">ἡ<unclear>μ</unclear>ι<supplied reason="lost"
                            >σεία</supplied>ς</seg> τῶν ΕΖ, ΗΘ,</seg>
                    <seg n="12" type="line" facs="z133r2_12">ἡ δὲ ἐκ τοῦ κέντρου τοῦ <supplied
                            reason="lost">Ρ</supplied> δυνάσθω τὸ</seg>
                    <seg n="13" type="line" facs="z133r2_13">περιεχόμενον ὑπὸ τῆς ΕΑ καὶ τῆς</seg>
                    <seg n="14" type="line" facs="z133r2_14">ἡμισείας τῶν <seg type="word"
                                >Η<unclear>Θ</unclear></seg>, ΓΔ, ἡ δὲ ἐκ τοῦ</seg>
                    <seg n="15" type="line" facs="z133r2_15">κέντρου τοῦ Σ δυνάσθω τὸ <choice>
                            <abbr>περιεχόμ</abbr>
                            <expan>περ<supplied reason="lost"
                                >ι</supplied><unclear>εχό</unclear>μενον</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z133r2_16"><seg type="word">ὑπ<supplied
                                reason="lost">ό</supplied></seg>
                        <seg type="word"><supplied reason="lost">τ</supplied>ε</seg> τῆς ΕΑ καὶ τῆς
                            <seg type="word">ἡμισ<supplied reason="lost">εία</supplied>ς</seg>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z133r2_17">ΓΔ, ΚΛ, ἡ δὲ ἐκ τοῦ κέντρου <seg
                            type="word">το<unclear>ῦ</unclear></seg> Τ <choice>
                            <abbr>δυνθω</abbr>
                            <expan>δυνάσθω</expan>
                        </choice></seg>
                </seg>
            </ab>
