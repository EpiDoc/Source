    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="40r1" type="folio">
                    <seg n="1" type="line" facs="z40r1_01">τὴν ΒΖ κύκλος, ὕψος δὲ ἡ ΔΚ· <w part="I"
                            >ἀντι</w></seg>
                    <seg n="2" type="line" facs="z40r1_02"><w part="F">πεπόνθασιν</w>
                        <expan>γὰρ</expan> αὐτῶν αἱ <choice>
                            <abbr>βασ</abbr>
                            <expan>βάσεις</expan>
                        </choice>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῖς</expan>
                        </choice>
                        <w part="I">ὕψε</w></seg>
                    <seg n="3" type="line" facs="z40r1_03"><w part="F">σιν.</w> ἀλλ’ οὕτως ὁ κῶνος <choice>
                            <abbr>ισ</abbr>
                            <expan>ἴσος</expan>
                        </choice>
                        <expan>ἐστὶ</expan> τῶι ΒΚ ΖΔ </seg>
                    <seg n="4" type="line" facs="z40r1_04"><seg type="word">στερεῶ<supplied
                                reason="lost">ι</supplied></seg>
                        <seg type="word">ῥόμβω<supplied reason="lost">ι</supplied>·</seg>
                        <expan>καὶ</expan> ὁ Ν ἄρα κῶνος, </seg>
                    <seg n="5" type="line" facs="z40r1_05"><choice>
                            <abbr>ττ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice> ἡ σφαῖρα, ἴση <expan>ἐστὶ</expan> τῶι ΒΖ ΚΔ </seg>
                    <seg n="6" type="line" facs="z40r1_06">στερεῶι ῥόμβωι. ὧν ὁ ΒΔΖ κῶνος </seg>
                    <seg n="7" type="line" facs="z40r1_07">ἴσος ἐδείχθη τῶι ΒΓΖ <choice>
                            <abbr>τμημαι</abbr>
                            <expan>τμήματι</expan>
                        </choice>
                    </seg>
                    <seg n="8" type="line" facs="z40r1_08">τῆς σφαίρας· λοιπὸς <expan>ἄρα</expan> ὁ
                        ΒΚΖ </seg>
                    <seg n="9" type="line" facs="z40r1_09">κῶνος ἴσος <expan>ἐστὶ</expan> τῶι ΒΑΖ
                        τμήματι </seg>
                    <seg n="10" type="line" facs="z40r1_10">τῆς σφαίρας. <choice>
                            <abbr>εξ</abbr>
                            <expan>ἑξῆς</expan>
                        </choice> Η ΚΑΤΑΓΡΑΦΗ </seg>
                    <figure n="2.2-1.2">
                        <figDesc>Figure 2 to corollary 2.2-1</figDesc>
                    </figure>
                </seg>
                <seg n="40r1" type="folio">
                    <seg n="11" type="line" facs="z40r1_11">τρίτον ἦν πρόβλημα τόδε· <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z40r1_12">δοθεῖσαν σφαῖραν ἐπιπέδωι <w part="I"
                        >τε</w></seg>
                    <seg n="13" type="line" facs="z40r1_13"><w part="F">μεῖν</w>, ὅπως αἱ τῶν
                        τμημάτων</seg>
                    <seg n="14" type="line" facs="z40r1_14">ἐπιφάνειαι <expan>πρὸς</expan> ἀλλήλας
                        λόγον <w part="I">ἔ</w></seg>
                    <seg n="15" type="line" facs="z40r1_15"><w part="F">χωσι</w> τὸν αὐτὸν τῶι
                        δοθέντι.</seg>
                </seg>
                <seg n="40r1" type="folio">
                    <seg n="15" type="line" facs="z40r1_15">
                        <sic>γε</sic>
                    </seg>
                    <gap/>
                </seg>
                <seg n="40r2" type="folio">
                    <gap/>
                    <seg n="1" type="line" facs="z40r2_01">οὕτως τὸ ἀπὸ ΑΔ <expan>πρὸς</expan> τὸ
                        ἀπὸ ΔΒ, <choice>
                            <abbr>ττ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice> ἡ</seg>
                    <seg n="2" type="line" facs="z40r2_02">ΑΓ <expan>πρὸς</expan>
                        <unclear>Γ</unclear>Β <seg type="word">δοθ<supplied reason="lost"
                                >εί</supplied><unclear>ς</unclear></seg>· <choice>
                            <abbr>τε</abbr>
                            <expan>ὥστε</expan>
                        </choice> δοθέν <expan>ἐστι</expan> τὸ Γ <choice>
                            <abbr>σημειο</abbr>
                            <expan>σημεῖον</expan>
                        </choice>.</seg>
                    <seg n="3" type="line" facs="z40r2_03"><expan>καί</expan>
                        <expan>ἐστι</expan> τῆι ΑΒ <expan>πρὸς</expan> ὀρθὰς ἡ ΔΕ· θέσει
                        <expan>ἄρα</expan> καὶ τὸ</seg>
                    <seg n="4" type="line" facs="z40r2_04"><expan>διὰ</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice> ΔΕ ἐπίπεδον.</seg>
                </seg>
                <seg n="40r2" type="folio">
                    <seg n="4" type="line" facs="z40r2_04">συντεθήσεται δὲ <expan>οὕτως</expan>·</seg>
                    <seg n="5" type="line" facs="z40r2_05">ἔστω σφαῖρα, ἧς μέγιστος <choice>
                            <abbr>κυκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice> ὁ ΑΒΔΕ</seg>
                    <seg n="6" type="line" facs="z40r2_06">καὶ διάμετρος ἡ ΑΒ, ὁ δὲ δοθεὶς
                            <expan>λόγος</expan> ὁ <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z40r2_07">Ζ <expan>πρὸς</expan> Η, καὶ τετμήσθω ἡ
                        ΑΒ κατὰ τὸ Γ,</seg>
                    <seg n="8" type="line" facs="z40r2_08"><choice>
                            <expan>ὥστε</expan>
                            <abbr>τε</abbr>
                        </choice>
                        <expan>εἶναι</expan>, ὡς τὴν Α<supplied reason="lost">Γ</supplied>
                        <expan>πρὸς</expan> ΒΓ, <expan>οὕτως</expan> τὴν Ζ <expan>πρὸς</expan> Η,</seg>
                    <seg n="9" type="line" facs="z40r2_09">καὶ <expan>διὰ</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> Γ ἐπιπέδω <seg type="word">τε<unclear>τ</unclear>μήσθω</seg> ἡ</seg>
                    <seg n="10" type="line" facs="z40r2_10">σφαῖρα <expan>πρὸς</expan> ὀρθὰς τῆι ΑΒ
                        εὐθείαι, <expan>καὶ</expan>
                    </seg>
                    <seg n="11" type="line" facs="z40r2_11">ἔστω κοινὴ τομὴ ἡ ΔΕ, καὶ <w part="I"
                            >ἐπεζεύ</w></seg>
                    <seg n="12" type="line" facs="z40r2_12"><w part="F">χθωσαν</w> δύο κύκλοι οἱ ΘΚ,
                        ὁ <choice>
                            <abbr>μ</abbr>
                            <expan>μὲν</expan>
                        </choice> Θ <choice>
                            <abbr>ισ</abbr>
                            <expan>ἴσην</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z40r2_13">ἔχων τὴν ἐκ τοῦ <choice>
                            <abbr>κεντρ</abbr>
                            <expan>κέντρου</expan>
                        </choice> τῆι ΑΔ, ὁ δὲ Κ</seg>
                    <seg n="14" type="line" facs="z40r2_14">τὴν ἐκ τοῦ κέντρου ἴσην ἔχων τῆι</seg>
                    <seg n="15" type="line" facs="z40r2_15">ΔΒ· <expan>ἔστιν</expan>
                        <expan>ἄρα</expan> ὁ <choice>
                            <abbr>μ</abbr>
                            <expan>μὲν</expan>
                        </choice> Θ κύκλος ἴσος τῆι <w part="I">ἐπιφα</w></seg>
                    <seg n="16" type="line" facs="z40r2_16"><w part="F">νείαι</w>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> ΔΑΕ τμήματος, ὁ δὲ Κ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z40r2_17">ΔΒΕ τμήματος· τοῦτο <expan>γὰρ</expan>
                        <seg type="expandedword">
                            <choice>
                                <abbr>προεδει</abbr>
                                <expan>προδέδεικται</expan>
                            </choice>
                        </seg></seg>
                    <seg n="18" type="line" facs="z40r2_18"><seg type="wordend">
                            <abbr>κτ</abbr>
                        </seg> ἐν τῶι πρώτωι βιβλίωι. καὶ ἐπεὶ</seg>
                    <seg n="19" type="line" facs="z40r2_19">δοθείσα <expan>ἐστιν</expan> ἡ ὑπὸ ΑΔΒ
                        καὶ <seg type="word">κάθετ<unclear>ο</unclear><supplied reason="lost"
                            >ς</supplied></seg></seg>
                    <seg n="20" type="line" facs="z40r2_20">ἡ ΓΔ, <expan>ἔστιν</expan>,
                        <expan>ὡς</expan> ἡ ΑΓ <expan>πρὸς</expan> ΓΒ, <choice>
                            <abbr>ττ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice> ἡ Ζ <expan>πρὸς</expan> Η, τὸ <w part="I">ἀ</w></seg>
                    <seg n="21" type="line" facs="z40r2_21"><w part="F">πὸ</w> ΑΔ <expan>πρὸς</expan>
                        <seg type="word">τὸ</seg>
                        <seg type="word">ἀπὸ</seg> ΔΒ, <choice>
                            <abbr>ττ</abbr>
                            <expan>τουτέστι</expan>
                        </choice> τὸ ἀπὸ <supplied reason="lost">τῆς</supplied></seg>
                    <seg n="22" type="line" facs="z40r2_22">ἐκ τοῦ κέντρου <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> Θ κύκλου <expan>πρὸς</expan> τὸ <w part="I">ἀ</w></seg>
                    <seg n="23" type="line" facs="z40r2_23"><w part="F">πὸ</w> τῆς ἐκ τοῦ <seg
                            type="word">
                            <supplied reason="lost">κ</supplied>
                            <unclear>έ</unclear>
                            <supplied reason="lost">ντρου</supplied>
                        </seg>
                        <choice>
                            <abbr>
                                <supplied reason="lost">τ</supplied>
                            </abbr>
                            <expan>τοῦ</expan>
                        </choice>
                        <unclear>Κ</unclear>
                        <choice>
                            <abbr>
                                <unclear>κ</unclear>
                                <supplied reason="lost">υ</supplied>
                                <unclear>κλ</unclear>
                            </abbr>
                            <expan>κύκλου</expan>
                        </choice></seg>
                    <gap/>
                </seg>
            </ab>
        </body>
