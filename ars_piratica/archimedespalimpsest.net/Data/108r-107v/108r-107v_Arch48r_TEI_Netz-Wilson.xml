    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="108r1" type="folio">
                    <seg n="1" type="line" facs="z108r1_01">αὐτῶν δίχα λείψομεν τμήματα</seg>
                    <seg n="2" type="line" facs="z108r1_02">ἐλάσσονα ὄντα τοῦ Θ χωρίου.</seg>
                </seg>
                <seg n="108r1" type="folio">
                    <seg n="2" type="line" facs="z108r1_02">
                        <w part="I">λε</w>
                    </seg>
                    <seg n="3" type="line" facs="z108r1_03"><w part="F">λείφθω</w> τὰ ἐπὶ τῶν ΑΕ,
                        ΕΒ, ΒΖ, ΖΓ</seg>
                    <seg n="4" type="line" facs="z108r1_04">εὐθειῶν, καὶ <seg type="word"
                                >ἐπεζεύχ<unclear>θ</unclear>ωσαν</seg> αἱ ΔΕ,</seg>
                    <seg n="5" type="line" facs="z108r1_05">ΔΖ. πάλιν τοίνυν κατὰ τὰ αὐτὰ</seg>
                    <seg n="6" type="line" facs="z108r1_06">ἡ μὲν ἐπιφάνεια τοῦ κώνου ἡ <w part="I"
                            >με</w></seg>
                    <seg n="7" type="line" facs="z108r1_07"><w part="F">ταξὺ</w> τῶν ΑΔΕ μετὰ τοῦ
                        ἐπὶ τῆς</seg>
                    <seg n="8" type="line" facs="z108r1_08">ΑΕ τμήματος μείζων ἐστὶν τοῦ</seg>
                    <seg n="9" type="line" facs="z108r1_09">ΑΔΕ τριγώνου, ἡ δὲ μεταξὺ τοῦ <w
                            part="I">Ε</w></seg>
                    <seg n="10" type="line" facs="z108r1_10"><w part="F">ΔΒ</w> μετὰ τοῦ ἐπὶ τῆς ΕΒ
                            <w part="I">τμήμα</w></seg>
                    <seg n="11" type="line" facs="z108r1_11"><w part="F">τος</w> μείζων ἐστὶν <seg
                            type="word">το<unclear>ῦ</unclear></seg> ΕΔΒ <w part="I">τριγώ</w></seg>
                    <seg n="12" type="line" facs="z108r1_12"><w part="F">νου</w>· ἡ
                        <expan>ἄρα</expan> ἐπιφάνεια ἡ μεταξὺ τῶν</seg>
                    <seg n="13" type="line" facs="z108r1_13">ΑΔΒ μετὰ τῶν ἐπὶ τῶν <seg type="word"
                                >Α<unclear>Ε</unclear></seg>, ΕΒ <w part="I">τμη</w></seg>
                    <seg n="14" type="line" facs="z108r1_14"><w part="F">μάτων</w> μείζων ἐστὶν τῶν
                        ΑΔΕ,</seg>
                    <seg n="15" type="line" facs="z108r1_15">ΕΒΔ τριγώνων. ἐπεὶ δὲ τὰ ΑΕΔ,</seg>
                    <seg n="16" type="line" facs="z108r1_16">ΔΕΒ τρίγωνα μείζονά ἐστιν τοῦ</seg>
                    <seg n="17" type="line" facs="z108r1_17">ΑΒΔ τριγώνου, καθὼς <choice>
                            <abbr>δεδεικτ</abbr>
                            <expan>δέδεικται</expan>
                        </choice>,</seg>
                    <seg n="18" type="line" facs="z108r1_18"><seg type="word"
                            >πολλ<unclear>ῶ</unclear></seg> ἄρα ἡ ἐπιφάνεια <seg type="word"
                                ><supplied reason="lost">τ</supplied>οῦ</seg>
                        <w part="I">κώ</w></seg>
                </seg>
                <seg n="107v1" type="folio">
                    <seg n="1" type="line" facs="z107v1_01"><w part="F">νου</w> ἡ μεταξὺ τῶν ΑΔΒ
                        μετὰ τῶν</seg>
                    <seg n="2" type="line" facs="z107v1_02">ἐπὶ τῶν ΑΕ, ΕΒ τμημάτων <choice>
                            <abbr>μειζ</abbr>
                            <expan>μείζων</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z107v1_03"><expan>ἐστὶ </expan>τοῦ ΑΔΒ τριγώνου.
                    </seg>
                </seg>
                <seg n="107v1" type="folio">
                    <seg n="3" type="line" facs="z107v1_03">διὰ τὰ αὐτὰ</seg>
                    <seg n="4" type="line" facs="z107v1_04">δὴ καὶ ἡ ἐπιφάνεια ἡ μεταξὺ</seg>
                    <seg n="5" type="line" facs="z107v1_05">τῶν ΑΒΓ μετὰ τῶν ἐπὶ τῶν ΒΖ,</seg>
                    <seg n="6" type="line" facs="z107v1_06">ΖΓ τμημάτων μείζων <expan>ἐστὶν</expan>
                        τοῦ ΔΒΓ</seg>
                    <seg n="7" type="line" facs="z107v1_07">τριγώνου· ὅλη ἄρα ἡ <choice>
                            <abbr>επιφανει</abbr>
                            <expan>ἐπιφάνεια</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z107v1_08">ἡ μεταξὺ τῶν ΑΔΓ μετὰ τῶν <w part="I"
                            >εἰρη</w></seg>
                    <seg n="9" type="line" facs="z107v1_09"><w part="F">μένων</w> τμημάτων μείζων
                            <expan>ἐστὶ</expan></seg>
                    <seg n="10" type="line" facs="z107v1_10">τῶν ΑΒΔ, ΔΒΓ τριγώνων. ταῦτα</seg>
                    <seg n="11" type="line" facs="z107v1_11">δέ <expan>ἐστιν</expan> ἴσα τῶι ΑΔΓ
                        τριγώνωι <expan>καὶ</expan></seg>
                    <seg n="12" type="line" facs="z107v1_12">τῶι Θ χωρίῶι· ὧν τὰ εἰρημένα</seg>
                    <seg n="13" type="line" facs="z107v1_13">τμήματα ἐλάσσονα τοῦ Θ <w part="I"
                        >χω</w></seg>
                    <seg n="14" type="line" facs="z107v1_14"><w part="F">ρίου</w>· λοιπὴ
                        <expan>ἄρα</expan> ἡ <seg type="word">ἐ<unclear>π</unclear>ιφάνεια</seg> ἡ</seg>
                    <seg n="15" type="line" facs="z107v1_15">μεταξὺ τῶν ΑΔΓ μείζων
                        <expan>ἐστὶν</expan> τοῦ ΑΔΕ</seg>
                    <seg n="16" type="line" facs="z107v1_16">τριγώνου. ἑξῆς τὸ ΣΧΗΜΑ</seg>
                </seg>
                <seg n="108r2" type="folio">
                    <num value="11">ΙΑ</num>
                </seg>
                <seg n="108r2" type="folio">
                    <seg n="1" type="line" facs="z108r2_01">ἐὰν ἐπιψαύουσαι <w part="I">ἀ</w></seg>
                    <seg n="2" type="line" facs="z108r2_02"><w part="F">χθῶσι</w> τοῦ κύκλου, ὅς
                        ἐστι βάσις <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z108r2_03">κώνου, ἐν τῶ αὐτῶ ἐπιπέδω <w part="I"
                        >οὖ</w></seg>
                    <seg n="4" type="line" facs="z108r2_04"><w part="F">σαι</w> τῶι κύκλωι καὶ <seg
                            type="word"><unclear>σ</unclear>υμπίπτουσαι</seg></seg>
                    <seg n="5" type="line" facs="z108r2_05">ἀλλήλαις, ἀπὸ δὲ τῶν ἁφῶν καὶ <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z108r2_06">συμπτώσεως ἐπὶ τὴν κορυφὴν τοῦ</seg>
                    <seg n="7" type="line" facs="z108r2_07">κώνου εὐθεῖαι ἀχθῶσιν, τὰ <w part="I"
                            >περιε</w></seg>
                    <seg n="8" type="line" facs="z108r2_08"><w part="F">χόμενα</w> τρίγωνα ὑπὸ τῶν
                            <w part="I">ἐπιψαυ</w></seg>
                    <seg n="9" type="line" facs="z108r2_09"><w part="F">ουσῶν</w> καὶ τῶν ἐπὶ τὴν
                        κορυφὴν</seg>
                    <seg n="10" type="line" facs="z108r2_10">τοῦ κώνου ἐπιζευχθεισῶν εὐθειῶν</seg>
                    <seg n="11" type="line" facs="z108r2_11">μείζονά ἐστιν τῆς <seg type="word"
                                >το<supplied reason="lost">ῦ</supplied></seg> κώνου <w part="I"
                        >ἐπι</w></seg>
                    <seg n="12" type="line" facs="z108r2_12"><w part="F">φανείας</w> τῆς <w part="I"
                            >ἀπολαμβανομέ</w></seg>
                    <seg n="13" type="line" facs="z108r2_13"><w part="F">νης</w> ὑπ᾽ αὐτῶν.</seg>
                </seg>
                <seg n="108r2" type="folio">
                    <seg n="13" type="line" facs="z108r2_13">ἔστω κῶνος οὗ <w part="I">βά</w></seg>
                    <seg n="14" type="line" facs="z108r2_14"><w part="F">σις</w> μὲν <supplied
                            reason="lost">ὁ</supplied> ΑΒΓ κύκλος, κορυφὴ δὲ</seg>
                </seg>
                <seg n="107v2" type="folio">
                    <seg n="1" type="line" facs="z107v2_01">τὸ Ε σημεῖον, καὶ τοῦ ΑΒΓ κύκλου</seg>
                    <seg n="2" type="line" facs="z107v2_02">ἐφαπτόμεναι ἤχθωσαν ἐν τῶι</seg>
                    <seg n="3" type="line" facs="z107v2_03">αὐτῶι ἐπιπέδωι οὖσαι αἱ ΑΔ, <seg
                            type="word"><supplied reason="lost">Γ</supplied>Δ</seg>,</seg>
                    <seg n="4" type="line" facs="z107v2_04">καὶ ἀπὸ τοῦ Ε σημείου, ὅ ἐστι <w
                            part="I">κορυ</w></seg>
                    <seg n="5" type="line" facs="z107v2_05"><w part="F">φὴ</w> τοῦ κώνου, ἐπὶ τὰ Α,
                        Δ, Γ <w part="I">ἐπεζεύ</w></seg>
                    <seg n="6" type="line" facs="z107v2_06"><w part="F">χθωσαν</w> αἱ ΕΑ, ΕΔ, ΕΓ·
                        λέγω ὅτι τὰ</seg>
                    <seg n="7" type="line" facs="z107v2_07">ΑΔΕ, ΔΕΓ τρίγωνα μείζονά ἐστι</seg>
                    <seg n="8" type="line" facs="z107v2_08">τῆς κωνικῆς ἐπιφανείας τῆς</seg>
                    <seg n="9" type="line" facs="z107v2_09">μεταξὺ τῶν ΑΕ, ΓΕ εὐθειῶν καὶ τῆς</seg>
                    <seg n="10" type="line" facs="z107v2_10">ΑΓ περιφερείας.</seg>
                </seg>
                <seg n="107v2" type="folio">
                    <seg n="10" type="line" facs="z107v2_10">ἤχθω γὰρ ἡ ΗΒΖ</seg>
                    <seg n="11" type="line" facs="z107v2_11">ἐφαπτομένη τοῦ κύκλου καὶ <w part="I"
                            >πα</w></seg>
                    <seg n="12" type="line" facs="z107v2_12"><w part="F">ράλληλος</w> οὖσα τῆι ΑΓ
                        δίχα <w part="I">τμηθεί</w></seg>
                    <seg n="13" type="line" facs="z107v2_13"><w part="F">σης</w> τῆς ΑΒΓ περιφανείας
                            <w part="I">κα</w></seg>
                    <seg n="14" type="line" facs="z107v2_14"><w part="F">τὰ</w> τὸ Β, καὶ ἀπὸ τῶν Η,
                        Ζ ἐπὶ τὸ Ε</seg>
                    <seg n="15" type="line" facs="z107v2_15">ἐπεζεύχθωσαν αἱ ΗΕ, ΖΕ. καὶ ἐπεὶ</seg>
                    <seg n="16" type="line" facs="z107v2_16">μείζους <expan>εἰσὶν</expan> αἱ ΗΔ, ΔΖ
                        τῆς ΗΖ, κοιναὶ</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
