    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="65r1" type="folio">
                    <seg n="1" type="line" facs="z65r1_01">ὀκτὼ κώνοις, ὧν ἐστι τὸ διὰ <seg
                            type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<unclear>οῦ</unclear></expan>
                            </choice>
                        </seg></seg>
                    <seg n="2" type="line" facs="z65r1_02">ἄξονος τρίγωνον τὸ ΑΒΔ, διὰ τὸ</seg>
                    <seg n="3" type="line" facs="z65r1_03">διπλῆν εἶναι τὴν ΕΖ τῆς ΒΔ.
                        <expan>ἄρα</expan></seg>
                    <seg n="4" type="line" facs="z65r1_04">ὀκτὼ κῶνοι οἱ εἰρημένοι ἴσοι εἰσὶ</seg>
                    <seg n="5" type="line" facs="z65r1_05">δυσὶ σφαίραις. τετραπλασίων</seg>
                    <seg n="6" type="line" facs="z65r1_06">ἄρα ἐστὶν ἡ σφαῖρα, ἧς μέγιστος</seg>
                    <seg n="7" type="line" facs="z65r1_07"><seg type="reason"
                            >κύκλο<unclear>ς</unclear></seg> ὁ ΑΒΓΔ, τοῦ κώνου, οὗ <w part="I"
                        >κορυ</w></seg>
                    <seg n="8" type="line" facs="z65r1_08"><w part="F">φὴ</w> μέν ἐστι τὸ Α σημεῖον,
                        βάσις</seg>
                    <seg n="9" type="line" facs="z65r1_09">δὲ ὁ περὶ διάμετρον τὴν ΒΔ <w part="I"
                        >κύ</w></seg>
                    <seg n="10" type="line" facs="z65r1_10"><w part="F">κλος</w> ὀρθὸς ὢν πρὸς τὴν
                        ΑΓ.</seg>
                </seg>
                <seg n="65r1" type="folio">
                    <seg n="10" type="line" facs="z65r1_10">
                        <w part="I">ἤχθω</w>
                    </seg>
                    <seg n="11" type="line" facs="z65r1_11"><w part="F">σαν</w> δὴ διὰ τῶν ΒΔ
                        σημείων ἐν</seg>
                    <seg n="12" type="line" facs="z65r1_12">τῶι ΛΖ <pb/> παραλληλογράμμωι τῆι</seg>
                    <seg n="13" type="line" facs="z65r1_13">ΑΓ παράλληλοι <unclear>οἱ</unclear> ΦΒ
                        ΧΨ ΔΩ, <expan>καὶ</expan></seg>
                    <seg n="14" type="line" facs="z65r1_14">νοείσθωσαν κύλινδροι, ὧν <choice>
                            <abbr>βάσ</abbr>
                            <expan>βάσεις</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z65r1_15">μὲν οἱ περὶ διαμέτρους τὰς ΦΨ</seg>
                    <seg n="16" type="line" facs="z65r1_16">ΧΩ κύκλους, ἄξων δὲ ὁ ΑΓ. ἐπεὶ</seg>
                    <seg n="17" type="line" facs="z65r1_17"><seg type="word">
                            <unclear>δ</unclear>
                            <supplied reason="lost">ὴ</supplied>
                        </seg>
                        <seg type="word"><unclear>δ</unclear><supplied reason="lost"
                                >ι</supplied>πλ<supplied reason="lost">άσι</supplied>ός</seg>
                        <expan>ἐστιν</expan> ὁ <seg type="word"><supplied reason="lost"
                            >κύλι</supplied>νδρος</seg>, οὗ</seg>
                    <seg n="18" type="line" facs="z65r1_18">ἐστι τὸ διὰ <supplied reason="lost">τοῦ</supplied>
                        <seg type="word"><supplied reason="lost">ἄ</supplied>ξονος</seg>
                        <w part="I">παραλλη</w></seg>
                    <seg n="19" type="line" facs="z65r1_19"><w part="F">λόγραμμον</w> τὸ ΦΩ, τοῦ <choice>
                            <abbr>κυλίνδρ</abbr>
                            <expan>κυλίνδρου</expan>
                        </choice>,</seg>
                    <seg n="20" type="line" facs="z65r1_20">
                        <supplied reason="lost">οὗ</supplied>
                        <supplied reason="lost">ἐστι</supplied>
                        <supplied reason="lost">τὸ</supplied>
                        <seg type="word">
                            <supplied reason="lost">δι</supplied>
                            <unclear>ὰ</unclear>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr>
                                    <supplied reason="lost">τ</supplied>
                                </abbr>
                                <expan>
                                    <supplied reason="lost">τοῦ</supplied>
                                </expan>
                            </choice>
                        </seg>
                        <seg type="word">
                            <unclear>ἄξονο</unclear>
                            <supplied reason="lost">ς</supplied>
                        </seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">παραλ</supplied>
                        </seg>
                    </seg>
                </seg>
                <seg n="72v1" type="folio">
                    <seg n="1" type="line" facs="z72v1_01"><seg type="wordend">ληλόγραμμον</seg> τὸ
                        ΦΔ, αὐτὸς δὲ <w part="I">οὗ</w></seg>
                    <seg n="2" type="line" facs="z72v1_02"><w part="F">τος</w> τριπλασίων ἐστὶν τοῦ
                        κώνου,</seg>
                    <seg n="3" type="line" facs="z72v1_03">οὗ ἐστι τὸ διὰ τοῦ ἄξονος <choice>
                            <abbr>τρίγων</abbr>
                            <expan>τρίγωνον</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z72v1_04">τὸ ΑΒΔ, <seg type="word">ὡ<supplied
                                reason="lost">ς</supplied></seg> ἐν τοῖς στοιχείοις, <w part="I"
                        >ἑξα</w></seg>
                    <seg n="5" type="line" facs="z72v1_05"><w part="F">πλασίων</w> ἄρα ὁ κύλινδρος,
                        οὗ <expan>ἐστι</expan></seg>
                    <seg n="6" type="line" facs="z72v1_06">τὸ διὰ τοῦ ἄξονος <w part="I"
                        >παραλληλό</w></seg>
                    <seg n="7" type="line" facs="z72v1_07"><w part="F">γραμμον</w> τὸ ΦΩ, τοῦ κώνου,
                        οὗ <expan>ἐστι</expan>
                    </seg>
                    <seg n="8" type="line" facs="z72v1_08">τὸ διὰ τοῦ ἄξονος τρίγωνον τὸ ΑΒΔ.</seg>
                    <seg n="9" type="line" facs="z72v1_09"><seg type="word"><supplied reason="lost"
                                >ἐδ</supplied>είχθη</seg> δὲ τοῦ αὐτοῦ κώνου <w part="I">τετρα</w></seg>
                    <seg n="10" type="line" facs="z72v1_10"><w part="F">πλασία</w> οὖσα ἡ σφαῖρα, ἧς
                            <w part="I">μέ</w></seg>
                    <seg n="11" type="line" facs="z72v1_11"><w part="F">γιστος</w> μέν ἐστιν ὁ
                        κύκλος ὁ ΑΒΓΔ·</seg>
                    <seg n="12" type="line" facs="z72v1_12">ἡμιόλιος ἄρα ὁ κύλινδρος τῆς</seg>
                    <seg n="13" type="line" facs="z72v1_13">σφαίρας· ὅπερ ἔδει δειχθῆναι.</seg>
                </seg>
                <seg n="72v1" type="folio">
                    <seg n="14" type="line" facs="z72v1_14">τούτου <seg type="word"
                            >δ<unclear>ὲ</unclear></seg> θεωρήματος, διότι <seg type="unclearword"
                                >π<unclear>ᾶ</unclear></seg></seg>
                    <seg n="15" type="line" facs="z72v1_15"><seg type="wordend">σα</seg> σφαῖρα
                        τετραπλασία ἐστὶ <seg type="word">το<unclear>ῦ</unclear></seg></seg>
                    <seg n="16" type="line" facs="z72v1_16">κώνου βάσιν μὲν ἔχοντος τὸν</seg>
                    <seg n="17" type="line" facs="z72v1_17">μέγιστον κύκλον, ὕψος δὲ ἴσον</seg>
                </seg>
                <seg n="65r2" type="folio">
                    <seg n="1" type="line" facs="z65r2_01">τῆι ἐκ τοῦ κέντρου τῆς σφαίρας,</seg>
                    <seg n="2" type="line" facs="z65r2_02">ἡ ἔννοια ἐγένετο, ὅτι πάσης <seg
                            type="suppliedword">σ<supplied reason="lost">φα</supplied>ί</seg></seg>
                    <seg n="3" type="line" facs="z65r2_03"><seg type="wordend">ρας</seg> ἡ ἐπιφάνεια
                        τετραπλασία <expan>ἐστὶ</expan></seg>
                    <seg n="4" type="line" facs="z65r2_04">τοῦ μεγίστου κύκλου τῶν ἐν τῆι <w
                            part="I">σφαί</w></seg>
                    <seg n="5" type="line" facs="z65r2_05"><w part="F">ραι</w>· ὑπόληψις
                        <expan>γὰρ</expan> ἦν καὶ <choice>
                            <abbr>δι</abbr>
                            <expan>διότι</expan>
                        </choice>
                        <seg type="word">π<supplied reason="lost">ᾶς</supplied></seg>
                        <choice>
                            <abbr><supplied reason="lost">κ</supplied>ύκλ</abbr>
                            <expan><supplied reason="lost">κ</supplied>ύκλος</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z65r2_06">ἴσος <expan>ἐστὶ</expan> τριγώνωι τῶι
                            <seg type="word"><supplied reason="lost">β</supplied>άσιν</seg> μὲν <w
                            part="I">ἔχον</w></seg>
                    <seg n="7" type="line" facs="z65r2_07"><w part="F">τι</w> τὴν τοῦ κύκλου
                        περιφέρειαν, <choice>
                            <abbr>ὕψ</abbr>
                            <expan>ὕψος</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z65r2_08">δὲ ἴσον τῆι ἐκ τοῦ κέντρου τοῦ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλου</expan>
                        </choice>,</seg>
                    <figure n="2.1">
                        <figDesc xml:lang="eng">Figure 2.1</figDesc>
                    </figure>
                    <seg n="9" type="line" facs="z65r2_09">καὶ <choice>
                            <abbr>δι</abbr>
                            <expan>διότι</expan>
                        </choice>
                        <w part="I">πᾶ</w></seg>
                    <seg n="10" type="line" facs="z65r2_10"><w part="F">σα</w> σφαῖρα</seg>
                    <seg n="11" type="line" facs="z65r2_11">ἴση ἐστὶ <w part="I">κώ</w></seg>
                    <seg n="12" type="line" facs="z65r2_12"><w part="F">νωι</w> τῶι <w part="I"
                        >βά</w></seg>
                    <seg n="13" type="line" facs="z65r2_13"><w part="F">σιν</w> μὲν <seg
                            type="expandedword">
                            <choice>
                                <abbr>ἔχ</abbr>
                                <expan>ἔχον</expan>
                            </choice>
                        </seg></seg>
                    <seg n="14" type="line" facs="z65r2_14"><seg type="wordend">τι</seg> τὴν <w
                            part="I">ἐπι</w></seg>
                    <seg n="15" type="line" facs="z65r2_15">
                        <w part="F">φάνειαν</w>
                        <seg type="word">τ<supplied reason="lost">ῆ</supplied>ς</seg>
                    </seg>
                    <seg n="16" type="line" facs="z65r2_16">σφαίρας, <choice>
                            <abbr>ὕψο</abbr>
                            <expan>ὕψος</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z65r2_17">δὲ ἴσον τῆι ἐκ</seg>
                    <seg n="18" type="line" facs="z65r2_18">τοῦ <choice>
                            <abbr>κέντρ</abbr>
                            <expan>κέντρου</expan>
                        </choice></seg>
                    <seg n="19" type="line" facs="z65r2_19">τῆς <choice>
                            <abbr>σφαίρ</abbr>
                            <expan>σφαίρας</expan>
                        </choice>.</seg>
                </seg>
                <seg n="65r2" type="folio">
                    <seg n="20" type="line" facs="z65r2_20">θεωρεῖται δὲ διὰ τοῦ τρόπου τούτου</seg>
                    <seg n="21" type="line" facs="z65r2_21"><seg type="word">
                            <gap extent="1"/>
                            <unclear>π</unclear>
                            <gap extent="1"/>
                        </seg> δ<gap extent="4"/>
                        <unclear>ἔσται</unclear>
                        <gap extent="2"/>
                        <seg type="word"><supplied reason="lost"
                                >σ</supplied><unclear>φ</unclear><supplied reason="lost"
                                >αιρο</supplied>ει<unclear>δ</unclear><gap extent="2"/></seg>
                        <supplied reason="lost">τ</supplied><unclear>ὸ</unclear></seg>
                </seg>
                <seg n="72v2" type="folio">
                    <seg n="1" type="line" facs="z72v2_01">σχῆμα ὁ κύλινδρος <unclear>ὁ</unclear>
                        <supplied reason="lost">β</supplied>ά<unclear>σιν</unclear>
                        <seg type="word">
                            <choice>
                                <abbr>μὲ</abbr>
                                <expan>
                                    <unclear>μὲν</unclear>
                                </expan>
                            </choice>
                        </seg></seg>
                    <seg n="2" type="line" facs="z72v2_02">ἔχων ἴσην τῶι μεγίστωι κύκλωι <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z72v2_03">ἐν τωι σφαιροειδεῖ, ὕψος δὲ ἴσον τῶι</seg>
                    <seg n="4" type="line" facs="z72v2_04">ἄξονι τοῦ σφαιροειδοῦς, ἡμιόλιός
                            <expan>ἐστι</expan></seg>
                    <seg n="5" type="line" facs="z72v2_05">τοῦ σφαιροειδοῦς· τούτου δὲ <w part="I"
                            >θεωρη</w></seg>
                    <seg n="6" type="line" facs="z72v2_06"><w part="F">θέντος</w> φανερόν,
                            <expan>ὅτι</expan> παντὸς <w part="I">σφα</w><supplied reason="lost">
                            <w part="M">ι</w>
                        </supplied></seg>
                    <seg n="7" type="line" facs="z72v2_07"><w part="F">ροειδοῦς</w> ἐπιπέδω
                        τμηθέντος <w part="I">δι</w></seg>
                    <seg n="8" type="line" facs="z72v2_08"><w part="F">ὰ</w> τοῦ κέντρου ὀρθῶι
                            <sic>πρόσθετoν</sic>
                        <w part="I">ἄ</w></seg>
                    <seg n="9" type="line" facs="z72v2_09"><w part="F">ξονα</w> τὸ ἥμισυ τοῦ
                        σφαιροειδοῦς <w part="I">δι</w></seg>
                    <seg n="10" type="line" facs="z72v2_10"><w part="F">πλάσιόν</w>
                        <expan>
                            <supplied reason="lost">ἐστι</supplied>
                        </expan> τοῦ κώνου τοῦ βάσιν</seg>
                    <seg n="11" type="line" facs="z72v2_11">μὲν ἔχον<supplied reason="lost"
                        >τος</supplied> τὴν αὐτὴν τῶι <w part="I">τμά</w></seg>
                    <seg n="12" type="line" facs="z72v2_12"><w part="F">ματι</w> κα<supplied
                            reason="lost">ὶ</supplied>
                        <supplied reason="lost">ἄ</supplied>ξονα τὸν αὐτόν.</seg>
                </seg>
                <seg n="72v2" type="folio">
                    <seg n="12" type="line" facs="z72v2_12">ἔστω <expan>γάρ</expan> τι</seg>
                    <seg n="13" type="line" facs="z72v2_13">σφαιροει<supplied reason="lost"
                        >δὲ</supplied>ς καὶ τετμήσθω <w part="I">ἐπιπέ</w></seg>
                    <seg n="14" type="line" facs="z72v2_14"><w part="F">δωι</w> διὰ τοῦ ἄξονος, καὶ
                        γινέσθω ἐν</seg>
                    <seg n="15" type="line" facs="z72v2_15">τῆι ἐπιφαν<supplied reason="lost">είαι</supplied>
                        <supplied reason="lost">α</supplied>ὐτοῦ ὀξυγωνίου</seg>
                    <seg n="16" type="line" facs="z72v2_16">κώνου τομὴ ἡ
                            ΑΒ<unclear>Γ</unclear><supplied reason="lost">Δ</supplied>, διάμετροι
                            <supplied reason="lost">δ</supplied>ὲ</seg>
                    <seg n="17" type="line" facs="z72v2_17">αὐτῆς ἔστωσαν αἱ ΑΓ ΒΔ, κέντρον</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
