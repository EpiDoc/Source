    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="145r-144v" type="folio">
                    <seg n="01" type="line" facs="z145r-144v_01">ἡμετέρας τὰ βέλτιστα συμβουλεύσει.
                            <unclear>κ</unclear>αὶ ταῦτα</seg>
                    <seg n="02" type="line" facs="z145r-144v_02">μὲν οὔ φησιν Διώνδας παράνομα
                        εἶναι, ἐγὼ δὲ <seg type="unclearword">
                            <unclear>κρί</unclear>
                        </seg></seg>
                    <seg n="03" type="line" facs="z145r-144v_03"><seg type="wordend">νομαι</seg>
                        περὶ <unclear>ὧ</unclear>ν ὑμεῖς πρότερον ἐγνώκατε. καίτοι,</seg>
                    <seg n="04" type="line" facs="z145r-144v_04">ὦ ἄνδρες δικασταί, ὥσπερ τοὺς
                        γράφοντας οἴεσθε</seg>
                    <seg n="05" type="line" facs="z145r-144v_05">δεῖν κολάζειν, ἐάν τις ἀσύμφορόν τι
                        τῶι <choice>
                            <sic>δήμω</sic>
                            <corr>δήμωι</corr>
                        </choice>
                        <w part="I">γρά</w></seg>
                    <seg n="06" type="line" facs="z145r-144v_06"><w part="F">φηι</w>, οὕτως καὶ τοὺς
                        τὰς γραφὰς γραφομένους <w part="I">ἐξε</w></seg>
                    <seg n="07" type="line" facs="z145r-144v_07"><w part="F">τάζετε</w>· οὐκ ἔλαττον
                        γάρ ἐστιν ἀδίκημ<supplied reason="lost">α</supplied> τὸ τὰ τῶι</seg>
                    <seg n="08" type="line" facs="z145r-144v_08">δήμωι συμφέροντα ἐμποδίζειν γραφὰς
                            <seg type="word">
                            <choice>
                                <abbr>ἐνιστάν</abbr>
                                <expan>ἐνιστάντα</expan>
                            </choice>
                        </seg></seg>
                    <seg n="09" type="line" facs="z145r-144v_09">τοῦ τὰ παράνομα γράφειν. Διώνδας
                            <unclear>δὲ</unclear>
                        <unclear>νῦν</unclear> ἐν <w part="I">πεν</w></seg>
                    <seg n="10" type="line" facs="z145r-144v_10"><w part="F">τήκοντα</w>
                            γραφα<unclear>ῖ</unclear>ς, ἃς ἐγράψατο, κατὰ μὲν <seg type="word">
                            <choice>
                                <abbr>τῶ</abbr>
                                <expan>τῶν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="11" type="line" facs="z145r-144v_11">ὑπὲρ Φιλίππου πολιτευομένων
                        οὐδεμίαν <w part="I">πώ</w></seg>
                    <seg n="12" type="line" facs="z145r-144v_12"><w part="F">ποτε</w> γραφὴν
                            ἀπ<unclear>ή</unclear>νεγκεν, ἀλλ’ οὐδὲ λόγωι <w part="I">ἐβλασ</w></seg>
                    <seg n="13" type="line" facs="z145r-144v_13"><w part="F">φήμησεν</w> οὐδὲ περὶ
                        ἑνὸς αὐτῶν, τοῖς δὲ <w part="I">τἀναν</w></seg>
                    <seg n="14" type="line" facs="z145r-144v_14"><w part="F">τία</w> ἐκείνωι
                        πολιτευομένοις <seg type="word">
                            <choice>
                                <abbr>λοιδορούμεν</abbr>
                                <expan>λοιδορούμενος</expan>
                            </choice>
                        </seg>
                        <w part="I">δια</w></seg>
                    <seg n="15" type="line" facs="z145r-144v_15"><w part="F">τελεῖ</w> ἐπὶ πάντων
                        τῶν ἀγώνων. ἐγράψατο δὲ</seg>
                    <seg n="16" type="line" facs="z145r-144v_16">Χαρίδημον μέν, ὃν νῦν ἐγκωμιάζει μὴ
                        ποιεῖν <w part="I">ἐ</w></seg>
                    <seg n="17" type="line" facs="z145r-144v_17"><w part="F">φ’</w> οἷς ἔλαβεν τὴν
                            δωρε<unclear>ὰν</unclear>
                        <unclear>τοῦ</unclear>
                        <supplied reason="lost">δήμ</supplied><unclear>ου</unclear> βουλόμενος</seg>
                    <seg n="18" type="line" facs="z145r-144v_18">διαπράξασθαι <supplied
                            reason="lost">οὐδὲν</supplied>
                        <supplied reason="lost">ὧν</supplied>
                        <supplied reason="lost">ἐ</supplied><unclear>δ</unclear><supplied
                            reason="lost">ίωκ</supplied><unclear>εν</unclear>
                        <supplied reason="lost">ἠ</supplied><unclear>δικηκότα</unclear>.</seg>
                    <seg n="19" type="line" facs="z145r-144v_19"><seg type="word">
                            <choice>
                                <abbr>Λυκοῦρ</abbr>
                                <expan>Λυκοῦργον</expan>
                            </choice>
                        </seg> δὲ οὐ μόνον παρανόμ<unclear>ων</unclear> ἐδίωξεν, <w part="I">ἀλ</w></seg>
                    <seg n="20" type="line" facs="z145r-144v_20"><w part="F">λὰ</w> καὶ ἀσεβείας
                            πρὸ<unclear>ς</unclear> τὸν <unclear>βασ</unclear>ιλέα. Δημοσθένη</seg>
                    <seg n="21" type="line" facs="z145r-144v_21">τε πλείους <choice>
                            <sic>εἰ</sic>
                            <corr>ἢ</corr>
                        </choice> πέντε καὶ δέκα γραφὰς ἐγράψατο,</seg>
                    <seg n="22" type="line" facs="z145r-144v_22">κατ’ ἐμοῦ δὲ τρεῖς
                        τῆ<unclear>ι</unclear> αὐτῆ<unclear>ι</unclear> ἡμέρα<unclear>ι</unclear>
                        <unclear>ἀπ</unclear>ήνεγ<unclear>κεν</unclear>.</seg>
                    <seg n="23" type="line" facs="z145r-144v_23">καὶ νῦν εἰς τοῦτο ἀναιδείας
                        ἐλήλυθεν, ὥστε <unclear>οὐκ</unclear>
                        <w part="I">ἐ</w></seg>
                    <seg n="24" type="line" facs="z145r-144v_24"><w part="F">ᾶι</w> ὑμᾶς χρῆσθαι
                            <unclear>οὔτε</unclear>
                        <unclear>οἷς</unclear> ὁ <seg type="word">
                            <choice>
                                <abbr>δῆμ</abbr>
                                <expan>δῆμος</expan>
                            </choice> ὁμογνώμων </seg></seg>
                    <seg n="25" type="line" facs="z145r-144v_25">ἐγένετο, ο<unclear>ὓ</unclear>ς ἐγὼ
                            προε<unclear>βού</unclear>λευσ<unclear>α</unclear>, οὔτε τῆι τοῦ <w
                            part="I">δικα</w></seg>
                    <seg n="26" type="line" facs="z145r-144v_26"><w part="F">στηρίου</w>
                            γνώ<unclear>σ</unclear>ει, πρὸς δ<unclear>ὲ</unclear>
                            τού<unclear>τ</unclear>ο<supplied reason="lost"
                        >ι</supplied><unclear>ς</unclear> οὐδ’ ὑπὲρ τοῦ <w part="I">και</w></seg>
                    <seg n="27" type="line" facs="z145r-144v_27"><w part="F">ροῦ</w> ἀκούειν <seg
                            type="word">
                            <choice>
                                <abbr>λεγοντ</abbr>
                                <expan>λεγοντ<unclear>ος</unclear></expan>
                            </choice>
                        </seg>
                        <unclear>οὗ</unclear> χ<unclear>ωρὶς</unclear> οὐδ<unclear>ὲ</unclear>ν τῶν
                            <seg type="word">
                            <choice>
                                <abbr>πάντ</abbr>
                                <expan>πάντων</expan>
                            </choice>
                        </seg></seg>
                    <seg n="28" type="line" facs="z145r-144v_28">χρήσιμόν ἐστιν.
                            <unclear>καί</unclear>τ<unclear>οι</unclear>
                        <unclear>π</unclear>ῶς οὐ δεινόν, εἰ τοῖς μὲν</seg>
                    <seg n="29" type="line" facs="z145r-144v_29">νῦν κατὰ τῆς π<supplied
                            reason="lost">όλ</supplied>εω<unclear>ς</unclear>
                        ὀνείδ<unclear>η</unclear> γράφουσιν ἔξεστι<unclear>ν</unclear></seg>
                    <seg n="30" type="line" facs="z145r-144v_30">τὸ τοῦ καιροῦ ὄνομα πολλάκις
                            λέγ<unclear>ουσ</unclear>ιν γράφειν,</seg>
                    <seg n="31" type="line" facs="z145r-144v_31">ὅ τι ἂν ἡγῶνται <supplied
                            reason="lost">Ἀ</supplied>λεξάνδρωι χαριεῖσ<unclear>θ</unclear>αι, τοῖς</seg>
                    <seg n="32" type="line" facs="z145r-144v_32">δ’ ὑπὲρ τῆς
                        πόλεω<unclear>ς</unclear> τότε τὰ <seg type="word">
                            <choice>
                                <abbr>συμφέρον</abbr>
                                <expan>συμφέροντα</expan>
                            </choice>
                        </seg> τῶι δήμωι</seg>
                </seg>
            </ab>
