    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="128v1" type="folio">
                    <seg n="1" type="line" facs="z128v1_01">ἔχει τὸ τετράγωνον τὸ ἀπὸ τῆς ΠΙ</seg>
                    <seg n="2" type="line" facs="z128v1_02">πρὸς <supplied reason="lost"
                        >τὸ</supplied> ἀπὸ τῆς ΙϘ ἢ τὸ ἀπὸ τῆς</seg>
                    <seg n="3" type="line" facs="z128v1_03">ΕΨ πρὸς τὸ ἀπὸ τῆς Ψ<supplied
                            reason="lost">Β·</supplied> καὶ ἡ ΚΡ</seg>
                    <seg n="4" type="line" facs="z128v1_04">ἄρα πρὸς τὴν ϘΙ ἐλάσσονα λόγον <w
                            part="I">ἔ</w></seg>
                    <seg n="5" type="line" facs="z128v1_05"><w part="F">χει</w>
                        <supplied>ἡ</supplied> ἡμίσεια τῆς ΚΡ πρὸς τὴν ΨΒ.</seg>
                    <seg n="6" type="line" facs="z128v1_06">μεῖζον ἄρα ἐστὶν ἢ διπλασίων ἡ</seg>
                    <seg n="7" type="line" facs="z128v1_07">ΙϘ τῆς ΨΒ· ἡ δὲ Ω ἔλασσον τῆς ΨΒ.</seg>
                    <seg n="8" type="line" facs="z128v1_08">ἔσται ἄρα καὶ ἡ ΠΗ ἐλάσσων τῆς Φ.</seg>
                    <seg n="9" type="line" facs="z128v1_09">ἡ δὲ ΜΠ τῆς ΦΧ <seg type="word">
                            <choice>
                                <abbr>η</abbr>
                                <expan><unclear>ἴσ</unclear>η·</expan>
                            </choice>
                        </seg> δῆλον, <expan>ὅτι</expan>
                        <choice>
                            <abbr>μειζω</abbr>
                            <expan>μείζων</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z128v1_10">ἡμιολία ἡ ΠΜ τῆς ΠΗ,
                        <unclear>ἡ</unclear> δὲ ΠΗ <w part="I">ἐ</w></seg>
                    <seg n="11" type="line" facs="z128v1_11"><w part="F">λάσσων</w> ἢ διπλασία τῆς
                        ΗΜ. ἔστω</seg>
                    <seg n="12" type="line" facs="z128v1_12">οὖν ἡ ΠΖ τῆς Ζ<unclear>Μ</unclear>
                        διπλῆ. πάλιν</seg>
                    <seg n="13" type="line" facs="z128v1_13">οὖν τοῦ μὲν ὅλου <seg type="word"
                                >κέντρο<unclear>ν</unclear></seg> ἔσται <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">οῦ</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="14" type="line" facs="z128v1_14"><seg type="word">βάρ<supplied
                                reason="lost">ο</supplied>υς</seg> Θ, τοῦ δ’ ἐν τῶι ὑγρῶι τὸ Ζ·</seg>
                    <seg n="15" type="line" facs="z128v1_15"><seg type="word">ἐπι<supplied
                                reason="lost">ζευχ</supplied>θείσης</seg> δὲ τῆς ΖΘ καὶ <w part="I"
                            >ἐκ</w></seg>
                    <seg n="16" type="line" facs="z128v1_16"><w part="F">βληθείσης</w> ἔσται τὸ
                        κέντρον τοῦ</seg>
                    <seg n="17" type="line" facs="z128v1_17">ἐκτὸς τοῦ ὑγροῦ <seg type="word"
                                >ἐπ<supplied reason="lost">ὶ</supplied></seg> τῆς <w part="I"
                            >ἐκβαλλο</w></seg>
                    <seg n="18" type="line" facs="z128v1_18"><w part="F">μένης.</w> ἔστω τὸ Γ,
                            <unclear>καὶ</unclear> ἤχθωσαν <w part="I">κά</w></seg>
                    <seg n="19" type="line" facs="z128v1_19"><w part="F">θετος</w> ἐπὶ τὴν τοῦ ὑγροῦ
                            <w part="I">ἐπιφάνει</w></seg>
                    <seg n="20" type="line" facs="z128v1_20"><w part="F">αν</w> αἱ διὰ τῶν ΖΓ <seg
                            type="word">π<supplied reason="lost">αρ</supplied>ὰ</seg> τὴν ΗΘ·</seg>
                </seg>
                <seg n="129r1" type="folio">
                    <seg n="1" type="line" facs="z129r1_01">δῆλον οὖν, <expan>ὅτι</expan> οὐ μενεῖ
                        τὸ <seg type="unclearword">ὅλ<unclear>ο</unclear>ν</seg>
                        <w part="I">τμῆ</w></seg>
                    <seg n="2" type="line" facs="z129r1_02"><w part="F">μα,</w> ἀλλὰ
                            <sic>κλειθήσεται,</sic> ὥστε τὸν <w part="I">ἄξο</w></seg>
                    <seg n="3" type="line" facs="z129r1_03"><w part="F">να</w> πρὸς <seg type="word"
                                ><unclear>τ</unclear>ὴν</seg>
                        <seg type="word"><supplied reason="lost">ἐπιφ</supplied>άνειαν</seg> τοῦ <choice>
                            <abbr>ὑγρ</abbr>
                            <expan>ὑγροῦ</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z129r1_04"><seg type="word"
                            >ποιεῖ<unclear>ν</unclear></seg>
                        <seg type="word"><unclear>γ</unclear>ωνίαν</seg> μείζονα ἧς νῦν</seg>
                    <seg n="5" type="line" facs="z129r1_05">ποιεῖ.</seg>
                </seg>
                <seg n="129r1" type="folio">
                    <seg n="5" type="line" facs="z129r1_05">καὶ ἐπὶ δὲ οὔται γωνίαν <seg
                            type="suppliedword">μεί</seg></seg>
                    <seg n="6" type="line" facs="z129r1_06"><seg type="wordend">ζο<supplied
                                reason="lost">να</supplied></seg>
                        <supplied reason="lost">τῆς</supplied> Β ποιοῦντος <supplied reason="lost"
                            >τοῦ</supplied> ἄξονος</seg>
                    <seg n="7" type="line" facs="z129r1_07"><seg type="word">πρὸ<supplied
                                reason="lost">ς</supplied></seg>
                        <supplied reason="lost">τὸ</supplied> ὑγρὸν <seg type="word">καθίστ<supplied
                                reason="lost">ηστα</supplied>ι</seg>
                        <seg type="word"><supplied reason="lost">τ</supplied>ὸ</seg>
                        <w part="I">τμῆ</w></seg>
                    <seg n="8" type="line" facs="z129r1_08"><w part="F">μα</w> οὔδ’ ἐλάσσονα, <seg
                            type="word">φαν<unclear>ε</unclear>ρό<supplied reason="lost"
                            >ν,</supplied></seg> ὅτι</seg>
                    <seg n="9" type="line" facs="z129r1_09">τηλικαύτην ποιοῦντος <seg
                            type="unclearword">ἀπο<unclear>κατ</unclear>α</seg></seg>
                    <seg n="10" type="line" facs="z129r1_10"><seg type="wordend">σταθήσεται·</seg>
                        οὕτως <expan>γὰρ</expan> ἔσται ἥ τε</seg>
                    <seg n="11" type="line" facs="z129r1_11"><unclear>Ι</unclear>Ο ἴση τῆι ΨΒ
                            <expan>καὶ</expan> ἡ <unclear>Ρ</unclear><gap extent="1"/> τῆι ΨΡ
                            <expan>καὶ</expan></seg>
                    <seg n="12" type="line" facs="z129r1_12">ἡ ΠΗ τῆ Φ· ἡμιολία
                        <unclear>οὐν</unclear> ἔσται ἡ ΜΠ</seg>
                    <seg n="13" type="line" facs="z129r1_13">τῆς Π<gap extent="1"/>, <supplied
                            reason="lost">ἡ</supplied>
                        <seg type="word">
                            <supplied reason="lost">δ</supplied>
                            <unclear>ε</unclear>
                        </seg> ΠΗ τῆς ΗΜ <seg type="suppliedword">διπλα</seg></seg>
                    <seg n="14" type="line" facs="z129r1_14"><seg type="wordend">σ<supplied
                                reason="lost">ία.</supplied></seg><seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ὸ</unclear>
                        </seg>
                        <supplied reason="lost">δὲ</supplied>
                        <unclear>Η</unclear> ἄρα τοῦ ἐν τῶι ὑγρῶι</seg>
                    <figure n="2.8.2">
                        <figDesc>Figure 2.8.2</figDesc>
                    </figure>
                </seg>
                <seg n="128v2" type="folio">
                    <seg n="1" type="line" facs="z128v2_01">βάρους κέντρον ἐστίν· ὥστε κατὰ</seg>
                    <seg n="2" type="line" facs="z128v2_02">τὴν αὐτὴν κάθετον <w part="I"
                        >ἀνενεχθήσε</w></seg>
                    <seg n="3" type="line" facs="z128v2_03"><w part="F">ται,</w> καὶ τὸ ἐκτὸς καὶ
                        οὐδὲν <w part="I">κατενε</w></seg>
                    <seg n="4" type="line" facs="z128v2_04"><w part="F">χθήσεται.</w> μενεῖ ἄρα·
                        ἀντωθοῦνται</seg>
                    <seg n="5" type="line" facs="z128v2_05">γὰρ ὑπ’ ἀλλήλων.</seg>
                </seg>
                <seg n="128v2" type="folio">
                    <seg n="5" type="line" facs="z128v2_05">τὸ ὀρθὸν</seg>
                    <seg n="6" type="line" facs="z128v2_06">τμῆμα τοῦ ὀρθογωνίου <seg type="word"><choice>
                                <abbr>κωνοειδ</abbr>
                                <expan>κωνοειδού</expan>
                            </choice>ς</seg>,</seg>
                    <seg n="7" type="line" facs="z128v2_07"><seg type="word"
                        ><unclear>ὅ</unclear>ταν</seg> τὸν ἄξονα ἔχη μείζονα μὲν</seg>
                    <seg n="8" type="line" facs="z128v2_08">ἡμιόλιον τῆς μέχρι τοῦ ἄξονος,</seg>
                    <seg n="9" type="line" facs="z128v2_09">ἐλάσσονα δὲ ἢ ὥστε τοῦτον <seg
                            type="word">ἔχει<supplied reason="lost">ν</supplied></seg>
                        <seg type="word"><supplied reason="lost">τὸ</supplied>ν</seg></seg>
                    <seg n="10" type="line" facs="z128v2_10">λόγον, ὃν ἔχει τὰ <num>ΙΕ</num> πρὸς
                            <num>Δ</num>, <seg type="word"><supplied reason="lost"
                        >ἐ</supplied>ὰν</seg></seg>
                    <seg n="11" type="line" facs="z128v2_11"><seg type="word"
                        >τῶ<unclear>ι</unclear></seg> βάρει πρὸς τὸ ὑγρὸν <seg type="word"
                                >μείζ<unclear>ον</unclear><supplied reason="lost">α</supplied></seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">λό</supplied>
                        </seg></seg>
                    <seg n="12" type="line" facs="z128v2_12"><seg type="wordend"
                            ><unclear>γ</unclear>ον</seg> ἔχηι τὸ ὃν ἔχει ἡ ὑπεροχή, ἧ <w part="I"
                            >μεῖ</w></seg>
                    <seg n="13" type="line" facs="z128v2_13"><w part="F">ζόν</w> ἐστι τὸ ἀπὸ τοῦ
                        ἄξονος <w part="I">τετρά</w></seg>
                    <seg n="14" type="line" facs="z128v2_14"><w part="F">γωνον</w> τοῦ τετραγώνου
                        τοῦ ἀπὸ τῆς</seg>
                    <seg n="15" type="line" facs="z128v2_15"><seg type="word"
                            >ὑπερο<unclear>χ</unclear>ῆς,</seg> ἧ μεῖζόν ἐστιν ὁ <choice>
                            <abbr>ἄξω</abbr>
                            <expan>ἄξων</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z128v2_16">ἢ ἡμιόλιος τῆς μέχρι <seg type="word"
                                >το<supplied reason="lost">ῦ</supplied></seg>
                        <seg type="word">
                            <choice>
                                <abbr>ἄξον</abbr>
                                <expan>ἄξον<supplied reason="lost">ος</supplied></expan>
                            </choice>
                        </seg>,</seg>
                    <seg n="17" type="line" facs="z128v2_17">πρὸς τὸ τετράγωνον τὸ ἀπὸ τοῦ <w
                            part="I">ἄ</w></seg>
                    <seg n="18" type="line" facs="z128v2_18"><w part="F">ξονος,</w> ἀφεθὲν εἰς τὸ
                        ὑγρὸν οὕτως,</seg>
                    <seg n="19" type="line" facs="z128v2_19">ὥστε τὴν βάσιν αὐτοῦ ὅλην εἶναι</seg>
                    <seg n="20" type="line" facs="z128v2_20">ἐν τῶι ὑγρῶι, τεθὲν κεκλιμένον <w
                            part="I">οὔ</w></seg>
                    <seg n="21" type="line" facs="z128v2_21"><w part="F">τε</w> καταστραφήσεται,
                        ὥστε τὸν <seg type="suppliedword">
                            <unclear>ἄ</unclear>
                            <supplied reason="lost">ξο</supplied>
                        </seg></seg>
                </seg>
                <seg n="129r2" type="folio">
                    <seg n="1" type="line" facs="z129r2_01"><seg type="wordend">να</seg> αὐτοῦ κατὰ
                        κάθετον <seg type="word">εἶν<unclear>αι</unclear></seg>, <supplied
                            reason="lost">οὔτε</supplied></seg>
                    <seg n="2" type="line" facs="z129r2_02">μενεῖ κεκλιμένον, πλὴν ὅταν ὁ</seg>
                    <seg n="3" type="line" facs="z129r2_03">ἄξων αὐτοῦ πρὸς <seg type="word"
                                >τ<supplied reason="lost">ὴ</supplied>ν</seg>
                        <choice>
                            <abbr>ἐπιφάνεια</abbr>
                            <expan>ἐπιφάνειαν</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z129r2_04">τοῦ ὑγροῦ ποιεῖ γωνίαν ἴσην τῆι</seg>
                    <seg n="5" type="line" facs="z129r2_05">ληφθείσηι ὁμοίως, ἧι πρότερον.</seg>
                </seg>
                <seg n="129r2" type="folio">
                    <seg n="6" type="line" facs="z129r2_06">ἔστω τμῆμα οἷον εἴρηται, καὶ <w part="I"
                            >κείσ</w></seg>
                    <seg n="7" type="line" facs="z129r2_07"><w part="F">θω</w> ἡ ΛΒ
                        Ϙ<unclear>Π</unclear> τῶι ἄξονι τοῦ <w part="I">τμήμα</w></seg>
                    <seg n="8" type="line" facs="z129r2_08"><w part="F">τος,</w> καὶ ἡ μὲν ΒΚ τῆς ΚΔ
                            <w part="I">διπλα</w></seg>
                    <seg n="9" type="line" facs="z129r2_09"><w part="F">σία</w> ἔστω, ἡ δὲ ΚΡ <seg
                            type="word">
                            <expan>ἴσ<add rend="superscript">η</add></expan>
                        </seg> τῆι <seg type="word">μέχρ<supplied reason="lost">ι</supplied></seg>
                        τοῦ</seg>
                    <seg n="10" type="line" facs="z129r2_10">ἄξονος, ἡ δὲ ΤΒ ἡμιολία τῆς ΒΡ,</seg>
                    <seg n="11" type="line" facs="z129r2_11">ὃν δὲ λόγον ἔχει τὸ τμῆμα <sic>το</sic>
                        βάρει</seg>
                    <seg n="12" type="line" facs="z129r2_12">πρὸς τὸ ὑγρόν, τοῦτον ἔχει ἡ</seg>
                    <seg n="13" type="line" facs="z129r2_13">ὑπεροχή, ἧ ὑπερέχει τὸ <w part="I"
                        >τε</w></seg>
                    <seg n="14" type="line" facs="z129r2_14"><w part="F">τράγωνον</w> τὸ ἀπὸ τῆς ΒΔ</seg>
                    <seg n="15" type="line" facs="z129r2_15">τοῦ τετραγώνου τοῦ <seg type="word"
                                >ἀπ<unclear>ὸ</unclear></seg>
                        <seg type="word">
                            <unclear>τῆ</unclear>
                            <supplied reason="lost">ς</supplied>
                        </seg></seg>
                    <seg n="16" type="line" facs="z129r2_16">ΦΧ, πρός τὸ <seg type="word"
                                >τετράγωνο<unclear>ν</unclear></seg> τὸ</seg>
                    <seg n="17" type="line" facs="z129r2_17">ἀπὸ τῆς Β<supplied reason="lost"
                        >Δ</supplied>, <seg type="word"><supplied reason="lost">ἔστ</supplied>ω</seg>
                        <unclear>δὲ</unclear>
                        <supplied reason="lost">ἡ</supplied> Φ</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
