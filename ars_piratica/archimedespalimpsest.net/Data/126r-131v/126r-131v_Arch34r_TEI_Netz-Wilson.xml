    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="126r1" type="folio">
                    <seg n="1" type="line" facs="z126r1_01">ἕλικος τὸ Α σημεῖον, ἀρχὰ δὲ τᾶς</seg>
                    <seg n="2" type="line" facs="z126r1_02">περιφορᾶς ἁ ΑΔ εὐθεῖα, καὶ <sic>
                            <seg type="clearword">ἐπι</seg>
                        </sic></seg>
                    <seg n="3" type="line" facs="z126r1_03"><sic>
                            <seg type="wordend">ψαέτω</seg>
                        </sic> τᾶς ἕλικος εὐθεῖά τις ἁ ΖΕ.</seg>
                    <seg n="4" type="line" facs="z126r1_04">φαμὶ δὴ καθ’ ἓν μόνον σαμεῖον</seg>
                    <seg n="5" type="line" facs="z126r1_05">ἐπιψαύειν αὐτᾶς.</seg>
                </seg>
                <seg n="126r1" type="folio">
                    <seg n="5" type="line" facs="z126r1_05">ἐπιψαυέτω γάρ, εἰ</seg>
                    <seg n="6" type="line" facs="z126r1_06">δυνατόν, κατὰ δύο σαμεῖα τὰ ΓΗ,</seg>
                    <seg n="7" type="line" facs="z126r1_07">καὶ ἐπεζεύχθωσαν αἱ ΑΓ ΑΗ, καὶ</seg>
                    <seg n="8" type="line" facs="z126r1_08">ἁ γωνία δίχα τετμάσθω ἁ <seg
                            type="clearword">περι</seg></seg>
                    <seg n="9" type="line" facs="z126r1_09"><seg type="wordend">εχόμενα</seg> ὑπὸ
                        τῶν ΑΗ ΑΓ, καθ’ ὃ</seg>
                    <seg n="10" type="line" facs="z126r1_10">δὲ σαμεῖον ἁ δίχα τέμνουσα <choice>
                            <abbr>τὰ</abbr>
                            <expan>τὰν</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z126r1_11">γωνίαν τᾶι ἕλικι ποτιπίπτει, ἔστω</seg>
                    <seg n="12" type="line" facs="z126r1_12">τὸ Θ. τῶι δὴ ἴσω ὑπερέχει ἅ τε</seg>
                    <seg n="13" type="line" facs="z126r1_13">ΑΗ τᾶς ΑΘ καὶ ἁ ΑΘ τᾶς ΑΓ,</seg>
                    <seg n="14" type="line" facs="z126r1_14">ἐπειδὴ ἴσας γωνίας <seg
                            type="clearword">
                            <choice>
                                <abbr>περιέχ</abbr>
                                <expan>περιέχου</expan>
                            </choice>
                        </seg></seg>
                    <seg n="15" type="line" facs="z126r1_15"><seg type="wordend">σι</seg> ποτ’
                        ἀλλάλας· ὥστε <seg type="clearword">διπλάσι</seg></seg>
                    <seg n="16" type="line" facs="z126r1_16"><seg type="wordend">αί</seg> ἐντι αἱ αΗ
                        αΓ τᾶς αΘ. ἀλλὰ</seg>
                    <seg n="17" type="line" facs="z126r1_17">τᾶς ἐν τῶι τριγώνωι τᾶς αΘ <seg
                            type="clearword">δί</seg></seg>
                    <seg n="18" type="line" facs="z126r1_18"><seg type="wordend">χα</seg> τεμνούσας
                        τὰν γωνίαν <seg type="clearword">μεί</seg></seg>
                    <seg n="19" type="line" facs="z126r1_19">
                        <seg type="wordend">ζων</seg>
                        <seg type="word">
                            <supplied reason="lost">ἐ</supplied>ντὶ</seg> ἢ διπλάσιαι· δῆλον
                            <expan>οὖν</expan>, </seg>
                </seg>
                <seg n="131v1" type="folio">
                    <seg n="1" type="line" facs="z131v1_01">ὅτι, καθ’ ὃ συμπίπτει σαμεῖον</seg>
                    <seg n="2" type="line" facs="z131v1_02">τᾶι ΓΗ εὐθεία ὡς αΘ, μεταξὺ</seg>
                    <seg n="3" type="line" facs="z131v1_03">τῶν ΘΑ ἐντὶ σαμείων· τέμνει
                        <expan>ἄρα</expan></seg>
                    <seg n="4" type="line" facs="z131v1_04">ἁ ΕΖ τὰν ἕλικα, ἐπειδή τι <seg
                            type="word"> τῶ<supplied reason="lost">ν</supplied>
                        </seg></seg>
                    <seg n="5" type="line" facs="z131v1_05">ἐν τᾶι ΓΗ σαμείων ἐντός ἐστι <seg
                            type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">ᾶς</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="6" type="line" facs="z131v1_06">ἕλικος. <sic>ὑπόκειτο</sic> δὲ <seg
                            type="clearword">ἐπιψαύου</seg></seg>
                    <seg n="7" type="line" facs="z131v1_07"><seg type="wordend">σα·</seg> καθ’ ἓν
                        ἄρα μόνον ἅπτεται</seg>
                    <seg n="8" type="line" facs="z131v1_08">ἁ ΕΗ τᾶς ἕλικος.</seg>
                    <figure n="13.1">
                        <figDesc>Figure 13.1</figDesc>
                    </figure>
                </seg>
                <seg n="126r2" type="folio">
                    <seg n="1" type="line" facs="z126r2_01">εἴ κα ποτὶ τὰν ἕλικα τὰν ἐν <supplied
                            reason="lost">τᾶι</supplied>
                        <supplied reason="lost">πρώται</supplied></seg>
                    <seg n="2" type="line" facs="z126r2_02">περιφορᾶν γεγραμμέναν <seg
                            type="suppliedword">πο<supplied reason="lost">τι</supplied></seg></seg>
                    <seg n="3" type="line" facs="z126r2_03"><seg type="wordend">πεσέωντι</seg> δύο
                        εὐθεῖαι ἀπὸ τοῦ <seg type="suppliedword">σαμ<supplied reason="lost"
                            >εί</supplied></seg></seg>
                    <seg n="4" type="line" facs="z126r2_04"><seg type="wordend">ου</seg>, ὅ ἐστιν
                        ἀρχὰ τᾶς ἕλικος, καὶ <seg type="unclearword">
                            <unclear>ἐκ</unclear>
                        </seg></seg>
                    <seg n="5" type="line" facs="z126r2_05"><seg type="wordend">βληθέων</seg> ποτὶ
                        τὰν τοῦ <seg type="word">πρώ<supplied reason="lost">του</supplied>
                        </seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">κύ</supplied>
                        </seg></seg>
                    <seg n="6" type="line" facs="z126r2_06"><seg type="wordend">κλου</seg>
                        περιφοράν, τὸν αὐτὸν <seg type="clearword">ἑξοῦν</seg></seg>
                    <seg n="7" type="line" facs="z126r2_07"><seg type="wordend">τι</seg> λόγον αἱ
                        ποτὶ τὰν ἕλικα <seg type="clearword">ποτιπί</seg></seg>
                    <seg n="8" type="line" facs="z126r2_08"><seg type="wordend">πτουσαι</seg> ποτ’
                        ἀλλάλας, ὧν αἱ <seg type="clearword">περι</seg></seg>
                    <seg n="9" type="line" facs="z126r2_09"><seg type="wordend">φέρειαι</seg> τοῦ
                        κύκλου αἱ μεταξὺ τοῦ</seg>
                    <seg n="10" type="line" facs="z126r2_10">πέρατος τᾶς ἕλικος καὶ τῶν <seg
                            type="clearword">πε</seg></seg>
                    <seg n="11" type="line" facs="z126r2_11"><seg type="wordend">ράτων</seg> τᾶν
                        ἐκβληθεισᾶν <choice>
                            <abbr>εὐθειᾶ</abbr>
                            <expan>εὐθειᾶν</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z126r2_12">τῶν <seg type="word">ἐπ<supplied
                                reason="lost">ὶ</supplied>
                        </seg> τᾶς περιφερείας <seg type="clearword">γινομέ</seg></seg>
                    <seg n="13" type="line" facs="z126r2_13"><seg type="wordend">νων</seg>, ἐπὶ τὰ
                        προαγούμενα <seg type="clearword">λαμβα</seg></seg>
                    <seg n="14" type="line" facs="z126r2_14"><seg type="wordend">νομενᾶν</seg> τᾶν
                        περιφερειᾶν ἀπὸ</seg>
                    <seg n="15" type="line" facs="z126r2_15">τοῦ πέρατος τᾶς ἕλικος.</seg>
                </seg>
                <seg n="126r2" type="folio">
                    <seg n="15" type="line" facs="z126r2_15">ἔστω ἕλιξ</seg>
                    <seg n="16" type="line" facs="z126r2_16">ἁ ΑΒ ΓΔ ΕΘ ἐν τᾶι πρώται <seg
                            type="clearword">περι</seg></seg>
                    <seg n="17" type="line" facs="z126r2_17"><seg type="wordend">φορᾶι</seg>
                        γεγραμμένα, ἀρχὰ δὲ τᾶς</seg>
                    <seg n="18" type="line" facs="z126r2_18">μὲν ἕλικος ἔστω τὸ Α σαμεῖον, ἁ</seg>
                </seg>
                <seg n="131v2" type="folio">
                    <seg n="1" type="line" facs="z131v2_01"><supplied reason="lost">δὲ</supplied>
                            Θ<unclear>Α</unclear>
                        <seg type="word">
                            <supplied reason="lost">εὐ</supplied>θεῖα</seg>
                        <seg type="word">
                            <supplied reason="lost">ἀ</supplied><unclear>ρ</unclear>χὰ </seg> τᾶς
                            <seg type="suppliedword">περιφο</seg>
                    </seg>
                    <seg n="2" type="line" facs="z131v2_02"><seg type="wordend">ρ<supplied
                                reason="lost">ᾶ</supplied>ς</seg> ἔστω, καὶ κύκλος ὁ ΘΚΗ ἔστω</seg>
                    <seg n="3" type="line" facs="z131v2_03">ὁ πρῶτος, ποτιπιπτόντων δὲ <seg
                            type="clearword">ἀ</seg></seg>
                    <seg n="4" type="line" facs="z131v2_04"><seg type="wordend">πὸ</seg> τοῦ Α
                        σαμείου ποτὶ τὰν ἕλικα</seg>
                    <seg n="5" type="line" facs="z131v2_05">αἱ αΕ ΑΔ καὶ ἐκπιπτόντων ποτὶ</seg>
                    <seg n="6" type="line" facs="z131v2_06">τὰν τοῦ κύκλου περιφέρειαν ἐπὶ</seg>
                    <seg n="7" type="line" facs="z131v2_07">τὰς ΖΗ. δεικτέον, <expan>ὅτι</expan> τὸν
                        αὐτὸν <seg type="clearword">ἔχον</seg></seg>
                    <seg n="8" type="line" facs="z131v2_08"><seg type="wordend">τι</seg> λόγον ἁ ΑΕ
                        ποτὶ τὰν ΑΔ, ὃν ἁ</seg>
                    <seg n="9" type="line" facs="z131v2_09">ΘΚΖ περιφέρεια ποτὶ τὰν ΘΚΗ</seg>
                    <seg n="10" type="line" facs="z131v2_10">περιφέρειαν.</seg>
                </seg>
                <seg n="131v2" type="folio">
                    <seg n="10" type="line" facs="z131v2_10">περιαγομένας <expan>γὰρ</expan></seg>
                    <seg n="11" type="line" facs="z131v2_11">τᾶς ΑΘ γραμμᾶς δῆλον ὡς τὸ <choice>
                            <abbr>μὲ</abbr>
                            <expan>μὲν</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z131v2_12">Θ σαμεῖον κατὰ τᾶς τοῦ ΘΚΗ</seg>
                    <seg n="13" type="line" facs="z131v2_13">κύκλου περιφερείας <seg
                            type="clearword">ἐνηνεγμέ</seg></seg>
                    <seg n="14" type="line" facs="z131v2_14"><seg type="wordend">νον</seg>
                        <expan>ἐστὶν</expan> ἰσοταχέως, τὸ δὲ Α κατὰ</seg>
                    <seg n="15" type="line" facs="z131v2_15">τᾶς εὐθείας <seg type="word">
                                φερό<unclear>μεν</unclear>ον</seg> τὰν ΑΘ</seg>
                    <seg n="16" type="line" facs="z131v2_16">γραμμὰν <seg type="word">π<supplied
                                reason="lost">επό</supplied><unclear>ρ</unclear>ευται </seg>, καὶ τὸ
                        Θ</seg>
                    <seg n="17" type="line" facs="z131v2_17"><seg type="word">
                            <supplied reason="lost">σα</supplied>μεῖον</seg>
                        <seg type="word">κ<unclear>α</unclear>τὰ</seg> τᾶς τοῦ κύκλου</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
