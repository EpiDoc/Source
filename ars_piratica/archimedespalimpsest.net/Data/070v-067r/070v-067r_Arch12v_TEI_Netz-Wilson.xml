    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="70v1" type="folio">
                    <seg n="1" type="line" facs="z70v1_01">ἅπτεσθαι τῆς τοῦ ὑγροῦ <w part="I"
                            >ἐπιφανεί</w></seg>
                    <seg n="2" type="line" facs="z70v1_02"><w part="F">ας,</w> καὶ τὸν ἄξονα αὐτοῦ
                        γωνίαν</seg>
                    <seg n="3" type="line" facs="z70v1_03">ποιεῖν πρὸς τὴν ἐπιφάνειαν τοῦ</seg>
                    <seg n="4" type="line" facs="z70v1_04">ὑγροῦ μείζονα τῆς Η. </seg>
                </seg>
                <seg n="70v1" type="folio">
                    <seg n="4" type="line" facs="z70v1_04">ἐὰν δὲ τὸ </seg>
                    <seg n="5" type="line" facs="z70v1_05">τμῆμα τῶι βάρει πρὸς τὸ ὑγρὸν</seg>
                    <seg n="6" type="line" facs="z70v1_06">τοῦτον ἔχη τὸν λόγον, τὸ <w part="I"
                            >τετρά</w></seg>
                    <seg n="7" type="line" facs="z70v1_07"><w part="F">γωνον</w> τὸ ἀπὸ τῆς ΞΟ πρὸς
                        τὸ <w part="I">τε</w></seg>
                    <seg n="8" type="line" facs="z70v1_08"><w part="F">τράγωνον</w> τὸ ἀπὸ τῆς ΒΔ,
                            <w part="I">ἀφε</w></seg>
                    <seg n="9" type="line" facs="z70v1_09"><w part="F">θὲν</w> ἐς τὸ ὑγρὸν
                        κεκλιμένον οὕτω,</seg>
                    <seg n="10" type="line" facs="z70v1_10">ὥστε τὴν βάσιν αὐτοῦ μὴ <choice>
                            <abbr>απτεσθ</abbr>
                            <expan>ἅπτεσθαι</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z70v1_11">τοῦ ὑγροῦ, καταστησεῖται <w part="I"
                            >κεκλι</w></seg>
                    <seg n="12" type="line" facs="z70v1_12"><w part="F">μένον</w> οὕτως, ὥστε τὴν
                        βάσιν <seg type="unclearword">α<unclear>ὐ</unclear></seg></seg>
                    <seg n="13" type="line" facs="z70v1_13"><seg type="wordend">τοῦ</seg> ἅπτεσθαι
                        καθ’ ἓν τῆς τοῦ <choice>
                            <abbr>υγρ</abbr>
                            <expan>ὑγροῦ</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z70v1_14">ἐπιφανείας, καὶ τὸν ἄξονα</seg>
                    <seg n="15" type="line" facs="z70v1_15">αὐτοῦ πρὸς τὴν ἐπιφάνειαν τοῦ </seg>
                    <seg n="16" type="line" facs="z70v1_16">ὑγροῦ γωνίαν ποιεῖν <sic>εἴση</sic> τῆ
                        Η.</seg>
                </seg>
                <seg n="70v1" type="folio">
                    <seg n="17" type="line" facs="z70v1_17">ἐὰν δὲ τὸ τμῆμα τῶι βάρει πρὸς</seg>
                    <seg n="18" type="line" facs="z70v1_18">τὸ ὑγρὸν ἐλάσσονα μὲν λόγον <w part="I"
                            >ἔ</w></seg>
                    <seg n="19" type="line" facs="z70v1_19"><w part="F">χη</w> τοῦ, ὃν ἔχη τὸ
                        τετράγωνον τὸ </seg>
                    <seg n="20" type="line" facs="z70v1_20">ἀπὸ τῆς ΞΟ πρὸς τὸ <choice>
                            <abbr>τετραγωνο</abbr>
                            <expan>τετράγωνον</expan>
                        </choice></seg>
                </seg>
                <seg n="67r1" type="folio">
                    <seg n="1" type="line" facs="z67r1_01"><seg type="word"><supplied reason="lost"
                                >τ</supplied>ὸ</seg> ἀπὸ τῆς ΒΔ, μείζονα δὲ τοῦ,</seg>
                    <seg n="2" type="line" facs="z67r1_02">ὃν ἔχει τὸ ἀπὸ τῆς ΠΦ πρὸς</seg>
                    <seg n="3" type="line" facs="z67r1_03">τὸ ἀπὸ τῆς ΒΔ, ἀφεθὲν ἐς τὸ <w part="I"
                        >ὑ</w></seg>
                    <seg n="4" type="line" facs="z67r1_04"><w part="F">γρὸν</w> καὶ τεθὲν κεκλιμένον</seg>
                    <seg n="5" type="line" facs="z67r1_05">οὕτως, ὥστε τὴν βάσιν αὐτοῦ μὴ</seg>
                    <seg n="6" type="line" facs="z67r1_06">ἅπτεσθαι τοῦ ὑγροῦ, <w part="I"
                        >καταστήσε</w></seg>
                    <seg n="7" type="line" facs="z67r1_07"><w part="F">ται</w> κεκλιμένον οὕτως,
                        ὥστε <choice>
                            <abbr>τη</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z67r1_08">δὲ βάσιν αὐτοῦ κατὰ πλείονα <w part="I"
                            >τό</w></seg>
                    <seg n="9" type="line" facs="z67r1_09"><w part="F">πον</w> τέμνεσθαι ὑπὸ τοῦ
                        ὑγροῦ.</seg>
                </seg>
                <seg n="67r1" type="folio">
                    <seg n="10" type="line" facs="z67r1_10">ἐν δὲ τὸ τμῆμα πρὸς τούτω βάρει</seg>
                    <seg n="11" type="line" facs="z67r1_11">πρὸς τὸ ὑγρὸν τοῦτον ἔχει τὸν <w
                            part="I">λό</w></seg>
                    <seg n="12" type="line" facs="z67r1_12"><w part="F">γον,</w> ὃν ἔχει τὸ
                        τετράγωνον τὸ <w part="I">ἀ</w></seg>
                    <seg n="13" type="line" facs="z67r1_13"><w part="F">πὸ</w> τῆς ΠΦ πρὸς τὸ <choice>
                            <abbr>τετραγωνο</abbr>
                            <expan>τετράγωνον</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z67r1_14">τὸ ἀπὸ τῆς ΒΔ, ἀφεθὲν ἐς τὸ <w part="I"
                            >ὑ</w></seg>
                    <seg n="15" type="line" facs="z67r1_15"><w part="F">γρὸν</w> καὶ τεθὲν
                        κεκλιμένον <expan>οὕτως</expan>,</seg>
                    <seg n="16" type="line" facs="z67r1_16">ὥστε τὴν βάσιν αὐτοῦ καθ’ ἓν <w part="I"
                            >ση</w></seg>
                    <seg n="17" type="line" facs="z67r1_17"><w part="F">μεῖον</w> ἅπτεσθαι τοῦ
                        ὑγροῦ, <w part="I">κατα</w></seg>
                </seg>
                <seg n="70v2" type="folio">
                    <seg n="1" type="line" facs="z70v2_01"><w part="F">στήσεται</w> δὲ <seg
                            type="word">κε<unclear>κ</unclear>λιμένον</seg> οὕτως,</seg>
                    <seg n="2" type="line" facs="z70v2_02">ὥστε τὴν βάσιν αὐτοῦ καθ’ ἓν <w part="I"
                            >ση</w></seg>
                    <seg n="3" type="line" facs="z70v2_03"><w part="F">μεῖον</w>
                        <seg type="unclearword">ἅπτεσθ<unclear>α</unclear>ι</seg> τοῦ ὑγροῦ <w
                            part="I">ἐπιφα</w></seg>
                    <seg n="4" type="line" facs="z70v2_04"><w part="F">νείας</w>, <expan>καὶ</expan>
                        τὸν ἄξονα αὐτοῦ ποιεῖ <w part="I">γω</w>
                    </seg>
                    <seg n="5" type="line" facs="z70v2_05"><w part="F">νίας</w> ἴσην τῆι Ψ.</seg>
                </seg>
                <seg n="70v2" type="folio">
                    <seg n="5" type="line" facs="z70v2_05">ἐὰν δὲ <seg type="word"
                            ><unclear>τ</unclear>ὸ</seg> τμῆμα</seg>
                    <seg n="6" type="line" facs="z70v2_06">τῶι βάρει πρὸς τὸ ὑγρὸν ἐλάσσονα</seg>
                    <seg n="7" type="line" facs="z70v2_07">λόγον ἔχη τοῦ, ὃν ἔχει τὸ <w part="I"
                            >τετράγω</w></seg>
                    <seg n="8" type="line" facs="z70v2_08"><w part="F">νον</w> τὸ ἀπὸ τῆς ΠΦ πρὸς
                            <seg type="word">τ<unclear>ὸ</unclear></seg>
                        <w part="I">τετρά</w></seg>
                    <seg n="9" type="line" facs="z70v2_09"><w part="F">γωνον</w> τὸ ἀπὸ τῆς ΒΔ,
                        ἀφεθὲν</seg>
                    <seg n="10" type="line" facs="z70v2_10">ἐς τὸ ὑγρὸν καὶ τεθὲν <choice>
                            <abbr>κεκλιμενο</abbr>
                            <expan>κεκλιμένον</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z70v2_11">οὕτως, ὥστε τὴν βάσιν αὐτοῦ μὴ <w
                            part="I">ἅ</w></seg>
                    <seg n="12" type="line" facs="z70v2_12"><w part="F">πτεσθαι</w> τοῦ ὑγροῦ,
                        καταστήσεται</seg>
                    <seg n="13" type="line" facs="z70v2_13">κεκλιμένον οὕτως, ὥστε τὸν μὲν</seg>
                    <seg n="14" type="line" facs="z70v2_14">ἄξονα αὐτοῦ πρὸς τὴν <w part="I"
                            >ἐπιφάνει</w></seg>
                    <seg n="15" type="line" facs="z70v2_15"><w part="F">αν</w> τοῦ ὑγροῦ γωνίαν
                        ποιεῖν <w part="I">ἐλάσ</w></seg>
                    <seg n="16" type="line" facs="z70v2_16"><w part="F">σονα</w> τῆς Ψ, τὴν δὲ βάσιν
                        τοῦ</seg>
                    <seg n="17" type="line" facs="z70v2_17">μηδὲ καθ’ ἓν ἅπτεσθαι τῆς τοῦ <w
                            part="I">ὑ</w></seg>
                    <seg n="18" type="line" facs="z70v2_18"><w part="F">γροῦ</w> ἐπιφανείας.</seg>
                </seg>
                <seg n="70v2" type="folio">
                    <seg n="18" type="line" facs="z70v2_18">δειχθήσεται</seg>
                    <seg n="19" type="line" facs="z70v2_19">δὲ ταῦτα ἑξῆς.</seg>
                </seg>
                <seg n="70v2" type="folio">
                    <seg n="19" type="line" facs="z70v2_19">ἐχέτω δὴ</seg>
                    <seg n="20" type="line" facs="z70v2_20">πρῶτον τὸ τμῆμα τῶι βάρει <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice></seg>
                </seg>
                <seg n="67r2" type="folio">
                    <seg n="1" type="line" facs="z67r2_01">τὸ ὑγρὸν μείζονα μὲν λόγον <choice>
                            <abbr>το</abbr>
                            <expan>τοῦ</expan>
                        </choice>,</seg>
                    <seg n="2" type="line" facs="z67r2_02">ὃν ἔχει τὸ ἀπὸ τῆς ΞΠ <w part="I"
                        >τετρά</w></seg>
                    <seg n="3" type="line" facs="z67r2_03"><w part="F">γωνον</w> πρὸς <seg
                            type="word">τ<supplied reason="lost">ὸ</supplied></seg> ἀπὸ τῆς ΒΔ, <seg
                            type="unclearword"><unclear>ἐ</unclear>λάσ</seg></seg>
                    <seg n="4" type="line" facs="z67r2_04"><seg type="wordend">σονα</seg> δὲ <seg
                            type="word">το<unclear>ῦ</unclear>,</seg> ὃν ἔχει τὸ ἀπὸ τῆς <w part="I"
                            >ὑ</w></seg>
                    <seg n="5" type="line" facs="z67r2_05"><w part="F">περοχῆς</w> τετράγωνον, ἧ
                            <seg type="word">
                            <choice>
                                <abbr>μειζω</abbr>
                                <expan>μείζ<unclear>ων</unclear></expan>
                            </choice>
                        </seg></seg>
                    <seg n="6" type="line" facs="z67r2_06"><expan>ἐστὶν</expan> ὁ ἄξων ἡμιόλιος τῆς
                        μέχρι</seg>
                    <seg n="7" type="line" facs="z67r2_07">τοῦ ἄξονος, πρὸς τὸ ἀπὸ τῆς ΒΔ</seg>
                    <seg n="8" type="line" facs="z67r2_08">τετράγωνον, <choice>
                            <abbr>κ</abbr>
                            <expan>καὶ</expan>
                        </choice> ὑποκείσθω τὸ</seg>
                    <seg n="9" type="line" facs="z67r2_09">πρότερον <seg type="word">
                            <choice>
                                <abbr>κατεσκευασμενο</abbr>
                                <expan>κατε<supplied reason="lost">σκευ</supplied>ασμ<supplied
                                        reason="lost">έ</supplied>νον</expan>
                            </choice>
                        </seg></seg>
                    <seg n="10" type="line" facs="z67r2_10">σχῆμα, ὃν δῆλον ἔχει τὸ τμῆμα</seg>
                    <seg n="11" type="line" facs="z67r2_11">τῶι βάρει πρὸς τὸ ὑγρόν, τοῦτον</seg>
                    <seg n="12" type="line" facs="z67r2_12">ἐχέτω τὸ ἀπὸ τῆς Ψ <w part="I"
                        >τετράγω</w></seg>
                    <seg n="13" type="line" facs="z67r2_13"><w part="F">νον</w> πρὸς τὸ ἀπὸ τῆς ΒΔ·
                        ἔστι</seg>
                    <seg n="14" type="line" facs="z67r2_14">δὴ Ψ τῆς μὲν ΞΠ μείζων <choice>
                            <abbr>εστι</abbr>
                            <expan>ἐστὶν</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z67r2_15">ὁ ἄξων ἢ ἡμιόλιος τῆς <seg type="word"
                                >μέχ<unclear>ρ</unclear><supplied reason="lost">ι</supplied></seg></seg>
                    <seg n="16" type="line" facs="z67r2_16"><seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<unclear>οῦ</unclear></expan>
                            </choice>
                        </seg>
                        <seg type="word">ἄ<unclear>ξ</unclear>ονος.</seg> ἐνηρμώσθω δέ τις</seg>
                    <seg n="17" type="line" facs="z67r2_17">μεταξὺ τῶν ΑΠΟ Λ<supplied reason="lost"
                            >Α</supplied> ΞΔ <choice>
                            <abbr>κωνω</abbr>
                            <expan>κώνων</expan>
                        </choice></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
