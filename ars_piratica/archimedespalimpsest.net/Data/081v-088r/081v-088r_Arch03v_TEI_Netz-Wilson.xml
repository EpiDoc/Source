    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="81v1" type="folio">
                    <seg n="1" type="line" facs="z81v1_01">κύκλου περιφέρειαν κέντρον <choice>
                            <abbr>εχ</abbr>
                            <expan>
                                <w part="I">ἔχου</w>
                            </expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z81v1_02"><w part="F">σαν</w> τὸ <seg type="word"
                                ><unclear>σ</unclear>αμεῖον</seg> δι’ οὗ τῶι ἐπιπέδωι <w part="I"
                        >τέ</w></seg>
                    <seg n="3" type="line" facs="z81v1_03"><w part="F">μνεται</w> σφαίρας ἔσται
                            <supplied reason="lost">ἁ</supplied> ἐπιφάνεια.</seg>
                </seg>
                <seg n="81v1" type="folio">
                    <seg n="4" type="line" facs="z81v1_04">ἔστω γὰρ <seg type="word">ἐπιφ<supplied
                                reason="lost">ά</supplied>νειά</seg> τις ἁ <w part="I">τεμνομέ</w></seg>
                    <seg n="5" type="line" facs="z81v1_05"><w part="F">να</w> διὰ τοῦ Κ <seg
                            type="word"><unclear>σ</unclear>αμείου</seg>
                        <seg type="word">ἐπιπέ<supplied reason="lost">δ</supplied>ωι</seg> ἀεὶ </seg>
                    <seg n="6" type="line" facs="z81v1_06">τὰν <seg type="word"
                            >τομ<unclear>ὰ</unclear>ν</seg> ποιοῦσα κύκλου <w part="I">περιφέ</w></seg>
                    <seg n="7" type="line" facs="z81v1_07"><w part="F">ρειαν,</w> κέντρον δὲ αὐτᾶς
                        τὸ Κ. εἰ οὖν</seg>
                    <seg n="8" type="line" facs="z81v1_08">μὴ ἐστὶν αὐτὰ ἁ ἐπιφάνεια <choice>
                            <abbr>σφαιρ</abbr>
                            <expan>σφαίρας</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z81v1_09"><seg type="word">ἐπιφάν<supplied
                                reason="lost">ει</supplied>α,</seg> οὐκ ἐσσοῦνται πᾶσαι</seg>
                    <seg n="10" type="line" facs="z81v1_10">αἱ ἀπὸ τοῦ κέντρου ποτὶ τὰν <w part="I"
                            >ἐπι</w></seg>
                    <seg n="11" type="line" facs="z81v1_11"><w part="F">φάνειαν</w> ποτιπίπτουσαι
                        εὐθεῖαι </seg>
                    <seg n="12" type="line" facs="z81v1_12">ἴσαι. <seg type="word">ἔσ<supplied
                                reason="lost">τ</supplied>ω</seg> δὴ τὰ ΑΒ σαμεῖα ἐν τῆι</seg>
                    <seg n="13" type="line" facs="z81v1_13"><seg type="word">ἐπιφαν<supplied
                                reason="lost">εί</supplied>αι</seg> καὶ ἄνισοι αἱ ΑΚ ΚΒ, <w part="I"
                            >δι</w></seg>
                    <seg n="14" type="line" facs="z81v1_14"><w part="F">ὰ</w> δὲ τῶν ΚΑ ΚΒ ἐπίπεδον
                            <w part="I">ἐκβε</w></seg>
                    <seg n="15" type="line" facs="z81v1_15"><w part="F">βλήσθω</w> καὶ ποιείτω τὰν
                        τομὰν ἐν</seg>
                    <seg n="16" type="line" facs="z81v1_16">τᾶι ἐπιφανείαι τὰν <unclear>Δ</unclear>Α
                        ΒΓ <w part="I">γραμ</w></seg>
                    <seg n="17" type="line" facs="z81v1_17"><w part="F">μὴν.</w> κύκλου ἄρα ἐστὶν
                        αὐτὰ, <choice>
                            <abbr>κεντρ</abbr>
                            <expan>κέντρον</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z81v1_18">δὲ αὐτᾶς τὸ Κ ἐπεὶ <sic>ὑπόκειτο</sic> ἁ
                            <w part="I">ἐπι</w></seg>
                    <seg n="19" type="line" facs="z81v1_19"><w part="F">φάνεια</w> τοιαύτα. οὐκ ἔστι
                        δὲ, ἄνισοι</seg>
                    <seg n="20" type="line" facs="z81v1_20">γὰρ αἱ ΚΑ καὶ Β. ἀναγκαῖον οὖν</seg>
                    <seg n="21" type="line" facs="z81v1_21">
                        <supplied reason="lost">ταύταν</supplied>
                        <supplied reason="lost">ἐπιφάνειαν</supplied>
                        <seg type="word"><supplied reason="lost">σ</supplied>φ<supplied
                                reason="lost">αίρας</supplied></seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ι</unclear>
                        </seg>
                    </seg>
                </seg>
                <seg n="88r1" type="folio">
                    <figure n="1.1.1">
                        <figDesc xml:lang="eng">Figure 1.1.1</figDesc>
                    </figure>
                    <seg n="1" type="line" facs="z88r1_01">εἶμεν <seg type="suppliedword">
                            <supplied reason="lost">ἐπι</supplied>
                        </seg></seg>
                    <seg n="2" type="line" facs="z88r1_02">
                        <seg type="wordend">φάνεια</seg>
                    </seg>
                    <seg n="3" type="line" facs="z88r1_03">
                        <choice>
                            <abbr>μερ</abbr>
                            <expan>με<unclear>ρ</unclear>ος</expan>
                        </choice>. </seg>
                </seg>
                <seg n="88r1" type="folio">
                    <seg n="3" type="line" facs="z88r1_03">
                        <seg type="suppliedword"><supplied>π</supplied>α<supplied>ν</supplied></seg>
                    </seg>
                    <seg n="4" type="line" facs="z88r1_04"><seg type="wordend">τὸς</seg> ὑγροῦ</seg>
                    <seg n="5" type="line" facs="z88r1_05">
                        <w part="I">καθεστα</w>
                    </seg>
                    <seg n="6" type="line" facs="z88r1_06">
                        <w part="F">κότος</w>
                        <expan>οὕτως</expan>, </seg>
                    <seg n="7" type="line" facs="z88r1_07">ὥστε <w part="I">μέ</w></seg>
                    <seg n="8" type="line" facs="z88r1_08">
                        <w part="F">νειν</w>
                        <w part="I">ἀκί</w>
                    </seg>
                    <seg n="9" type="line" facs="z88r1_09">
                        <w part="F">νητον</w>, <choice>
                            <abbr>τα</abbr>
                            <expan>τὰν</expan>
                        </choice>
                    </seg>
                    <seg n="10" type="line" facs="z88r1_10">ἐπιφάνειαν σφαίρας ἕξει <w part="I"
                        >σχῆ</w></seg>
                    <seg n="11" type="line" facs="z88r1_11"><w part="F">μα</w> τὸ αὐτὸ κέντρον
                        ἐχούσας</seg>
                    <seg n="12" type="line" facs="z88r1_12">τᾶι γᾶι. νοείσθω γὰρ τὸ ὑγρὸν <w
                            part="I">κα</w></seg>
                    <seg n="13" type="line" facs="z88r1_13"><w part="F">θεστακότος</w>, ὥστε μένειν
                            <w part="I">ἀκίνη</w></seg>
                    <seg n="14" type="line" facs="z88r1_14"><w part="F">τον</w>, καὶ τετμάσθω αὐτοῦ
                        ἁ <w part="I">ἐπι</w></seg>
                    <seg n="15" type="line" facs="z88r1_15"><w part="F">φάνεια</w>
                        <seg type="word">ἐπ<supplied>ι</supplied>π<supplied>έ</supplied>δωι</seg>
                        <seg type="word">δι<supplied>ὰ</supplied></seg> τοῦ <w part="I">κέν</w></seg>
                    <seg n="16" type="line" facs="z88r1_16"><w part="F">τρου</w> τᾶς γᾶς, ἔστω δὲ
                        τᾶς γᾶς</seg>
                    <seg n="17" type="line" facs="z88r1_17">κέντρον τὸ Κ, τᾶς <unclear>δ’</unclear>
                        <choice>
                            <abbr>επιφανει</abbr>
                            <expan>ἐπιφανείας</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z88r1_18">τομὰ ἁ ΑΒ ΓΔ γραμμὰ. φαμὶ</seg>
                </seg>
                <seg n="81v2" type="folio">
                    <seg n="1" type="line" facs="z81v2_01">δὴ, τὰν ΑΒ ΓΔ γραμμὰν κύκλου</seg>
                    <seg n="2" type="line" facs="z81v2_02">περιφέρειαν <seg type="word"
                                ><supplied>εἶ</supplied>μ<supplied>εν,</supplied></seg> κέντρον</seg>
                    <seg n="3" type="line" facs="z81v2_03">δὲ αὐτᾶς τὸ Κ. εἰ γὰρ μή ἐστιν, <seg
                            type="suppliedword">οὐ</seg></seg>
                    <seg n="4" type="line" facs="z81v2_04"><seg type="wordend">
                            <supplied>κ</supplied>
                        </seg> ἐσσοῦνται ἴσαι <seg type="word">ἀ<supplied>πὸ</supplied></seg>
                        <seg type="word">το<supplied>ῦ</supplied></seg> Κ ποτὶ</seg>
                    <seg n="5" type="line" facs="z81v2_05"><seg type="word"
                        ><unclear>τ</unclear>ὰν</seg> ΑΒ ΓΔ γραμμὰν <w part="I">ποτιπί</w></seg>
                    <seg n="6" type="line" facs="z81v2_06"><w part="F">πτουσαι</w> εὐθεῖαι. λελάφθω
                        δή τις</seg>
                    <seg n="7" type="line" facs="z81v2_07">εὐθεῖα, ἅ ἐστι τινῶν μὲν <seg
                            type="suppliedword">ποτιπι</seg></seg>
                    <seg n="8" type="line" facs="z81v2_08"><seg type="wordend"><supplied
                                reason="lost">πτ</supplied>ουσ<unclear>ᾶ</unclear>ν</seg> ἀπὸ τοῦ Κ
                        ἐπὶ τὰν ΑΒ ΓΔ</seg>
                    <seg n="9" type="line" facs="z81v2_09">γραμμὰν μείζων, τινῶν δ’ <w part="I"
                        >ἐλάσ</w></seg>
                    <seg n="10" type="line" facs="z81v2_10"><w part="F">σων</w>, καὶ κέντρωι μὲν τῶι
                        Κ, <w part="I">δια</w></seg>
                    <seg n="11" type="line" facs="z81v2_11"><w part="F">στάματι</w> δὲ τᾶι ληφθείσαι
                            <w part="I">γραμ</w></seg>
                    <seg n="12" type="line" facs="z81v2_12"><w part="F">μᾶι</w> κύκλος γεγράφθω· <choice>
                            <abbr>πεσειτ</abbr>
                            <expan>πεσεῖται</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z81v2_13">οὖν ἁ περιφέρεια τοῦ κύκλου</seg>
                    <seg n="14" type="line" facs="z81v2_14">τὰ μὲν ἐντὸς ἔχουσαι τὰς ΑΒ</seg>
                    <seg n="15" type="line" facs="z81v2_15">ΓΔ γραμμάς, τὰ δ’ ἐκτός, <choice>
                            <abbr>επει</abbr>
                            <expan>ἐπειδὴ</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z81v2_16">ἁ ἐκ τοῦ κέντρου τινῶν μέν ἐστι</seg>
                    <seg n="17" type="line" facs="z81v2_17">μεῖζον τᾶν ἀπὸ τοῦ Κ <seg
                            type="unclearword">ποτιπι</seg></seg>
                    <seg n="18" type="line" facs="z81v2_18"><seg type="wordend"
                            >πτουσ<unclear>ῶ</unclear>ν</seg>
                        <seg type="word">ποτ<supplied reason="lost">ὶ</supplied></seg> τὰν ΑΒΓΔ <w
                            part="I">γραμ</w></seg>
                    <seg n="19" type="line" facs="z81v2_19"><w part="F">μάν,</w> τινῶν δὲ ἐλάσσων.
                        ἔστω</seg>
                    <seg n="20" type="line" facs="z81v2_20">οὖν τοῦ καταγραφέντος κύκλου</seg>
                    <seg n="21" type="line">
                        <supplied reason="lost">περιφέρεια</supplied>
                        <supplied reason="lost">ἁ</supplied>
                        <supplied reason="lost">
                            <gap extent="3"/>
                        </supplied>
                        <supplied reason="lost">καὶ</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἀ</supplied>
                        </seg>
                    </seg>
                </seg>
                <seg n="88r2" type="folio">
                    <seg n="1" type="line" facs="z88r2_01"><seg type="wordend">πὸ</seg> τοῦ Β ἐπὶ τὸ
                        Κ ἐυθεῖα ἄχθω,</seg>
                    <seg n="2" type="line" facs="z88r2_02"><expan>καὶ</expan> ἐπεζεύχθωσαν αἱ ΒΚ καὶ</seg>
                    <seg n="3" type="line" facs="z88r2_03">ΚΑ ΚΛ ἴσας ποιοῦσαι γωνίας,</seg>
                    <seg n="4" type="line" facs="z88r2_04">γεγράφθω δὲ καὶ κέντρωι τῶι</seg>
                    <seg n="5" type="line" facs="z88r2_05">Κ περιφέρειά τις ἁ ΞΟΠ ἐν τῶι</seg>
                    <seg n="6" type="line" facs="z88r2_06">ἐπιπέδωι καὶ ἐν τῶι ὑγρῶι· τὰ </seg>
                    <seg n="7" type="line" facs="z88r2_07">δὴ μέρη τοῦ ὑγροῦ τὰ κατὰ τᾶς</seg>
                    <seg n="8" type="line" facs="z88r2_08">ΞΟΠ περιφερείας ἐξ ἴσου τε <w part="I"
                            >κεί</w></seg>
                    <seg n="9" type="line" facs="z88r2_09"><w part="F">μενα</w> καὶ συνεχόμενα
                        ἀλλήλοις.</seg>
                    <seg n="10" type="line" facs="z88r2_10">θλίβονται τὸ μὲν κατὰ τὴν ΞΟ</seg>
                    <seg n="11" type="line" facs="z88r2_11">περιφέρειαν τῶι ὑγρῶι τῶι <w part="I"
                        >κα</w></seg>
                    <seg n="12" type="line" facs="z88r2_12"><w part="F">τὰ</w> τὸν ΞΒΑ τόπον, τὰ δὲ
                        κατὰ</seg>
                    <seg n="13" type="line" facs="z88r2_13">τὰν ΠΟ περιφέρειαν τῶι ὑγρῶι</seg>
                    <seg n="14" type="line" facs="z88r2_14">τῶι κατὰ τὸν ΠΟ ΒΛ τόπον· <choice>
                            <abbr>ισσ</abbr>
                            <expan>ἵ<unclear>σ</unclear>σον</expan>
                        </choice>
                    </seg>
                    <seg n="15" type="line" facs="z88r2_15">οὖν θλίβονται τὰ μέρη τοῦ <choice>
                            <abbr>υγρ</abbr>
                            <expan>ὑγροῦ</expan>
                        </choice>
                    </seg>
                    <seg n="16" type="line" facs="z88r2_16">τὰ κατὰ τὰν ΞΟ περιφέρειαν</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
