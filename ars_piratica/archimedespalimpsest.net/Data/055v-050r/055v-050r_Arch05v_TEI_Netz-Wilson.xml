    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="55v1" type="folio">
                    <seg n="1" type="line" facs="z55v1_01">δὲ τοῦ μὲν <seg type="word">μεγέ<supplied
                                reason="lost">θ</supplied>εος</seg> τοῦ ἐν ὧι Α </seg>
                    <seg n="2" type="line" facs="z55v1_02">βάρος τὸ Β, τοῦ <seg type="word"
                                >δ<supplied reason="lost">ὲ</supplied></seg> ὑγροῦ τοῦ ἴσον <w
                            part="I">ὄγ</w></seg>
                    <seg n="3" type="line" facs="z55v1_03"><w part="F">κον</w> ἔχοντος τῶι Α τὸ ΒΓ.
                        δεικτέον <expan>ὅτι</expan></seg>
                    <seg n="4" type="line" facs="z55v1_04">τὸ Α μέγεθος βιασθὲν ἐς τὸ ὑγρὸν <w
                            part="I">ἀ</w></seg>
                    <seg n="5" type="line" facs="z55v1_05"><w part="F">νοισεῖται.</w> ἔστω ἄνω
                        τοσαύτα <seg type="word">β<unclear>ί</unclear>α,</seg></seg>
                    <seg n="6" type="line" facs="z55v1_06">ὅσον ἐστὶ τὸ βάρος τὸ Γ.</seg>
                </seg>
                <seg n="55v1" type="folio">
                    <seg n="6" type="line" facs="z55v1_06">λελάφθω γάρ</seg>
                    <seg n="7" type="line" facs="z55v1_07">τι μέγεθος τὸ ἄνω τὸ Δ βάρος <choice>
                            <abbr>ισο</abbr>
                            <expan>ἴσον</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z55v1_08">ἔχον τῶι Γ· τὸ δὴ μέγεθος τὸ ἐξ <w
                            part="I">ἀμ</w></seg>
                    <seg n="9" type="line" facs="z55v1_09"><w part="F">φοτέρων</w> τῶν ἐν οἷς ΑΔ <choice>
                            <abbr>μεγεθω</abbr>
                            <expan>μεγεθῶν</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z55v1_10">ἔστω <sic>
                            <seg type="word">α<supplied reason="lost">ὐ</supplied>τὸς</seg>
                        </sic> συντεθὲν <choice>
                            <abbr>κουφοτερο</abbr>
                            <expan>κουφότερόν</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z55v1_11">ἐστι τοῦ ὑγροῦ· ἔστι γὰρ τοῦ μὲν <w
                            part="I">με</w></seg>
                    <seg n="12" type="line" facs="z55v1_12"><w part="F">γέθεος</w> τοῦ ἐξ ἀμφοτέρων
                        βάρος</seg>
                    <seg n="13" type="line" facs="z55v1_13">τὸ ΒΓ, τοῦ δὲ ὑγροῦ τοῦ ἴσον <choice>
                            <abbr>ογκο</abbr>
                            <expan>ὄγκον</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z55v1_14">ἔχοντος αὐτῶι μεῖζον τοῦ ΒΓ <w part="I"
                            >δι</w></seg>
                    <seg n="15" type="line" facs="z55v1_15"><w part="F">ὰ</w> τὸ τοῦ ἴσον ἔχοντος
                        ἀυτῶι τὸ</seg>
                    <seg n="16" type="line" facs="z55v1_16">Α τὸ βάρος εἶμεν τὸ ΒΓ. <w part="I"
                        >ἀφε</w></seg>
                    <seg n="17" type="line" facs="z55v1_17"><w part="F">θὲν</w> οὖν ἔστω τὸ ὑγρὸν τὸ <choice>
                            <abbr>μεγεθ</abbr>
                            <expan>μέγεθος</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z55v1_18">τὸ ἐξ ἀμφοτέρων τῶν ΑΔ <w part="I"
                        >συγ</w></seg>
                    <seg n="19" type="line" facs="z55v1_19"><w part="F">κεμένων</w> ἐς τοσοῦτον
                        δυσεῖται,</seg>
                </seg>
                <seg n="50r1" type="folio">
                    <seg n="1" type="line" facs="z50r1_01">ἔστ’ ἄν <expan>καὶ</expan> ὁ <seg
                            type="word">ταλικοῦτο<supplied reason="lost">ς</supplied></seg> ὄγκος
                        τοῦ</seg>
                    <seg n="2" type="line" facs="z50r1_02">ὑγροῦ, <sic>
                            <seg type="word">ἄδ<supplied reason="lost">ι</supplied>κον</seg>
                        </sic> καὶ τὸ δεδυκὸς <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z50r1_03">μεγέθεος, ἴσον βάρος ἔχει τῶι</seg>
                    <seg n="4" type="line" facs="z50r1_04"><seg type="word"
                        >ὅλ<unclear>ω</unclear>ι</seg> μεγέθει· δέδεικται γὰρ <w part="I">τοῦ</w></seg>
                    <seg n="5" type="line" facs="z50r1_05"><w part="F">το.</w> ἔστω δὲ ἐπιφάνειά
                        τινος <w part="I">ὑ</w></seg>
                    <seg n="6" type="line" facs="z50r1_06"><w part="F">γροῦ</w> ἁ ΑΒΓΔ περιφερείας.
                        ἐπεὶ </seg>
                    <seg n="7" type="line" facs="z50r1_07">οὖν ὁ ταλικοῦτος ὄγκος τοῦ <w part="I"
                        >ὑ</w></seg>
                    <seg n="8" type="line" facs="z50r1_08"><w part="F">γροῦ,</w> ἠλίκον <choice>
                            <abbr>εστι</abbr>
                            <expan>ἐστὶν</expan>
                        </choice> τὸ Α <seg type="word">μέγε<unclear>θο</unclear>ς,</seg></seg>
                    <seg n="9" type="line" facs="z50r1_09">ἴσον βάρος ἔχει τοῖς ΑΔ <w part="I"
                            >μεγέθε</w></seg>
                    <seg n="10" type="line" facs="z50r1_10"><w part="F">σιν,</w> δῆλον ὡς τὸ δεδυκὸς
                        αὐτοῦ </seg>
                    <seg n="11" type="line" facs="z50r1_11">ἐσσεῖται τὸ Α μέγεθος, τὸ δὲ <choice>
                            <abbr>λοιπ</abbr>
                            <expan>λοιπὸν</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z50r1_12"><seg type="word"><supplied reason="lost"
                                >ὑ</supplied>περάνω,</seg>
                        <seg type="word">ἐσσεῖ<unclear>τ</unclear>αι</seg> ὅλον τᾶς</seg>
                    <seg n="13" type="line" facs="z50r1_13">τοῦ ὑγροῦ ἐπιφανείας· εἰ γὰρ <w part="I"
                            >αὐ</w></seg>
                    <seg n="14" type="line" facs="z50r1_14"><w part="F">τᾶς</w> δεδυκὸς
                        <sic>εἶ</sic> τέλειον, ἐσσεῖται </seg>
                    <seg n="15" type="line" facs="z50r1_15"><seg type="word">δεδυκ<supplied
                                reason="lost">ὸ</supplied>ς.</seg> τούτου δεδειγμένου <seg
                            type="unclearword">δῆ</seg></seg>
                    <seg n="16" type="line" facs="z50r1_16"><seg type="wordend">
                            <unclear>λον</unclear>
                        </seg>
                        <seg type="word">οὖ<unclear>ν</unclear></seg>
                        <expan>
                            <unclear>ὅτι</unclear>
                        </expan> ὅσα βίαι <seg type="word">ἀναφ<supplied reason="lost"
                            >έρ</supplied>εται</seg></seg>
                    <seg n="17" type="line" facs="z50r1_17"><seg type="word"
                        >τ<unclear>ὸ</unclear></seg> Α μέγεθος <seg type="word">ἐ<supplied
                                reason="lost">ς</supplied></seg>
                        <seg type="word">τ<supplied reason="lost">ὼ</supplied></seg>
                        <supplied>ἄνω</supplied>
                        <seg type="suppliedword">το<supplied reason="lost"
                            >σ</supplied><unclear>αῦ</unclear></seg></seg>
                    <seg n="18" type="line" facs="z50r1_18"><supplied reason="lost">τα</supplied>
                        <seg type="word"><supplied>θ</supplied><unclear>λ</unclear><supplied
                                reason="lost">ίβ</supplied>ε<unclear>τ</unclear>αι</seg> ὑπὸ τοῦ ἄνω
                            <seg type="word"><supplied reason="lost">τ</supplied>οῦ</seg> Δ·</seg>
                </seg>
                <seg n="55v2" type="folio">
                    <seg n="1" type="line" facs="z55v2_01">ἐς τὼ κάτω, ἐπεὶ οὐδέτερον ὑπ’ <seg
                            type="suppliedword">οὐ</seg></seg>
                    <seg n="2" type="line" facs="z55v2_02"><seg type="wordend">δε<supplied
                                reason="lost">τ</supplied>έρου</seg>
                        <seg type="word"><unclear>ἐ</unclear>ξωθεῖτο.</seg> ἀλλὰ τὸ Δ ἐς τὸ </seg>
                    <seg n="3" type="line" facs="z55v2_03">κάτω θλίβει τοσούτω βάρει, <choice>
                            <abbr>αλικ</abbr>
                            <expan>ἁλίκον</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z55v2_04">ἐστὶ τὸ Γ· ὑπέκειτο γὰρ τὸ βάρος</seg>
                    <seg n="5" type="line" facs="z55v2_05">τὸ ἐν ὧι τὸ Δ εἶμεν ἴσον τὧι Γ· <w
                            part="I">δῆ</w></seg>
                    <seg n="6" type="line" facs="z55v2_06"><w part="F">λον</w> οὖν ὃ ἔδει δεῖξαι. <choice>
                            <abbr>ΕΞ</abbr>
                            <expan>ΕΞΗΣ</expan>
                        </choice>
                    </seg>
                    <seg n="7" type="line" facs="z55v2_07">Η ΚΑΤΑΓΡΑΦΗ ΤΟΥ ΣΧΑΜΑΤΟΣ</seg>
                    <figure n="1.6.1">
                        <figDesc xml:lang="eng">Figure 1.6.1</figDesc>
                    </figure>
                </seg>
                <seg n="55v2" type="folio">
                    <seg n="8" type="line" facs="z55v2_08">τὰ βαρύτερα τοῦ ὑγροῦ ἀφεθέντα</seg>
                    <seg n="9" type="line" facs="z55v2_09">εἰς τὸ ὑγρὸν οἰσεῖται κάτω, ἔστ᾽ ἂν</seg>
                    <seg n="10" type="line" facs="z55v2_10">καταβᾶντι, καὶ ἐσσοῦνται <w part="I"
                            >κουφότε</w></seg>
                    <seg n="11" type="line" facs="z55v2_11"><w part="F">ρα</w> ἐν τῶι ὑγρῶι
                        τοσοῦτον, ὅσον</seg>
                    <seg n="12" type="line" facs="z55v2_12">ἔχει τὸ βάρος τοῦ ὑγροῦ τοῦ <seg
                            n="ταλικοῦτον" type="clearword">
                            <choice>
                                <abbr>ταλικ</abbr>
                                <expan>ταλικοῦ</expan>
                            </choice>
                        </seg></seg>
                </seg>
                <seg n="50r2" type="folio">
                    <seg n="1" type="line" facs="z50r2_01"><seg type="wordend">τον</seg> ὄγκον
                        ἔχοντος, <seg type="word"
                                ><unclear>ἁ</unclear>λίκ<unclear>ο</unclear><supplied reason="lost"
                                >ς</supplied></seg>
                        <seg type="word">ἐστὶ<supplied reason="lost">ν</supplied></seg></seg>
                    <seg n="2" type="line" facs="z50r2_02">ὁ τοῦ <seg type="word"><supplied
                                reason="lost">σ</supplied>τερεοῦ</seg> μεγέθεος ὄγκος.</seg>
                </seg>
                <seg n="50r2" type="folio">
                    <seg n="2" type="line" facs="z50r2_02">
                        <expan>ὅτι</expan>
                    </seg>
                    <seg n="3" type="line" facs="z50r2_03">μὲν οὖν <seg type="word">
                            <supplied reason="lost">ο</supplied>
                            <unclear>ἰ</unclear>
                            <supplied reason="lost">σ</supplied>εῖται</seg> ἐς τὸ κάτω, ἔστ᾽ <seg
                            type="word">ἂ<unclear>ν</unclear></seg></seg>
                    <seg n="4" type="line" facs="z50r2_04">καταβᾶντα, <seg type="word">δ<supplied
                                reason="lost">ῆ</supplied>λον·</seg> τὰ γὰρ <w part="I">ὑπο</w></seg>
                    <seg n="5" type="line" facs="z50r2_05"><w part="F">κάτω</w> αὐτοῦ μέρη τοῦ ὑγροῦ
                            <w part="I">θλι</w></seg>
                    <seg n="6" type="line" facs="z50r2_06"><w part="F">ψοῦνται</w> μᾶλλον τῶν ἐξ
                            <seg type="word"><supplied reason="lost">ἴ</supplied>σου</seg> αὐτοῖς</seg>
                    <seg n="7" type="line" facs="z50r2_07">κειμένων μέρων, ἐπειδὴ <w part="I"
                        >βαρύ</w></seg>
                    <seg n="8" type="line" facs="z50r2_08"><w part="F">τερον</w> ὑπόκειται τὸ
                        στερεὸν <w part="I">μέ</w></seg>
                    <seg n="9" type="line" facs="z50r2_09"><w part="F">γεθος</w> τοῦ <seg
                            type="word">ὑγρο<unclear>ῦ</unclear>·</seg> ὅτι δὲ <choice>
                            <abbr>κφοτερα</abbr>
                            <expan>κουφότερα</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z50r2_10">ἐσσοῦνται, ὡς εἴρηται, <choice>
                            <abbr>δειχθησετ</abbr>
                            <expan>δειχθήσεται</expan>
                        </choice></seg>
                </seg>
                <seg n="50r2" type="folio">
                    <seg n="11" type="line" facs="z50r2_11">Ἔστω τι μέγεθος τὸ Α, ὅ
                        <expan>ἐστι</expan> βαρύτερον <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z50r2_12">ὑγροῦ, βάρος δὲ ἔστω τοῦ μὲν ἐν ὧι</seg>
                    <seg n="13" type="line" facs="z50r2_13">Α μεγέθεος τὸ ΒΓ, τοῦ δὲ ὑγροῦ τοῦ</seg>
                    <seg n="14" type="line" facs="z50r2_14">ἴσον ὄγκον ἔχοντος τῶι Α τὸ Β. <w
                            part="I">δει</w></seg>
                    <seg n="15" type="line" facs="z50r2_15"><w part="F">κτέον,</w>
                        <expan>ὅτι</expan> τὸ Α μέγεθος ἐν τῶι ὑγρῶι</seg>
                    <seg n="16" type="line" facs="z50r2_16">ἐὸν βάρος ἕξει ἴσον τῶι Γ.</seg>
                </seg>
                <seg n="50r2" type="folio">
                    <seg n="16" type="line" facs="z50r2_16">
                        <seg type="suppliedword">λελ<supplied reason="lost">ά</supplied>
                        </seg>
                    </seg>
                    <seg n="17" type="line" facs="z50r2_17"><seg type="wordend">φθω</seg> γάρ τι
                        μέγεθος τὸ ἐν ὧι <seg type="word"><unclear>τ</unclear>ὸ</seg></seg>
                </seg>
            </ab>
        </body>
    </text>
