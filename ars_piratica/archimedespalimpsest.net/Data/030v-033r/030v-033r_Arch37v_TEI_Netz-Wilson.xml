    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="30v1" type="folio">
                    <seg n="1" type="line" facs="z30v1_01">ἐπ’ αὐτὰν ἀγμέναν· δυνατὸν οὖν
                            <expan>ἐστιν</expan></seg>
                    <seg n="2" type="line" facs="z30v1_02">ἀπὸ τοῦ Α ποτιβαλεῖν τὰν ΑΣ</seg>
                    <seg n="3" type="line" facs="z30v1_03">ποτὶ τὰν ΤΜΝ ἐκβεβλημέναν, <seg
                            type="clearword">ὥσ</seg></seg>
                    <seg n="4" type="line" facs="z30v1_04"><seg type="wordend">τε</seg> τὰν μεταξὺ
                        τᾶς <seg type="word">περιφερεί<supplied reason="lost">ας</supplied>
                        </seg></seg>
                    <seg n="5" type="line" facs="z30v1_05">καὶ τᾶς ἐκβεβλημένας τὰν ΡΣ</seg>
                    <seg n="6" type="line" facs="z30v1_06">ποτὶ τὰν ΤΡ τὸν αὐτὸν ἔχειν <choice>
                            <abbr>λόγο</abbr>
                            <expan>λόγον</expan>
                        </choice>,</seg>
                    <seg n="7" type="line" facs="z30v1_07">ὃν ἁ ΤΑ ποτὶ τὰν ΑΛ· τεμεῖ δὴ ἁ ΑΣ</seg>
                    <seg n="8" type="line" facs="z30v1_08">τὸν <seg type="word">
                            <choice>
                                <abbr>μὲ</abbr>
                                <expan>μὲ<supplied reason="lost">ν</supplied></expan>
                            </choice>
                        </seg> κύκλον κατὰ τὸ Ρ, τὰν δὲ <seg type="clearword">ἕ</seg></seg>
                    <seg n="9" type="line" facs="z30v1_09"><seg type="wordend">λικα</seg> κατὰ τὸ Χ·
                        καὶ ἐναλλὰξ <choice>
                            <abbr>τὸ</abbr>
                            <expan>τὸν</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z30v1_10">αὐτὸν ἕξει λόγον ἁ ΡΣ ποτὶ τὰν</seg>
                    <seg n="11" type="line" facs="z30v1_11">ΤΑ, ὃν ἁ ΤΡ ποτὶ τὰν ΑΛ. <seg
                            type="clearword">ἐλάσσο</seg></seg>
                    <seg n="12" type="line" facs="z30v1_12"><seg type="wordend">να</seg> λόγον ἔχει
                        ἁ ΤΡ περιφέρεια <seg type="clearword">πο</seg></seg>
                    <seg n="13" type="line" facs="z30v1_13"><seg type="wordend">τὶ</seg> τὰν
                        διπλασίαν τοῦ ΜΝ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλου</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z30v1_14">περιφέρειαν· ἔστι γὰρ ἁ μὲν ΤΡ</seg>
                    <seg n="15" type="line" facs="z30v1_15">εὐθεῖα ἐλάσσων τᾶς ΤΡ <seg
                            type="clearword">περιφε</seg></seg>
                    <seg n="16" type="line" facs="z30v1_16"><seg type="wordend">ρείας</seg>, ἁ δὲ ΑΛ
                        εὐθεῖα μείζων ἢ</seg>
                    <seg n="17" type="line" facs="z30v1_17">διπλασία τᾶς τοῦ ΤΜΝ <seg
                            type="clearword">περι</seg></seg>
                    <seg n="18" type="line" facs="z30v1_18"><seg type="wordend">φερείας·</seg>
                        ἐλάσσονα ἄρα λόγον</seg>
                    <seg n="19" type="line" facs="z30v1_19">ἔχει ἁ ΡΣ ποτὶ τὰν Α<unclear>Ρ</unclear>
                        ἢ ἁ ΤΡ <seg type="clearword">πε</seg>
                    </seg>
                </seg>
                <seg n="33r1" type="folio">
                    <seg n="1" type="line" facs="z33r1_01">
                        <seg type="wordend">ριφέρεια</seg> ποτὶ τὰν διπλασίαν</seg>
                    <seg n="2" type="line" facs="z33r1_02">τᾶς τοῦ ΤΜΝ κύκλου <seg type="word">
                            <choice>
                                <abbr>περιφερεί</abbr>
                                <expan>π<unclear>ε</unclear>ριφερ<unclear>εί</unclear>ας</expan>
                            </choice>
                        </seg>·</seg>
                    <seg n="3" type="line" facs="z33r1_03">ὅλα οὖν ἁ ΣΑ ποτὶ τὰν <supplied
                            reason="lost">ΑΡ</supplied>
                        <seg type="suppliedword">ἐ<supplied reason="lost">λ</supplied>άσσ</seg></seg>
                    <seg n="4" type="line" facs="z33r1_04"><seg type="wordend">ονα</seg> λόγον ἔχει
                        ἢ ἁ ΤΡ <seg type="clearword">περιφέ</seg></seg>
                    <seg n="5" type="line" facs="z33r1_05"><seg type="wordend">ρεια</seg> μετὰ τᾶς
                        τοῦ ΤΜΝ κύκλου</seg>
                    <seg n="6" type="line" facs="z33r1_06">περιφερείας δὶς εἰρημένας</seg>
                    <seg n="7" type="line" facs="z33r1_07">ποτὶ τὰν τοῦ ΤΜΝ κύκλου <seg
                            type="clearword">πε</seg></seg>
                    <seg n="8" type="line" facs="z33r1_08"><seg type="wordend">ριφέρειαν</seg> δὶς
                        εἰρημέναν. ὃν δὲ</seg>
                    <seg n="9" type="line" facs="z33r1_09">λόγον ἔχουσιν αἱ εἰρημέναι <seg
                            type="clearword">
                            <expan>περι</expan>
                        </seg></seg>
                    <seg n="10" type="line" facs="z33r1_10"><seg type="wordend">φέρειαι</seg>,
                        τοῦτον ἔχει τὸν λόγον ἁ</seg>
                    <seg n="11" type="line" facs="z33r1_11">ΧΑ ποτὶ τὰν ΑΤ· δέδεικται γὰρ</seg>
                    <seg n="12" type="line" facs="z33r1_12">τοῦτο· ἐλάσσονα ἄρα λόγον ἔχει</seg>
                    <seg n="13" type="line" facs="z33r1_13">ἁ ΑΣ ποτὶ τὰν ΑΡ ἢ ἁ ΧΑ ποτὶ</seg>
                    <seg n="14" type="line" facs="z33r1_14">τὰν ΤΑ· <choice>
                            <abbr>ὅ</abbr>
                            <expan>ὅπερ</expan>
                        </choice> ἀδύνατον. οὐκ ἄρα</seg>
                    <seg n="15" type="line" facs="z33r1_15">μείζων ἐστὶν ἢ διπλασία ἁ ΖΑ</seg>
                </seg>
                <seg n="30v2" type="folio">
                    <seg n="1" type="line" facs="z30v2_01">εὐθεῖα τᾶς τοῦ ΤΜΝ κύκλου περι</seg>
                    <seg n="2" type="line" facs="z30v2_02">φερείας. ὁμοίως δὲ δειχθήσεται,</seg>
                    <seg n="3" type="line" facs="z30v2_03">οὐδὲ ἐλάσσων ἢ διπλασία. <seg
                            type="clearword">δῆ</seg></seg>
                    <seg n="4" type="line" facs="z30v2_04"><seg type="wordend">λον</seg> οὖν,
                            <expan>ὅτι</expan> διπλασία ἐστίν.</seg>
                </seg>
                <seg n="30v2" type="folio">
                    <seg n="4" type="line" facs="z30v2_04">διὰ δὲ</seg>
                    <seg n="5" type="line" facs="z30v2_05">τοῦ αὐτοῦ τρόπου δεικτέον, καὶ εἴ</seg>
                    <seg n="6" type="line" facs="z30v2_06">κα τᾶς ἐν <seg type="word">
                            <supplied reason="lost">ὁ</supplied>ποιαοῦν</seg> περιφορᾶι</seg>
                    <seg n="7" type="line" facs="z30v2_07">γεγραμμένας ἕλικος ἐπιψαύηι</seg>
                    <seg n="8" type="line" facs="z30v2_08">τις εὐθεῖα κατὰ τὸ πέρας τᾶς</seg>
                    <seg n="9" type="line" facs="z30v2_09">ἕλικος, καὶ ἀπὸ τᾶς ἀρχᾶς τᾶς</seg>
                    <seg n="10" type="line" facs="z30v2_10">ἕλικος ποτ’ ὀρθὰς ἀχθεῖσα τᾶι</seg>
                    <seg n="11" type="line" facs="z30v2_11">ἀρχᾶι τᾶς περιφορᾶς συμπίπτει</seg>
                    <seg n="12" type="line" facs="z30v2_12">ποτὶ τὰν ἐπιψαύουσαν, <seg
                            type="clearword">πολλα</seg></seg>
                    <seg n="13" type="line" facs="z30v2_13"><seg type="wordend">πλασίαν</seg> ἐστὶν
                        τᾶς τοῦ κύκλου <seg type="clearword">πε</seg></seg>
                    <seg n="14" type="line" facs="z30v2_14"><seg type="wordend">ριφερείας</seg> τοῦ
                        κατὰ τὸν <choice>
                            <abbr>ἀριθμ</abbr>
                            <expan>ἀριθμὸν</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z30v2_15">τᾶς περιφορᾶς λεγομένου τῶι</seg>
                    <seg n="16" type="line" facs="z30v2_16">αὐτῶι ἀριθμῶι.</seg>
                    <figure n="20.1">
                        <figDesc>Figure 20.1</figDesc>
                    </figure>
                </seg>
                <seg n="33r2" type="folio">
                    <seg n="1" type="line" facs="z33r2_01">εἴ κα τᾶς ἕλικος τᾶς ἐν τᾶι <seg
                            type="clearword">πρώ</seg></seg>
                    <seg n="2" type="line" facs="z33r2_02"><seg type="wordend">ται</seg> περιφορᾶι
                        γεγραμμένας</seg>
                    <seg n="3" type="line" facs="z33r2_03">εὐθεῖα γραμμὰ ἐπιψαύη μὴ
                        <expan>κατὰ</expan></seg>
                    <seg n="4" type="line" facs="z33r2_04">τὸ πέρας τᾶς ἕλικος, ἀπὸ δὲ</seg>
                    <seg n="5" type="line" facs="z33r2_05">τᾶς ἁφᾶς ἐπὶ τὰν ἀρχὰν τᾶς</seg>
                    <seg n="6" type="line" facs="z33r2_06">ἕλικος εὐθεῖα ἐπιζευχθῆι, καὶ <seg
                            type="clearword">
                            <choice>
                                <abbr>κέ</abbr>
                                <expan>κέν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="7" type="line" facs="z33r2_07"><seg type="wordend">τρωι</seg> μὲν τᾶι
                        ἀρχᾶι τᾶς ἕλικος,</seg>
                    <seg n="8" type="line" facs="z33r2_08">διαστάματι δὲ τᾶι ἐπιζευχθείσαι</seg>
                    <seg n="9" type="line" facs="z33r2_09">κύκλος γραφῆι, ἀπὸ δὲ τᾶς ἀρχᾶς</seg>
                    <seg n="10" type="line" facs="z33r2_10">τᾶς ἕλικος ἀχθῆ τις ποτ’ ὀρθὰς</seg>
                    <seg n="11" type="line" facs="z33r2_11">τᾶ ἀπὸ τᾶς ἁφᾶς ἐπὶ τὰν <choice>
                            <abbr>ἀρχ</abbr>
                            <expan>ἀρχὰν</expan>
                        </choice></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
