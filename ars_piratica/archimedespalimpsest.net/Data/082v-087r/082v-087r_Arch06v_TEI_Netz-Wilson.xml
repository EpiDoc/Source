    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="82v1" type="folio">
                    <seg n="1" type="line" facs="z82v1_01">κάθετον <sic>εἰ μέν</sic>. </seg>
                </seg>
                <seg n="82v1" type="folio">
                    <seg n="1" type="line" facs="z82v1_01">ἐπεὶ <expan>γὰρ</expan> ὑπόκειται <w
                            part="I">κε</w></seg>
                    <seg n="2" type="line" facs="z82v1_02"><w part="F">κλίσθαι</w> τὸ σχᾶμα, οὐκ
                        ἔστι τὰ ΖΕ <w part="I">κα</w></seg>
                    <seg n="3" type="line" facs="z82v1_03"><w part="F">τὰ</w> κάθετον ἄχθω δὴ διὰ
                        τοῦ Κ καὶ</seg>
                    <seg n="4" type="line" facs="z82v1_04">τοῦ ΛΑ ΚΑ, τὸ δὲ Λ κέντρον <w part="I"
                            >ὑποκείσ</w></seg>
                    <seg n="5" type="line" facs="z82v1_05"><w part="F">θω</w> τᾶς γᾶς· τὸ δὴ σχᾶμα
                        τὸ ἐν <choice>
                            <abbr>τ</abbr>
                            <expan>τῶ</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z82v1_06">ὑγρῶι ἀπολελημμένον ὑπὸ τᾶς </seg>
                    <seg n="7" type="line" facs="z82v1_07">τοῦ ὑγροῦ ἐπιφανείας τὸν ἄξονα</seg>
                    <seg n="8" type="line" facs="z82v1_08">ἔχει ἐπὶ τῆς ΚΛ· εἰ γάρ κα <seg
                            type="word">δύ<supplied reason="lost">ο</supplied></seg>
                        <w part="I">σφαι</w></seg>
                    <seg n="9" type="line" facs="z82v1_09"><w part="F">ρῶν</w> ἐπιφάνειαι τέμνοντι
                        ἀλλήλας,</seg>
                    <seg n="10" type="line" facs="z82v1_10">τομὰ κύκλος ἐστὶν <seg type="word"
                                >ὀρθ<unclear>ὸ</unclear>ν</seg> ποτὶ τὰν</seg>
                    <seg n="11" type="line" facs="z82v1_11">εὐθεῖαν τὰν ἐπιζευγνύουσαν τὰ </seg>
                    <seg n="12" type="line" facs="z82v1_12">κέντρα τῆς σφαίρας. ἔστιν οὖν </seg>
                    <seg n="13" type="line" facs="z82v1_13">τοῦ σχάματος τοῦ κατὰ τὰν ΒΗΓ</seg>
                    <seg n="14" type="line" facs="z82v1_14">
                        <choice>
                            <abbr>περιφερεια</abbr>
                            <expan>περιφέρειαν</expan>
                        </choice>
                        <choice>
                            <abbr>ἀπολαμβανομέν</abbr>
                            <expan>ἀπολαμβανομένου</expan>
                        </choice>
                    </seg>
                    <seg n="15" type="line" facs="z82v1_15">ἐν τῶι ὑγρῶι τὸ κέντρον τοῦ <w part="I"
                            >βάρε</w></seg>
                    <seg n="16" type="line" facs="z82v1_16"><w part="F">ος</w> ἐπὶ τᾶς ΚΛ· ἔστω τὸ
                        Ρ. τοῦ δὲ <w part="I">τμά</w></seg>
                    <seg n="17" type="line" facs="z82v1_17"><w part="F">ματος</w> ὅλου τοῦ κατὰ τὰν
                        ΘΗΖ <expan>
                            <w part="I">περι</w>
                        </expan></seg>
                    <seg n="18" type="line" facs="z82v1_18"><w part="F">φέρειαν</w> τὸ κέντρον ἐστὶ
                        τοῦ <w part="I">βάρε</w></seg>
                    <seg n="19" type="line" facs="z82v1_19"><w part="F">ος</w> ἐπὶ τᾶς ΖΘ· ἔστω τὸ
                        Ξ. τοῦ ἄρα</seg>
                    <seg n="20" type="line" facs="z82v1_20"><seg type="word"><supplied reason="lost"
                                >λ</supplied>οιποῦ</seg> σχάματος ὅ <seg type="word"><supplied
                                reason="lost">ἐστ</supplied>ιν</seg> ἐκτὸς</seg>
                </seg>
                <seg n="87r1" type="folio">
                    <seg n="1" type="line" facs="z87r1_01">τᾶς τοῦ ὑγροῦ ἐπιφανείας τὸ <seg
                            n="κέντρον" type="suppliedword">κέν</seg></seg>
                    <seg n="2" type="line" facs="z87r1_02"><seg type="wordend">τ<supplied
                                reason="lost">ρ</supplied>ον</seg> τοῦ βάρεος ἐπὶ τᾶς ΡΞ <w part="I"
                            >ἐκβλη</w></seg>
                    <seg n="3" type="line" facs="z87r1_03"><w part="F">φθείσας</w>
                        <expan>καὶ</expan> ἀπολαφθείσας τινὸς ἁ ΕΞ</seg>
                    <seg n="4" type="line" facs="z87r1_04">ποτὶ τὰν ΞΡ τὸν αὐτὸν λόγον, ὃν</seg>
                    <seg n="5" type="line" facs="z87r1_05">ἔχει τὸ <seg type="word"
                            ><unclear>β</unclear>άρος</seg> τοῦ κατὰ τὰν ΒΜΓ</seg>
                    <seg n="6" type="line" facs="z87r1_06"><seg type="word"
                            >περιφ<unclear>έρ</unclear>ειαν</seg>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> τμάματος ποτὶ</seg>
                    <seg n="7" type="line" facs="z87r1_07">τὸ <seg type="word">βάρ<supplied
                                reason="lost">ο</supplied>ς</seg> τοῦ ἐκτὸς τοῦ ὑγροῦ· <w part="I"
                            >δέδει</w></seg>
                    <seg n="8" type="line" facs="z87r1_08"><w part="F">κται</w> γὰρ ταῦτα. ἔστω δὴ
                        τὸ Σ <w part="I">κέν</w></seg>
                    <seg n="9" type="line" facs="z87r1_09"><w part="F">τρον</w> τοῦ εἰρημένου
                        σχάματος. </seg>
                    <seg n="10" type="line" facs="z87r1_10">ἐπεὶ οὖν τοῦ μὲν σχάματος, ὅ <choice>
                            <abbr>εστι</abbr>
                            <expan>ἐστιν</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z87r1_11">ἐκτὸς τοῦ ὑγροῦ, τὸ βάρος ἐς <seg
                            type="word">τ<supplied reason="lost">ὸ</supplied></seg>
                        <seg type="suppliedword">
                            <expan>κατὰ</expan>
                        </seg></seg>
                    <seg n="12" type="line" facs="z87r1_12"><seg type="wordend"><supplied
                                reason="lost">φ</supplied>έ<unclear>ρ</unclear>εται</seg> κα τὰν
                            <seg type="word"><supplied reason="lost">ε</supplied>ὐθεῖαν</seg> τὰν
                        ΛΣ,</seg>
                    <seg n="13" type="line" facs="z87r1_13">τὸ δὲ <unclear>ΕΝ</unclear>
                        <seg type="word">τ<unclear>ῶ</unclear></seg>
                        <seg type="word"><supplied reason="lost">ὑγ</supplied>ρῶι</seg> ἔστω ἄν κατὰ </seg>
                    <seg n="14" type="line" facs="z87r1_14">τὰς εὐθεῖας <seg type="word"
                            ><unclear>τ</unclear>ὰς</seg>
                        <seg type="word"><unclear>Ρ</unclear>Κ</seg>, <seg type="word"><supplied
                                reason="lost">δ</supplied>ῆλον</seg>, ὡς </seg>
                    <seg n="15" type="line" facs="z87r1_15">οὐ μενεῖ τὸ σχᾶμα, ἀλλὰ <seg type="word"
                                >τ<supplied reason="lost">ὸ</supplied></seg> μὲν <seg n="ποτὶ"
                            type="suppliedword">πο</seg></seg>
                    <seg n="16" type="line" facs="z87r1_16"><seg type="wordend">
                            <supplied reason="lost">τὶ</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ὰ</unclear>
                            <supplied reason="lost">ν</supplied>
                        </seg> Η μέρη αὐτοῦ <seg type="word">ἔστ<unclear>ω</unclear></seg>
                        <seg type="word">κά<supplied reason="lost">τω</supplied></seg></seg>
                </seg>
                <seg n="82v2" type="folio">
                    <seg n="1" type="line" facs="z82v2_01">οἰσοῦνται, τὰ δὲ ποτὶ τὰν Η ἔστω</seg>
                    <seg n="2" type="line" facs="z82v2_02">ἄνω, καὶ ἀεὶ ἐς τὸ αὐτὸ οἰσοῦνται, <w
                            part="I">ἕ</w></seg>
                    <seg n="3" type="line" facs="z82v2_03"><w part="F">ως</w> κα ἁ ΖΘ κατὰ κάθετον
                            <seg n="γένηται" type="suppliedword">γέ</seg></seg>
                    <seg n="4" type="line" facs="z82v2_04"><seg type="wordend"><supplied
                                reason="lost">ν</supplied>ηται.</seg> κατὰ κάθετον δὲ <w part="I"
                            >γενομέ</w></seg>
                    <seg n="5" type="line" facs="z82v2_05"><w part="F">νας</w> τᾶς ΖΘ τὰ κέντρα τοῦ
                            <w part="I">βά</w></seg>
                    <seg n="6" type="line" facs="z82v2_06"><w part="F">ρεος</w> ἐσσοῦνται τοῦ ἐν τῶι
                        ὑγρῶι <expan>καὶ</expan></seg>
                    <seg n="7" type="line" facs="z82v2_07">τοῦ ἐκτὸς ἐπὶ τᾶς <seg type="word"
                                >αὐ<unclear>τ</unclear>ᾶς</seg>
                        <w part="I">καθέ</w></seg>
                    <seg n="8" type="line" facs="z82v2_08"><w part="F">του·</w>· ἐπιγραφὰς τᾶς ΖΘ
                        ἐσσοῦνται· </seg>
                    <seg n="9" type="line" facs="z82v2_09">ἀντιθλιψοῦνται οὖν ἀλλήλοις τὰ</seg>
                    <seg n="10" type="line" facs="z82v2_10">ΒΙΑ κατὰ τὰν αὐτὰν κάθετον, τὸ </seg>
                    <seg n="11" type="line" facs="z82v2_11">μὲν ἐς <sic>τὼ</sic> κάτω φερόμενον, τὸ
                        δὲ ἐς</seg>
                    <seg n="12" type="line" facs="z82v2_12"><sic>τὼ</sic> ἄνω. ὤστε μενεῖ τὸ σχᾶμα·</seg>
                    <seg n="13" type="line" facs="z82v2_13">οὐδέτερον γὰρ ὑπ᾽ οὐδετέρου <w part="I"
                            >ἐξωθή</w></seg>
                    <seg n="14" type="line" facs="z82v2_14">
                        <w part="F">σει.</w>
                    </seg>
                </seg>
                <seg n="82v2" type="folio">
                    <seg n="14" type="line" facs="z82v2_14">τὰ δ᾽ αὐτὰ <sic>ἐρείται</sic> καὶ εἴ
                        κατὰ</seg>
                    <seg n="15" type="line" facs="z82v2_15">τὸ σχᾶμα ἡμισφαίριον ἢ τῆι <w part="I"
                            >ἔλασ</w></seg>
                    <seg n="16" type="line" facs="z82v2_16">
                        <w part="F">σον</w>
                        <seg type="word">ἡμισφαιρίο<supplied reason="lost">υ.</supplied></seg>
                    </seg>
                    <figure n="1.8.1">
                        <figDesc xml:lang="eng">Figure 1.8.1</figDesc>
                    </figure>
                </seg>
                <seg n="87r2" type="folio">
                    <seg n="1" type="line" facs="z87r2_01"><expan>ΚΑῚ</expan> τὸ νῦν, εἰς τὸ σχᾶμα
                        κουφότερον ἐὸν</seg>
                    <seg n="2" type="line" facs="z87r2_02"><sic>ἐὸν</sic> τοῦ ὑγροῦ ἀφεθῆ ἐς τὸ
                        ὑγρὸν <expan>οὕτως</expan>,</seg>
                    <seg n="3" type="line" facs="z87r2_03">ὥστε τὴν βάσιν αὐτοῦ ὅλην εἶμεν</seg>
                    <seg n="4" type="line" facs="z87r2_04">ἐν τῶ ὑγρῶι, ὀρθὸν <choice>
                            <abbr>κατατασειτ</abbr>
                            <expan>κατατασεῖται</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z87r2_05">τὸ σχᾶμα οὕτως, ἔσ<unclear>τω</unclear>
                        τὸν ἄξονα</seg>
                    <seg n="6" type="line" facs="z87r2_06">αὐτοῦ καθ᾽ ἐαυτὸν εἶμεν.</seg>
                </seg>
                <seg n="87r2" type="folio">
                    <seg n="6" type="line" facs="z87r2_06">νοείσθω</seg>
                    <seg n="7" type="line" facs="z87r2_07">γάρ τι μέγεθος, οἷον εἴρηται, εἰς</seg>
                    <seg n="8" type="line" facs="z87r2_08">τὸ ὑγρὸν ἀφεώμενον, <seg type="word"
                                >νοεί<unclear>σ</unclear>θω</seg>
                        <seg type="word">δ<unclear>ὴ</unclear></seg></seg>
                    <seg n="9" type="line" facs="z87r2_09"><expan>καὶ</expan> ἐπίπεδον ἀγόμενον <choice>
                            <abbr>δ</abbr>
                            <expan>διὰ</expan>
                        </choice>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice>
                        <choice>
                            <abbr>αξον</abbr>
                            <expan>ἄ<supplied reason="lost">ξ</supplied>ονος</expan>
                        </choice>
                    </seg>
                    <seg n="10" type="line" facs="z87r2_10">τοῦ τμάματος <expan>καὶ</expan> διὰ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice>
                        <seg type="word"><supplied reason="lost">κ</supplied>έντρου</seg></seg>
                    <seg n="11" type="line" facs="z87r2_11">τοῦ ΓΛΑ, τομὰ <seg type="word"
                                >δ<unclear>ὲ</unclear></seg> ἔστω τᾶς μὲν <w part="I">ἐπι</w></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
