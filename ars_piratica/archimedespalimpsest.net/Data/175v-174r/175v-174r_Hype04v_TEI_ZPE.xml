    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="175v-174r" type="folio">
                    <seg n="01" type="line" facs="z175v-174r_01">φε<supplied reason="lost"
                            >ρόν</supplied><unclear>των</unclear>; ἔστι γὰρ <supplied reason="lost"
                            >σ</supplied>υκοφάντου μὲν τὸ πολλοὺς <unclear>π</unclear><supplied
                            reason="lost">ο</supplied><unclear>ιῆ</unclear><supplied reason="lost"
                            >σαι</supplied></seg>
                    <seg n="02" type="line" facs="z175v-174r_02">ἀ<unclear>γῶνας</unclear>,
                            πολί<unclear>το</unclear>υ <unclear>δὲ</unclear> δικαίου τὸ ἐξελέγχειν
                            τοὺ<unclear>ς</unclear>
                        <unclear>ἐν</unclear>
                        <unclear>τοῖς</unclear></seg>
                    <seg n="03" type="line" facs="z175v-174r_03">δι<supplied reason="lost"
                        >καστηρίοι</supplied>ς. <unclear>σ</unclear>ὺ δὲ τοῦ νόμου ἀπαγορεύοντος
                            μηδ<supplied reason="lost">ένα</supplied></seg>
                    <seg n="04" type="line" facs="z175v-174r_04"
                            >πρ<unclear>οσ</unclear>ι<unclear>έ</unclear>ν<unclear>αι</unclear>
                        <unclear>πρὸ</unclear>ς <unclear>τὰ</unclear> δικαστήρια πρὶν ἂν
                            τρ<unclear>ιάκ</unclear>οντα</seg>
                    <seg n="05" type="line" facs="z175v-174r_05">ἔτη γένηται, πρὶν πέντε καὶ εἴκοσι
                        ἔτη σοι γενέσθαι,</seg>
                    <seg n="06" type="line" facs="z175v-174r_06">διπλασίας αὐτῶν γραφὰς ἀπενήνοχας,
                        καὶ <seg type="word">
                            <choice>
                                <abbr>ταῦ</abbr>
                                <expan>ταῦτα</expan>
                            </choice>
                        </seg></seg>
                    <seg n="07" type="line" facs="z175v-174r_07">ἑτέροις ὑπηρετῶν, <unclear>οἳ</unclear>
                        <unclear>αὐ</unclear>τοὶ μὲν οὐ τολμῶσιν <w part="I">γράφεσ</w></seg>
                    <seg n="08" type="line" facs="z175v-174r_08"><w part="F">θαι</w>, ἵνα
                            μ<unclear>ή</unclear>
                        <supplied reason="lost">τι</supplied> ἂν <unclear>χ</unclear>ει<supplied
                            reason="lost">ρ</supplied><unclear>ωθ</unclear>ῶσιν, τῆι δὲ σῆι
                        προπετείαι</seg>
                    <seg n="09" type="line" facs="z175v-174r_09">καταχρῶνται. καὶ ὑπὲρ <seg
                            type="word">
                            <choice>
                                <abbr>τούτω</abbr>
                                <expan>τούτων</expan>
                            </choice>
                        </seg>, ὦ ἄνδρες <w part="I">δικα</w></seg>
                    <seg n="10" type="line" facs="z175v-174r_10"><w part="F">σταί</w>,
                        φα<unclear>νε</unclear>ρῶς ὤιετο δεῖν τοὺς μισθοὺς κομίζεσθαι.</seg>
                    <seg n="11" type="line" facs="z175v-174r_11">ὅτε <unclear>γὰρ</unclear>
                            Φίλ<unclear>ιππο</unclear>ς ἐκέλευεν ἡμᾶς εἰς τὸ σύνταγμα <w part="I"
                            >πέμ</w></seg>
                    <seg n="12" type="line" facs="z175v-174r_12"><w part="F">πειν</w>
                        <unclear>ἑ</unclear>ξακοσίους στρατιώτας καὶ <seg type="word">
                            <choice>
                                <abbr>ἑξήκον</abbr>
                                <expan><unclear>ἑξή</unclear>κοντα</expan>
                            </choice>
                        </seg> ἱππέας,</seg>
                    <seg n="13" type="line" facs="z175v-174r_13">Διώνδας τούτων ἐθελοντὴς ἦν
                            <unclear>καὶ</unclear> ἔλαβεν τότε</seg>
                    <seg n="14" type="line" facs="z175v-174r_14">ἐκ τῶν δημοτῶν ἀργύριον καὶ τὸ παρὰ
                        τῆς <w part="I">πό</w></seg>
                    <seg n="15" type="line" facs="z175v-174r_15"><w part="F">λεως</w>, καί φησιν
                        εὔνους εἶναι τῶι δήμωι, ὃς αὑτὸν</seg>
                    <seg n="16" type="line" facs="z175v-174r_16">προσέταξεν τῶν στρατιωτῶν εἶναι,
                        οὓς ὁ δῆμος</seg>
                    <seg n="17" type="line" facs="z175v-174r_17"><supplied reason="lost"
                        >Φιλίππωι</supplied> ἔ<supplied reason="lost">π</supplied>εμπεν. καὶ
                            <unclear>εἰ</unclear> ἐγένετο ἡ στρατεία,</seg>
                    <seg n="18" type="line" facs="z175v-174r_18"><supplied reason="lost"
                        >ἀπο</supplied>λιπὼν ἂν τοὺς κατ’ ἐμοῦ ἀγῶνας, οὕς φησιν</seg>
                    <seg n="19" type="line" facs="z175v-174r_19">ὑπὲρ ὑμῶν <supplied reason="lost"
                            >γ</supplied>εγράφ<unclear>θ</unclear>αι, <choice>
                            <sic>ὤχετο</sic>
                            <corr>ὤιχετο</corr>
                        </choice> ἂν πρὸς Φίλιππον</seg>
                    <seg n="20" type="line" facs="z175v-174r_20">πάλιν παρ’ ἐκείνου <seg type="word">
                            <choice>
                                <abbr>καρπούμενο</abbr>
                                <expan>καρπούμενος</expan>
                            </choice>
                        </seg> ὧν ἐνθάδε ὑπὲρ</seg>
                    <seg n="21" type="line" facs="z175v-174r_21">αὐτοῦ ἠγωνίζετο. λογίσασθε δέ, ὦ
                        ἄνδρες <w part="I">δικα</w></seg>
                    <seg n="22" type="line" facs="z175v-174r_22"><w part="F">σταί</w>, διὰ τί, ἂν <choice>
                            <sic>τούτω</sic>
                            <corr>τούτωι</corr>
                        </choice> πιστεύσαντες ἐμοῦ <w part="I">καταψη</w></seg>
                    <seg n="23" type="line" facs="z175v-174r_23"><w part="F">φίσησθε</w>·
                            <unclear>ὅτ</unclear>ι καὶ πρότερον πώποτε ἔδοξέ τ<unclear>ι</unclear></seg>
                    <seg n="24" type="line" facs="z175v-174r_24">λέγειν; ἀλλ’ <choice>
                            <sic>εὶ</sic>
                            <corr>ἀεὶ</corr>
                        </choice> παρ’ ὑμῖν συκοφάντης ὢν ἐλέγχε<unclear>τα</unclear>ι.</seg>
                    <seg n="25" type="line" facs="z175v-174r_25">ἀλλὰ – νὴ Δία – οἱ οἰκεῖοι
                        ὁμογνώμονες αὐτῶι εἰσιν</seg>
                    <seg n="26" type="line" facs="z175v-174r_26"><unclear>οἷ</unclear>ς
                            πρά<unclear>ττει</unclear>; ἀλλὰ τοὺς μὲν ἄλλους ἐάσω, ὃ δὲ
                            <unclear>πρὸς</unclear></seg>
                    <seg n="27" type="line" facs="z175v-174r_27">αὐτοῦ <unclear>ἐστ</unclear>ιν
                        πάντων τῶν ἀγώνων· κα<unclear>ὶ</unclear>
                        <unclear>π</unclear>ρὸς <unclear>τ</unclear>α<unclear>ῖς</unclear></seg>
                    <seg n="28" type="line" facs="z175v-174r_28">ἀνακρίσεσιν δεῖται αὐτοὺς
                        παύσασθαι, καὶ <w part="I">οὐ</w></seg>
                    <seg n="29" type="line" facs="z175v-174r_29"><w part="F">κ</w> ἀρέσκειν αὐτῶι
                        φησιν ἃ οὗτος πράττει, οὐδ’ <w part="I">ἐθέ</w></seg>
                    <seg n="30" type="line" facs="z175v-174r_30"><w part="F">λειν</w> αὐτὸν
                        πείθεσθαι αὐτῶι, ἀλλ’ ὅμω<unclear>ς</unclear> ἐμοῦ <seg type="unclearword">
                            <unclear>κα</unclear>
                        </seg></seg>
                    <seg n="31" type="line" facs="z175v-174r_31"><seg type="wordend">τηγόρει</seg>,
                        ὅτι τοὺς δούλους τοὺς συναγωνι<unclear>ου</unclear>μένους τῶι</seg>
                    <seg n="32" type="line" facs="z175v-174r_32">δήμωι ἔγραψα ἐλευθέρους εἶναι ὑπὲρ
                        ὧν <w part="I">πολ</w></seg>
                </seg>
            </ab>
