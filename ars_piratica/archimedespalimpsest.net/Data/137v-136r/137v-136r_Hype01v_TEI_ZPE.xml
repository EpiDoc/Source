    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="137v-136r" type="folio">
                    <seg n="01" type="line" facs="z137v-136r_01">διὰ τῶν ὑμετέρων κινδύνων
                            ὥσπε<supplied reason="lost">ρ</supplied> καὶ πρότερον</seg>
                    <seg n="02" type="line" facs="z137v-136r_02">τοὺς Ἕλληνας ἐλευθερῶσαι. δεῖ δὲ
                        τῶν κινδύνων</seg>
                    <seg n="03" type="line" facs="z137v-136r_03">πάντων τὰς μὲν ἀρχὰς καὶ τὰς
                        ὑποθέσεις εἰς τοὺς</seg>
                    <seg n="04" type="line" facs="z137v-136r_04">πράττοντας ἀναφέρειν, τὰ δ’ ἐκ
                        τούτων <seg type="clearword">ἀποβαί</seg></seg>
                    <seg n="05" type="line" facs="z137v-136r_05"><seg type="wordend">
                            <choice>
                                <abbr>νον</abbr>
                                <expan>νοντα</expan>
                            </choice>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr>εἰ</abbr>
                                <expan>εἰς</expan>
                            </choice>
                        </seg> τὴν τύχην. Διώνδας δὲ τοὐναντίον ἀξιοῖ <w part="I">γε</w></seg>
                    <seg n="06" type="line" facs="z137v-136r_06"><w part="F">νέσθαι</w>· μὴ
                        Δημοσθένην τῆς προαιρέσεως ἕνεκα</seg>
                    <seg n="07" type="line" facs="z137v-136r_07">ἐπαινεῖσθαι, ἀλλ’ ἐμὲ τῆς τύχης
                        ἕνεκα <seg type="word">
                            <choice>
                                <abbr>εὐθύν</abbr>
                                <expan>εὐθύνας</expan>
                            </choice>
                        </seg></seg>
                    <seg n="08" type="line" facs="z137v-136r_08">δοῦναι. καίτοι εἴ τις πρὸ τοῦ
                        καθαιρεθῆναι <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τὴν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="09" type="line" facs="z137v-136r_09">στήλην τὴν Θηβαίοις πρὸς Φίλιππον
                        ἑστηκυῖαν</seg>
                    <seg n="10" type="line" facs="z137v-136r_10">ἐπηγγέ<unclear>λ</unclear>λετο
                        Φιλίππωι μὲν Θηβαίους πείσειν</seg>
                    <seg n="11" type="line" facs="z137v-136r_11">πολεμεῖν, ἡμῖν δὲ συμμάχους εἶναι,
                            <w part="I">ἀντεῖ</w></seg>
                    <seg n="12" type="line" facs="z137v-136r_12"><w part="F">πε</w> τις αὐτὸν
                        ἐπαινέσαι, ἀλλ’ οὐκ ἂν τὰς μεγίστας</seg>
                    <seg n="13" type="line" facs="z137v-136r_13">δωρεὰς τῶι ταῦτα πράξαντι δοῦναι;
                        εἶτ’ οὐ δεινόν</seg>
                    <seg n="14" type="line" facs="z137v-136r_14">ἐστιν πρὸ μ<unclear>ὲ</unclear>ν
                        τοῦ τυχεῖν τούτων οὐδὲν ὅ τι οὐκ <choice>
                            <sic>ἄν</sic>
                            <corr>ἂν</corr>
                        </choice>
                        <choice>
                            <sic>πω</sic>
                            <corr>τῶι</corr>
                        </choice></seg>
                    <seg n="15" type="line" facs="z137v-136r_15">ἐπαγγελλομένωι ὑποσχέσθαι, ἐπεὶ δ’
                        ἐπράχθη</seg>
                    <seg n="16" type="line" facs="z137v-136r_16">ἀχαρίστους φαίνεσθαι ὄντας; καὶ μὴν
                            <seg type="word">
                            <choice>
                                <abbr>κακεῖν</abbr>
                                <expan>κακεῖνο</expan>
                            </choice>
                        </seg></seg>
                    <seg n="17" type="line" facs="z137v-136r_17">σκέψασθε, ὦ ἄνδρες Ἀθηναῖοι, εἰ
                        παρὰ Φιλίππωι</seg>
                    <seg n="18" type="line" facs="z137v-136r_18">συλληφθέντες ἡμεῖς ἐκρινόμεθα, τί
                        ἂν <seg type="word">
                            <choice>
                                <abbr>ἡμῶ</abbr>
                                <expan>ἡμῶν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="19" type="line" facs="z137v-136r_19">κατηγόρει; οὐχ ὅτι <supplied
                            reason="lost">Β</supplied>υζάντιον μὲν αὐτὸν <w part="I">ἐκωλύ</w></seg>
                    <seg n="20" type="line" facs="z137v-136r_20"><w part="F">σαμεν</w>
                            λαβ<unclear>εῖ</unclear>ν, τὴν δ’ <unclear>Εὔβο</unclear>ιαν
                        ἀπεστήσαμεν, τὴν δὲ</seg>
                    <seg n="21" type="line" facs="z137v-136r_21">πρὸς Θηβαίους ὑπάρχουσαν αὐτῶι
                        συμμαχίαν <w part="I">κα</w></seg>
                    <seg n="22" type="line" facs="z137v-136r_22"><w part="F">θείλομεν</w>, ὑμῖν δὲ
                        συμμάχους ἐποιήσαμεν; τί δ’ <seg type="clearword">
                            <choice>
                                <sic>ἂν</sic>
                                <corr>ἀν</corr>
                            </choice>
                        </seg></seg>
                    <seg n="23" type="line" facs="z137v-136r_23"><seg type="wordend">
                            <choice>
                                <sic>επάθομεν</sic>
                                <corr>τεπάθομεν</corr>
                            </choice>
                        </seg> ἂν ὑπ’ ἐκεῖνου; ἆρ’ οὐκ ἂν ἀπεθάνομεν;</seg>
                    <seg n="24" type="line" facs="z137v-136r_24">ἐγὼ μὲν οἶμαι. εἶτ’ οὐ δεινόν, ὦ
                        ἄνδρες Ἀθηναῖοι,</seg>
                    <seg n="25" type="line" facs="z137v-136r_25">εἰ ὑπὲρ τῶν αὐτῶν ἡμᾶς δεήσει πρὸς
                        τοὺς <w part="I">ἀντι</w></seg>
                    <seg n="26" type="line" facs="z137v-136r_26"><w part="F">πάλους</w> καὶ πρὸς
                        ὑμᾶς κινδυνεύειν; καὶ <w part="I">Φίλιπ</w></seg>
                    <seg n="27" type="line" facs="z137v-136r_27"><w part="F">πος</w> μὲν τοὺς ὑπὲρ
                        αὐτοῦ τι καθ’ ἡμῶν <seg type="word">
                            <choice>
                                <abbr>πράξαν</abbr>
                                <expan>πράξαντας</expan>
                            </choice>
                        </seg></seg>
                    <seg n="28" type="line" facs="z137v-136r_28">οὐ μόνον αὐτὸς ἐτίμησεν ἀλλὰ καὶ
                        παρ’ ἡμῖν <w part="I">ἔσ</w></seg>
                    <seg n="29" type="line" facs="z137v-136r_29"><w part="F">πευσεν</w> ὅπως
                        τιμηθῶσιν — καί εἰσι πρόξενοι <w part="I">ἀνα</w></seg>
                    <seg n="30" type="line" facs="z137v-136r_30"><w part="F">γεγραμμένοι</w> — ἡμεῖς
                        δὲ οὐδὲ ἃς παρ’ ἡμῖν αὐτοῖς</seg>
                    <seg n="31" type="line" facs="z137v-136r_31">ἔδομεν τιμὰς <choice>
                            <sic>μυρίας</sic>
                            <corr resp="scribe">κυρίας</corr>
                        </choice> ἐάσομεν εἶναι; ἀλλ’ οὐ <w part="I">προ</w></seg>
                    <seg n="32" type="line" facs="z137v-136r_32"><w part="F">σήκει</w> ὑμῖν, ὦ
                        ἄνδρες δικασταί, τοιούτου<unclear>ς</unclear> εἶναι</seg>
                </seg>
            </ab>
