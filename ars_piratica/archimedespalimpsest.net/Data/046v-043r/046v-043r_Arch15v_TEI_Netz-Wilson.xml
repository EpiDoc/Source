    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="46v1" type="folio">
                    <seg n="1" type="line" facs="z46v1_01">ἐγγραφῆι τὰς μὲν βάσεις ἔχων</seg>
                    <seg n="2" type="line" facs="z46v1_02">πρὸς τοῖς κατεναντίον <w part="I"
                        >παραλλη</w></seg>
                    <seg n="3" type="line" facs="z46v1_03"><w part="F">λογράμμοις</w>, τὴν δὲ
                        ἐπιφάνειαν</seg>
                    <seg n="4" type="line" facs="z46v1_04"><seg type="word"><supplied reason="lost"
                                >τ</supplied>ῶν</seg> λοιπῶν τεσσάρων <w part="I">ἐπιπέ</w></seg>
                    <seg n="5" type="line" facs="z46v1_05"><w part="F">δων</w> ἐφαπτόμενος, ἐγγραφῆ
                            <expan>καὶ</expan></seg>
                    <seg n="6" type="line" facs="z46v1_06">ἄλλος κύλινδρος εἰς τὸν αὐτὸν <w part="I"
                            >κύ</w></seg>
                    <seg n="7" type="line" facs="z46v1_07"><w part="F">βον</w> τὰς μὲν βάσεις ἔχων
                        ἐν <choice>
                            <abbr>ἄλλο</abbr>
                            <expan>ἄλλοις</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z46v1_08">παραλληλογράμμοις, <seg type="word"
                                >τῆ<unclear>ι</unclear></seg> δὲ <w part="I">ἐπι</w></seg>
                    <seg n="9" type="line" facs="z46v1_09"><w part="F">φανείαι</w> τῶν λοιπῶν <choice>
                            <abbr>τεσσάρ</abbr>
                            <expan>τεσσάρων</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z46v1_10">ἐπιπέδων <seg type="word"
                            >ἐφαπ<unclear>τ</unclear>όμενος</seg> τὸ <w part="I">πε</w></seg>
                    <seg n="11" type="line" facs="z46v1_11"><w part="F">ριληφθὲν</w> σχῆμα ὑπὸ τῶν
                            <w part="I">ἐπι</w></seg>
                    <seg n="12" type="line" facs="z46v1_12"><w part="F">φανειῶν</w> τῶν κυλίνδρων, ὅ <choice>
                            <abbr>ἐστι</abbr>
                            <expan>ἐστιν</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z46v1_13">ἐν ἀμφοτέροις τοῖς <choice>
                            <abbr>κυλίνδρ</abbr>
                            <expan>κυλίνδροις</expan>
                        </choice>,</seg>
                    <seg n="14" type="line" facs="z46v1_14"><seg type="word"
                            >δίμ<unclear>οι</unclear>ρόν</seg> ἐστι τοῦ ὅλου κύβου. <w part="I"
                        >συμ</w></seg>
                    <seg n="15" type="line" facs="z46v1_15"><w part="F">βαίνει</w> δὲ ταῦτα τὰ <choice>
                            <abbr>θεωρήμα</abbr>
                            <expan>θεωρήματα</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z46v1_16">διαφέρειν τῶν πρότερον <w part="I"
                        >εὑρη</w></seg>
                    <seg n="17" type="line" facs="z46v1_17"><w part="F">μένων</w>· ἐκεῖνα μὲν γὰρ τὰ
                            <w part="I">σχή</w></seg>
                    <seg n="18" type="line" facs="z46v1_18"><w part="F">ματα</w>, τά τε κωνοειδῆ καὶ</seg>
                    <seg n="19" type="line" facs="z46v1_19">σφαιροειδῆ καὶ τὰ τμήματα</seg>
                </seg>
                <seg n="43r1" type="folio">
                    <seg n="1" type="line" facs="z43r1_01">τὰ <seg type="word"
                            >αὐ<unclear>τ</unclear>ά</seg> τε πρὸς ἄλληλα καὶ <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice>
                    </seg>
                    <seg n="2" type="line" facs="z43r1_02">κώνων καὶ κυλίνδρων <w part="I">συνε</w></seg>
                    <seg n="3" type="line" facs="z43r1_03"><w part="F">κρίναμεν</w>, ἐπιπέδων δὲ <w
                            part="I">περι</w></seg>
                    <seg n="4" type="line" facs="z43r1_04"><w part="F">εχομένω</w> στερεῶι σχήματι
                            <w part="I">οὐ</w></seg>
                    <seg n="5" type="line" facs="z43r1_05"><w part="F">δὲν</w> αὐτῶν ἴσον ἐὸν
                        εὕρηται,</seg>
                    <seg n="6" type="line" facs="z43r1_06">τούτων δὲ τῶν σχημάτων τὸ <choice>
                            <abbr>μ</abbr>
                            <expan>μὲν</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z43r1_07">δυσὶν ἐπιπέδοις καὶ <w part="I"
                        >ἐπιφανεί</w></seg>
                    <seg n="8" type="line" facs="z43r1_08"><w part="F">ας</w> κυλίνδρων ἕκαστον
                        αὐτῶν</seg>
                    <seg n="9" type="line" facs="z43r1_09">ἐπιπέδωι περιεχομένωι <w part="I"
                        >στερε</w></seg>
                    <seg n="10" type="line" facs="z43r1_10"><w part="F">ῶι</w> σχήματι ἴσον
                        εὑρίσκεται.</seg>
                </seg>
                <seg n="43r1" type="folio">
                    <seg n="11" type="line" facs="z43r1_11">τούτων δὴ τῶν θεωρημάτων</seg>
                    <seg n="12" type="line" facs="z43r1_12">τὰς ἀποδείξεις ἐν τῶιδε τῶι <w part="I"
                            >βι</w></seg>
                    <seg n="13" type="line" facs="z43r1_13"><w part="F">βλίωι</w>
                        <seg type="word">γρά<unclear>ψ</unclear>ας</seg> ἀποστελῶ σοι.</seg>
                </seg>
                <seg n="43r1" type="folio">
                    <seg n="14" type="line" facs="z43r1_14">ὁρῶν δέ σε, <choice>
                            <abbr>καθάπ</abbr>
                            <expan>καθάπερ</expan>
                        </choice> λέγω, <w part="I">σπου</w></seg>
                    <seg n="15" type="line" facs="z43r1_15"><w part="F">δαῖον</w> καὶ <seg
                            type="word">φιλοσοφί<unclear>ας</unclear></seg>
                        <w part="I">προεστῶ</w></seg>
                    <seg n="16" type="line" facs="z43r1_16"><w part="F">τα</w>
                        <seg type="word">ἀξ<supplied reason="lost"
                            >ι</supplied><unclear>ο</unclear>λόγως</seg> καὶ τὴν ἐν τοῖς</seg>
                    <seg n="17" type="line" facs="z43r1_17">
                        <seg type="word">μα<supplied reason="lost">θ</supplied>ήμ<supplied
                                reason="lost">α</supplied>σιν</seg> κατὰ τὸ <choice>
                            <abbr>ὑποπίπτ</abbr>
                            <expan>ὑποπίπτον</expan>
                        </choice></seg>
                </seg>
                <seg n="46v2" type="folio">
                    <seg n="1" type="line" facs="z46v2_01">θεωρίαν τετιμηκότα <w part="I"
                        >ἐδοκίμα</w></seg>
                    <seg n="2" type="line" facs="z46v2_02"><w part="F">σα</w> γράψαι σοι καὶ εἰς τὸ
                        αὐτὸ <w part="I">βιβλί</w></seg>
                    <seg n="3" type="line" facs="z46v2_03"><w part="F">ον</w> ἐξορίσας τρόπου τινὸς
                            <w part="I">ἰδιό</w></seg>
                    <seg n="4" type="line" facs="z46v2_04"><w part="F">τητα</w>, καθ᾽ ὅν
                        ἐπιπορευόμενον</seg>
                    <seg n="5" type="line" facs="z46v2_05">ἔσται λαμβάνειν ἀφορμὰς εἰς</seg>
                    <seg n="6" type="line" facs="z46v2_06">τὸ δύνασθαί τινα τῶν ἐν τοῖς</seg>
                    <seg n="7" type="line" facs="z46v2_07">μαθήμασι θεωρεῖν διὰ <seg type="word"
                                >τῶ<unclear>ν</unclear></seg></seg>
                    <seg n="8" type="line" facs="z46v2_08">μηχανικῶν. τοῦτο <unclear>δὲ</unclear>
                        <sic>πέπισμαι</sic>
                        <seg type="suppliedword">
                            <supplied reason="lost">χρή</supplied>
                        </seg></seg>
                    <seg n="9" type="line" facs="z46v2_09"><seg type="wordend">σιμον</seg> εἶναι
                        οὐδὲν <seg type="word">ἧ<unclear>σσ</unclear>ο<unclear>ν</unclear></seg> καὶ
                            <unclear>εἰς</unclear>
                        <seg type="word">
                            <choice>
                                <abbr>
                                    <unclear>τὴ</unclear>
                                </abbr>
                                <expan>
                                    <unclear>τὴν</unclear>
                                </expan>
                            </choice>
                        </seg></seg>
                    <seg n="10" type="line" facs="z46v2_10">ἀπόδειξιν αὐτῶν τῶν <w part="I"
                        >θεωρη</w></seg>
                    <seg n="11" type="line" facs="z46v2_11"><w part="F">μάτων</w>. καὶ γὰρ προτέρων
                        μοι <w part="I">φα</w></seg>
                    <seg n="12" type="line" facs="z46v2_12"><w part="F">νέντων</w> μηχανικῶς <seg
                            type="word">
                            <choice>
                                <abbr><supplied reason="lost">ὕ</supplied>στερν</abbr>
                                <expan>ὕστερον</expan>
                            </choice>
                        </seg>
                        <w part="I">γε</w></seg>
                    <seg n="13" type="line" facs="z46v2_13"><w part="F">ωμετρικῶς</w> ἀπεδείχθη <choice>
                            <abbr>δ</abbr>
                            <expan>διὰ</expan>
                        </choice> τὸ</seg>
                    <seg n="14" type="line" facs="z46v2_14">χωρὶς ἀποδείξεως <expan>εἶναι</expan>
                        τὴν <choice>
                            <abbr>δ</abbr>
                            <expan>διὰ</expan>
                        </choice>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z46v2_15">τρόπου θεωρίαν· ἑτοιμότερον
                        <expan>γάρ</expan></seg>
                    <seg n="16" type="line" facs="z46v2_16">ἐστι προλαβόντα <choice>
                            <abbr>δ</abbr>
                            <expan>διὰ</expan>
                        </choice> τοῦ τρόπου <w part="I">γνῶ</w></seg>
                    <seg n="17" type="line" facs="z46v2_17"><w part="F">σίν</w> τινα τῶν ζητημάτων
                            <w part="I">πο</w></seg>
                    <seg n="18" type="line" facs="z46v2_18"><w part="F">ρίσασθαι</w> τὴν ἀπόδειξιν
                        μᾶλλον</seg>
                    <seg n="19" type="line" facs="z46v2_19">ἢ μηδενὸς ἐγνωσμένου <seg type="word"
                                ><supplied reason="lost">ζ</supplied>ητεῖν</seg>.</seg>
                </seg>
                <seg n="43r2" type="folio">
                    <seg n="1" type="line" facs="z43r2_01">διὸ καὶ τὰς <choice>
                            <abbr>εὑρήσ</abbr>
                            <expan>εὑρήσεις</expan>
                        </choice> τῶν <seg type="unclearword">θεωρ<unclear>η</unclear></seg></seg>
                    <seg n="2" type="line" facs="z43r2_02"><seg type="wordend">μάτων</seg> τούτων
                        Εὔδοξος <w part="I">ἐξήνεγ</w></seg>
                    <seg n="3" type="line" facs="z43r2_03"><w part="F">κε</w> πρῶτος τὴν ἀπόδειξίν</seg>
                    <seg n="4" type="line" facs="z43r2_04">τε τοῦ κώνου καὶ τῆς <choice>
                            <abbr>πυραμί</abbr>
                            <expan>πυραμίδος</expan>
                        </choice>,</seg>
                    <seg n="5" type="line" facs="z43r2_05"><expan>ὅτι</expan> τρίτον μέρος
                            <expan>ἐστὶν</expan> ὁ μὲν <seg type="word">
                            <choice>
                                <abbr>κῶνς</abbr>
                                <expan>κῶν<supplied reason="lost">
                                        <add>ο</add>
                                    </supplied>ς</expan>
                            </choice>
                        </seg></seg>
                    <seg n="6" type="line" facs="z43r2_06">τοῦ κυλίνδρου, ἡ δὲ πυραμὶς <seg
                            type="word">
                            <choice>
                                <abbr>
                                    <unclear>τ</unclear>
                                </abbr>
                                <expan><unclear>τ</unclear>οῦ</expan>
                            </choice>
                        </seg></seg>
                    <seg n="7" type="line" facs="z43r2_07">πρίσματος, τῶν βάσιν <w part="I">ἐχόν</w></seg>
                    <seg n="8" type="line" facs="z43r2_08"><w part="F">των</w> τὴν αὐτὴν καὶ ὕψος
                        ἴσον, οὐ</seg>
                    <seg n="9" type="line" facs="z43r2_09">μικρὰν ἀπονείμαι τις <w part="I">Δημο</w></seg>
                    <seg n="10" type="line" facs="z43r2_10"><w part="F">κρίτωι</w> μερίδα <seg
                            type="word">πρώ<unclear>τ</unclear><supplied reason="lost"
                                >ω</supplied><unclear>ι</unclear></seg> τὴν <w part="I">ἀ</w></seg>
                    <seg n="11" type="line" facs="z43r2_11"><w part="F">πόφασιν</w> τὴν περὶ <seg
                            type="word">το<unclear>ῦ</unclear></seg>
                        <seg type="unclearword">εἰρ<unclear>η</unclear>μέ</seg></seg>
                    <seg n="12" type="line" facs="z43r2_12"><seg type="wordend">νου</seg> σχήματος
                        χωρὶς <w part="I">ἀποδείξε</w></seg>
                    <seg n="13" type="line" facs="z43r2_13"><w part="F">ως</w> ἀποφηναμένωι. ἡμῖν
                            <seg type="word"><supplied reason="lost">δ</supplied>ὲ</seg></seg>
                    <seg n="14" type="line" facs="z43r2_14">συμβαίνει καὶ τοῦ <unclear>νῦν</unclear>
                        <seg type="suppliedword">ἐκδι<supplied reason="lost"
                            >δ</supplied><unclear>ο</unclear></seg></seg>
                    <seg n="15" type="line" facs="z43r2_15"><seg type="wordend">μένου</seg>
                        θεωρήματος τὴν <seg type="word">
                            <choice>
                                <abbr>εὕρεσ</abbr>
                                <expan>εὕρεσ<unclear>ιν</unclear></expan>
                            </choice>
                        </seg></seg>
                    <seg n="16" type="line" facs="z43r2_16">ὁμοίαν <seg type="word"
                            >τα<unclear>ῖ</unclear>ς</seg> πρότερον <seg type="word">
                            <choice>
                                <abbr>γεγεν<unclear>ῆ</unclear>σθ</abbr>
                                <expan>γεγεν<unclear>ῆ</unclear>σθαι</expan>
                            </choice>
                        </seg>·</seg>
                    <seg n="17" type="line" facs="z43r2_17"><seg type="word"
                            >ἠβουλήθ<unclear>η</unclear>ν</seg> δὲ τὸν τρόπον <seg
                            type="suppliedword">ἀ<supplied reason="lost">ν</supplied>α</seg></seg>
                    <seg n="18" type="line" facs="z43r2_18"><seg type="wordend">γράψας</seg>
                        ἐξενεγκεῖν ἅμα μὲν</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
