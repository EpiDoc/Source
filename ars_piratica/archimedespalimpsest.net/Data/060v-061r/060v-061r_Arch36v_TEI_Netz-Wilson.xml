    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="60v1" type="folio">
                    <seg n="1" type="line" facs="z60v1_01">μείζων <seg type="word">
                            <supplied reason="lost">το</supplied>ῦ</seg> ὃν ἔχει ἁ ἡμίσεια <seg
                            type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">ᾶς</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="2" type="line" facs="z60v1_02">Η<supplied reason="lost">Θ</supplied>
                        <supplied reason="lost">ποτὶ</supplied>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ὰν</seg> ἀπὸ τοῦ Α <choice>
                            <abbr>κάθετο</abbr>
                            <expan>κάθετον</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z60v1_03">ἐπ’ αὐτὸν ἀγομέναν, <choice>
                            <abbr>δι</abbr>
                            <expan>διότι</expan>
                        </choice> καὶ τοῦ,</seg>
                    <seg n="4" type="line" facs="z60v1_04">ὃν ἔχει ἁ ΘΑ <expan>πρὸς</expan> ΑΖ·
                        δυνατὸν <seg type="word">ο<supplied reason="lost">ὖν</supplied>
                        </seg></seg>
                    <seg n="5" type="line" facs="z60v1_05">ἐστιν ἀπὸ <seg type="word">
                            <supplied reason="lost">τ</supplied>ο<supplied reason="lost"
                            >ῦ</supplied>
                        </seg> Α ποτιβαλεῖν <seg type="clearword">πο</seg></seg>
                    <seg n="6" type="line" facs="z60v1_06"><seg type="wordend">τὶ</seg> τὰν
                        ἐκβεβλημέναν τὰν ΑΝ,</seg>
                    <seg n="7" type="line" facs="z60v1_07"><seg type="word">ὥ<supplied reason="lost"
                                >σ</supplied>τε</seg> τὰν μεταξὺ τᾶς <choice>
                            <abbr>περιφερεί</abbr>
                            <expan>περιφερείας</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z60v1_08">καὶ τᾶς <seg type="word">ἐκβεβλ<supplied
                                reason="lost">η</supplied>μένας</seg> τὰν</seg>
                    <seg n="9" type="line" facs="z60v1_09">ΝΡ <expan>πρὸς</expan> ΘΡ τὸν <seg
                            type="word">
                            <supplied reason="lost">α</supplied>ὐτὸν</seg> ἔχειν λόγον,</seg>
                    <seg n="10" type="line" facs="z60v1_10">ὃν ἁ <supplied reason="lost"
                        >ΘΑ</supplied> ποτὶ τὰν ΑΛ· ἕξει οὖν</seg>
                    <seg n="11" type="line" facs="z60v1_11">ΝΡ ποτὶ τὰν ΡΑ λόγον, ὃν ἁ ΘΡ</seg>
                    <seg n="12" type="line" facs="z60v1_12">εὐθεῖα ποτὶ τὰν ΑΛ. ἁ δὲ ΘΡ <seg
                            type="clearword">πο</seg></seg>
                    <seg n="13" type="line" facs="z60v1_13"><seg type="wordend">τὶ</seg> τὰν ΑΛ
                        ἐλάσσονα λόγον ἔχει</seg>
                    <seg n="14" type="line" facs="z60v1_14">ἢ ἁ Θ<supplied reason="lost"
                        >Ρ</supplied> περιφέρεια ποτὶ τὰν</seg>
                    <seg n="15" type="line" facs="z60v1_15">τοῦ ΘΗΚ κύκλου <seg type="word"
                                >περι<supplied reason="lost">φ</supplied>έρειαν</seg>·</seg>
                    <seg n="16" type="line" facs="z60v1_16">ἁ μὲν γὰρ ΘΡ εὐθεῖα ἐλάσσων
                        <expan>ἐστὶ</expan></seg>
                    <seg n="17" type="line" facs="z60v1_17">τᾶς ΘΡ <seg type="word">
                            <supplied reason="lost">περι</supplied>φερείας</seg>, ἁ δὲ ΑΛ <seg
                            type="unclearword">εὐ</seg></seg>
                </seg>
                <seg n="61r1" type="folio">
                    <seg n="1" type="line" facs="z61r1_01">
                        <seg type="wordend">θ<unclear>εῖ</unclear>α</seg> τᾶς τοῦ ΘΗΚ κύκλου περιφε</seg>
                    <seg n="2" type="line" facs="z61r1_02">ρείας μείζων· ἐλάσσονα οὖν
                        <expan>λόγον</expan></seg>
                    <seg n="3" type="line" facs="z61r1_03">ἕξει καὶ ἁ ΝΡ <expan>πρὸς</expan> ΡΑ ἢ ἁ
                        ΘΡ <seg type="clearword">πε</seg></seg>
                    <seg n="4" type="line" facs="z61r1_04"><seg type="wordend">ριφέρεια</seg> ποτὶ
                        τὰν τοῦ ΘΗΚ <seg type="suppliedword">κύ</seg></seg>
                    <seg n="5" type="line" facs="z61r1_05"><seg type="wordend">κ<supplied
                                reason="lost">λ</supplied>ου</seg> περιφέρειαν· καὶ <seg type="word"
                            > ὅλα<unclear>ς</unclear>
                        </seg>
                        <expan>οὖν</expan></seg>
                    <seg n="6" type="line" facs="z61r1_06">ἁ ΝΑ ποτὶ τὰν αΡ ἐλάσσονα</seg>
                    <seg n="7" type="line" facs="z61r1_07">λόγον ἔχει <seg type="word">ἤπε<supplied
                                reason="lost">ρ</supplied>
                        </seg> ἁ Θ<supplied reason="lost">Ρ</supplied>
                        <seg type="clearword">περιφέρει</seg></seg>
                    <seg n="8" type="line" facs="z61r1_08"><seg type="wordend">α</seg> μεθ’ ὅλας τοῦ
                        κύκλου <seg type="word">
                            <choice>
                                <abbr>περιφερεί</abbr>
                                <expan>περιφερεί<supplied reason="lost">ας</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="9" type="line" facs="z61r1_09">ποτὶ τὰν τοῦ ΘΗΚ κύκλου <seg
                            type="clearword">περιφέ</seg></seg>
                    <seg n="10" type="line" facs="z61r1_10"><seg type="wordend">ρειαν</seg>. ὃν δὲ
                        λόγον ἔχει ἁ ΘΡ <seg type="clearword">περι</seg></seg>
                    <seg n="11" type="line" facs="z61r1_11"><seg type="wordend">φέρεια</seg> μεθ’
                        ὅλας τᾶς τοῦ ΘΗΚ</seg>
                    <seg n="12" type="line" facs="z61r1_12"><seg type="word">κύ<supplied
                                reason="lost">κλο</supplied>υ</seg> περιφερείας ποτὶ τὰν</seg>
                    <seg n="13" type="line" facs="z61r1_13"><supplied reason="lost">τοῦ</supplied>
                        <seg type="word">
                            <supplied reason="lost">Θ</supplied>Η<supplied reason="lost"
                            >Κ</supplied>
                        </seg>
                        <seg type="word">κ<supplied reason="lost">ύ</supplied>κλου</seg>
                        <seg type="word">περ<supplied reason="lost">ι</supplied>φέρειαν </seg>,</seg>
                    <seg n="14" type="line" facs="z61r1_14"><seg type="word">
                            <unclear>τ</unclear>
                            <supplied reason="lost">οῦ</supplied>
                            <unclear>τ</unclear>
                            <supplied reason="lost">ον</supplied>
                        </seg>
                        <seg type="word">ἔχ<supplied reason="lost">ει</supplied>
                        </seg>
                        <unclear>ἁ</unclear>
                        <unclear>Χ</unclear>Α ποτὶ τὰν ΑΘ·</seg>
                    <seg n="15" type="line" facs="z61r1_15">δέδεικται <seg type="word">
                            <supplied reason="lost">γ</supplied>ὰ<supplied reason="lost"
                            >ρ</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τοῦτ</supplied>ο</seg>· ἐλάσσονα <seg
                            type="word">
                            <choice>
                                <abbr>λόγ</abbr>
                                <expan><supplied reason="lost">λό</supplied>γ<supplied reason="lost"
                                        >ον</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="16" type="line" facs="z61r1_16"><seg type="word">ἔχ<supplied
                                reason="lost">ει</supplied>
                        </seg>
                        <supplied reason="lost">ἁ</supplied>
                        <unclear>Ν</unclear>Α <seg type="word">
                            <unclear>ποτ</unclear>ὶ</seg> τὰν ΑΡ <choice>
                            <abbr>ἤ</abbr>
                            <expan>ἤπερ</expan>
                        </choice> ἁ</seg>
                </seg>
                <seg n="60v2" type="folio">
                    <seg n="1" type="line" facs="z60v2_01">ΧΑ ποτὶ τὰν ΑΘ· <seg type="word"
                                >ὅ<supplied reason="lost">π</supplied>ερ</seg>
                        <seg type="word">
                            <choice>
                                <abbr>ἀδύνατ</abbr>
                                <expan>ἀδ<unclear>ύν</unclear>α<supplied reason="lost"
                                    >τ</supplied>ον</expan>
                            </choice>
                        </seg>·</seg>
                    <seg n="2" type="line" facs="z60v2_02">ἁ <expan>μὲν</expan>
                        <expan>γὰρ</expan> ΝΑ <seg type="word"> μεί<unclear>ζ</unclear><supplied
                                reason="lost">ω</supplied>ν</seg>
                        <supplied reason="lost">
                            <expan>ἐστὶ</expan>
                        </supplied>
                        <supplied reason="lost">τᾶς</supplied> ΑΧ, <unclear>ἁ</unclear>
                        <supplied reason="lost">δὲ</supplied></seg>
                    <seg n="3" type="line" facs="z60v2_03">ΑΡ ἴσα ἐστὶ τᾶι <supplied reason="lost"
                            >ΘΑ.</supplied>
                        <supplied reason="lost">οὐκ</supplied>
                        <seg type="word">
                            <supplied reason="lost">ἄ</supplied>ρα</seg>
                        <seg type="clearword">μεί</seg></seg>
                    <seg n="4" type="line" facs="z60v2_04"><seg type="wordend">ζων</seg> ἁ ΖΑ τᾶς
                            <seg type="word">τ<supplied reason="lost">οῦ</supplied>
                        </seg>
                        <supplied reason="lost">κύκλου</supplied>
                        <seg type="suppliedword"><supplied reason="lost">π</supplied>εριφε</seg></seg>
                    <seg n="5" type="line" facs="z60v2_05"><seg type="wordend">ρείας</seg> τοῦ ΘΗΚ.</seg>
                    <figure n="18.1">
                        <figDesc>Figure 18.1</figDesc>
                    </figure>
                </seg>
                <seg n="60v2" type="folio">
                    <seg n="6" type="line" facs="z60v2_06">ἔστω δὴ πάλιν, εἰ δυνατόν, <choice>
                            <abbr>ἐλάσσω</abbr>
                            <expan>ἐλάσσων</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z60v2_07">ἁ <supplied reason="lost">Ζ</supplied>Α
                        τᾶς τοῦ ΘΗΚ κύκλου <seg type="suppliedword"
                                >περι<unclear>φ</unclear><supplied reason="lost">ε</supplied></seg></seg>
                    <seg n="8" type="line" facs="z60v2_08"><seg type="wordend">ρείας</seg>. ἔλαβον
                        δή τινα <sic>ευθαν</sic></seg>
                    <seg n="9" type="line" facs="z60v2_09">πάλιν τὴν ΑΛ τᾶς μὲν ΑΖ <seg
                            type="clearword">μεί</seg></seg>
                    <seg n="10" type="line" facs="z60v2_10"><seg type="wordend">ζονα</seg>, τᾶς δὲ
                            <seg type="word">
                            <unclear>το</unclear>ῦ</seg> ΘΗΚ κύκλου <seg type="unclearword">
                            <expan>περι</expan>
                        </seg></seg>
                </seg>
                <seg n="61r2" type="folio">
                    <seg n="1" type="line" facs="z61r2_01">
                        <seg type="wordend"><unclear>φερεί</unclear>ας</seg> ἐλάσσονα, <expan>καὶ</expan>
                        <supplied reason="lost">ἄγω</supplied>
                        <seg type="word">ἀ<unclear>π</unclear>ὸ</seg></seg>
                    <seg n="2" type="line" facs="z61r2_02">τοῦ Θ τὰν ΘΜ παράλληλον τᾶι</seg>
                    <seg n="3" type="line" facs="z61r2_03">ΑΖ. πάλιν οὖν <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice>
                        <expan>ἐστὶν</expan> ὁ ΘΗΚ <expan>καὶ</expan></seg>
                    <seg n="4" type="line" facs="z61r2_04">ἐν αὐτῶι ἐλάσσων <seg type="word">
                                γρα<unclear>μ</unclear>μὰ</seg> τᾶς</seg>
                    <seg n="5" type="line" facs="z61r2_05">διαμέτρου ἁ Θ<supplied reason="lost">Η</supplied>
                        <seg type="word">
                            <supplied reason="lost">κ</supplied>αὶ</seg> ἄλλα <seg
                            type="suppliedword">ἐπ<supplied reason="lost">ι</supplied></seg></seg>
                    <seg n="6" type="line" facs="z61r2_06"><seg type="wordend">ψαύουσα</seg> τοῦ
                        κύκλου κατὰ τὸ Θ</seg>
                    <seg n="7" type="line" facs="z61r2_07">καὶ <expan>λόγος</expan>, ὃν ἔχει ἁ ΑΘ
                        ποτὶ τὰν</seg>
                    <seg n="8" type="line" facs="z61r2_08">Α<unclear>Λ</unclear>, ἐλάσσων τοῦ, ὃν
                        ἔχει <unclear>ἡ</unclear>
                        <seg type="unclearword"><unclear>ἡ</unclear>μί</seg></seg>
                    <seg n="9" type="line" facs="z61r2_09"><seg type="wordend">σεια</seg> τᾶς ΘΗ
                        ποτὶ τὰν <seg type="word">ἀπ<supplied reason="lost">ὸ</supplied>
                        </seg> τοῦ</seg>
                    <seg n="10" type="line" facs="z61r2_10">Α κάθετον ἐπ’ αὐτὰν ἀγμέναν,</seg>
                    <seg n="11" type="line" facs="z61r2_11">ἐπειδὴ καὶ τοῦ ὃν ἔχει ἁ ΘΑ
                        <expan>πρὸς</expan> ΑΖ,</seg>
                    <seg n="12" type="line" facs="z61r2_12">ἐλάσσων <expan>ἐστί</expan>· δυνατὸν οὖν
                        ἐστιν <seg type="word">
                            <unclear>ἀπ</unclear>
                            <supplied reason="lost">ὸ</supplied>
                        </seg></seg>
                    <seg n="13" type="line" facs="z61r2_13">τοῦ Α ἀγαγεῖν τὰν ΑΠ ποτὶ τὰν <seg
                            type="clearword">ἐπι</seg></seg>
                    <seg n="14" type="line" facs="z61r2_14"><seg type="wordend">ψαύουσαν</seg>, ὥστε
                        τὰν <seg type="word">
                            <supplied reason="lost">Ρ</supplied>
                            <unclear>Ν</unclear>
                        </seg> τὰν <seg type="clearword">μετα</seg></seg>
                    <seg n="15" type="line" facs="z61r2_15"><seg type="wordend">ξὺ</seg> τᾶς ἐν τῶι
                            <seg type="word">
                            <unclear>κ</unclear>
                            <supplied reason="lost">ύκλωι</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">εὐθεί</supplied>ας</seg> καὶ</seg>
                    <seg n="16" type="line" facs="z61r2_16">τᾶς περιφερείας <seg type="word">
                            <supplied reason="lost">πο</supplied><unclear>τ</unclear>ὶ</seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ὰν</seg> ΘΠ <choice>
                            <abbr>τὰ</abbr>
                            <expan>τὰν</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z61r2_17">ἀπολαφθεῖσαν ἀπὸ τᾶς <sic>
                            <seg type="unclearword">ἐπιψαύ</seg>
                        </sic></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
