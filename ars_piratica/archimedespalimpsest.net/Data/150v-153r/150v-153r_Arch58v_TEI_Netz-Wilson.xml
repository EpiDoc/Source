    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="150v1" type="folio">
                    <seg n="1" type="line" facs="z150v1_01">εἰ γὰρ δυνατόν, ἔστω· καὶ ὁμοίως</seg>
                    <seg n="2" type="line" facs="z150v1_02"><seg type="word"><supplied reason="lost"
                                >εὑρήσ</supplied>θωσαν</seg> αἱ Β, Γ εὐθεῖαι ὥστε</seg>
                    <seg n="3" type="line" facs="z150v1_03">τὴν Β <expan>πρὸς</expan> Γ ἐλάσσονα
                        λόγον ἔχειν</seg>
                    <seg n="4" type="line" facs="z150v1_04">τοῦ ὃν ἔχει ὁ Α κύκλος
                        <expan>πρὸς</expan> τὴν <w part="I">ἐπιφά</w></seg>
                    <seg n="5" type="line" facs="z150v1_05"><w part="F">νειαν</w> τῆς σφαίρας, καὶ
                        τῶν Β, Γ <w part="I">μέ</w></seg>
                    <seg n="6" type="line" facs="z150v1_06"><w part="F">ση</w> ἀνάλογον ἡ Δ, καὶ
                        ἐγγεγράφθω</seg>
                    <seg n="7" type="line" facs="z150v1_07"><expan>καὶ</expan> περιγεγράφθω πάλιν,
                        πότε <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z150v1_08">τοῦ περιγεγραμμένου ἐλάσσονα</seg>
                    <seg n="9" type="line" facs="z150v1_09">λόγον ἔχειν τοῦ τῆς Β πρὸς Δ
                        <expan>καὶ</expan></seg>
                    <seg n="10" type="line" facs="z150v1_10">τὰ διπλάσια ἄρα· ἡ ἐπιφάνεια </seg>
                    <seg n="11" type="line" facs="z150v1_11">ἄρα τοῦ περιγεγραμμένου πρὸς</seg>
                    <seg n="12" type="line" facs="z150v1_12">τὴν ἐπιφάνειαν τοῦ <w part="I"
                            >ἐγγεγραμμέ</w></seg>
                    <seg n="13" type="line" facs="z150v1_13"><w part="F">νου</w> ἐλάσσονα λόγον ἔχει <choice>
                            <abbr>η</abbr>
                            <expan>ἤπερ</expan>
                        </choice> ἡ Β</seg>
                    <seg n="14" type="line" facs="z150v1_14"><expan>πρὸς</expan> Γ. ἠ δὲ Β
                            <expan>πρὸς</expan> Γ ἐλάσσονα λόγον <w part="I">ἔ</w></seg>
                    <seg n="15" type="line" facs="z150v1_15"><w part="F">χει</w>
                        <choice>
                            <abbr>η</abbr>
                            <expan>ἤπερ</expan>
                        </choice> ὁ Α κύκλος <expan>πρὸς</expan> τὴν <w part="I">ἐπι</w></seg>
                    <seg n="16" type="line" facs="z150v1_16"><w part="F">φάνειαν</w> τῆς σφαίρας· <choice>
                            <abbr>ο</abbr>
                            <expan>ὅπερ</expan>
                        </choice>
                        <w part="I">ἄτο</w></seg>
                    <seg n="17" type="line" facs="z150v1_17"><w part="F">πον</w>· ἡ μὲν γὰρ τοῦ <w
                            part="I">περιγεγραμ</w></seg>
                    <seg n="18" type="line" facs="z150v1_18"><w part="F">μένου</w> ἐπιφάνεια μείζων
                            <expan>ἐστὶ</expan> τοῦ Α</seg>
                </seg>
                <seg n="153r1" type="folio">
                    <seg n="1" type="line" facs="z153r1_01">κύκλου, ἡ δὲ τοῦ ἐγγεγραμμένου <w
                            part="I">ἐ</w></seg>
                    <seg n="2" type="line" facs="z153r1_02"><w part="F">λάσσων</w> τῆς ἐπιφανείας
                        τῆς <seg type="expandedword">
                            <choice>
                                <expan>σφαίρας</expan>
                                <abbr>σφ</abbr>
                            </choice>
                        </seg></seg>
                    <seg n="3" type="line" facs="z153r1_03"><seg type="wordend">
                            <abbr>ρας</abbr>
                        </seg>.</seg>
                </seg>
                <seg n="153r1" type="folio">
                    <seg n="3" type="line" facs="z153r1_03">οὐκ <expan>ἄρα</expan> οὐδὲ ἐλάσσων ἡ <w
                            part="I">ἐπιφά</w></seg>
                    <seg n="4" type="line" facs="z153r1_04"><w part="F">νεια</w> τῆς σφαίρας τοῦ Α
                        κύκλου.</seg>
                    <seg n="5" type="line" facs="z153r1_05">ἐδείχθη δὲ <expan>ὅτι</expan> οὐδὲ
                        μείζων· ἡ <expan>ἄρα</expan>
                        <w part="I">ἐπιφά</w></seg>
                    <seg n="6" type="line" facs="z153r1_06"><w part="F">νεια</w> τῆς σφαίρας ἴση
                            <expan>ἐστὶ</expan> τῶι Α <w part="I">κύ</w></seg>
                    <seg n="7" type="line" facs="z153r1_07"><w part="F">κλωι</w>, <choice>
                            <abbr>τουτ</abbr>
                            <expan>τουτέστι</expan>
                        </choice> τῶι τετραπλασίωι <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z153r1_08">μεγίστου κύκλου.</seg>
                    <figure n="1.33.2">
                        <figDesc>Figure 1.33.2</figDesc>
                    </figure>
                </seg>
                <seg n="150v2" type="folio">
                    <seg n="1" type="line" facs="z150v2_01">πᾶσα σφαῖρα <choice>
                            <abbr>τεπλασια</abbr>
                            <expan>τετραπλασία</expan>
                        </choice> ἐστὶ <choice>
                            <abbr>κων</abbr>
                            <expan>κώνου</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z150v2_02">τοῦ βάσιν μὲν ἔχοντος ἴσην τῶι</seg>
                    <seg n="3" type="line" facs="z150v2_03">μεγίστω κύκλωι τῶν ἐν τῆι <w part="I"
                            >σφαί</w></seg>
                    <seg n="4" type="line" facs="z150v2_04"><w part="F">ραι</w>, ὕψος δὲ τὴν ἐκ τοῦ
                        κέντρου <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z150v2_05">σφαίρας.</seg>
                </seg>
                <seg n="150v2" type="folio">
                    <seg n="5" type="line" facs="z150v2_05">ἔστω γὰρ σφαῖρά τις <expan>καὶ</expan></seg>
                    <seg n="6" type="line" facs="z150v2_06">ἐν αὐτῆι μέγιστος κύκλος ὁ ΑΒ ΓΔ.</seg>
                    <seg n="7" type="line" facs="z150v2_07">εἰ οὖν μή <expan>ἐστιν</expan> ἡ σφαῖρα
                        τετραπλασία</seg>
                    <seg n="8" type="line" facs="z150v2_08">τοῦ εἰρημένου κώνου, ἔστω, εἰ <w
                            part="I">δυ</w></seg>
                    <seg n="9" type="line" facs="z150v2_09"><w part="F">νατόν</w>, μείζων ἢ
                        τετραπλασία·</seg>
                    <seg n="10" type="line" facs="z150v2_10">ἔστω δὲ ὁ Ξ κῶνος βάσιν μὲν <w part="I"
                            >ἔ</w></seg>
                    <seg n="11" type="line" facs="z150v2_11"><w part="F">χων</w> τετραπλασίαν τοῦ
                        ΑΒΓΔ</seg>
                    <seg n="12" type="line" facs="z150v2_12">κύκλου, ὕψος δὲ ἴσον τῆι ἐκ τοῦ <seg
                            type="expandedword">
                            <choice>
                                <abbr>κε</abbr>
                                <expan>κέντρου</expan>
                            </choice>
                        </seg></seg>
                    <seg n="13" type="line" facs="z150v2_13"><seg type="wordend">
                            <abbr>τρου</abbr>
                        </seg> τῆς σφαίρας· μείζων οὖν</seg>
                    <seg n="14" type="line" facs="z150v2_14"><expan>ἐστιν</expan> ἡ σφαῖρα τοῦ Ξ
                        κώνου. Ἔσται δὴ</seg>
                    <seg n="15" type="line" facs="z150v2_15">δύο μεγέθη ἄνισα ἥ τε σφαῖρα</seg>
                    <seg n="16" type="line" facs="z150v2_16"><expan>καὶ</expan> ὁ κῶνος· δυνατὸν οὖν
                        δύο <choice>
                            <abbr>ευθει</abbr>
                            <expan>εὐθείας</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z150v2_17">λαβεῖν ἀνίσους, ὥστε ἔχειν τὴν</seg>
                    <seg n="18" type="line" facs="z150v2_18">μείζονα <expan>πρὸς</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice>
                        <seg type="word">ἐλάσ<supplied reason="lost">σο</supplied>να</seg>
                        <w part="I">ἐλάσσο</w></seg>
                </seg>
                <seg n="153r2" type="folio">
                    <seg n="1" type="line" facs="z153r2_01"><w part="F">να</w> λόγον τοῦ ὃν ἔχει ἡ
                        σφαῖρα</seg>
                    <seg n="2" type="line" facs="z153r2_02">πρὸς τὸν Ξ κῶνον ἔστωσαν οὖν</seg>
                    <seg n="3" type="line" facs="z153r2_03">αἱ ΚΗ, αἱ δὲ ΙΘ εἰλημμέναι, <w part="I"
                            >ὥσ</w></seg>
                    <seg n="4" type="line" facs="z153r2_04"><w part="F">τε</w> τῶι ἴσωι ἀλλήλων
                        ὑπερέχειν</seg>
                    <seg n="5" type="line" facs="z153r2_05">τὴν Κ τῆς Ι καὶ τὴν Ι τῆς Θ
                        <expan>καὶ</expan></seg>
                    <seg n="6" type="line" facs="z153r2_06">τὴν Θ τῆς Η, νοείσθω δὲ
                        <expan>καὶ</expan> εἰς τὸν Α</seg>
                    <seg n="7" type="line" facs="z153r2_07">ΒΓΔ κύκλον ἐγγεγραμμένον <w part="I"
                        >πο</w></seg>
                    <seg n="8" type="line" facs="z153r2_08"><w part="F">λύγωνον</w>, οὗ τὸ πλῆθος
                        τῶν <w part="I">πλευ</w></seg>
                    <seg n="9" type="line" facs="z153r2_09"><w part="F">ρῶν</w> μετρείσθω ὑπὸ
                        τετράδος, <expan>καὶ</expan></seg>
                    <seg n="10" type="line" facs="z153r2_10">ἄλλο περιγεγραμμένον ὅμοιον</seg>
                    <seg n="11" type="line" facs="z153r2_11">τῶι ἐγγεγραμμένωι, καθάπερ</seg>
                    <seg n="12" type="line" facs="z153r2_12">ἐπὶ τῶν πρότερον, ἡ δὲ τοῦ <w part="I"
                            >περιγε</w></seg>
                    <seg n="13" type="line" facs="z153r2_13"><w part="F">γραμμένου</w> πολυγώνου
                        πλευρὰ</seg>
                    <seg n="14" type="line" facs="z153r2_14"><expan>πρὸς</expan> τὴν τοῦ
                        ἐγγεγραμμένου <w part="I">ἐλάσ</w></seg>
                    <seg n="15" type="line" facs="z153r2_15"><w part="F">σονα</w> λόγον ἐχέτω τοῦ ὃν
                        ἔχει ἡ</seg>
                    <seg n="16" type="line" facs="z153r2_16">Κ <expan>πρὸς</expan> Ι, καὶ ἔστωσαν αἱ
                        ΑΒΓΔ <expan>διὰ</expan>
                    </seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
