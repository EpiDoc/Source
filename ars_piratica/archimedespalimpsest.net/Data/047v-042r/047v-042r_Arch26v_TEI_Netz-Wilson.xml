    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="47v1" type="folio">
                    <seg n="1" type="line" facs="z47v1_01">ἄρα τὰ παραλληλόγραμμα τὰ <w part="I"
                            >γενό</w></seg>
                    <seg n="2" type="line" facs="z47v1_02"><w part="F">μενα</w> ἐν τῶι <seg
                            type="word">ἡμικυλινδρί<unclear>ω</unclear><supplied reason="lost"
                            >ι</supplied></seg>
                        <seg type="word"><supplied reason="lost">αὐ</supplied>τοῦ</seg></seg>
                    <seg n="3" type="line" facs="z47v1_03">μένοντα <seg type="word">ἰσορρο<supplied
                                reason="lost">π</supplied>ήσει</seg> περὶ τὸ</seg>
                    <seg n="4" type="line" facs="z47v1_04">Θ σημεῖον πᾶσι τοῖς <w part="I"
                        >παραλληλο</w></seg>
                    <seg n="5" type="line" facs="z47v1_05"><w part="F">γράμμοις</w> τοῖς γενομένοις
                        ἐν</seg>
                    <seg n="6" type="line" facs="z47v1_06">τῶι τμήματι τῶι ἀποτμηθέντι</seg>
                    <seg n="7" type="line" facs="z47v1_07">ἀπὸ τοῦ <seg type="word"
                            ><unclear>κυ</unclear>λίνδρου</seg>
                        <w part="I">μετενηνεγμέ</w></seg>
                    <seg n="8" type="line" facs="z47v1_08"><w part="F">νους</w> κειμένους <seg
                            type="word">το<supplied reason="lost">ῦ</supplied></seg> ζυγοῦ κατὰ</seg>
                    <seg n="9" type="line" facs="z47v1_09"><seg type="word"><supplied reason="lost"
                                >τ</supplied>ὸ</seg> Ξ σημεῖον <seg type="word"
                            >ἰσορροπ<unclear>εῖ</unclear>ν</seg> καὶ τὸ <w part="I">ἡ</w></seg>
                    <seg n="10" type="line" facs="z47v1_10"><w part="F">μικυλίνδριον</w> αὐτοῦ μένον
                            <seg type="word">περ<supplied reason="lost">ὶ</supplied></seg></seg>
                    <seg n="11" type="line" facs="z47v1_11">τὸ Θ σημεῖον τῶι τμήματι τῶι <w part="I"
                            >ἀ</w></seg>
                    <seg n="12" type="line" facs="z47v1_12"><w part="F">ποτμηθέντι</w> καὶ τεθέντι
                        τοῦ <choice>
                            <abbr>ζυγ</abbr>
                            <expan>ζυγοῦ</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z47v1_13">κατὰ τὸ Ξ οὕτως, ὥστε κέντρον
                            <expan>εἶναι</expan></seg>
                    <seg n="14" type="line" facs="z47v1_14">αὐτοῦ τοῦ βάρους τὸ Ξ σημεῖον.</seg>
                    <figure n="12.1">
                        <figDesc xml:lang="eng">Figure 12.1</figDesc>
                    </figure>
                </seg>
                <seg n="42r1" type="folio">
                    <seg n="1" type="line" facs="z42r1_01">Ἔστω δὴ πάλιν τὸ περὶ <seg type="word"
                                ><supplied reason="lost">μ</supplied>έσον</seg> τὸν <w part="I"
                        >ἄ</w></seg>
                    <seg n="2" type="line" facs="z42r1_02"><w part="F">ξονα</w> παραλληλόγραμμον τὸ
                            Μ<unclear>Ν</unclear></seg>
                    <seg n="3" type="line" facs="z42r1_03">καὶ ὁ κύκλος ὁ ΞΟ ΠΡ, <seg type="word"
                                ><supplied reason="lost">κ</supplied>αὶ</seg>
                        <seg type="suppliedword">ἐπεζεύχ<supplied reason="lost">θω</supplied></seg></seg>
                    <seg n="4" type="line" facs="z42r1_04"><seg type="wordend">σαν</seg> αἱ ΘΗ ΘΜ
                        καὶ <seg type="word"><unclear>ἀ</unclear>νεστάτω</seg> ἐπὶ</seg>
                    <seg n="5" type="line" facs="z42r1_05">αὐτῶν ἐπίπεδα ὀρθὰ <expan>πρὸς</expan> τὸ
                            <w part="I">ἐπί</w></seg>
                    <seg n="6" type="line" facs="z42r1_06"><w part="F">πεδον</w>, ἐν ὧι
                        <expan>ἐστιν</expan> τὸ ΞΟ ΠΡ κύκλος, <expan>καὶ</expan></seg>
                </seg>
                <seg n="47v2" type="folio">
                    <seg n="1" type="line" facs="z47v2_01">ἐκβεβλήσθω ἐφ᾽ ἑκάτερα τὰ</seg>
                    <seg n="2" type="line" facs="z47v2_02">εἰρημένα ἐπίπεδα· <seg type="word"
                                >ἔσ<supplied reason="lost">τ</supplied>αι</seg> δή τι</seg>
                    <seg n="3" type="line" facs="z47v2_03">πρίσμα βάσιν μὲν <seg type="word"
                                ><supplied reason="lost">ἔ</supplied>χον</seg>
                        <w part="I">τηλικαύ</w></seg>
                    <seg n="4" type="line" facs="z47v2_04"><w part="F">την</w>, ἡλίκη
                        <expan>ἐστὶ</expan> τὸ ΘΜΗ τρίγωνον, <w part="I">ὕ</w></seg>
                    <seg n="5" type="line" facs="z47v2_05"><w part="F">ψος</w> δὲ ἴσον τῶι ἄξονι τοῦ
                            <w part="I">κυλίν</w></seg>
                    <seg n="6" type="line" facs="z47v2_06"><w part="F">δρου</w>, καί
                        <expan>ἐστι</expan> τὸ πρίσμα τοῦτο <choice>
                            <abbr>τέταρ</abbr>
                            <expan>τέταρτον</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z47v2_07">μέρος τοῦ ὅλου πρίσματος <seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ο</unclear>
                            <supplied reason="lost">ῦ</supplied>
                        </seg></seg>
                    <seg n="8" type="line" facs="z47v2_08">περὶ ὅλον τὸν κύλινδρον. <choice>
                            <abbr>ἤχθωσ</abbr>
                            <expan>ἤχθωσαν</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z47v2_09">δέ τινες εὐθεῖαι ἐν τῶι ΟΠΡ <seg
                            type="suppliedword">ἡμι<supplied reason="lost">κυ</supplied></seg></seg>
                    <seg n="10" type="line" facs="z47v2_10"><seg type="wordend">κλίω</seg> καὶ ἐν
                        τῶι ΜΝ <seg type="word">τετραγώνω<unclear>ι</unclear></seg></seg>
                    <seg n="11" type="line" facs="z47v2_11">αἱ ΚΛ ΤΥ ἴσον ἀπέχουσαι <seg type="word"
                                ><unclear>τ</unclear>ῆι</seg></seg>
                    <seg n="12" type="line" facs="z47v2_12">ΣΠΞ· τέμνουσιν δὴ αὗται τὴν <choice>
                            <abbr>μ</abbr>
                            <expan>μὲν</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z47v2_13">τοῦ ΟΠΡ ἡμικυκλίου <seg type="word">
                            <choice>
                                <abbr>περιφ<unclear>έρ</unclear>ει<unclear>α</unclear></abbr>
                                <expan>περιφ<unclear>έρ</unclear>ει<unclear>α</unclear>ν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="14" type="line" facs="z47v2_14">κατὰ τὰ ΚΤ σημεῖα, τὴν δὲ ΟΡ</seg>
                    <seg n="15" type="line" facs="z47v2_15"><choice>
                            <abbr>μετρον</abbr>
                            <expan>διάμετρον</expan>
                        </choice> κατὰ τὰ ΕΖ, τὰς δὲ ΘΗ</seg>
                    <seg n="16" type="line" facs="z47v2_16">ΘΜ κατὰ τὰ ΦΧ, καὶ ἀνεστάτω <w part="I"
                            >ἀ</w></seg>
                    <seg n="17" type="line" facs="z47v2_17"><w part="F">πὸ</w> τῶν ΚΛ ΤΥ ἐπίπεδα
                        ὀρθὰ</seg>
                    <seg n="18" type="line" facs="z47v2_18"><expan>πρὸς</expan> τὴν ΟΡ καὶ
                        ἐκβεβλήσθω ἐφ᾽ <w part="I">ἑ</w></seg>
                    <seg n="19" type="line" facs="z47v2_19"><w part="F">κάτερα</w> τοῦ ἐπιπέδου, ἐν
                        ὧι <choice>
                            <abbr>ἐστι</abbr>
                            <expan>ἐστιν</expan>
                        </choice></seg>
                </seg>
                <seg n="42r2" type="folio">
                    <seg n="1" type="line" facs="z42r2_01">ὁ ΟΞ ΠΡ <seg type="word">
                            <choice>
                                <abbr><unclear>κ</unclear><supplied reason="lost">ύκ</supplied>λ</abbr>
                                <expan><unclear>κ</unclear><supplied reason="lost"
                                >ύκ</supplied>λος</expan>
                            </choice>
                        </seg>· ποιοῦσι δὴ τὸ αὗται ἐν</seg>
                    <seg n="2" type="line" facs="z42r2_02"><seg type="word"><supplied reason="lost"
                                >μ</supplied>ὲν</seg> τῶι ἡμικυλινδρίωι, οὗ βάσις</seg>
                    <seg n="3" type="line" facs="z42r2_03">μέν ἐστιν τὸ ΟΠΡ ἡμικύκλιον, <choice>
                            <abbr>ὕψ</abbr>
                            <expan>ὕψος</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z42r2_04">δὲ τὸ αὐτὸ τῶι κυλίνδρω, τομὴν <seg
                            type="suppliedword">πα</seg></seg>
                    <seg n="5" type="line" facs="z42r2_05"><seg type="wordend"><supplied
                                reason="lost">ρ</supplied>αλληλό<unclear>γ</unclear>ραμμον</seg>, οὗ
                        ἔσται μία μὲν</seg>
                    <seg n="6" type="line" facs="z42r2_06">πλευρὰ ἴση τῆι Κ<unclear>Ε</unclear>, ἡ
                        δὲ ἑτέρα</seg>
                    <seg n="7" type="line" facs="z42r2_07">ἴση τῶι ἄξονι τοῦ κυλίνδρου, ἐν</seg>
                    <seg n="8" type="line" facs="z42r2_08">δὲ τῶι πρίσματι τῶι ΘΗ ΝΜ <w part="I"
                            >ὁμοί</w></seg>
                    <seg n="9" type="line" facs="z42r2_09"><w part="F">ως</w> παραλληλόγραμμον, οὗ
                        ἔσται</seg>
                    <seg n="10" type="line" facs="z42r2_10">μία μὲν ἴση τῆι Χ, ἡ δὲ ἑτέρα ἴση</seg>
                    <seg n="11" type="line" facs="z42r2_11">τῶι ἄξονι· διὰ τὰ <seg type="word"
                                >α<supplied reason="lost">ὐ</supplied>τὰ</seg>
                        <seg type="word"><supplied reason="lost">τ</supplied>ῶ</seg> ἐν τῶι</seg>
                    <seg n="12" type="line" facs="z42r2_12">αὐτῶι <seg type="word">ἡμ<supplied
                                reason="lost">ι</supplied>κυλινδρίωι</seg> ἔσται τὸ <seg type="word">
                            <choice>
                                <abbr>μ</abbr>
                                <expan>μ<supplied reason="lost">ὲν</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="13" type="line" facs="z42r2_13">παραλληλόγραμμον, οὗ ἔσται <w part="I"
                            >μί</w></seg>
                    <seg n="14" type="line" facs="z42r2_14"><w part="F">α</w> μὲν <seg type="word"
                                >πλ<supplied reason="lost">ευ</supplied><unclear>ρ</unclear>ὰ</seg>
                        <sic>ἴσηι</sic> τῆι ΤΖ, ἡ δὲ</seg>
                    <seg n="15" type="line" facs="z42r2_15">ἑτέρα ἴση τῶι <seg type="word"
                                >ἄξο<supplied reason="lost">νι</supplied></seg>
                        <seg type="word"><supplied reason="lost">τ</supplied>ο<supplied
                                reason="lost">ῦ</supplied></seg>
                        <w part="I">κυλίν</w></seg>
                    <seg n="16" type="line" facs="z42r2_16"><w part="F">δρου</w>, ἐν δὲ τῶι <seg
                            type="word">π<supplied reason="lost"
                                >ρίσ</supplied><unclear>μ</unclear>α<supplied reason="lost"
                                >τ</supplied><unclear>ι</unclear></seg>
                        <seg type="word"><unclear>μί</unclear>α</seg>
                        <choice>
                            <abbr>μ</abbr>
                            <expan>μὲν</expan>
                        </choice>
                    </seg>
                    <seg n="17" type="line" facs="z42r2_17">πλευρὰ ἴση <seg type="word">τ<supplied
                                reason="lost">ῆι</supplied></seg> ΦΥ, ἡ <seg type="word"><supplied
                                reason="lost">δ</supplied>ὲ</seg> ἑτέρα</seg>
                    <seg n="18" type="line" facs="z42r2_18">ἴση τῶι <seg type="word">ἄξον<supplied
                                reason="lost">ι</supplied></seg> τοῦ κυλίνδρου
                    <expan>καὶ</expan></seg>
                </seg>
            </ab>
