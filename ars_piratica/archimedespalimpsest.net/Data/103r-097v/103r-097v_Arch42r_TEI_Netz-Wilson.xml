    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="103r1" type="folio">
                    <seg n="1" type="line">
                        <supplied reason="lost">αἱ</supplied>
                        <supplied reason="lost">ἀπὸ</supplied>
                        <supplied reason="lost">τοῦ</supplied>
                        <supplied reason="lost">Θ</supplied>
                        <supplied reason="lost">ποτὶ</supplied>
                        <supplied reason="lost">τὰν</supplied>
                        <supplied reason="lost">ἕλικα</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">ποτι</supplied>
                        </seg>
                    </seg>
                    <seg n="2" type="line">
                        <seg type="wordend">
                            <supplied reason="lost">πίπτουσαι,</supplied>
                        </seg>
                        <supplied reason="lost">ἇν</supplied>
                        <supplied reason="lost">ἐστι</supplied>
                        <supplied reason="lost">μεγίστα</supplied>
                        <supplied reason="lost">μὲν</supplied>
                    </seg>
                    <seg n="3" type="line" facs="z103r1_03"><sic>ω</sic> ΘΑ, ἐλαχίστα δὲ
                        <unclear>ἁ</unclear> ΘΕ, ἐντὶ δὲ</seg>
                    <seg n="4" type="line" facs="z103r1_04">καὶ <seg type="word">
                            <unclear>ἄλ</unclear>λαι</seg> γραμμαὶ αἱ ἀπὸ τοῦ</seg>
                    <seg n="5" type="line" facs="z103r1_05">Θ ἐπὶ τὰν τοῦ ΑΖΗΙ κύκλου <seg
                            type="clearword">πε</seg></seg>
                    <seg n="6" type="line" facs="z103r1_06"><seg type="wordend">ριφέρειαν</seg>
                        <seg type="word">ποτιπίπ<supplied reason="lost">τ</supplied>ουσαι </seg>,
                        τῶι</seg>
                    <seg n="7" type="line" facs="z103r1_07"><seg type="word">
                            <supplied reason="lost">μὲ</supplied>ν</seg> πλήθει μιᾶι ἔλασσον <seg
                            type="clearword">ἑαυ</seg></seg>
                    <seg n="8" type="line" facs="z103r1_08"><seg type="wordend">τᾶν</seg>, τῶι δὲ
                        μεγέθει ἀλλάλαις τε</seg>
                    <seg n="9" type="line" facs="z103r1_09">ἴσαι καὶ τᾶ <sic>μὲν στα</sic>, καὶ <seg
                            type="clearword">ἀνα</seg></seg>
                    <seg n="10" type="line" facs="z103r1_10"><seg type="wordend">γεγράφαται</seg>
                        ὁμοῖοι τομέες <seg type="clearword">ἀ</seg></seg>
                    <seg n="11" type="line" facs="z103r1_11"><seg type="wordend">πὸ</seg> τᾶν ἰσᾶν
                        τᾶ μεγίστα καὶ <seg type="clearword">ἀ</seg></seg>
                    <seg n="12" type="line" facs="z103r1_12"><seg type="wordend">πὸ</seg> τᾶν τῶι
                        ἴσωι ἀλλαλᾶν <seg type="clearword">ὑπε</seg></seg>
                    <seg n="13" type="line" facs="z103r1_13"><seg type="wordend">ρεχουσᾶν</seg>, ἀπὸ
                        δὲ τᾶς <choice>
                            <abbr>ἐλαχίστ</abbr>
                            <expan>ἐλαχίστας</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z103r1_14">οὐκ <sic>ἀναγεγράφεται</sic>· <seg
                            type="word"> ο<supplied reason="lost">ἱ</supplied>
                        </seg>
                        <supplied reason="lost">ἄρα</supplied></seg>
                    <seg n="15" type="line" facs="z103r1_15">τομέες οἱ ἀπὸ τᾶν <seg type="word">
                                ἰ<unclear>σ</unclear><supplied reason="lost">ᾶν</supplied>
                        </seg>
                        <supplied reason="lost">τᾶ</supplied></seg>
                    <seg n="16" type="line" facs="z103r1_16">μεγίστα ποτὶ τοὺς <seg type="word"
                                >το<supplied reason="lost">μέας</supplied>
                        </seg></seg>
                    <seg n="17" type="line" facs="z103r1_17">τοὺς ἀπὸ τᾶν τῶι ἴσωι <seg type="word">
                                ἀλλαλ<unclear>ᾶ</unclear><supplied reason="lost">ν</supplied>
                        </seg></seg>
                    <seg n="18" type="line" facs="z103r1_18">ὑπερεχουσᾶν χωρὶς τοῦ ἀπὸ</seg>
                    <seg n="19" type="line" facs="z103r1_19">
                        <supplied reason="lost">τᾶς</supplied>
                        <seg type="word">
                            <supplied reason="lost">ἐλαχίστα</supplied>ς</seg>
                        <seg type="word">
                            <supplied reason="lost">ἐ</supplied>λ<unclear>άσ</unclear>σονα</seg>
                        <seg type="clearword">λό</seg>
                    </seg>
                </seg>
                <seg n="97v1" type="folio">
                    <seg n="1" type="line" facs="z97v1_01"><seg type="wordend">γον</seg> ἔχοντι ἢ τὸ
                        τετράγωνον τὸ</seg>
                    <seg n="2" type="line" facs="z97v1_02">ἀπὸ τᾶς μεγίστας τᾶς ΘΑ <seg
                            type="clearword">πο</seg></seg>
                    <seg n="3" type="line" facs="z97v1_03"><seg type="wordend">τὶ</seg> τὰ
                        συναμφότερα τό τε ὑπὸ</seg>
                    <seg n="4" type="line" facs="z97v1_04">τᾶν ΑΘ ΘΕ περιεχόμενον <expan>καὶ</expan></seg>
                    <seg n="5" type="line" facs="z97v1_05">τὸ τρίτον μέρος τοῦ ἀπὸ τᾶς</seg>
                    <seg n="6" type="line" facs="z97v1_06">Ε<unclear>Α</unclear> τετραγώνου·
                        δέδεικται γὰρ</seg>
                    <seg n="7" type="line" facs="z97v1_07">τοῦτο. ἐντὶ τοῖς μὲν τομέσι τοῖς</seg>
                    <seg n="8" type="line" facs="z97v1_08">ἀπὸ τᾶν ἰσᾶν ἀλλάλας καὶ</seg>
                    <seg n="9" type="line" facs="z97v1_09">τᾶ μεγίστα ἴσος ὁ ΑΖΗ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice>,</seg>
                    <seg n="10" type="line" facs="z97v1_10">τοῖς δὲ τομέεσσι τοῖς ἀπὸ τᾶν τῶι</seg>
                    <seg n="11" type="line" facs="z97v1_11">ἴσωι ἀλλαλᾶν ὑπερεχουσᾶν <seg
                            type="unclearword">χ<unclear>ω</unclear></seg></seg>
                    <seg n="12" type="line" facs="z97v1_12"><seg type="wordend"
                            >ρ<unclear>ὶς</unclear></seg> τοῦ ἀπὸ τᾶς ἐλαχίστας <seg type="word">
                            <choice>
                                <abbr>ἴσ</abbr>
                                <expan>ἴσ<supplied reason="lost">ον</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="13" type="line" facs="z97v1_13">τὸ περιγεγραμμένον· <choice>
                            <abbr>ἐλάσσον</abbr>
                            <expan>ἐλάσσονα</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z97v1_14">ἄρα λόγον ἔχει ὁ κύκλος ποτὶ</seg>
                    <seg n="15" type="line" facs="z97v1_15">τὸ περιγεγραμμένον σχᾶμα</seg>
                    <seg n="16" type="line" facs="z97v1_16">
                        <supplied reason="lost">ἢ</supplied>
                        <supplied reason="lost">τὸ</supplied>
                        <supplied reason="lost">τετράγωνον</supplied>
                        <supplied reason="lost">τὸ</supplied>
                        <supplied reason="lost">ἀπὸ</supplied>
                        <supplied reason="lost">τᾶς</supplied>
                        <supplied reason="lost">ΑΘ</supplied>
                    </seg>
                </seg>
                <seg n="103r2" type="folio">
                    <seg n="1" type="line">
                        <supplied reason="lost">ποτὶ</supplied>
                        <supplied reason="lost">τὰ</supplied>
                        <supplied reason="lost">συναμφότερα</supplied>
                        <supplied reason="lost">τό</supplied>
                        <supplied reason="lost">τε</supplied>
                    </seg>
                    <seg n="2" type="line" facs="z103r2_02">
                        <supplied reason="lost">ὑπὸ</supplied>
                        <supplied reason="lost">τᾶν</supplied>
                        <supplied reason="lost">ΑΘ</supplied>
                        <supplied reason="lost">ΘΕ</supplied>
                        <supplied reason="lost">καὶ</supplied>
                        <supplied reason="lost">τὸ</supplied>
                        <supplied reason="lost">τρίτον</supplied>
                        <supplied reason="lost">μέρος</supplied>
                    </seg>
                    <seg n="3" type="line" facs="z103r2_03"><seg type="word">το<supplied
                                reason="lost">ῦ</supplied>
                        </seg> ἀπὸ τᾶς ΑΒ τετραγώνου. ὃν δὲ</seg>
                    <seg n="4" type="line" facs="z103r2_04">ἔχει λόγον τὸ τετράγωνον τὸ <seg
                            type="word"> ἀπ<supplied reason="lost">ὸ</supplied>
                        </seg></seg>
                    <seg n="5" type="line" facs="z103r2_05">τᾶς ΘΑ ποτὶ τὸ ὑπὸ τᾶν ΘΑ ΑΕ <seg
                            type="word">
                            <unclear>κ</unclear>
                            <supplied reason="lost">αὶ</supplied>
                        </seg></seg>
                    <seg n="6" type="line" facs="z103r2_06">τὸ τρίτον μέρος <seg type="word">
                            <unclear>τ</unclear>οῦ</seg> ἀπὸ τᾶς ΑΕ <seg type="suppliedword">
                            <unclear>τ</unclear>
                            <supplied reason="lost">ε</supplied>
                        </seg></seg>
                    <seg n="7" type="line" facs="z103r2_07"><seg type="wordend">τραγώνου</seg>,
                        τοῦτον ἔχει ὁ ΑΖΗΙ <seg type="word">κύκλ<supplied reason="lost"
                            >ος</supplied>
                        </seg></seg>
                    <seg n="8" type="line" facs="z103r2_08"><expan>πρὸς</expan> τὸν Ϟ κύκλον·
                        ἐλάσσονα οὖν <seg type="word">λόγ<supplied reason="lost">ον</supplied>
                        </seg></seg>
                    <seg n="9" type="line" facs="z103r2_09">ἔχει ὁ ΑΖΗΙ κύκλος ποτὶ τὸ <seg
                            type="clearword">περι</seg></seg>
                    <seg n="10" type="line" facs="z103r2_10"><seg type="wordend">γεγραμμένον</seg>
                        σχᾶμα ἢ ποτὶ τὸν</seg>
                    <seg n="11" type="line" facs="z103r2_11">κύκλον· ὥστε ἐλάσσων ἐστὶν ὁ Ϟ
                            <supplied reason="lost">κύκλος</supplied></seg>
                    <seg n="12" type="line" facs="z103r2_12">τοῦ <seg type="word">
                                περιγεγρα<unclear>μ</unclear>μένου</seg>
                        <seg type="word">σχάματο<supplied reason="lost">ς.</supplied>
                        </seg></seg>
                    <seg n="13" type="line" facs="z103r2_13">οὐκ ἔστι δέ, ἀλλὰ μείζων· οὐκ
                            <expan>ἄρα</expan>
                        <seg type="word">μ<supplied reason="lost">είζων</supplied>
                        </seg></seg>
                    <seg n="14" type="line" facs="z103r2_14">ἐστὶν ὁ Ϟ κύκλος <seg type="word">
                                το<unclear>ῦ</unclear>
                        </seg>
                        <seg type="word">χωρίο<unclear>υ</unclear>
                        </seg> τοῦ <seg type="suppliedword">
                            <supplied reason="lost">πε</supplied>
                        </seg></seg>
                    <seg n="15" type="line" facs="z103r2_15"><seg type="wordend">ριεχομένου</seg>
                        <seg type="word">
                            <unclear>ὑ</unclear>πό</seg> τε τᾶς ΑΒΓΔΕ <seg type="suppliedword">
                            <supplied reason="lost">ἕλι</supplied>
                        </seg></seg>
                    <seg n="16" type="line" facs="z103r2_16"><seg type="wordend">
                            <choice>
                                <abbr>κ</abbr>
                                <expan>κος</expan>
                            </choice>
                        </seg> καὶ τᾶς ΑΕ εὐθείας.</seg>
                </seg>
                <seg n="103r2" type="folio">
                    <seg n="17" type="line" facs="z103r2_17">Οὐδὲ τοίνυν <seg type="word">
                                ἐλά<unclear>σσων</unclear>
                        </seg>. <seg type="word">
                            <unclear>ἔ</unclear>
                            <supplied reason="lost">στω</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">γ</supplied>άρ</seg>, εἰ <seg
                            type="suppliedword">δ<supplied reason="lost">υ</supplied></seg></seg>
                    <seg n="18" type="line" facs="z103r2_18"><seg type="wordend">νατόν</seg>,
                        ἐλάσσων. πάλιν <seg type="word">ο<supplied reason="lost">ὖν</supplied>
                        </seg>
                        <seg type="suppliedword">δ<supplied reason="lost">υ</supplied></seg></seg>
                </seg>
                <seg n="97v2" type="folio">
                    <seg n="1" type="line" facs="z97v2_01"><seg type="wordend"
                            ><unclear>να</unclear>τόν</seg>
                        <expan>ἐστιν</expan> εἰς τὸ χωρίον <seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ὸ</unclear>
                        </seg>
                        <seg type="suppliedword">περι<supplied reason="lost">ε</supplied></seg></seg>
                    <seg n="2" type="line" facs="z97v2_02"><seg type="wordend"
                            >χόμ<unclear>ε</unclear>νον</seg> ὑπό τε τᾶς ἕλικος <seg type="word"
                                >κ<supplied reason="lost">αὶ</supplied>
                        </seg></seg>
                    <seg n="3" type="line" facs="z97v2_03">τᾶς ΑΕ εὐθείας ἐγγράψαι <seg type="word">
                                σχᾶ<supplied reason="lost">μα</supplied>
                        </seg></seg>
                    <seg n="4" type="line" facs="z97v2_04">ἐπίπεδον <seg type="word"
                            >ὑπ<unclear>ὸ</unclear>
                        </seg>
                        <seg type="word">ὁμοίω<unclear>ν</unclear>
                        </seg>
                        <seg type="word">τομέω<supplied reason="lost">ν</supplied>
                        </seg></seg>
                    <seg n="5" type="line" facs="z97v2_05">συγκείμενον, <seg type="word">ὥ<supplied
                                reason="lost">σ</supplied>τε</seg> τὸ <seg type="clearword"
                                >περ<unclear>ι</unclear>εχό<unclear>μ</unclear><supplied
                                reason="lost">ε</supplied></seg></seg>
                    <seg n="6" type="line" facs="z97v2_06"><seg type="wordend">νον</seg> χωρίον ὑπό
                        τε τᾶς ΑΒΓΔΕ <seg type="clearword">ἕ</seg></seg>
                    <seg n="7" type="line" facs="z97v2_07"><seg type="wordend">λικος</seg> καὶ τᾶς
                        ΑΕ εὐθείας <seg type="suppliedword">μείζ<supplied reason="lost"
                        >ο</supplied></seg></seg>
                    <seg n="8" type="line" facs="z97v2_08"><seg type="wordend">νι</seg> μὲν τοῦ
                        ἐγγεγραμμένου <seg type="suppliedword">σχάμ<supplied reason="lost"
                            >α</supplied></seg></seg>
                    <seg n="9" type="line" facs="z97v2_09"><seg type="wordend">τος</seg> ἐλάσσονι, ἢ
                        ὧι ὑπερέχει τὸ</seg>
                    <seg n="10" type="line" facs="z97v2_10">αὐτὸ χωρίον τοῦ Ϟ <seg type="word">
                                κύκλο<supplied reason="lost">υ</supplied>
                        </seg>. <seg type="clearword">ἐγγε</seg></seg>
                    <seg n="11" type="line" facs="z97v2_11"><seg type="wordend">γράφθω</seg>
                        <seg type="word">οὖ<unclear>ν</unclear>
                        </seg>, καὶ ἔστω τῶν <seg type="clearword">τομέ</seg></seg>
                    <seg n="12" type="line" facs="z97v2_12"><seg type="wordend">ων</seg>, ἐξ ὧν <choice>
                            <abbr>σύγκειτ</abbr>
                            <expan>σύγκειται</expan>
                        </choice> τὸ <seg type="clearword">ἐγγεγραμμέ</seg></seg>
                    <seg n="13" type="line" facs="z97v2_13"><seg type="wordend">νον</seg> σχᾶμα,
                            <seg type="word">μέγιστο<supplied reason="lost">ς</supplied>
                        </seg> μὲν ὁ ΘΧΡ <seg type="clearword">το</seg></seg>
                    <seg n="14" type="line" facs="z97v2_14"><seg type="wordend">μεύς</seg>,
                        ἐλάχιστος δὲ ὁ ΘΕο· δῆλον</seg>
                    <seg n="15" type="line" facs="z97v2_15">οὖν, <expan>ὅτι</expan> τὸ ἐγγεγραμμένον
                        σχᾶμα</seg>
                    <seg n="16" type="line" facs="z97v2_16">μεῖζόν ἐστι τοῦ Ϟ κύκλου. <seg
                            type="suppliedword"><supplied reason="lost">ἐ</supplied>κβε</seg></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
