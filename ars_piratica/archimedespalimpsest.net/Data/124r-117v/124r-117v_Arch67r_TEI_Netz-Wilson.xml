    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="124r1" type="folio">
                    <seg n="1" type="line" facs="z124r1_01">συντεθήσεται δὴ τὸ πρόβλημα
                        <expan>οὕτως</expan>·</seg>
                    <seg n="2" type="line" facs="z124r1_02">ἔστω, ὧ μὲν δεῖ ἴσον τμῆμα <w part="I"
                            >συστή</w></seg>
                    <seg n="3" type="line" facs="z124r1_03"><w part="F">σθαι</w>, τὸ ΑΒΓ, ὡς δὲ
                        ὅμοιον, τὸ ΕΖΗ,</seg>
                    <seg n="4" type="line" facs="z124r1_04">καὶ ἔστωσαν μέγιστοι κύκλοι τῶν</seg>
                    <seg n="5" type="line" facs="z124r1_05">σφαιρῶν οἱ ΑΒΓ ΝΕ ΗΖΟ, <choice>
                            <abbr>μετροι</abbr>
                            <expan>διάμετροι</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z124r1_06">δὲ αὐτῶν αἱ ΓΝ ΗΘ καὶ κέντρα</seg>
                    <seg n="7" type="line" facs="z124r1_07">τὰ ΠΣ, καὶ πεποιήσθω, <expan>ὡς</expan>
                        μὲν <w part="I">συ</w></seg>
                    <seg n="8" type="line" facs="z124r1_08"><w part="F">ναμφότερος</w> ἡ ΠΝ ΝΤ
                            <expan>πρὸς</expan> ΝΤ, <expan>οὕτως</expan></seg>
                    <seg n="9" type="line" facs="z124r1_09">ἡ ΧΤ <expan>πρὸς</expan> ΤΓ, ὡς δὲ <choice>
                            <abbr>συναμφοτερ</abbr>
                            <expan>συναμφότερος</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z124r1_10">ἡ ΣΟΦ <expan>πρὸς</expan> ΟΦ, ἡ ΩΦ
                            <expan>πρὸς</expan> ΦΗ· ἴσος <expan>ἄρα</expan>
                        <expan>ἐστὶν</expan> ὁ</seg>
                    <seg n="11" type="line" facs="z124r1_11">μὲν ΧΑΒ κῶνος τῶι ΑΒΓ <w part="I"
                        >τμήμα</w></seg>
                    <seg n="12" type="line" facs="z124r1_12"><w part="F">τι</w> τῆς σφαίρας, ὁ δὲ
                        ΖΩΕ τῶ Ε</seg>
                    <seg n="13" type="line" facs="z124r1_13">ΗΖ. πεποιήσθω, ὡς ἡ ΩΦ
                        <expan>πρὸς</expan> ΕΖ, <choice>
                            <abbr>ουτ</abbr>
                            <expan>οὕτως</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z124r1_14">ἡ ΧΓ <expan>πρὸς</expan> Δ, καὶ δύο
                        δοθεισῶν <w part="I">εὐθει</w></seg>
                    <seg n="15" type="line" facs="z124r1_15"><w part="F">ῶν</w> τῶν ΑΒ, Δ δύο μέσαι
                        ἀνάλογον</seg>
                    <seg n="16" type="line" facs="z124r1_16">εἰλήφθωσαν αἱ ΘΚ Ϛ, ὥστε <choice>
                            <abbr>ειν</abbr>
                            <expan>εἶναι</expan>
                        </choice>,</seg>
                    <seg n="17" type="line" facs="z124r1_17">ὡς τὴν ΑΒ <expan>πρὸς</expan> ΘΚ,
                            <expan>οὕτως</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice> ΚΘ <expan>πρὸς</expan> Ϛ <expan>καὶ</expan> τὴν</seg>
                    <seg n="18" type="line" facs="z124r1_18">Ϛ <expan>πρὸς</expan> Δ, καὶ ἐπὶ <seg
                            type="word"><supplied reason="lost">τῆ</supplied>ς</seg> ΘΚ <seg
                            type="word">κύκλο<supplied reason="lost">υ</supplied></seg>
                        <seg type="suppliedword">τμῆ</seg></seg>
                    <seg n="19" type="line" facs="z124r1_19"><seg type="wordend">
                            <supplied reason="lost">μα</supplied>
                        </seg>
                        <supplied reason="lost">ἐπεστάσθω</supplied>
                        <seg type="word"><supplied reason="lost">τ</supplied>ὸ</seg>
                            Θ<unclear>Κ</unclear><supplied reason="lost">Λ</supplied>
                        <choice>
                            <abbr><supplied reason="lost">ομ</supplied>οιο</abbr>
                            <expan>ὅμοιον</expan>
                        </choice></seg>
                </seg>
                <seg n="117v1" type="folio">
                    <seg n="1" type="line" facs="z117v1_01">τῶ ΕΖΗ κύκλου τμήματι, καὶ</seg>
                    <seg n="2" type="line" facs="z117v1_02">ἀναπεπληρώσθω ὁ κύκλος,
                        <expan>καὶ</expan></seg>
                    <seg n="3" type="line" facs="z117v1_03">ἔστω αὐτοῦ <choice>
                            <abbr>μετρος</abbr>
                            <expan>διάμετρος</expan>
                        </choice> ἡ ΛΞ, καὶ <w part="I">νο</w></seg>
                    <seg n="4" type="line" facs="z117v1_04"><w part="F">είσθω</w> σφαῖρα, ἧς
                        μέγιστος <choice>
                            <abbr>κυκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z117v1_05">
                        <expan>ἐστὶν</expan> ὁ ΛΘ ΞΚ, κέντρον δὲ τὸ Ρ, καὶ <expan>διὰ</expan></seg>
                    <seg n="6" type="line" facs="z117v1_06">τῆς ΘΚ ἐπίπεδον ὀρθὸν <w part="I"
                        >ἐκβε</w></seg>
                    <seg n="7" type="line" facs="z117v1_07"><w part="F">βλήσθω</w>
                        <expan>πρὸς</expan> τὴν ΛΞ· <choice>
                            <abbr>εστ</abbr>
                            <expan>ἔσται</expan>
                        </choice> δὴ τὸ <w part="I">τμῆ</w></seg>
                    <seg n="8" type="line" facs="z117v1_08"><w part="F">μα</w> τῆς σφαίρας τὸ ἐπὶ τὰ
                        αὐτὰ</seg>
                    <seg n="9" type="line" facs="z117v1_09">τῶι Λ ὅμοιον τῶι ΕΗΖ τμήματι</seg>
                    <seg n="10" type="line" facs="z117v1_10">τῆς σφαίρας, ἐπειδὴ καὶ τῶν</seg>
                    <seg n="11" type="line" facs="z117v1_11">κύκλων τὰ τμήματα ἦν ὅμοια.</seg>
                    <seg n="12" type="line" facs="z117v1_12">λέγω δὲ <expan>ὅτι</expan>
                        <expan>καὶ</expan> ἴσον <expan>ἐστὶ</expan> τῶι ΑΒΓ <w part="I">τμή</w></seg>
                    <seg n="13" type="line" facs="z117v1_13"><w part="F">ματι</w> τῆς σφαίρας. <seg
                            type="word">πε<supplied reason="lost">ποι</supplied>ήσθω</seg>,</seg>
                    <seg n="14" type="line" facs="z117v1_14"><expan>ὡς</expan> συναμφότερος ἡ ΡΞ ΞΥ
                            <expan>πρὸς</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z117v1_15">ΞΥ, <expan>οὕτως</expan> ἡ ΨΥ
                            <expan>πρὸς</expan> ΥΛ· ἴσος <expan>ἄρα</expan> ὁ ΨΘΚ <w part="I">κῶ</w></seg>
                    <seg n="16" type="line" facs="z117v1_16"><w part="F">νος</w> τῶι ΘΚΛ τμήματι <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice>
                        <choice>
                            <abbr>σφρ</abbr>
                            <expan>σφαίρας</expan>
                        </choice>.</seg>
                </seg>
                <seg n="124r2" type="folio">
                    <seg n="1" type="line" facs="z124r2_01">καὶ <seg type="word"><supplied
                                reason="lost">ἐ</supplied>πειδὴ</seg> ὅμοιός <expan>ἐστιν</expan> ὁ
                        ΨΘΚ κῶνος</seg>
                    <seg n="2" type="line" facs="z124r2_02">τῶι ΖΩΕ κώνω, <expan>ἔστιν</expan>
                        <expan>ἄρα</expan>, ὡς ἡ ΦΩ <expan>πρὸς</expan> ΕΖ, <seg type="expandedword">
                            <choice>
                                <expan>τουτέστιν</expan>
                                <abbr>του</abbr>
                            </choice>
                        </seg></seg>
                    <seg n="3" type="line" facs="z124r2_03"><seg type="wordend">
                            <abbr>τέστιν</abbr>
                        </seg> ἡ ΧΤ <expan>πρὸς</expan> Δ, <expan>οὕτως</expan> ἡ ΨΥ
                        <expan>πρὸς</expan> ΘΚ· καὶ <w part="I">ἐναλ</w></seg>
                    <seg n="4" type="line" facs="z124r2_04"><w part="F">λὰξ</w> καὶ <seg type="word"
                                ><unclear>ἀ</unclear>νάπαλιν</seg>· <expan>ὡς</expan>
                        <expan>ἄρα</expan> ἡ ΨΥ <expan>πρὸς</expan> ΧΤ, <expan>οὕτως</expan>
                    </seg>
                    <seg n="5" type="line" facs="z124r2_05">ἡ ΘΚ <expan>πρὸς</expan> Δ. Καὶ ἐπειδὴ
                        ἀνάλογόν <expan>εἰσιν</expan> αἱ</seg>
                    <seg n="6" type="line" facs="z124r2_06">ΑΒ ΚΘ Ϛ Δ <expan>ἔστιν</expan>,
                            <expan>ὡς</expan> τὸ ἀπὸ ΑΒ <expan>πρὸς</expan> τὸ ἀπὸ</seg>
                    <seg n="7" type="line" facs="z124r2_07">ΘΚ, ἡ ΘΚ <expan>πρὸς</expan> Δ.
                            <expan>ὡς</expan> δὲ ἡ Θ<unclear>Κ</unclear>
                        <expan>πρὸς</expan> Δ, ἡ ΨΥ <expan>πρὸς</expan></seg>
                    <seg n="8" type="line" facs="z124r2_08">ΧΤ· <expan>καὶ</expan>
                        <expan>ὡς</expan>
                        <expan>ἄρα</expan> τὸ ἀπὸ ΑΒ <expan>πρὸς</expan> τὸ ἀπὸ ΚΘ, <seg
                            type="expandedword">
                            <expan>τουτέστιν</expan>
                            <abbr>του</abbr>
                        </seg></seg>
                    <seg n="9" type="line" facs="z124r2_09"><seg type="wordend">
                            <abbr>τέστιν</abbr>
                        </seg> ὁ περὶ <choice>
                            <abbr>μετρο</abbr>
                            <expan>διάμετρον</expan>
                        </choice> τὴν ΘΚ κύκλος,</seg>
                    <seg n="10" type="line" facs="z124r2_10">
                        <expan>οὕτως</expan> ἡ ΨΥ <expan>πρὸς</expan> τὴν ΧΤ· ἴσος <expan>ἄρα</expan>
                        <expan>ἐστὶν</expan> ὁ ΧΑΒ <w part="I">κῶ</w></seg>
                    <seg n="11" type="line" facs="z124r2_11"><w part="F">νος</w> τῶι ΨΘΚ κώνωι· <choice>
                            <abbr>τε</abbr>
                            <expan>ὥστε</expan>
                        </choice>
                        <expan>καὶ</expan> τὸ ΑΒΓ</seg>
                    <seg n="12" type="line" facs="z124r2_12">τμῆμα τῆς σφαίρας ἴσον <expan>ἐστὶ</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τῶ</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z124r2_13">ΘΚΛ τμήματι <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice> σφαίρας. τῶι <w part="I">δο</w></seg>
                    <seg n="14" type="line" facs="z124r2_14"><w part="F">θέντι</w>
                        <expan>ἄρα</expan> τμήματι τῶι ΑΓΒ ἴσον</seg>
                    <seg n="15" type="line" facs="z124r2_15">καὶ ἄλλωι τῶι δοθέντι ὅμοιον <choice>
                            <abbr>τ</abbr>
                            <expan>τῶ</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z124r2_16">ΕΖΗ τὸ αὐτὸ συνέσταται τὸ ΘΚΛ.</seg>
                    <seg n="17" type="line" facs="z124r2_17"><choice>
                            <abbr>εξ</abbr>
                            <expan>ἑξῆς</expan>
                        </choice> Η ΚΑΤΑΓΡΑΦΗ</seg>
                </seg>
                <seg n="117v2" type="folio">
                    <figure n="2.5.1">
                        <figDesc>Figure 2.5.1</figDesc>
                    </figure>
                    <seg n="1" type="line" facs="z117v2_01">δύο δοθεισῶν σφαίρας <choice>
                            <abbr>τμηματω</abbr>
                            <expan>τμημάτων</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z117v2_02">εἴτε τῆς αὐτῆς εἴτε <seg type="word"
                                ><unclear>μ</unclear>ὴ</seg> εὑρεῖν <w part="I">τμῆ</w></seg>
                    <seg n="3" type="line" facs="z117v2_03"><w part="F">μα</w> σφαίρας, ὃ ἔσται ἑνὶ
                        μὲν τῶν</seg>
                    <seg n="4" type="line" facs="z117v2_04">δοθέντων ὅμοιον, τὴν δὲ <choice>
                            <abbr>επιφ<unclear>α</unclear>νει</abbr>
                            <expan>ἐπιφάνειαν</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z117v2_05">ἕξει ἴσην τῆι <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> ἑτέρου <seg type="word">τμ<unclear>ήματ</unclear><supplied
                                reason="lost">ος</supplied></seg></seg>
                    <seg n="6" type="line" facs="z117v2_06">ἐπιφανείαι.</seg>
                </seg>
                <seg n="117v2" type="folio">
                    <seg n="6" type="line" facs="z117v2_06">ἔστω τὰ δύο τμήματα</seg>
                </seg>
            </ab>
        </body>
    </text>
