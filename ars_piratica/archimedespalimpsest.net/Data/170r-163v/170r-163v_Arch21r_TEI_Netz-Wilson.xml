            <ab>
                <seg n="170r1" type="folio">
                    <seg n="1" type="line" facs="z170r1_01">
                        <supplied reason="lost">ὥστε</supplied>
                        <supplied reason="lost">τὴν</supplied>
                        <supplied reason="lost">ΑΘ</supplied>
                        <supplied reason="lost">πρὸς</supplied>
                        <supplied reason="lost">αὐτὴν</supplied>
                        <supplied reason="lost">τοῦτον</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἔ</supplied>
                        </seg>
                    </seg>
                    <seg n="2" type="line" facs="z170r1_02"><seg type="wordend">
                            <supplied reason="lost">χειν</supplied>
                        </seg>
                        <supplied reason="lost">τὸν</supplied>
                        <seg type="word"><supplied reason="lost"
                                >λό</supplied><unclear>γ</unclear>ο<unclear>ν</unclear></seg>,
                            <supplied reason="lost">ὃν</supplied>
                        <seg type="word"><supplied reason="lost">ἔχ</supplied>ει</seg>
                        <seg type="word">τ<unclear>ὸ</unclear></seg>
                        <seg type="word"><unclear>τμ</unclear>ῆμ<unclear>α</unclear></seg></seg>
                    <seg n="3" type="line" facs="z170r1_03"><supplied reason="lost">
                            <expan>πρὸς</expan>
                        </supplied>
                        <supplied reason="lost">τὸν</supplied>
                        <supplied reason="lost">κῶνον.</supplied>
                        <seg type="word"><supplied reason="lost"
                            >ἡμ</supplied><unclear>ι</unclear>όλιον</seg>
                        <seg type="word"><unclear>δ</unclear>έ</seg> ἐστιν <seg type="word"
                                ><unclear>τ</unclear>ὸ</seg></seg>
                    <seg n="4" type="line" facs="z170r1_04"><supplied reason="lost">τμῆμα</supplied>
                        <supplied reason="lost">τοῦ</supplied>
                        <seg type="word"><supplied reason="lost">κ</supplied>ώνου</seg>· ἡμιόλιος
                            <expan>ἄρα</expan></seg>
                    <seg n="5" type="line" facs="z170r1_05"><expan>ἐστὶ</expan> καὶ <supplied
                            reason="lost">ἡ</supplied>
                        <supplied reason="lost">Θ</supplied>Α τῆς ΑΚ, <supplied reason="lost">καί</supplied>
                        <seg type="word"><supplied reason="lost">ἐ</supplied>σ<supplied
                                reason="lost">τ</supplied>ιν</seg> τὸ</seg>
                    <seg n="6" type="line" facs="z170r1_06"><unclear>Κ</unclear>
                        <seg type="word"><unclear>κ</unclear><supplied reason="lost"
                            >έ</supplied>ντρον</seg>
                        <seg type="word">το<supplied reason="lost">ῦ</supplied></seg>
                        <seg type="word">β<supplied reason="lost">ά</supplied>ρους</seg> τοῦ <seg
                            type="suppliedword">ὀρθογω</seg></seg>
                    <seg n="7" type="line" facs="z170r1_07"><seg type="wordend"><supplied
                                reason="lost">νί</supplied>ο<supplied reason="lost">υ</supplied></seg>
                        <seg type="word">κωνοει<unclear>δ</unclear>έος</seg> τῆς ΑΔ <w part="I"
                            >τετμη</w></seg>
                    <seg n="8" type="line" facs="z170r1_08"><w part="F">μένης</w> οὕτως, ὥστε
                        διπλάσιον <expan>εἶναι</expan></seg>
                    <seg n="9" type="line" facs="z170r1_09">τὸ μέρος αὐτῆς <supplied reason="lost"
                            >τὸ</supplied> πρὸς τῆι <seg type="suppliedword">κ<supplied
                                reason="lost">ορ</supplied>υ</seg></seg>
                    <seg n="10" type="line" facs="z170r1_10"><seg type="wordend">φῆι</seg> τοῦ
                        τμήματος τοῦ λοιποῦ <w part="I">τμή</w></seg>
                    <seg n="11" type="line" facs="z170r1_11"><w part="F">ματος</w>.</seg>
                    <figure n="5.1">
                        <figDesc xml:lang="eng">Figure 5.1</figDesc>
                    </figure>
                </seg>
                <seg n="163v1" type="folio">
                    <seg n="1" type="line" facs="z163v1_01">
                        <seg type="word">εἶ<supplied reason="lost">ναι</supplied></seg>
                        <supplied reason="lost">τοῦ</supplied>
                        <supplied reason="lost">βάρους</supplied>
                        <supplied reason="lost">ἐπὶ</supplied>
                        <supplied reason="lost">τῆς</supplied>
                        <supplied reason="lost">εὐθείας,</supplied>
                        <supplied reason="lost">ἥ</supplied>
                    </seg>
                    <seg n="2" type="line" facs="z163v1_02"><expan>ἐστιν</expan> ἄξων τοῦ ἡμιολίου, <choice>
                            <abbr>τμηθείσ</abbr>
                            <expan>τμηθείσης</expan>
                        </choice>
                    </seg>
                    <seg n="3" type="line" facs="z163v1_03">οὕτως, ὥστε τὸ τμῆμα αὐτῆς τὸ</seg>
                    <seg n="4" type="line" facs="z163v1_04">πρὸς τῆι κορυφῆι τοῦ <w part="I"
                        >ἡμισφαι</w></seg>
                    <seg n="5" type="line" facs="z163v1_05"><w part="F">ρίου</w>
                        <expan>πρὸς</expan> τὸ λοιπὸν τμῆμα <w part="I">τοῦ</w></seg>
                    <seg n="6" type="line" facs="z163v1_06"><w part="F">τον</w> ἔχειν τὸν λόγον, ὃν
                        ἔχει τὰ</seg>
                    <seg n="7" type="line" facs="z163v1_07">πέντε πρὸς τὰ τρία.</seg>
                </seg>
                <seg n="163v1" type="folio">
                    <seg n="7" type="line" facs="z163v1_07">ἔστω <w part="I">σφαῖ</w></seg>
                    <seg n="8" type="line" facs="z163v1_08"><w part="F">ρα</w> καὶ τετμήσθω ἐπιπέδωι</seg>
                    <seg n="9" type="line" facs="z163v1_09">διὰ τοῦ κέντρου, καὶ γινέσθω ἐν</seg>
                    <seg n="10" type="line" facs="z163v1_10">τῆι ἐπιφανείαι τομὴ ὁ ΑΒ ΓΔ</seg>
                    <seg n="11" type="line" facs="z163v1_11">κύκλος, διάμετροι δὲ ἔστωσαν</seg>
                    <seg n="12" type="line" facs="z163v1_12"><choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> κύκλου <expan>πρὸς</expan> ὀρθὰς ἀλλήλαις</seg>
                    <seg n="13" type="line" facs="z163v1_13">αἱ ΑΓ ΒΔ, ἀπὸ δὲ τῆς ΒΔ <w part="I"
                            >ἐπίπε</w></seg>
                    <seg n="14" type="line" facs="z163v1_14"><w part="F">δον</w> ἂν ἔστω ὀρθὸν
                            <expan>πρὸς</expan> τὴν ΑΓ, <expan>καὶ</expan></seg>
                    <seg n="15" type="line" facs="z163v1_15">ἔστω κῶνος ὁ βάσιν ἔχων</seg>
                    <seg n="16" type="line" facs="z163v1_16">τὸν περὶ διάμετρον τὴν ΒΔ</seg>
                    <seg n="17" type="line" facs="z163v1_17">κύκλον, κορυφὴν δὲ τὸ Α <w part="I"
                            >σημεῖ</w></seg>
                    <seg n="18" type="line" facs="z163v1_18"><w part="F">ον</w>, πλευραὶ δὲ ἔστωσαν
                        τοῦ <w part="I">κώ</w></seg>
                </seg>
                <seg n="170r2" type="folio">
                    <seg n="1" type="line" facs="z170r2_01"><w part="F">νου</w> αἱ
                        Β<unclear>Α</unclear> ΑΔ, <unclear>καὶ</unclear>
                        <seg type="word"><supplied reason="lost">ἐκβεβ</supplied>λήσθω</seg> ἡ</seg>
                    <seg n="2" type="line" facs="z170r2_02">ΓΑ, καὶ <supplied reason="lost">κείσθω</supplied>
                        <supplied reason="lost">τῆι</supplied>
                        <supplied reason="lost">ΓΑ</supplied>
                        <seg type="word"><supplied reason="lost">ἴσ</supplied>η</seg> ἡ Α<supplied
                            reason="lost">Θ,</supplied>
                        <supplied reason="lost">
                            <expan>καὶ</expan>
                        </supplied></seg>
                    <seg n="3" type="line" facs="z170r2_03"><seg type="word"><supplied reason="lost"
                                >νο</supplied>εί<unclear>σθω</unclear></seg>
                        <seg type="word"><supplied reason="lost">ζυγ</supplied>ὸς</seg> ἡ Θ<supplied
                            reason="lost">Γ</supplied>
                        <seg type="word">ε<supplied reason="lost">ὐθ</supplied>εῖ<supplied
                                reason="lost">α,</supplied></seg>
                        <seg type="word">
                            <choice>
                                <abbr><supplied reason="lost">μ</supplied>έ<supplied reason="lost"
                                        >σο</supplied></abbr>
                                <expan><supplied reason="lost">μ</supplied>έ<supplied reason="lost"
                                        >σο</supplied>ν</expan>
                            </choice>
                        </seg></seg>
