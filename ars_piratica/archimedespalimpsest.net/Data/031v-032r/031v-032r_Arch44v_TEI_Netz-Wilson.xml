                        </seg>
                        <seg type="word">
                            <supplied reason="lost">μ</supplied>έ<supplied reason="lost"
                            >ρ</supplied>ος</seg> τοῦ <seg type="clearword">ἀ</seg></seg>
                    <seg n="12" type="line" facs="z32r2_12"><seg type="wordend">πὸ</seg> τᾶς ΔΓ·
                        ὑπερέχει δὲ τὰ <seg type="clearword">συναμ</seg></seg>
                    <seg n="13" type="line" facs="z32r2_13"><seg type="wordend">φότερα</seg> τῶν
                            <seg type="word">σ<supplied reason="lost">υ</supplied>ναμφοτέρων </seg>,</seg>
                    <seg n="14" type="line" facs="z32r2_14">ὧι καὶ τὸ ὑπὸ τῶν ΕΘΔ τοῦ ὑπὸ</seg>
                    <seg n="15" type="line" facs="z32r2_15">τῶν ΔΘΓ, <seg type="word">
                                ὑπερέχ<unclear>ε</unclear><supplied reason="lost">ι</supplied>
                        </seg> δὲ τῶι ὑπὸ</seg>
                    <seg n="16" type="line" facs="z32r2_16"><seg type="word">
                            <supplied reason="lost">τ</supplied>ῶν</seg> ΔΘΓ ὑπερέχει δὲ τῶι ὑπὸ <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                </seg>
