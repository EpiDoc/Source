    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="150r1" type="folio">
                    <figure n="1.33.1">
                        <figDesc>Figure 1.33.1</figDesc>
                    </figure>
                    <seg n="1" type="line" facs="z150r1_01">τοῦ μεγίστου κύκλου ὁ Α· λέγω
                        <expan>ὅτι</expan> ὁ Α</seg>
                    <seg n="2" type="line" facs="z150r1_02">ἴσος ἐστὶν τῆι ἐπιφανείαι τῆς</seg>
                    <seg n="3" type="line" facs="z150r1_03">σφαίρας</seg>
                </seg>
                <seg n="150r1" type="folio">
                    <seg n="3" type="line" facs="z150r1_03">εἰ γὰρ μή, ἤτοι μείζων
                        <expan>ἐστὶν</expan></seg>
                    <seg n="4" type="line" facs="z150r1_04">ἢ ἐλάσσων. ἔστω πρότερον <choice>
                            <abbr>μειζ</abbr>
                            <expan>μείζων</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z150r1_05">ἡ ἐπιφάνεια τῆς σφαίρας
                        <expan>καὶ</expan> ὁ Α</seg>
                    <seg n="6" type="line" facs="z150r1_06">κύκλος· δυνατὸν <expan>ἄρα</expan>
                        <expan>ἐστὶ</expan> λαβεῖν δύο</seg>
                    <seg n="7" type="line" facs="z150r1_07"><seg type="word"
                                ><unclear>ε</unclear><supplied reason="lost">ὐθ</supplied>είας</seg>
                        <seg type="word">ἀ<supplied reason="lost">ν</supplied>ίσους</seg>, ὥστε τὴν
                            <w part="I">μείζο</w></seg>
                </seg>
                <seg n="153v1" type="folio">
                    <seg n="1" type="line" facs="z153v1_01"><w part="F">να</w>
                        <expan>πρὸς</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice> ἐλάσσονα λόγον ἔχειν</seg>
                    <seg n="2" type="line" facs="z153v1_02">ἐλάσσονα τοῦ ὃν ἔχει ἡ <w part="I"
                            >ἐπιφάνει</w></seg>
                    <seg n="3" type="line" facs="z153v1_03"><w part="F">α</w> τῆς σφαίρας
                            <expan>πρὸς</expan> τὸν κύκλον. <w part="I">εἰλή</w></seg>
                    <seg n="4" type="line" facs="z153v1_04"><w part="F">φθωσαν</w> αἱ Β, Γ,
                            <expan>καὶ</expan> τῶν Β, Γ μέση <w part="I">ἀ</w></seg>
                    <seg n="5" type="line" facs="z153v1_05"><w part="F">νάλογον</w> ἔστω ἡ Δ,
                        νοείσθω δὲ καὶ ἡ</seg>
                    <seg n="6" type="line" facs="z153v1_06">σφαῖρα ἐπιπέδωι τετμημένη
                        <expan>διὰ</expan></seg>
                    <seg n="7" type="line" facs="z153v1_07">τοῦ κέντρου <seg type="word"
                                >κ<unclear>ατ</unclear>ὰ</seg> τὸν ΕΖΗΘ <choice>
                            <abbr>κυκλ</abbr>
                            <expan>κύκλον</expan>
                        </choice>,</seg>
                    <seg n="8" type="line" facs="z153v1_08">νοείσθω δὲ καὶ εἰς τὸν κύκλον <seg
                            type="suppliedword">ἐγγε</seg></seg>
                    <seg n="9" type="line" facs="z153v1_09"><seg type="wordend"
                                >γραμμ<unclear>έν</unclear>ο<supplied reason="lost"
                        >ν</supplied></seg> πολύγωνον, ὥστε <seg type="suppliedword">ὅμοι</seg></seg>
                    <seg n="10" type="line" facs="z153v1_10"><seg type="wordend">ο<supplied
                                reason="lost">ν</supplied></seg>
                        <expan>εἶναι</expan> τὸ <seg type="word"
                        >π<unclear>ε</unclear>ριγεγραμμένον</seg> τῶ <w part="I">ἐγ</w></seg>
                    <seg n="11" type="line" facs="z153v1_11"><w part="F">γεγραμμένωι</w> πολυγώνωι
                        καὶ τὴν</seg>
                    <seg n="12" type="line" facs="z153v1_12">τοῦ περιγεγραμμένου <seg type="word"
                                >πλευ<supplied reason="lost">ρὰν</supplied></seg></seg>
                    <seg n="13" type="line" facs="z153v1_13">ἐλάσσονα λόγον ἔχειν τοῦ ὃν <w part="I"
                            >ἔ</w></seg>
                    <seg n="14" type="line" facs="z153v1_14"><w part="F">χει</w> ἡ Β
                        <expan>πρὸς</expan> Δ <expan>καὶ</expan> ὁ διπλάσιος <expan>ἄρα</expan>
                        <choice>
                            <abbr>λογ</abbr>
                            <expan>λόγος</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z153v1_15">τοῦ <seg type="word"
                                >διπλασί<unclear>ο</unclear><supplied reason="lost">υ</supplied></seg>
                        <seg type="word"><unclear>λό</unclear>γου</seg> ἐστὶν <choice>
                            <abbr>ελασσ</abbr>
                            <expan>ἐλάσσων</expan>
                        </choice>.</seg>
                    <seg n="16" type="line" facs="z153v1_16"><expan>καὶ</expan> τοῦ μὲν τῆς Β
                            <expan>πρὸς</expan> Δ διπλάσιός <expan>ἐστιν</expan></seg>
                </seg>
                <seg n="150r2" type="folio">
                    <figure n="1.33.1">
                        <figDesc>Figure 1.33.1</figDesc>
                    </figure>
                    <seg n="1" type="line" facs="z150r2_01">ὁ τῆς Β <expan>πρὸς</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice> Γ, τῆς δὲ πλευρᾶς τοῦ <seg type="expandedword"/></seg>
                    <seg n="2" type="line" facs="z150r2_02"><seg type="wordend">
                            <choice>
                                <abbr>γεγραμμένου</abbr>
                                <expan>περιγεγραμμένου</expan>
                            </choice>
                        </seg> πολυγώνου <expan>πρὸς</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice>
                        <w part="I">πλευ</w></seg>
                    <seg n="3" type="line" facs="z150r2_03"><w part="F">ρὰν</w> τοῦ ἐγγεγραμμένου
                        διπλάσιος</seg>
                    <seg n="4" type="line" facs="z150r2_04">ὁ τῆς ἐπιφανείας τοῦ <w part="I"
                            >περιγεγραμ</w></seg>
                    <seg n="5" type="line" facs="z150r2_05"><w part="F">μένου</w> στερεοῦ
                            <expan>πρὸς</expan> τὴν ἐπιφάνειαν <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z150r2_06">ἐγγεγραμμένου· ἡ ἐπιφάνεια ἄρα</seg>
                    <seg n="7" type="line" facs="z150r2_07">τοῦ περιγεγραμμένου σχήματος</seg>
                </seg>
                <seg n="153v2" type="folio">
                    <seg n="1" type="line" facs="z153v2_01">περὶ τὴν σφαῖραν <expan>πρὸς</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice>
                        <choice>
                            <abbr>επιφανεια</abbr>
                            <expan>ἐπιφάνειαν</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z153v2_02">τοῦ ἐγγεγραμμένου σχήματος <w part="I"
                            >ἐλάσ</w></seg>
                    <seg n="3" type="line" facs="z153v2_03"><w part="F">σονα</w> λόγον ἔχει <choice>
                            <abbr>η</abbr>
                            <expan>ἤπερ</expan>
                        </choice> ἡ <choice>
                            <abbr>επιφανει</abbr>
                            <expan>ἐπιφάνεια</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z153v2_04">τῆς σφαίρας πρὸς τὸν Α κύκλον· <choice>
                            <abbr>ο</abbr>
                            <expan>ὅπερ</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z153v2_05">ἄτοπον· ἡ μὲν γὰρ ἐπιφάνεια τοῦ</seg>
                    <seg n="6" type="line" facs="z153v2_06">περιγεγραμμένου τῆς <choice>
                            <abbr>επιφανει</abbr>
                            <expan>ἐπιφάνειας</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z153v2_07">τῆς σφαίρας μείζων <expan>ἐστίν</expan>,
                        ἡ δὲ <w part="I">ἐπι</w></seg>
                    <seg n="8" type="line" facs="z153v2_08"><w part="F">φάνεια</w> τοῦ ἐγγεγραμμένου <choice>
                            <abbr>σχημα</abbr>
                            <expan>σχήματος</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z153v2_09">τοῦ Α κύκλου ἐλάσσων <expan>ἐστί</expan>
                        <seg type="word">δέδ<supplied reason="lost">ει</supplied>κται</seg></seg>
                    <seg n="10" type="line" facs="z153v2_10">γὰρ ἡ ἐπιφάνεια τοῦ <w part="I"
                            >ἐγγεγραμμέ</w></seg>
                    <seg n="11" type="line" facs="z153v2_11"><w part="F">νου</w> ἐλάσσων τοῦ
                        μεγίστου κύκλου</seg>
                    <seg n="12" type="line" facs="z153v2_12">τῶν ἐν τῆι σφαίραι ἢ <w part="I"
                            >τετραπλα</w></seg>
                    <seg n="13" type="line" facs="z153v2_13"><w part="F">σία</w> τοῦ δὲ <seg
                            type="word">μεγίστο<unclear>υ</unclear></seg> κύκλου <seg
                            type="expandedword">
                            <choice>
                                <abbr>τετραπλ</abbr>
                                <expan>τετραπλάσιός</expan>
                            </choice>
                        </seg></seg>
                    <seg n="14" type="line" facs="z153v2_14"><seg type="wordend">
                            <abbr>σιός</abbr>
                        </seg>
                        <expan>ἐστιν</expan> ὁ Α κύκλος. οὐκ ἄρα ἡ <w part="I">ἐπιφά</w></seg>
                    <seg n="15" type="line" facs="z153v2_15"><w part="F">νεια</w> τῆς σφαίρας μείζων
                            <expan>ἐστὶν</expan> τοῦ Α</seg>
                    <seg n="16" type="line" facs="z153v2_16">κύκλου.</seg>
                </seg>
