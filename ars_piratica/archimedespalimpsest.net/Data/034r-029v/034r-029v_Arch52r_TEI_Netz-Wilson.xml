    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="34r1" type="folio">
                    <seg n="1" type="line" facs="z34r1_01">παραλλήλων ἐπιπέδων <expan>καὶ</expan>
                        τῆς</seg>
                    <seg n="2" type="line" facs="z34r1_02">ἴσης ἀμφοτέραις ταῖς ἐκ τῶν</seg>
                    <seg n="3" type="line" facs="z34r1_03">κέντρων τῶν παραλλήλων <w part="I"
                        >ἐπι</w></seg>
                    <seg n="4" type="line" facs="z34r1_04"><w part="F">πέδων</w>. <expan>καὶ</expan>
                        τῆς ἴσης ἀμφοτέραις</seg>
                    <seg n="5" type="line" facs="z34r1_05">ταῖς ἐκ τῶν κέντρων τῶν <choice>
                            <abbr>κύκλω</abbr>
                            <expan>κύκλων</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z34r1_06">τῶν ἐν τοῖς παραλλήλοις <w part="I"
                        >ἐπιπέ</w></seg>
                    <seg n="7" type="line" facs="z34r1_07">
                        <w part="F">δοις</w>
                    </seg>
                </seg>
                <seg n="34r1" type="folio">
                    <seg n="7" type="line" facs="z34r1_07">ἔστω κῶνος, οὗ τὸ διὰ τοῦ</seg>
                    <seg n="8" type="line" facs="z34r1_08">ἄξονος τρίγωνον ἴσον τῶι ΑΒΓ,</seg>
                    <seg n="9" type="line" facs="z34r1_09"><expan>καὶ</expan>
                        <seg type="word"><unclear>τ</unclear>ετμήσθω</seg> παραλλήλωι <w part="I"
                            >ἐπιπέ</w></seg>
                    <seg n="10" type="line" facs="z34r1_10"><w part="F">δωι</w> τῆι βάσει,
                            <expan>καὶ</expan> ποιείτω τομὴν</seg>
                    <seg n="11" type="line" facs="z34r1_11">τὴν ΔΕ, ἄξων δὲ τοῦ κώνου ἔστω ὁ ΒΗ,</seg>
                    <seg n="12" type="line" facs="z34r1_12">κύκλος δέ τις ἐκκείσθω, οὗ ἡ ἐκ τοῦ</seg>
                    <seg n="13" type="line" facs="z34r1_13">κέντρου μέση ἀνάλογόν ἐστι τῆς</seg>
                    <seg n="14" type="line" facs="z34r1_14">τε ΑΔ καὶ συναμφοτέρου τῆς</seg>
                    <seg n="15" type="line" facs="z34r1_15">ΔΖ, ΗΑ, ἔστω δὲ ὁ κύκλος ὁ Θ· λέγω</seg>
                    <seg n="16" type="line" facs="z34r1_16"><expan>ὅτι</expan> ὁ Θ κύκλος ἴσος
                            <expan>ἐστὶ</expan> τῆι <w part="I">ἐπιφα</w></seg>
                    <seg n="17" type="line" facs="z34r1_17"><w part="F">νείαι</w> τοῦ κώνου τῆι
                        μεταξὺ τῶν</seg>
                    <seg n="18" type="line" facs="z34r1_18">ΔΕ, ΑΓ.</seg>
                </seg>
                <seg n="34r1" type="folio">
                    <seg n="18" type="line" facs="z34r1_18">ἐκκείσθωσαν γὰρ κύκλοι οἱ</seg>
                    <seg n="19" type="line" facs="z34r1_19">ΛΚ καὶ <seg type="word">τ<supplied
                                reason="lost">ο</supplied><unclear>ῦ</unclear></seg> μὲν Κ κύκλου ἡ
                        ἐκ</seg>
                </seg>
                <seg n="29v1" type="folio">
                    <seg n="1" type="line" facs="z29v1_01">τοῦ κέντρου δυνάσθω τὸ ὑπὸ τὸ</seg>
                    <seg n="2" type="line" facs="z29v1_02">ΒΔΖ, τοῦ δὲ Λ ἡ ἐκ τοῦ κέντρου <w
                            part="I">δυ</w></seg>
                    <seg n="3" type="line" facs="z29v1_03"><w part="F">νάσθω</w> τὸ ὑπὸ ΒΑΗ· ὁ μὲν
                        ἄρα Λ</seg>
                    <seg n="4" type="line" facs="z29v1_04">κύκλος ἴσος ἐστὶν τῆι ἐπιφανείαι</seg>
                    <seg n="5" type="line" facs="z29v1_05"><choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice>
                        <seg type="word">Δ<unclear>Ε</unclear>Β</seg>. καὶ ἐπεὶ τὸ ὑπὸ τῶν ΒΑ, ΑΗ <choice>
                            <abbr>ἴσ</abbr>
                            <expan>ἴσον</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z29v1_06"><expan>ἐστὶ</expan> τῶ τε ὑπὸ τῶν ΒΔ, ΔΖ
                        καὶ τῶι</seg>
                    <seg n="7" type="line" facs="z29v1_07">ὑπὸ τῆς ΑΔ καὶ συναμφοτέρου</seg>
                    <seg n="8" type="line" facs="z29v1_08">τῆς ΔΖ, <unclear>ΑΗ</unclear> διὰ τὸ
                        παράλληλον <expan>εἶναι</expan></seg>
                    <seg n="9" type="line" facs="z29v1_09">τὴν ΔΖ τῆι ΑΗ, ἀλλὰ τὸ μὲν ὑπὸ</seg>
                    <seg n="10" type="line" facs="z29v1_10">ΑΒ, ΑΗ δύναται ἡ ἐκ τοῦ κέντρου <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z29v1_11">Λ κύκλου, τὸ δὲ ὑπὸ ΒΔ, ΔΖ δύναται</seg>
                    <seg n="12" type="line" facs="z29v1_12">ἡ ἐκ τοῦ κέντρου τοῦ Κ κύκλου,</seg>
                    <seg n="13" type="line" facs="z29v1_13">τὸ <seg type="word"
                            >δ<unclear>ὲ</unclear></seg> ὑπὸ τῆς <seg type="word">Δ<supplied
                                reason="lost">Α</supplied></seg> καὶ <w part="I">συναμφοτέ</w></seg>
                    <seg n="14" type="line" facs="z29v1_14"><w part="F">ρου</w> τῆς ΔΖ, ΑΗ δύναται ἡ
                        ἐκ τοῦ</seg>
                    <seg n="15" type="line" facs="z29v1_15">κέντρου τοῦ Θ, τὸ <expan>ἄρα</expan> ἀπὸ
                        τῆς ἐκ</seg>
                    <seg n="16" type="line" facs="z29v1_16">τοῦ κέντρου τοῦ Λ κύκλου ἴσον
                            <expan>ἐστὶ</expan></seg>
                    <seg n="17" type="line" facs="z29v1_17">τοῖς ἀπὸ τῶν ἐκ τῶν κέντρων</seg>
                </seg>
                <seg n="34r2" type="folio">
                    <seg n="1" type="line" facs="z34r2_01">τῶν ΚΘ κύκλων· <seg type="word"
                                >ὥσ<unclear>τ</unclear>ε</seg>
                        <unclear>καὶ</unclear>
                        <supplied reason="lost">ὁ Λ κύκλος</supplied></seg>
                    <seg n="2" type="line" facs="z34r2_02">ἴσος <expan>ἐστὶ</expan> τοῖς <seg
                            type="word">Κ<unclear>Θ</unclear></seg>
                        <seg type="word">κ<supplied reason="lost">ύ</supplied>κλ<supplied
                                reason="lost">οι</supplied>ς</seg>. <supplied reason="lost">ἀλλ᾽ ὁ
                            μὲν</supplied>
                        <unclear>Λ</unclear></seg>
                    <seg n="3" type="line" facs="z34r2_03"><seg type="word">ἴσο<unclear>ς</unclear></seg>
                        <expan>ἐστὶ</expan>
                        <seg type="word"><unclear>τ</unclear>ῆι</seg>
                        <seg type="word">ἐπ<unclear>ι</unclear><supplied reason="lost"
                            >φανείαι</supplied></seg>
                        <supplied reason="lost">τοῦ ΒΑΓ κώνου</supplied>,</seg>
                    <seg n="4" type="line" facs="z34r2_04">ὁ δὲ Κ <supplied reason="lost">τῆι</supplied>
                        <seg type="word">ἐπι<supplied reason="lost">φανείαι</supplied></seg>
                        <supplied reason="lost">τοῦ</supplied>
                        <seg type="word">
                            <supplied reason="lost">Δ</supplied>
                            <unclear>Β</unclear>
                            <supplied reason="lost">Ε</supplied>
                        </seg>
                        <supplied reason="lost">κώνου</supplied>·</seg>
                    <seg n="5" type="line" facs="z34r2_05">λοιπὴ <expan>
                            <unclear>ἄρα</unclear>
                        </expan> ἡ <seg type="word">ἐπι<unclear>φάν</unclear>εια</seg>
                        <seg type="word">τ<supplied reason="lost">οῦ</supplied></seg>
                        <seg type="word"><supplied reason="lost">κ</supplied>ώνου</seg> ἡ</seg>
                    <seg n="6" type="line" facs="z34r2_06">
                        <seg type="word"><supplied reason="lost">μ</supplied>εταξὺ</seg>
                        <seg type="word"><supplied reason="lost">τῶ</supplied>ν</seg>
                        <seg type="word"><supplied reason="lost"
                                >π</supplied>αρ<unclear>α</unclear>λλήλ<unclear>ω</unclear><supplied
                                reason="lost">ν</supplied></seg>
                        <seg type="word"><unclear>ἐ</unclear>π<supplied reason="lost"
                            >ιπέδων</supplied></seg>
                    </seg>
                    <seg n="7" type="line" facs="z34r2_07">τῶν ΔΕ, ΑΓ ἴση <expan>ἐστὶ</expan>
                        <seg type="word">τ<unclear>ῶι</unclear></seg> Θ <seg type="word">
                            <supplied reason="lost">κύκλ</supplied>
                            <unclear>ω</unclear>
                            <supplied reason="lost">ι</supplied>
                        </seg>. </seg>
                </seg>
                <seg n="34r2" type="folio">
