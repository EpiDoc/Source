    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="124v1" type="folio">
                    <seg n="1" type="line" facs="z124v1_01">τὰ <seg type="word">σφαιρι<supplied
                                reason="lost">κ</supplied>ὰ</seg> κατὰ τὰς Α<supplied reason="lost"
                            >ΒΓ</supplied>
                        <supplied reason="lost">Δ</supplied>Ε<supplied reason="lost">Ζ</supplied></seg>
                    <seg n="2" type="line" facs="z124v1_02"><seg type="word"><supplied reason="lost"
                                >πε</supplied>ρι<supplied reason="lost"
                            >φερεία</supplied><unclear>ς</unclear></seg>, <seg type="word"
                                >κα<supplied reason="lost">ὶ</supplied></seg>
                        <seg type="word"><supplied reason="lost"
                        >ἔσ</supplied><unclear>τ</unclear>ω</seg>, <unclear>ὧ μὲν δεῖ</unclear></seg>
                    <seg n="3" type="line" facs="z124v1_03"><supplied reason="lost">ὅμοιον εὑρεῖν,
                            τὸ</supplied> κατὰ <supplied reason="lost">τὴν</supplied>
                        <unclear>Α</unclear>Β<supplied reason="lost">Γ</supplied>
                        <seg type="expandedword"/></seg>
                    <seg n="4" type="line" facs="z124v1_04"><seg type="wordend">
                            <choice>
                                <abbr>φερεια</abbr>
                                <expan>περιφέρειαν</expan>
                            </choice>
                        </seg>, <seg type="word"><unclear>ο</unclear>ὗ</seg> δὲ τὴν ἐπιφάνειαν ἴσην</seg>
                    <seg n="5" type="line" facs="z124v1_05">
                        <supplied reason="lost">ἔχειν τῆι</supplied>
                        <seg type="word"><unclear>ἐ</unclear>πιφανείαι</seg>, τὸ κατὰ τὴν</seg>
                    <seg n="6" type="line" facs="z124v1_06">Δ<unclear>ΕΖ</unclear>. </seg>
                </seg>
                <seg n="124v1" type="folio">
                    <seg n="6" type="line" facs="z124v1_06">καὶ γεγενήσθω, καὶ ἔστω <seg type="word"
                                >τ<unclear>ὸ</unclear></seg> Κ<supplied reason="lost">Λ</supplied>Μ</seg>
                    <seg n="7" type="line" facs="z124v1_07">τῆς <seg type="word">σφαί<supplied
                                reason="lost">ρ</supplied>ας</seg> τῶι μὲν ΑΒΓ <seg
                            type="unclearword">τμή<unclear>μα</unclear></seg></seg>
                    <seg n="8" type="line" facs="z124v1_08"><seg type="wordend">τι</seg> ὅμοιον, τὴν
                        δὲ ἐπιφάνειαν ἴσην</seg>
                    <seg n="9" type="line" facs="z124v1_09"><seg type="word"><supplied reason="lost"
                                >ἐχ</supplied><unclear>έ</unclear>τω</seg> τῆι τοῦ ΔΕΖ τμήματος <w
                            part="I">ἐπιφα</w></seg>
                    <seg n="10" type="line" facs="z124v1_10"><w part="F">νείαι</w>, <seg type="word">
                            <unclear>κ</unclear>
                            <supplied reason="lost">αὶ</supplied>
                        </seg> νοείσθω τὰ <seg type="word">κέ<unclear>ν</unclear><supplied
                                reason="lost">τρα</supplied></seg> τῶν</seg>
                    <seg n="11" type="line" facs="z124v1_11">
                        <seg type="word"><supplied reason="lost">σφαιρ</supplied>ῶν</seg>, καὶ δι’
                        αὐτῶν ἐπίπεδα</seg>
                    <seg n="12" type="line" facs="z124v1_12">ἐκβεβλήσθω ὀρθὰ <expan>πρὸς</expan> τὰς
                        τῶν <w part="I">τμη</w></seg>
                    <seg n="13" type="line" facs="z124v1_13"><w part="F">μάτων</w> βάσεις, καὶ ἐν
                        μὲν <choice>
                            <abbr>τ</abbr>
                            <expan>ταῖς</expan>
                        </choice>
                        <seg type="expandedword">
                            <choice>
                                <expan>σφαίραις</expan>
                                <abbr>σφ</abbr>
                            </choice>
                        </seg></seg>
                    <seg n="14" type="line" facs="z124v1_14"><seg type="wordend">
                            <abbr>ραις</abbr>
                        </seg> τομαὶ ἔστωσαν οἱ ΚΛΜ<supplied reason="lost">Ν</supplied>ΒΑ</seg>
                    <seg n="15" type="line" facs="z124v1_15">ΓΘ ΕΖ ΗΔ μέγιστοι κύκλοι, ἐν δὲ ταῖς</seg>
                    <seg n="16" type="line" facs="z124v1_16">βάσεσι τῶν τμημάτων αἱ ΚΜ ΑΓ</seg>
                    <seg n="17" type="line" facs="z124v1_17">ΔΖ εὐθεῖαι, διάμετροι δὲ τῶν <choice>
                            <abbr>σφαιρ</abbr>
                            <expan>σφαιρῶν</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z124v1_18"><expan>πρὸς</expan> ὀρθὰς οὖσαι <choice>
                            <abbr>τς</abbr>
                            <expan>ταῖς</expan>
                        </choice> ΚΜ ΑΓ Δ<unclear>Ζ</unclear>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἔστω</supplied>
                        </seg></seg>
                    <seg n="19" type="line" facs="z124v1_19">
                        <seg type="wordend">
                            <supplied reason="lost">σαν</supplied>
                        </seg>
                        <supplied reason="lost">αἱ ΛΝ ΒΘ ΕΗ, καὶ </supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἐπεζεύχθω</supplied>
                        </seg>
                    </seg>
                </seg>
                <seg n="117r1" type="folio">
                    <seg n="1" type="line" facs="z117r1_01"><seg type="wordend">σαν</seg> αἱ ΛΜ ΒΓ
                        ΕΖ. Καὶ ἐπεὶ ἴση <expan>ἐστὶν</expan> ἡ</seg>
                    <seg n="2" type="line" facs="z117r1_02">τοῦ ΚΛΜ <seg type="word"
                                >τμήματο<unclear>ς</unclear></seg>
                        <seg type="word"><supplied reason="lost">τῆ</supplied>ς</seg>
                        <supplied reason="lost">σφαίρας</supplied></seg>
                    <seg n="3" type="line" facs="z117r1_03">ἐπιφάνεια τῆι τοῦ ΔΕΖ <choice>
                            <abbr>τμημα</abbr>
                            <expan>τμήματος</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z117r1_04">ἐπιφανείαι, ἴσος <expan>ἄρα</expan>
                        <expan>ἐστὶν</expan> καὶ ὁ <choice>
                            <abbr>κυκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice>,</seg>
                    <seg n="5" type="line" facs="z117r1_05">οὗ ἡ ἐκ <seg type="word">τ<supplied
                                reason="lost">ο</supplied>ῦ</seg> κέντρου ἴση <expan>ἐστὶ</expan>
                        τῆι ΛΜ,</seg>
                    <seg n="6" type="line" facs="z117r1_06">τῶι <seg type="word"><supplied
                                reason="lost">κ</supplied>ύ<unclear>κ</unclear>λωι</seg>, οὗ ἡ ἐκ
                        τοῦ κέντρου ἴση</seg>
                    <seg n="7" type="line" facs="z117r1_07"><expan>ἐστὶ</expan> τῆι ΕΖ — αἱ
                            <expan>γὰρ</expan> ἐπιφάνειαι τῶν <seg type="suppliedword">εἰρη</seg></seg>
                    <seg n="8" type="line" facs="z117r1_08"><seg type="wordend">μέν<supplied
                                reason="lost">ων</supplied></seg>
                        <seg type="word"><unclear>τμη</unclear><supplied reason="lost"
                            >μ</supplied>άτων</seg> ἴσαι <seg type="unclearword"
                            >ἐ<unclear>δ</unclear>είχθη</seg></seg>
                    <seg n="9" type="line" facs="z117r1_09"><seg type="wordend">σαν</seg> κύκλοις,
                        ὧν αἱ ἐκ τῶν <seg type="suppliedword"><supplied reason="lost"
                        >κ</supplied>έν</seg></seg>
                    <seg n="10" type="line" facs="z117r1_10"><seg type="wordend">τ<supplied
                                reason="lost">ρ</supplied>ων</seg> ἴσαι <expan>εἰσὶν</expan>
                        <seg type="word"><unclear>τ</unclear>αῖς</seg> ἀπὸ τῶν <w part="I">κο</w></seg>
                    <seg n="11" type="line" facs="z117r1_11"><w part="F">ρυφῶν</w> τῶν τμημάτων ἐπὶ
                        τὰς</seg>
                    <seg n="12" type="line" facs="z117r1_12">βάσεις <choice>
                            <abbr>επ<supplied reason="lost">ι</supplied>ζευγνυο<supplied
                                    reason="lost">υ</supplied>σσ</abbr>
                            <expan>ἐπιζευγνυούσαις</expan>
                        </choice>— · ὥστε καὶ</seg>
                    <seg n="13" type="line" facs="z117r1_13">ἡ <supplied reason="lost">ΜΛ</supplied>
                        τῆι ΕΖ ἴση <expan>ἐστίν</expan>. ἐπεὶ δὲ ὅμοιόν <expan>ἐστι</expan></seg>
                    <seg n="14" type="line" facs="z117r1_14">
                        <supplied reason="lost">τὸ</supplied>
                        <supplied reason="lost">Κ</supplied>ΛΜ τῶι ΑΒΓ <seg type="word"
                                >τμήμα<supplied reason="lost">τι</supplied></seg>,
                        <expan>ἔστιν</expan>, ὡς <supplied reason="lost">ἡ</supplied>
                    </seg>
                    <seg n="15" type="line" facs="z117r1_15"><unclear>ΛΡ</unclear>
                        <expan>πρὸς</expan> ΡΝ, ἡ ΒΠ <expan>πρὸς</expan> ΠΘ· καὶ <choice>
                            <abbr>αναπαλι</abbr>
                            <expan>ἀνάπαλιν</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z117r1_16"><supplied reason="lost">
                            <expan>καὶ</expan>
                        </supplied>
                        <seg type="word"><supplied reason="lost">συν</supplied>θέντι</seg>, ὡς ἡ
                            <unclear>Ν</unclear>Λ <expan>πρὸς</expan> ΛΡ, οὕτως</seg>
                </seg>
                <seg n="124v2" type="folio">
                    <seg n="1" type="line" facs="z124v2_01">ἡ ΒΘ <expan>πρὸς</expan> ΘΠ. <seg
                            type="word">ἀλλ<unclear>ὰ</unclear></seg> καί, ὡς ἡ ΡΛ
                        <expan>πρὸς</expan> ΛΜ,</seg>
                    <seg n="2" type="line" facs="z124v2_02">οὕτως ἡ ΒΠ <expan>πρὸς</expan> ΓΒ —
                        ὅμοια <expan>γὰρ</expan> τὰ <w part="I">τρίγω</w></seg>
                    <seg n="3" type="line" facs="z124v2_03"><w part="F">να</w>— · ὡς
                        <expan>ἄρα</expan> ἡ Ν<supplied reason="lost">Λ</supplied>
                        <expan>πρὸς</expan> ΛΜ, <choice>
                            <abbr>τουτ</abbr>
                            <expan>τουτέστι</expan>
                        </choice>
                        <expan>πρὸς</expan> ΕΖ, <expan>οὕτως</expan></seg>
                    <seg n="4" type="line" facs="z124v2_04">ἡ ΘΒ <expan>πρὸς</expan> ΒΓ. καὶ <seg
                            type="word"><supplied reason="lost"
                            >ἐ</supplied>ν<unclear>α</unclear>λλάξ</seg>· <seg type="word"
                                >λό<unclear>γ</unclear>ος</seg>
                        <supplied reason="lost">δὲ</supplied>
                    </seg>
                    <seg n="5" type="line" facs="z124v2_05">τῆς ΕΖ <expan>πρὸς</expan> ΒΓ· δοθεῖσα
                        γὰρ <seg type="word"><unclear>ἑ</unclear><supplied reason="lost"
                            >κα</supplied>τέρα</seg>·</seg>
                    <seg n="6" type="line" facs="z124v2_06"><seg type="word"
                            ><unclear>λ</unclear>όγος</seg>
                        <expan>ἄρα</expan>
                        <expan>καὶ</expan> τῆς ΛΝ <expan>πρὸς</expan> Β<supplied reason="lost"
                        >Θ</supplied>. <supplied reason="lost">καί</supplied>
                        <expan>ἐστι</expan>
                        <seg type="suppliedword"><unclear>δ</unclear>οθεῖ</seg></seg>
                    <seg n="7" type="line" facs="z124v2_07"><seg type="wordend">
                            <supplied reason="lost">σα</supplied>
                        </seg>
                        <supplied reason="lost">ἡ ΒΘ</supplied>· δοθεῖσα <expan>ἄρα</expan> καὶ ἡ
                        ΛΝ· <w part="I">ὥσ</w></seg>
                    <seg n="8" type="line" facs="z124v2_08"><w part="I">τε</w> καὶ ἡ <supplied
                            reason="lost">σφαῖρα</supplied>
                        <seg type="word"><supplied reason="lost">δ</supplied>οθεῖσά</seg> ἐστιν.</seg>
                    <figure n="2.6.1">
                        <figDesc>Figure 2.6.1</figDesc>
                    </figure>
                </seg>
                <seg n="117r2" type="folio">
                    <seg n="1" type="line" facs="z117r2_01"><seg type="word"><supplied reason="lost"
                                >συντεθή</supplied>σετ<unclear>α</unclear>ι</seg> δὴ οὕτως· ἔστω τὰ </seg>
                    <seg n="2" type="line" facs="z117r2_02">δοθέντα δύο τμήματα σφαίρας</seg>
                    <seg n="3" type="line" facs="z117r2_03">τὰ ΑΒΓ ΔΕΖ, τὸ μὲν ΑΒΓ, ὧ δεῖ <choice>
                            <abbr>ομοι</abbr>
                            <expan>ὅμοιον</expan>
                        </choice>,</seg>
                    <seg n="4" type="line" facs="z117r2_04">τὸ δὲ ΔΕΖ, οὗ τὴν ἐπιφάνειαν <choice>
                            <abbr>ιση</abbr>
                            <expan>ἴσην</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z117r2_05">
                        <choice>
                            <abbr>εχει</abbr>
                            <expan>ἔχειν</expan>
                        </choice>
                        <seg type="word">τῆ<supplied reason="lost">ι</supplied></seg> ἐπιφανείαι,
                        καὶ τὰ <seg type="suppliedword">α<supplied reason="lost">ὐ</supplied></seg></seg>
                    <seg n="6" type="line" facs="z117r2_06"><seg type="wordend">τὰ</seg>
                        κατεσκευάσθω τοῖς ἐπὶ <seg type="word"><supplied reason="lost"
                            >τ</supplied>ῆς</seg></seg>
                    <seg n="7" type="line" facs="z117r2_07">ἀναλύσεως, καὶ <seg type="word"
                                >πεποιήσ<supplied reason="lost">θ</supplied>ω</seg>, ὡς</seg>
                    <seg n="8" type="line" facs="z117r2_08">μὲν ἡ ΒΓ <expan>πρὸς</expan> ΕΖ, οὕτως ἡ
                        ΒΘ <expan>πρὸς</expan> ΛΝ,</seg>
                    <seg n="9" type="line" facs="z117r2_09"><expan>καὶ</expan> περὶ <choice>
                            <abbr>μετρον</abbr>
                            <expan>διάμετρον</expan>
                        </choice>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice> ΛΝ κύκλος</seg>
                    <seg n="10" type="line" facs="z117r2_10">γεγράφθω, καὶ νοείσθω <seg type="word"
                                >σφαῖρ<supplied reason="lost">α</supplied></seg>,</seg>
                    <seg n="11" type="line" facs="z117r2_11">ἧς μέγιστος ἔστω <choice>
                            <abbr>κυκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice> ὁ ΛΚ<supplied reason="lost">Ν</supplied>Μ,</seg>
                    <seg n="12" type="line" facs="z117r2_12">καὶ τετμήσθω ἡ ΝΛ κατὰ <seg type="word"
                                ><unclear>τ</unclear>ὸ</seg> Ρ, <seg type="expandedword"/></seg>
                    <seg n="13" type="line" facs="z117r2_13"><seg type="wordend">
                            <abbr>τε</abbr>
                            <expan>ὥστε</expan>
                        </seg> εἶναι, <expan>ὡς</expan> τὴν ΘΠ <expan>πρὸς</expan> ΠΒ, τὴν <supplied
                            reason="lost">Ν</supplied>Ρ</seg>
                    <seg n="14" type="line" facs="z117r2_14"><expan>πρὸς</expan> ΡΛ, καὶ διὰ τοῦ Ρ
                        ἐπιπέδωι <seg type="suppliedword">τε<supplied reason="lost"
                        >τμ</supplied>ήσ</seg></seg>
                    <seg n="15" type="line" facs="z117r2_15"><seg type="wordend">θω</seg> ἡ
                        ἐπιφάνεια ὀρθῶι <expan>πρὸς</expan>
                        <seg type="word">τ<supplied reason="lost">ὴν</supplied></seg>
                        <unclear>Α</unclear>Ν,</seg>
                    <seg n="16" type="line" facs="z117r2_16"><expan>καὶ</expan> ἐπεζεύχθω ἡ ΛΜ· <seg
                            type="word">ὅμο<supplied reason="lost">ια</supplied></seg>
                        <supplied reason="lost">
                            <expan>ἄρα</expan>
                            <expan>ἐστὶν</expan>
                        </supplied></seg>
                </seg>
            </ab>
        </body>
