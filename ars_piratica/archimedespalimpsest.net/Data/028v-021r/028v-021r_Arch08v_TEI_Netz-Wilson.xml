    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="28v1" type="folio">
                    <seg n="1" type="line" facs="z28v1_01"><seg type="word"><supplied reason="lost"
                                >αὐ</supplied>τοῦ</seg>
                        <supplied reason="lost">ὅλην</supplied>
                        <supplied reason="lost">εἶναι</supplied> ἐν τῶι ὑγρῶι, <seg
                            type="suppliedword">
                            <supplied reason="lost">τε</supplied>
                        </seg></seg>
                    <seg n="2" type="line" facs="z28v1_02"><seg type="wordend">
                            <supplied reason="lost">θὲν</supplied>
                        </seg>
                        <seg type="word"><supplied reason="lost"
                                >κ</supplied>εκλιμ<unclear>έ</unclear><supplied reason="lost"
                            >νον</supplied></seg> οὐ <seg type="word">μ<supplied reason="lost"
                            >ενεῖ</supplied></seg>
                        <seg type="unclearword">
                            <supplied reason="lost">κεκλι</supplied>
                        </seg></seg>
                    <seg n="3" type="line" facs="z28v1_03"><seg type="wordend"
                            >μέν<unclear>ο</unclear>ν,</seg> ἀλλ’ <seg type="word"
                                ><unclear>ἀ</unclear><supplied reason="lost"
                                >πο</supplied>κατα<supplied reason="lost">στ</supplied>η<supplied
                                reason="lost">σεῖται</supplied></seg></seg>
                    <seg n="4" type="line" facs="z28v1_04">οὕτως, <seg type="word">ὥ<supplied
                                reason="lost">στε</supplied></seg>
                        <seg type="word"><unclear>τ</unclear>ὸν</seg>
                        <seg type="word">ἄξο<supplied reason="lost">να</supplied></seg>
                        <seg type="word"><supplied reason="lost">αὐτ</supplied>οῦ</seg>
                        <w part="I">κα</w></seg>
                    <seg n="5" type="line" facs="z28v1_05"><w part="F">τὰ</w> κάθετον εἶναι.</seg>
                </seg>
                <seg n="28v1" type="folio">
                    <seg n="5" type="line" facs="z28v1_05"><seg type="word"
                                >ἀφ<unclear>εί</unclear>σθ<supplied reason="lost">ω</supplied></seg>
                        <seg type="word"><supplied reason="lost">γά</supplied>ρ</seg> τι</seg>
                    <seg n="6" type="line" facs="z28v1_06">τμᾶμα εἰς τὸ ὑγρόν, οἷον <seg type="word">
                            <choice>
                                <abbr>ειρητ</abbr>
                                <expan><supplied reason="lost">εἴ</supplied>ρηται</expan>
                            </choice>
                        </seg>, </seg>
                    <seg n="7" type="line" facs="z28v1_07">καὶ ἔσται <seg type="word"
                            >αὐτ<unclear>οῦ</unclear></seg> ἡ βάσει ἐν τῶι <w part="I">ὑ</w></seg>
                    <seg n="8" type="line" facs="z28v1_08"><w part="F">γρῶι</w>, <seg type="word"
                                >τμη<supplied reason="lost">θέ</supplied>ντος</seg> δὲ αὐτοῦ <w
                            part="I">ἐπιπέ</w></seg>
                    <seg n="9" type="line" facs="z28v1_09"><w part="F">δῶι</w>
                        <seg type="word">δι<supplied reason="lost">ὰ</supplied></seg>
                        <supplied reason="lost">τοῦ</supplied>
                        <supplied reason="lost">ἄξονος</supplied>
                        <seg type="word"><supplied reason="lost">ὀ</supplied>ρθῶι</seg> πρὸς</seg>
                    <seg n="10" type="line" facs="z28v1_10">τὴν <seg type="word">ἐπιφάνει<supplied
                                reason="lost">αν</supplied></seg> τοῦ ὑγροῦ <seg type="suppliedword"
                            >το</seg></seg>
                    <seg n="11" type="line" facs="z28v1_11"><seg type="wordend">
                            <supplied reason="lost">μὴ</supplied>
                        </seg> ἔστω <supplied reason="lost">ἡ</supplied>
                        <supplied reason="lost">ΑΠΟΛ</supplied> ὀρθογωνίου</seg>
                    <seg n="12" type="line" facs="z28v1_12"><seg type="word">κών<supplied
                                reason="lost">ου</supplied></seg>
                        <seg type="word"><supplied reason="lost">το</supplied>μή,</seg> ἄξων δὲ τοῦ
                            <w part="I">τμά</w></seg>
                    <seg n="13" type="line" facs="z28v1_13"><w part="F">ματος</w> καὶ διὰ τῆς τομῆς
                        ἡ ΠΦ,</seg>
                    <seg n="14" type="line" facs="z28v1_14">τῆς δ’ ἐπιφανείας τοῦ ὑγροῦ <w part="I"
                            >το</w></seg>
                    <seg n="15" type="line" facs="z28v1_15"><w part="F">μὴ</w> ἡ ΙΣ. ἔπειθ’ οὖν
                        κεκλιμένον</seg>
                    <seg n="16" type="line" facs="z28v1_16">κεῖται τὸ τμᾶμα, οὐκ ἔσται <w part="I"
                            >κα</w></seg>
                    <seg n="17" type="line" facs="z28v1_17"><w part="F">τὰ</w> κάθετον ὁ ἄξων· οὐκ
                        ἄρα</seg>
                    <seg n="18" type="line" facs="z28v1_18">ποιήσει ἡ ΠΦ ἴσας γωνίας</seg>
                    <seg n="19" type="line" facs="z28v1_19">πρὸς τῆι ΙΣ <sic>η η ΧΘ ω</sic> δή τις ἡ</seg>
                    <seg n="20" type="line" facs="z28v1_20">
                        <supplied reason="lost">ΚΩ</supplied>
                        <supplied reason="lost">παρὰ</supplied>
                        <supplied reason="lost">τὴν</supplied>
                        <supplied reason="lost">ΙΣ</supplied>
                        <supplied reason="lost">ἐφαπτομένα</supplied>
                        <supplied reason="lost">κατὰ</supplied>
                    </seg>
                </seg>
                <seg n="21r1" type="folio">
                    <seg n="1" type="line" facs="z21r1_01"><supplied reason="lost">τὸ</supplied>
                        <supplied reason="lost">Ο</supplied>
                        <supplied reason="lost">τᾶς</supplied>
                        <supplied reason="lost">ΑΠΟΛ</supplied>
                        <seg type="word"><supplied reason="lost">το</supplied>μῆς,</seg> καὶ τοῦ
                            <seg type="word">
                            <choice>
                                <abbr>μ</abbr>
                                <expan>μ<supplied reason="lost">ὲν</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="2" type="line" facs="z21r1_02">ΑΠΟΛ στερεοῦ ἔστω τοῦ βάρους</seg>
                    <seg n="3" type="line" facs="z21r1_03">
                        <supplied reason="lost">τὸ</supplied>
                        <supplied reason="lost">Ρ,</supplied>
                        <supplied reason="lost">τοῦ</supplied>
                        <supplied reason="lost">δὲ</supplied>
                        <supplied reason="lost">ΙΠΟΣ</supplied>
                        <supplied reason="lost">στερεοῦ</supplied>
