    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="162r1" type="folio">
                    <seg n="1" type="line" facs="z162r1_01">ἐκ τούτου οὖν φανερὸν ὅτι <sic>τω</sic>
                        <seg type="clearword">τε</seg></seg>
                    <seg n="2" type="line" facs="z162r1_02"><seg type="wordend">τράγωνα</seg> πάντα
                        ἀπὸ τᾶν ἰσᾶν</seg>
                    <seg n="3" type="line" facs="z162r1_03">τᾶ μεγίστα τῶν μὲν τετραγώνων</seg>
                    <seg n="4" type="line" facs="z162r1_04">τῶν ἀπὸ τᾶν τῶι ἴσω ἀλλαλᾶν</seg>
                    <seg n="5" type="line" facs="z162r1_05">ὑπερεχουσᾶν ἐλάσσονά ἐστιν</seg>
                    <seg n="6" type="line" facs="z162r1_06">ἢ τριπλάσια, ἐπειδὴ <seg
                            type="clearword">ποτιλα</seg></seg>
                    <seg n="7" type="line" facs="z162r1_07"><seg type="wordend">βόντα</seg> τινὰ
                        τριπλάσιά ἐντι,</seg>
                    <seg n="8" type="line" facs="z162r1_08">τῶν δὲ λοιπῶν χωρὶς τοῦ ἀπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τᾶς</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z162r1_09">μεγίστας τετραγώνου μείζονα</seg>
                    <seg n="10" type="line" facs="z162r1_10">ἢ τριπλάσια, ἐπειδὴ τὰ <seg
                            type="clearword">ποτιλα</seg></seg>
                    <seg n="11" type="line" facs="z162r1_11"><seg type="wordend">φθέντα</seg>
                        ἐλάσσονά ἐντι ἢ <seg type="clearword">
                            <choice>
                                <abbr>τριπλ</abbr>
                                <expan>τριπλά</expan>
                            </choice>
                        </seg></seg>
                    <seg n="12" type="line" facs="z162r1_12"><seg type="wordend">σια</seg> τοῦ ἀπὸ
                        τᾶς μεγίστας <seg type="clearword">τετρα</seg></seg>
                    <seg n="13" type="line" facs="z162r1_13"><seg type="wordend">γώνου</seg>.
                            <expan>καὶ</expan>
                        <seg type="word">
                            <unclear>τ</unclear>οίνυν</seg>, εἴ κα ὁμοῖα εἴδεα</seg>
                    <seg n="14" type="line" facs="z162r1_14"><seg type="word">ἀναγρα<supplied
                                reason="lost">φέ</supplied>ωντι</seg> ἀπὸ πασᾶν, <seg
                            type="clearword">ἀ</seg></seg>
                    <seg n="15" type="line" facs="z162r1_15"><seg type="wordend">πὸ</seg> τὲ <seg
                            type="word">τᾶ<unclear>ν</unclear>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ῶ</seg> ἴσω ἀλλαλᾶν <seg
                            type="clearword">ὑπε<unclear>ρ</unclear></seg></seg>
                    <seg n="16" type="line" facs="z162r1_16"><seg type="wordend">εχουσᾶν</seg> καὶ
                        ἀπὸ τᾶν ἰσᾶν τᾶ <seg type="clearword">με</seg></seg>
                    <seg n="17" type="line" facs="z162r1_17"><seg type="wordend">γίστα</seg> τῶν μὲν
                        ἀπὸ τᾶν τῶ ἴσωι</seg>
                    <seg n="18" type="line" facs="z162r1_18">ἀλλαλᾶν ὑπερεχουσᾶν εἰδέων</seg>
                </seg>
                <seg n="155v1" type="folio">
                    <seg n="1" type="line" facs="z155v1_01">ἐλάσσονα ἐσσοῦνται ἢ <seg
                            type="unclearword">τρι<unclear>π</unclear>λά</seg></seg>
                    <seg n="2" type="line" facs="z155v1_02"><seg type="wordend">σια</seg>, τῶν δὲ
                        λοιπῶν χωρὶς τοῦ</seg>
                    <seg n="3" type="line" facs="z155v1_03">ἀπὸ τᾶς μεγίστας <seg type="word">
                                εἴδεο<unclear>ς</unclear>
                        </seg>
                        <seg type="clearword">μείζο</seg></seg>
                    <seg n="4" type="line" facs="z155v1_04"><seg type="wordend">να</seg> ἢ
                        τριπλάσια· τὸν γὰρ αὐτὸν</seg>
                    <seg n="5" type="line" facs="z155v1_05">ἑξοῦντι <seg type="word">
                            <unclear>λ</unclear>όγον</seg>
                        <seg type="word">
                            <unclear>τ</unclear>
                            <supplied reason="lost">ὰ</supplied>
                        </seg>
                        <seg type="word">
                            <unclear>ὁ</unclear>μοῖα</seg> εἴδεα</seg>
                    <seg n="6" type="line" facs="z155v1_06">τοῖς τετραγώνοις.</seg>
                </seg>
                <seg n="155v1" type="folio">
                    <seg n="7" type="line" facs="z155v1_07">εἴ κα γραμμαὶ ἑξῆς τεθέωντι <seg
                            type="clearword">
                            <choice>
                                <abbr>ὁποσ</abbr>
                                <expan>ὁποσαι</expan>
                            </choice>
                        </seg></seg>
                    <seg n="8" type="line" facs="z155v1_08"><seg type="wordend">οῦν</seg> τῶι ἴσωι
                        ἀλληλᾶν <seg type="clearword">
                            <choice>
                                <abbr>ὑπερέχ</abbr>
                                <expan>ὑπερέχου</expan>
                            </choice>
                        </seg></seg>
                    <seg n="9" type="line" facs="z155v1_09"><seg type="wordend">σαι</seg>, καὶ ἄλλαι
                        γραμμαὶ <seg type="clearword">τεθέων</seg></seg>
                    <seg n="10" type="line" facs="z155v1_10"><seg type="wordend">τι</seg> τῶ μὲν
                        πλάθει μιᾶι <seg type="clearword">
                            <choice>
                                <abbr>ἐλάσσο</abbr>
                                <expan>ἐλάσσον</expan>
                            </choice>
                        </seg></seg>
                    <seg n="11" type="line" facs="z155v1_11"><seg type="wordend">ες</seg> τᾶν τῶ ἴσω
                            <sic>ἀλλᾶν</sic>
                        <choice>
                            <abbr>ὑπερεχουσᾶ</abbr>
                            <expan>ὑπερεχουσᾶν</expan>
                        </choice>,</seg>
                    <seg n="12" type="line" facs="z155v1_12">τὸ δὲ μεγέθει ἑκάστα ἴσα τᾶι <seg
                            type="clearword">μεγίσ</seg></seg>
                    <seg n="13" type="line" facs="z155v1_13"><seg type="wordend">τα</seg>, τὰ
                        τετράγωνα πάντα τὰ <seg type="clearword">ἀ</seg></seg>
                    <seg n="14" type="line" facs="z155v1_14"><seg type="wordend">πὸ</seg> τᾶν ἰσᾶν
                        τᾶ μεγίστα ποτὶ μὲν</seg>
                    <seg n="15" type="line" facs="z155v1_15">τετράγωνα τὰ ἀπὸ τᾶν ἰσᾶν τῶι ἴσωι</seg>
                    <seg n="16" type="line" facs="z155v1_16">ἀλλαλᾶν ὑπερεχουσᾶν χωρὶς τᾶς</seg>
                    <seg n="17" type="line" facs="z155v1_17">ἐλαχίστας ἐλάσσονα λόγον <seg
                            type="clearword">ἔ</seg></seg>
                </seg>
                <seg n="162r2" type="folio">
                    <seg n="1" type="line" facs="z162r2_01"><seg type="wordend">χοντι</seg> ἢ τὸ
                        τετράγωνον τὸ ἀπὸ τᾶς</seg>
                    <seg n="2" type="line" facs="z162r2_02">μεγίστας ποτὶ τὸν ἴσον <seg
                            type="clearword">ἀμφοτέ</seg></seg>
                    <seg n="3" type="line" facs="z162r2_03"><seg type="wordend">ροις</seg> τῶι τε
                        περιεχομένω ὑπό τε <choice>
                            <abbr>τ</abbr>
                            <expan>τᾶς</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z162r2_04">μεγίστας καὶ τᾶς ἐλαχίστας καὶ</seg>
                    <seg n="5" type="line" facs="z162r2_05"><seg type="word">
                            <supplied reason="lost">τ</supplied><unclear>ῶ</unclear>ι</seg> τρίτωι
                        μέρει ἀπὸ τᾶς <choice>
                            <abbr>ὑπεροχ</abbr>
                            <expan>ὑπεροχᾶς</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z162r2_06">τετραγώνωι, ἇ ὑπερέχει τὰ μεγίστα</seg>
                    <seg n="7" type="line" facs="z162r2_07">τᾶς ἐλαχίστας, ποτὶ δὲ τὰ <seg
                            type="clearword">τετράγω</seg></seg>
                    <seg n="8" type="line" facs="z162r2_08"><seg type="wordend">να</seg> τὰ ἀπὸ τᾶν
                        τῶι ἴσωι ἀλλαλᾶν</seg>
                    <seg n="9" type="line" facs="z162r2_09">ὑπερχουσᾶν χωρὶς τοῦ ἀπὸ τᾶς</seg>
                    <seg n="10" type="line" facs="z162r2_10">μεγίστας τετραγώνου μείζονα τοῦ</seg>
                    <seg n="11" type="line" facs="z162r2_11">αὐτοῦ λόγου.</seg>
                </seg>
                <seg n="162r2" type="folio">
                    <seg n="11" type="line" facs="z162r2_11">ἔστωσαν γὰρ <seg type="clearword"
                        >γραμ</seg></seg>
                    <seg n="12" type="line" facs="z162r2_12"><seg type="wordend">μαὶ</seg> ὁποσαιοῦν
                        τῶι ἴσωι ἀλλαλᾶν <seg type="clearword">ὑ</seg></seg>
                    <seg n="13" type="line" facs="z162r2_13"><seg type="wordend">περέχουσαι</seg>
                        ἑξῆς κείμεναι, ἁ μὲν</seg>
                    <seg n="14" type="line" facs="z162r2_14">ΑΒ τᾶς ΓΔ, ἁ δὲ ΓΔ τᾶς ΕΖ, ἁ δὲ</seg>
                    <seg n="15" type="line" facs="z162r2_15">ΕΖ τᾶς ΗΘ, ἁ δὲ ΗΘ τᾶς ΙΚ, ἁ δὲ</seg>
                    <seg n="16" type="line" facs="z162r2_16">ΙΚ τᾶς ΛΜ, ἁ δὲ ΛΜ τᾶς ΝΞ,</seg>
                    <seg n="17" type="line" facs="z162r2_17">ποτικείσθω δὴ ποτὶ μὲν τὰν ΓΔ ἴσα</seg>
                    <seg n="18" type="line" facs="z162r2_18">μιᾶι ὑπεροχᾶι ἁ ΓΟ, ποτὶ δὲ τὰν ΕΞ</seg>
                    <seg n="19" type="line" facs="z162r2_19">ἴσα <seg type="word">δυσ<supplied
                                reason="lost">ὶ</supplied>ν</seg>
                        <seg type="word">ὑπε<unclear>ρ</unclear>οχαῖς</seg> ἁ ΕΠ, <supplied
                            reason="lost">ποτὶ</supplied> δὲ</seg>
                </seg>
                <seg n="155v2" type="folio">
                    <seg n="1" type="line" facs="z155v2_01">τὰν ΗΘ ἴσα τρισὶν ὑπεροχαῖς ἁ</seg>
                    <seg n="2" type="line" facs="z155v2_02">ΗΡ, καὶ ποτὶ τὰς ἄλλας τὸν αὐτὸν</seg>
                    <seg n="3" type="line" facs="z155v2_03">τρόπον· ἐσσοῦνται δὴ αἱ <seg
                            type="clearword">γενόμε</seg></seg>
                    <seg n="4" type="line" facs="z155v2_04"><seg type="wordend">ναι</seg> ἀλλήλαις
                        ἴσαι καὶ ἑκάστα τᾶ</seg>
                    <seg n="5" type="line" facs="z155v2_05">μεγίστα. δεικτέον οὖν <expan>ὅτι</expan>
                        τὰ ἀπὸ <seg type="clearword">πα</seg></seg>
                    <seg n="6" type="line" facs="z155v2_06"><seg type="wordend">σᾶν</seg> τᾶν
                        γενομενᾶν τετράγωνα <seg type="clearword">πο</seg></seg>
                    <seg n="7" type="line" facs="z155v2_07"><seg type="wordend">τὶ</seg> μὲν πάντα
                        τὰ τετράγωνα τὰ</seg>
                    <seg n="8" type="line" facs="z155v2_08">ἀπὸ πασᾶν τᾶν τῶι ἴσωι <choice>
                            <abbr>ἀλλαλᾶ</abbr>
                            <expan>ἀλλαλᾶν</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z155v2_09">ὑπερεχουσᾶν χωρὶς τοῦ <seg type="word">
                            <supplied reason="lost">ἀ</supplied>πὸ</seg> τᾶς</seg>
                    <seg n="10" type="line" facs="z155v2_10">ΝΞ τετραγώνου <seg type="word">
                            <unclear>ἐλ</unclear>άσσονα</seg>
                        <seg type="word">
                            <unclear>λ</unclear>όγον</seg></seg>
                    <seg n="11" type="line" facs="z155v2_11">ἔχει ἢ τὸ ἀπὸ τᾶς ΑΒ <choice>
                            <abbr>τετράγωνο</abbr>
                            <expan>τετράγωνον</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z155v2_12">ποτὶ τὸ ἴσον ἀμφοτέροις τῶ τε <seg
                            type="suppliedword">
                            <expan>περι</expan>
                        </seg></seg>
                    <seg n="13" type="line" facs="z155v2_13"><seg type="wordend"
                                >εχόμεν<unclear>ο</unclear><supplied reason="lost"
                        >ν</supplied></seg> ὑπὸ τᾶν ΑΒ ΝΞ καὶ τῶι</seg>
                    <seg n="14" type="line" facs="z155v2_14">τρίτωι μέρει τοῦ ἀπὸ τᾶς ΝΥ <seg
                            type="clearword"><unclear>τ</unclear>ε</seg></seg>
                    <seg n="15" type="line" facs="z155v2_15"><seg type="wordend">τραγώνου</seg>,
                        ποτὶ δὲ τὰ <seg type="word">τετ<unclear>ράγ</unclear>ωνα</seg> τὰ</seg>
                    <seg n="16" type="line" facs="z155v2_16"><seg type="word"
                        >ἀ<unclear>π</unclear>ὸ</seg> τᾶν αὐτᾶν χωρὶς <seg type="word">το<supplied
                                reason="lost">ῦ</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">ἀ</supplied>πὸ</seg>
                        <seg type="word">τᾶ<supplied reason="lost">ς</supplied>
                        </seg></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
