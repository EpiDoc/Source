    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="70r1" type="folio">
                    <seg n="1" type="line" facs="z70r1_01"><seg type="word"
                        >ἴσ<unclear>η</unclear></seg> τῆι μέχρι τοῦ ἄξονος, τῆς</seg>
                    <seg n="2" type="line" facs="z70r1_02">ΔΕ ΒΡ ἡμίσεια ἔστω ἡ ΡΣ· ἔστι δὲ καὶ</seg>
                    <seg n="3" type="line" facs="z70r1_03">ἡ ΣΒ ἡμιολία τῆς ΒΡ. <choice>
                            <abbr>επιζευχθεις</abbr>
                            <expan>ἐπιζευχθείσης</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z70r1_04">δὲ τῆς ΑΒ καὶ τῆς ΤΕ ὀρθῆς <seg
                            type="suppliedword"><supplied reason="lost">ἀ</supplied>χθεί</seg></seg>
                    <seg n="5" type="line" facs="z70r1_05"><seg type="wordend">σης</seg> ἤχθω ἡ ΕΖ
                        παρὰ τὴν ΒΔ, καὶ</seg>
                    <seg n="6" type="line" facs="z70r1_06">πάλιν τῆς ΑΒ δίχα τμηθείσης <w part="I"
                            >κα</w></seg>
                    <seg n="7" type="line" facs="z70r1_07"><w part="F">τὰ</w> τὸ Θ <seg type="word"
                                >ἤ<unclear>χ</unclear>θω</seg> παρὰ τὴν ΒΔ ἡ Θ<unclear>Η</unclear>,</seg>
                    <seg n="8" type="line" facs="z70r1_08">καὶ εἰλήφθω ὀρθογωνίου κώνου</seg>
                    <seg n="9" type="line" facs="z70r1_09">τομὴ ἡ ΑΕΙ περὶ διάμετρον τὴν</seg>
                    <seg n="10" type="line" facs="z70r1_10">ΕΖ καὶ ἡ ΑΘΔ περὶ διάμετρον <choice>
                            <abbr>τη</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z70r1_11">ΘΗ, ὥστ’ ὅμοιαν εἶναι τὰ ΑΘ ΙΑ</seg>
                    <seg n="12" type="line" facs="z70r1_12">ΘΔ τμήματα τῶι ΑΒΛ <w part="I">τμήμα</w></seg>
                    <seg n="13" type="line" facs="z70r1_13"><w part="F">τι·</w> γραφήσεται δὴ ἡ ΑΕΙ
                        κώνου</seg>
                    <seg n="14" type="line" facs="z70r1_14">τομὴ διὰ τοῦ Κ, ἡ δὲ ἀπὸ τοῦ Ρ <w
                            part="I">ὀρ</w></seg>
                    <seg n="15" type="line" facs="z70r1_15"><w part="F">θὴ</w> ἀχθεῖσα τῆι ΒΔ τεμεῖ
                        τὴν ΑΕΙ.</seg>
                    <seg n="16" type="line" facs="z70r1_16">τεμνέτω κατὰ τὰ Υ Γ, καὶ διὰ</seg>
                    <seg n="17" type="line" facs="z70r1_17">τῶν ΥΓ ἤχθωσαν παρὰ τὴν ΒΔ</seg>
                    <seg n="18" type="line" facs="z70r1_18">αἱ ΥΧ ΓΝ, τεμνέτωσαν δὲ αὗται</seg>
                    <seg n="19" type="line" facs="z70r1_19">τὴν ΑΒΔ τομὴν κατὰ τὰ ΞΦ, <w part="I"
                        >ἤ</w></seg>
                    <seg n="20" type="line" facs="z70r1_20"><w part="F">χθωσαν</w> δὲ καὶ αἱ ΠΨ ΟΗ
                            <w part="I">ἐφα</w></seg>
                </seg>
                <seg n="67v1" type="folio">
                    <seg n="1" type="line" facs="z67v1_01"><w part="F">πτόμεναι</w> τᾶς ΑΠ ΟΛ τομῆς
                            <w part="I">κα</w></seg>
                    <seg n="2" type="line" facs="z67v1_02"><w part="F">τὰ</w> τὰ ΟΠ.
                        <sic>ομενα</sic> δή τινα τρία</seg>
                    <seg n="3" type="line" facs="z67v1_03"><sic>τρήματα</sic> τὰ ΑΠΟΛ ΑΕΙ ΑΘΔ</seg>
                    <seg n="4" type="line" facs="z67v1_04">περιεχόμενα ὑπὸ τῶν <choice>
                            <abbr>ευθειω</abbr>
                            <expan>εὐθειῶν</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z67v1_05">καὶ τῶν ὀρθογωνίων <choice>
                            <abbr>κωνω</abbr>
                            <expan>κώνων</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z67v1_06">τομῶν ὀρθὰ <expan>καὶ</expan> ὅμοια, και
                            <w part="I">ἄνι</w></seg>
                    <seg n="7" type="line" facs="z67v1_07"><w part="F">σα</w> καὶ ἀπείληπται ἀφ’ <w
                            part="I">ἑκάσ</w></seg>
                    <seg n="8" type="line" facs="z67v1_08"><w part="F">της</w> βάσεως, ἀπὸ δὲ τοῦ Ν
                            <seg type="unclearword">ἀνηγ</seg></seg>
                    <seg n="9" type="line" facs="z67v1_09"><seg type="wordend"
                            >μένα<unclear>ι</unclear></seg> αἱ ΝΞ <unclear>Γ</unclear>Ν <supplied
                            reason="lost">Ο</supplied>Γ· ὁ τῆς ΒΓ <expan>ἄρα</expan></seg>
                    <seg n="10" type="line" facs="z67v1_10">πρὸς τὴν ΣΞ τὸν συγκείμενον</seg>
                    <seg n="11" type="line" facs="z67v1_11">λόγον ἕξει <supplied reason="lost"
                        >Ι</supplied>Λ πρὸς ΛΑ, και ὃν <w part="I">ἔ</w></seg>
                    <seg n="12" type="line" facs="z67v1_12"><w part="F">χει</w> ἡ ΑΔ πρὸς ΔΙ, ἔχει
                        δὲ καὶ ἡ Λ<unclear>Ι</unclear></seg>
                    <seg n="13" type="line" facs="z67v1_13">πρὸς ΛΑ, ὃν δύο <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice>
                        <num>Ε·</num> ἡ <expan>γὰρ</expan> ΤΒ <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z67v1_14">ΒΔ ἐστί, ὡς δύο <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice>
                        <num>Ε,</num> καὶ ἡ ΕΒ <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z67v1_15">ΒΑ καὶ ἡ ΔΖ πρὸς ΔΑ, τούτων</seg>
                    <seg n="16" type="line" facs="z67v1_16">δὲ διπλῶς αἱ ΛΙ ΛΑ· ἡ ΔΕ Α<supplied
                            reason="lost">Δ</supplied>
                        <expan>πρὸς</expan></seg>
                    <seg n="17" type="line" facs="z67v1_17">ΔΙ ἔχει, ὅσον πέντε πρὸς μίαν,</seg>
                </seg>
                <seg n="70r2" type="folio">
                    <seg n="1" type="line" facs="z70r2_01">ὁ δὲ συνημμένος <seg type="word"
                                >λό<unclear>γος</unclear></seg>
                        <seg type="word"><unclear>ἐ</unclear>ξ</seg>
                        <seg type="word">ο<unclear>ὗ</unclear></seg> ὃν ἔχει</seg>
                    <seg n="2" type="line" facs="z70r2_02">τὰ δύο πρὸς τὰ <num>Ε</num> καὶ ἐξ <seg
                            type="word">ο<unclear>ὗ</unclear></seg> ὃν ἔχει τὰ</seg>
                    <seg n="3" type="line" facs="z70r2_03">πέντε πρὸς τὸ ἕν, ὁ αὐτός ἐστι <sic>
                            <unclear>τὸ</unclear>
                        </sic>
                        <sic>ὧν</sic></seg>
                    <seg n="4" type="line" facs="z70r2_04">ἔχει τὰ δύο πρὸς τὸ <sic>
                            <num>ΑΔ·</num>
                        </sic> ἔστιν ἡ ΟΓΔ <choice>
                            <abbr>τ</abbr>
                            <expan>ῆς</expan>
                        </choice>
                    </seg>
                    <seg n="5" type="line" facs="z70r2_05">ΓΞ. διὰ τὰ αὐτὰ δὴ καὶ ἡ ΠΥ τῆς</seg>
                    <seg n="6" type="line" facs="z70r2_06">ΥΦ. <seg type="word"
                            >ἐπείπε<unclear>ρ</unclear></seg> ἐστὶν ἡ ΔΣ ἡμιολία <seg type="word"
                                >τ<expan>ῆς</expan></seg></seg>
                    <seg n="7" type="line" facs="z70r2_07">ΚΡ, δῆλον, ὅτι ἡ ΒΣ <seg type="word"
                                >ὑπερ<unclear>ο</unclear>χή</seg> ἐστι,</seg>
                    <seg n="8" type="line" facs="z70r2_08">ἧ μείζων ἐστὶν ὁ ἄξων ἡμιόλιος</seg>
                    <seg n="9" type="line" facs="z70r2_09">τῆς μέχρι τοῦ ἄξονος. </seg>
                </seg>
                <seg n="70r2" type="folio">
                    <seg n="9" type="line" facs="z70r2_09">εἰ μὲν οὖν</seg>
                    <seg n="10" type="line" facs="z70r2_10">τὸ τμῆμα τῶι βάρει πρὸς τὸ <choice>
                            <abbr>ὑγρὸ</abbr>
                            <expan>ὑγρὸν</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z70r2_11">τοῦτον ἔχει τὸν λόγον, ὃν τὸ ἀπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z70r2_12">ΒΕ πρὸς τὸ ἀπὸ τῆς ΒΔ, ἢ μείζων</seg>
                    <seg n="13" type="line" facs="z70r2_13">τούτου τοῦ λόγου, ἀφεθὲν τὸ τμῆμα</seg>
                    <seg n="14" type="line" facs="z70r2_14">εἰς τὸ ὑγρὸν οὕτως, ὥστε τὴν βάσιν</seg>
                    <seg n="15" type="line" facs="z70r2_15">αὐτοῦ μὴ ἅπτεσθαι τοῦ ὑγροῦ, <w part="I"
                            >ὀρ</w></seg>
                    <seg n="16" type="line" facs="z70r2_16"><w part="F">θὸν</w> καταστήσεται·
                        δέδεικται <expan>γὰρ</expan>
                    </seg>
                    <seg n="17" type="line" facs="z70r2_17">πρότερον, ὅτι ἐὰν τμῆμα <w part="I"
                            >μείζο</w></seg>
                    <seg n="18" type="line" facs="z70r2_18"><w part="F">να</w> ἔχον τὸν ἄξονα ἢ
                        ἡμιόλιον <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice></seg>
                    <seg n="19" type="line" facs="z70r2_19">μέχρι τοῦ ἄξονος, ἐὰν τῶι βάρει</seg>
                    <seg n="20" type="line" facs="z70r2_20">πρὸς τὸ ὑγρὸν μὴ ἐλάσσονα λόγον</seg>
                </seg>
                <seg n="67v2" type="folio">
                    <seg n="1" type="line" facs="z67v2_01">ἔχηι τοῦ, ὃν ἔχει τὸ <seg type="word"
                                >τετράγωνο<supplied reason="lost">ν</supplied></seg>
                        <supplied reason="lost">τὸ</supplied></seg>
                    <seg n="2" type="line" facs="z67v2_02">ἀπὸ τῆς ὑπεροχῆς, ἧ μείζων <choice>
                            <abbr>εστι</abbr>
                            <expan>ἐστὶν</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z67v2_03">ὁ ἄξων ἢ ἡμιόλιος τῆς μέχρι</seg>
                    <seg n="4" type="line" facs="z67v2_04">τοῦ ἄξονος, πρὸς τὸ <choice>
                            <abbr>τετραγωνο</abbr>
                            <expan>τετράγωνον</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z67v2_05">τὸ ἀπὸ τῆς τοῦ ἄξονος, ἀφεθὲν </seg>
                    <seg n="6" type="line" facs="z67v2_06">ἐς τὸ ὑγρὸν οὕτως, εἴρηται, ὀρθὸν</seg>
                    <seg n="7" type="line" facs="z67v2_07">καταστήσηται.</seg>
                </seg>
                <seg n="67v2" type="folio">
                    <seg n="7" type="line" facs="z67v2_07">ἐπὰν δὲ τὸ <w part="I">τμῆ</w></seg>
                    <seg n="8" type="line" facs="z67v2_08"><w part="F">μα</w> τῶι βάρει πρὸς τὸ
                        ὑγρὸν <w part="I">ἐλάσ</w></seg>
                    <seg n="9" type="line" facs="z67v2_09"><w part="F">σονα</w> μὲν ἔχη τοῦ, ὃν ἔχει
                        τὸ ἀπὸ</seg>
                    <seg n="10" type="line" facs="z67v2_10">τῆς ΣΒ πρὸς τὸ τετράγωνον τὸ <w part="I"
                            >ἀ</w></seg>
                    <seg n="11" type="line" facs="z67v2_11"><w part="F">πὸ</w> τῆς ΒΔ, μείζονα δὲ
                        τοῦ, ὃν ἔχει</seg>
                    <seg n="12" type="line" facs="z67v2_12">τὸ ἀπὸ τῆς ΞΘ τετράγωνον τὸ <w part="I"
                            >ἀ</w></seg>
                    <seg n="13" type="line" facs="z67v2_13"><w part="F">πὸ</w> τῆς ΒΔ, ἀφεθὲν ἐς τὸ
                        ὑγρὸν</seg>
                    <seg n="14" type="line" facs="z67v2_14">κεκλιμένον οὕτως, ὥστε τὴν <w part="I"
                            >βά</w></seg>
                    <seg n="15" type="line" facs="z67v2_15"><w part="F">σιν</w> αὐτοῦ μὴ ἅπτεσθαι
                        τοῦ <choice>
                            <abbr>υγρ</abbr>
                            <expan>ὑγροῦ</expan>
                        </choice>,</seg>
                    <seg n="16" type="line" facs="z67v2_16">καταστήσεται κεκλιμένον <choice>
                            <abbr>ουτ</abbr>
                            <expan>οὕτως</expan>
                        </choice>,</seg>
                    <seg n="17" type="line" facs="z67v2_17">ὥστε τὴν βάσιν αὐτοῦ μηδὲν καθ’ ἓν</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
