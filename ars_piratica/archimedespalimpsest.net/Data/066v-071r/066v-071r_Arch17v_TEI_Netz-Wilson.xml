    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="66v1" type="folio">
                    <seg n="1" type="line" facs="z66v1_01">κύκλον, οὗ <supplied reason="lost">ἔσται</supplied>
                        <seg type="word"><supplied reason="lost">διάμετ</supplied>ρος</seg> ἡ ΞΟ,
                        <pb/> ἐν</seg>
                    <seg n="2" type="line" facs="z66v1_02">δὲ τὸ ΑΕΖ <seg type="word">κ<supplied
                                reason="lost">ώνω</supplied>ι</seg>
                        <seg type="word">κ<supplied reason="lost">ύκλο</supplied>ν</seg>, οὗ ἔσται ἡ
                            <w part="I">δι</w></seg>
                    <seg n="3" type="line" facs="z66v1_03"><w part="F">μετρος</w>
                        <supplied reason="lost">ἡ</supplied> ΠΡ,</seg>
                </seg>
                <seg n="66v1" type="folio">
                    <seg n="3" type="line" facs="z66v1_03">καὶ ἐπεὶ ἴσον ἐστὶν τὸ</seg>
                    <seg n="4" type="line" facs="z66v1_04">ὑπὸ ΓΑΣ, τὸ ὑπὸ ΜΣ ΣΠ, ἴση <sic>γὰρ
                        γὰρ</sic></seg>
                    <seg n="5" type="line" facs="z66v1_05">ἡ μὲν ΑΓ τῆι ΣΜ, ἡ δὲ ΑΣ τῆι ΠΣ, τὸ δὲ</seg>
                    <seg n="6" type="line" facs="z66v1_06">ὑπὸ ΓΑ ΑΣ ἴσον ἐστὶν τὸ ἀπὸ ΑΞ, <seg
                            type="suppliedword">το<supplied reason="lost">υ</supplied></seg></seg>
                    <seg n="7" type="line" facs="z66v1_07"><seg type="wordend">τέστιν</seg> τὰ ἀπὸ
                        ΞΣ ΣΠ, ἴσον ἄρα τὸ <w part="I">ἀ</w></seg>
                    <seg n="8" type="line" facs="z66v1_08"><w part="F">πὸ</w> τῶν ΜΣ ΣΠ τοῖς ἀπὸ τῶν
                        ΞΣ ΣΠ.</seg>
                    <seg n="9" type="line" facs="z66v1_09">καὶ ἐπεί ἐστιν ὡς ἡ ΓΑ
                        <expan>πρὸς</expan> ΑΣ, οὕτως ἡ</seg>
                    <seg n="10" type="line" facs="z66v1_10">ΜΣ <expan>πρὸς</expan> ΣΠ, ἴση δὲ ἡ ΓΑ
                        τῆι ΑΘ, ὡς ἄρα</seg>
                    <seg n="11" type="line" facs="z66v1_11">ἡ ΘΑ πρὸς ΑΣ, ἡ ΜΣ <expan>πρὸς</expan>
                        ΣΠ, <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>του</expan>
                            </choice>
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τέστι</expan>
                            </choice>
                        </seg> τὸ ἀπὸ</seg>
                    <seg n="12" type="line" facs="z66v1_12">ΜΣ πρὸς τὸ ὑπὸ ΜΣ ΣΠ. τὸ δὲ ὑπὸ ΜΣ</seg>
                    <seg n="13" type="line" facs="z66v1_13">ΣΠ ἴσα ἐδείχθη τὰ ἀπὸ ΞΣ ΟΠ· ὡς
                            <expan>ἄρα</expan></seg>
                    <seg n="14" type="line" facs="z66v1_14">ἡ ΑΘ <expan>πρὸς</expan> ΑΣ, οὕτως τὸ
                        ἀπὸ ΜΣ πρὸς τὸ</seg>
                    <seg n="15" type="line" facs="z66v1_15">ἀπὸ ΞΣ ΣΠ. ὡς δὲ τὸ ἀπὸ ΜΣ
                        <expan>πρὸς</expan> τὰ</seg>
                    <seg n="16" type="line" facs="z66v1_16">ἀπὸ ΞΣ ΣΠ, οὕτως τὰ ἀπὸ ΜΝ
                        <expan>πρὸς</expan> τὰ</seg>
                    <seg n="17" type="line" facs="z66v1_17">ἀπὸ ΞΟ ΠΡ, ὡς δὲ τὰ ἀπὸ ΜΝ
                        <expan>πρὸς</expan> τὰ</seg>
                    <seg n="18" type="line" facs="z66v1_18">ἀπὸ ΞΟ ΠΡ, οὕτως ὁ κύκλος ὁ ἐν τῶι</seg>
                    <seg n="19" type="line" facs="z66v1_19">κυλίνδρωι, οὗ διάμετρος ἡ ΜΝ,
                            <expan>πρὸς</expan>
                    </seg>
                    <seg n="20" type="line" facs="z66v1_20">ἀμφοτέρους τοὺς κύκλους τῶν τε</seg>
                </seg>
                <seg n="71r1" type="folio">
                    <seg n="1" type="line" facs="z71r1_01"><supplied reason="lost">ἐν</supplied> τῶι
                            <seg type="word">κ<supplied reason="lost"
                                >ώ</supplied><unclear>νω</unclear><supplied reason="lost"
                            >ι</supplied></seg>, οὗ <seg type="word">ἐστ<unclear>ὶ</unclear></seg>
                        διάμετρος ἡ ΠΡ,</seg>
                    <seg n="2" type="line" facs="z71r1_02"><expan>καὶ</expan> τὸν <seg type="word"
                                >ἐ<supplied reason="lost">ν</supplied></seg>
                        <supplied reason="lost">τῆι</supplied>
                        <seg type="word"><supplied reason="lost">σ</supplied>φ<supplied
                                reason="lost">α</supplied>ίραι</seg>, οὗ ἐστιν ἡ <w part="I">διά</w></seg>
                    <seg n="3" type="line" facs="z71r1_03"><w part="F">μετρος</w> ἡ ΞΟ· <supplied
                            reason="lost">ὡς</supplied> ἄρα ἡ ΘΑ <expan>πρὸς</expan> ΑΣ, οὕτως</seg>
                    <seg n="4" type="line" facs="z71r1_04">ὁ κύκλος ὁ ἐν τῶι κυλίνδρωι <expan>πρὸς</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοὺς</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z71r1_05">κύκλους τόν τε ἐν τῆι σφαίραι καὶ</seg>
                    <seg n="6" type="line" facs="z71r1_06">τὸν ἐν τῶι κώνωι. ἐπεὶ οὖν ὡς ἡ ΘΑ</seg>
                    <seg n="7" type="line" facs="z71r1_07">πρὸς ΑΣ, οὕτως ὁ αὐτὸς κύκλος ὁ ἐν</seg>
                    <seg n="8" type="line" facs="z71r1_08">τῶι κυλίνδρωι αὐτοῦ μένων <w part="I"
                            >ἀμφο</w></seg>
                    <seg n="9" type="line" facs="z71r1_09"><w part="F">τέροις</w> τοῖς κύκλοις, ὧν <choice>
                            <abbr>εἰσι</abbr>
                            <expan>εἰσιν</expan>
                        </choice>
                        <w part="I">διάμε</w></seg>
                    <seg n="10" type="line" facs="z71r1_10"><w part="F">τροι</w> αἱ ΞΟ, ΠΡ,
                        μετενεχθεῖσιν καὶ <w part="I">τε</w></seg>
                    <seg n="11" type="line" facs="z71r1_11"><w part="F">θεῖσιν</w> οὕτως ἐπὶ τὸ Θ,
                        ὥστε <choice>
                            <abbr>ἑκατέρ</abbr>
                            <expan>ἑκατέρου</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z71r1_12">αὐτῶν κέντρον εἶναι τοῦ βάρους τὸ</seg>
                    <seg n="13" type="line" facs="z71r1_13">Θ, ἰσορροπήσουσι κατὰ τὸ Α <w part="I"
                            >σημεῖ</w></seg>
                    <seg n="14" type="line" facs="z71r1_14"><w part="F">ον</w>. ὁμοίως δὲ
                        δειχήσεται, καὶ ἐὰν <w part="I">ἄλ</w></seg>
                    <seg n="15" type="line" facs="z71r1_15"><w part="F">λη</w> ἀχθῆ ἐν τῶι
                            Α<unclear>Ζ</unclear>
                        <w part="I">παραλληλογράμ</w></seg>
                    <seg n="16" type="line" facs="z71r1_16"><w part="F">μωι</w> παρὰ τὴν <supplied
                            reason="lost">Ε</supplied>Ζ, καὶ ἀπὸ τῆς <w part="I">ἀ</w></seg>
                    <seg n="17" type="line" facs="z71r1_17"><w part="F">χθείσης</w> ἐπίπεδον
                        ἀνασταθῆ <choice>
                            <abbr>ὀρθὸ</abbr>
                            <expan>ὀρθὸν</expan>
                        </choice></seg>
                </seg>
                <seg n="66v2" type="folio">
                    <seg n="1" type="line" facs="z66v2_01"><expan>πρὸς</expan> τὴν ΑΓ,
                        <expan>ὅτι</expan> ὁ γενόμενος <seg type="word">κ<supplied reason="lost"
                            >ύκ</supplied>λος</seg>
                        <seg type="word">
                            <supplied reason="lost">ἐν</supplied>
                        </seg></seg>
                    <seg n="2" type="line" facs="z66v2_02">τῶι κυλίνδρωι <seg type="word"
                                >ἰσορρ<supplied reason="lost">ο</supplied>πή<supplied reason="lost"
                                >σ</supplied><unclear>ει</unclear></seg>
                        <seg type="unclearword">
                            <unclear>πε</unclear>
                        </seg></seg>
                    <seg n="3" type="line" facs="z66v2_03"><seg type="wordend">ρὶ</seg> τὸ Α σημεῖον
                        <pb/> αὐτοῦ <seg type="word">μέν<supplied reason="lost">ω</supplied>ν</seg>
                        <w part="I">ἀμ</w></seg>
                    <seg n="4" type="line" facs="z66v2_04"><w part="F">φοτέροις</w> τοῖς κύκλοις τῶι
                        τε</seg>
                    <seg n="5" type="line" facs="z66v2_05">ἐν τῆι σφαίραι γινομένωι καὶ τῶι</seg>
                    <seg n="6" type="line" facs="z66v2_06">ἐν τῶι κώνωι μετενεχθεῖσι καὶ <w part="I"
                            >τε</w></seg>
                    <seg n="7" type="line" facs="z66v2_07"><w part="F">θεῖσιν</w> ἐπὶ τοῦ ζυγοῦ κατὰ
                            <seg type="word">τ<supplied reason="lost">ὸ</supplied></seg> Θ
                            <expan>οὕτως</expan>,</seg>
                    <seg n="8" type="line" facs="z66v2_08">ὥστε ἑκατέρου αὐτῶν κέντρον
                        <expan>εἶναι</expan></seg>
                    <seg n="9" type="line" facs="z66v2_09">τοῦ βάρους περὶ τὸ Θ. <w part="I"
                            >συμπληρω</w></seg>
                    <seg n="10" type="line" facs="z66v2_10"><w part="F">θέντος</w> οὖν τοῦ κυλίνδρου
                        ὑπὸ <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z66v2_11">ληφθέντων κύκλων καὶ τῆς <w part="I"
                            >σφαί</w></seg>
                    <seg n="12" type="line" facs="z66v2_12"><w part="F">ρας</w> καὶ τοῦ κώνου
                        ἰσορροπήσει</seg>
                    <seg n="13" type="line" facs="z66v2_13">ὁ κύλινδρος περὶ τὸ Α σημεῖον <w
                            part="I">αὐ</w></seg>
                    <seg n="14" type="line" facs="z66v2_14"><w part="F">τοῦ</w> μένων συναμφοτέροις
                        τῆι</seg>
                    <seg n="15" type="line" facs="z66v2_15">τε σφαίραι <expan>καὶ</expan> τῶι κώνωι
                            <w part="I">μετενε</w></seg>
                    <seg n="16" type="line" facs="z66v2_16"><w part="F">χθεῖσι</w> καὶ τεθεῖσι ἐπὶ
                        τοῦ ζυγοῦ <expan>κατὰ</expan></seg>
                    <seg n="17" type="line" facs="z66v2_17">τὸ Θ, ὥστε ἑκατέρου αὐτῶν <seg
                            type="word">
                            <choice>
                                <abbr>κέντρ</abbr>
                                <expan>κέν<supplied reason="lost"
                                >τ</supplied><unclear>ρ</unclear>ον</expan>
                            </choice>
                        </seg></seg>
                    <seg n="18" type="line" facs="z66v2_18">
                        <expan>εἶναι</expan> τοῦ βάρους τὸ Θ. ἐπεὶ οὖν <seg type="word"
                                >ἰσορρο<unclear>π</unclear>εῖ</seg></seg>
                    <seg n="19" type="line" facs="z66v2_19">τὰ εἰρημένα μεγέθη κατὰ τὸ Α <seg
                            type="suppliedword">σ<supplied reason="lost">η</supplied></seg></seg>
                </seg>
                <seg n="71r2" type="folio">
                    <seg n="1" type="line" facs="z71r2_01"><seg type="wordend">μ<supplied
                                reason="lost">εῖο</supplied>ν</seg> καὶ τοῦ κυλίνδρου <seg
                            type="word">
                            <unclear>τ</unclear>
                            <supplied reason="lost">ὸ</supplied>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr><supplied reason="lost">κ</supplied>έντρ</abbr>
                                <expan><supplied reason="lost">κ</supplied>έντρον</expan>
                            </choice>
                        </seg></seg>
                    <seg n="2" type="line" facs="z71r2_02">τοῦ βάρους τὸ Κ, τῆς <seg type="word"
                                >δ<supplied reason="lost">ὲ</supplied></seg>
                        <seg type="word">σφα<supplied reason="lost">ί</supplied>ρας</seg>
                        <expan>καὶ</expan></seg>
                    <seg n="3" type="line" facs="z71r2_03">τοῦ κώνου <seg type="word"
                                >μετενηνεγμέν<supplied reason="lost">ω</supplied>ν</seg>, ὡς</seg>
                    <seg n="4" type="line" facs="z71r2_04">εἴρηται, περὶ κέντρον <seg type="word"
                                >β<unclear>ά</unclear>ρ<unclear>ο</unclear>υς</seg> τὸ Θ,</seg>
                    <seg n="5" type="line" facs="z71r2_05">ἔσται, ὡς ἡ ΘΑ <expan>πρὸς</expan> ΑΚ,
                        οὕτως ὁ <seg type="suppliedword">κ<supplied reason="lost"
                                >ύ</supplied><unclear>λ</unclear>ι<supplied reason="lost"
                            >ν</supplied></seg></seg>
                    <seg n="6" type="line" facs="z71r2_06"><seg type="wordend">δρος</seg>
                        <expan>πρὸς</expan> τὴν σφαῖραν καὶ τὸν <w part="I">κῶ</w></seg>
                    <seg n="7" type="line" facs="z71r2_07"><w part="F">νον</w>. διπλασία δὲ ἡ ΘΑ τῆι
                        ΑΚ· <seg type="unclearword">διπλ<unclear>ά</unclear></seg></seg>
                    <seg n="8" type="line" facs="z71r2_08"><seg type="wordend">σιον</seg>
                        <seg type="word">ἄ<unclear>ρ</unclear>α</seg> καὶ ὁ κύλινδρος <w part="I"
                            >συναμ</w></seg>
                    <seg n="9" type="line" facs="z71r2_09"><w part="F">φοτέρου</w> τῆς τε σφαίρας
                        καὶ τοῦ</seg>
                    <seg n="10" type="line" facs="z71r2_10">κώνου. αὐτοῦ τὲ τοῦ κώνου <w part="I"
                            >τριπλα</w></seg>
                    <seg n="11" type="line" facs="z71r2_11"><w part="F">σίων</w> ἐστί· <choice>
                            <abbr>τρ</abbr>
                            <expan>τρεῖς</expan>
                        </choice> ἄρα κῶνοι ἴσοι <supplied reason="lost">
                            <expan>εἰσὶ</expan>
                        </supplied>
                        <w part="I">δυ</w></seg>
                    <seg n="12" type="line" facs="z71r2_12"><w part="F">σὶ</w> κώνοις τοῖς αὐτοῖς
                        καὶ δυσὶ <seg type="expandedword">
                            <choice>
                                <abbr>σφ</abbr>
                                <expan>σφαί</expan>
                            </choice>
                        </seg></seg>
                    <seg n="13" type="line" facs="z71r2_13"><seg type="wordend">ραις</seg>. <seg
                            type="word">κοινο<supplied reason="lost">ὶ</supplied></seg> ἀφηιρήσθωσαν
                        δύο</seg>
                    <seg n="14" type="line" facs="z71r2_14">κῶνοι· εἷς ἄρα κῶνος <seg type="word"
                                ><supplied reason="lost">ο</supplied>ὗ</seg>
                        <seg type="word">ἔσ<unclear>τ</unclear>ιν</seg> τὸ</seg>
                    <seg n="15" type="line" facs="z71r2_15">διὰ <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<unclear>οῦ</unclear></expan>
                            </choice>
                        </seg> ἄξονος τρίγωνον τὸ <supplied reason="lost">ΑΕ</supplied>Ζ</seg>
                    <seg n="16" type="line" facs="z71r2_16">ἴσος ἐστὶ ταῖς εἰρημέναις δυσὶ</seg>
                    <seg n="17" type="line" facs="z71r2_17"><seg type="word"
                            ><unclear>σ</unclear>φαίραις</seg>. ἀλλ’ ὁ κῶνος, οὗ <seg type="word"
                                >τ<unclear>ὸ</unclear></seg> διὰ</seg>
                    <seg n="18" type="line" facs="z71r2_18">τοῦ ἄξονος <expan>ἐστὶ</expan>
                            τρί<unclear>γ</unclear>ωνον τὸ ΑΕ<supplied reason="lost">Ζ,</supplied>
                        <choice>
                            <abbr>
                                <supplied reason="lost">ἴσ</supplied>
                            </abbr>
                            <expan>
                                <supplied reason="lost">ἴσος</supplied>
                            </expan>
                        </choice>
                        <expan>ἐστὶν</expan></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
