    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="123r1" type="folio">
                    <seg n="1" type="line" facs="z123r1_01">
                        <seg type="word">ἡμισ<unclear>εί</unclear><supplied reason="lost"
                            >ας</supplied></seg>
                        <seg type="word"><unclear>τ</unclear>ῆς</seg>
                        <seg type="word">β<supplied reason="lost"
                            >ά</supplied><unclear>σ</unclear>εως</seg>
                        <seg type="word">το<supplied reason="lost">ῦ</supplied></seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">εἰρη</supplied>
                        </seg>
                    </seg>
                </seg>
                <seg n="118v1" type="folio">
                    <seg n="1" type="line" facs="z118v1_01"><seg type="wordend"
                            >μεν<unclear>ου</unclear></seg> πολυγωνίου <seg type="word"
                                >γεγρ<supplied reason="lost">α</supplied>μμ<supplied reason="lost"
                            >έ</supplied>νον</seg></seg>
                    <seg n="2" type="line" facs="z118v1_02">σχῆμα ἐγγεγραμμένον <expan>ἐστὶν</expan>
                        εἰς τὸ <w part="I">τμῆ</w></seg>
                    <seg n="3" type="line" facs="z118v1_03"><w part="F">μα</w> τῆς μείζονος σφαίρας,
                            <w part="I">τοῦ</w></seg>
                    <seg n="4" type="line" facs="z118v1_04"><w part="F">το</w> δὲ δῆλον
                        <expan>διὰ</expan> τὸ <seg type="word">προγεγραμμένο<supplied reason="lost"
                                >ν</supplied></seg>.</seg>
                    <figure n="1.39-1.1">
                        <figDesc>Figure 1 of corollary 1 of proposition 1.39.</figDesc>
                    </figure>
                </seg>
                <seg n="118v1" type="folio">
                    <seg n="5" type="line" facs="z118v1_05">τοῦ περιγεγραμμένου <w part="I"
                        >σχήμα</w></seg>
                    <seg n="6" type="line" facs="z118v1_06"><w part="F">τος</w> τῶι τομεῖ ἡ <seg
                            type="word">ἐπιφάν<supplied reason="lost"
                            >ε</supplied><unclear>ι</unclear>α</seg>
                        <w part="I">μεί</w></seg>
                    <seg n="7" type="line" facs="z118v1_07"><w part="F">ζων</w>
                        <expan>ἐστὶν</expan> κύκλου, οὗ ἡ ἐκ τοῦ <w part="I">κέν</w></seg>
                    <seg n="8" type="line" facs="z118v1_08"><w part="F">τρου</w> ἴση
                        <expan>ἐστὶ</expan> τῆι ἀπὸ τῆς <choice>
                            <abbr>κορυφ</abbr>
                            <expan>κορυφῆς</expan>
                        </choice></seg>
                </seg>
                <seg n="123r2" type="folio">
                    <seg n="1" type="line" facs="z123r2_01">
                        <seg type="word"><supplied reason="lost">Μ</supplied>Θ</seg>
                        <supplied reason="lost">ΗΖ</supplied>
                        <seg type="word"><supplied reason="lost">περι</supplied>εχομ<supplied
                                reason="lost">έ</supplied>ν<unclear>ο</unclear><supplied
                                reason="lost">υ</supplied></seg>
                        <supplied reason="lost">αλλ’</supplied>
                        <supplied reason="lost">ἡ</supplied>
                        <supplied reason="lost">μὲν</supplied>
                    </seg>
                </seg>
                <seg n="118v2" type="folio">
                    <seg n="1" type="line" facs="z118v2_01">ΗΖ <seg type="word"
                            >μεί<unclear>ζω</unclear>ν</seg>
                        <expan>ἐστὶ</expan>
                        <seg type="word">τ<supplied reason="lost">ῆς</supplied></seg> ΔΞ ὅ <supplied
                            reason="lost">
                            <expan>ἐστιν</expan>
                        </supplied>
                        <seg type="word"><supplied reason="lost">ὕ</supplied>ψος</seg>
                        <seg type="word">τ<supplied reason="lost">οῦ</supplied></seg>
                        <seg type="unclearword">
                            <unclear>ἐλ</unclear>ά<add>σ</add>
                        </seg></seg>
                    <seg n="2" type="line" facs="z118v2_02"><seg type="wordend">σονος</seg>
                        τμήματος· ἐὰν γὰρ <w part="I">ἐπι</w></seg>
                    <seg n="3" type="line" facs="z118v2_03"><w part="F">ζεύξωμεν</w> τὴν ΚΖ, <choice>
                            <abbr>εστ</abbr>
                            <expan>ἔσται</expan>
                        </choice>
                        <w part="I">παράλλη</w></seg>
                    <seg n="4" type="line" facs="z118v2_04"><w part="F">λος</w>
                        <seg type="word">τῆ<unclear>ι</unclear></seg> ΔΑ. <expan>ἔστιν</expan> δὲ
                        καὶ ἡ ΑΒ τῆι ΚΛ <w part="I">πα</w></seg>
                    <seg n="5" type="line" facs="z118v2_05"><w part="F">ράλληλος</w>, καὶ κοινὴ ἡ
                        ΖΕ· ὅμοιον</seg>
                    <seg n="6" type="line" facs="z118v2_06"><expan>ἄρα</expan> τὸ ΖΚΗ τρίγωνον τῶι
                        ΔΑΞ <w part="I">τριγώ</w></seg>
                    <seg n="7" type="line" facs="z118v2_07"><w part="F">νωι</w>. καί
                        <expan>ἐστιν</expan> μείζων ἡ ΖΚ τῆς ΑΔ·</seg>
                    <seg n="8" type="line" facs="z118v2_08">μείζων <expan>ἄρα</expan> καὶ ἡ ΖΗ τῆς
                        ΔΞ, ἴση δὲ</seg>
                    <seg n="9" type="line" facs="z118v2_09">ἡ ΜΘ τῆι <choice>
                            <abbr>μετρωι</abbr>
                            <expan>διαμέτρωι</expan>
                        </choice> τῆι ΓΔ· ἐὰν γὰρ </seg>
                    <seg n="10" type="line" facs="z118v2_10">ἐπιζευχθῆι ἡ Ε<unclear>Ο</unclear>,
                        ἐπεὶ ἴση <expan>ἐστὶν</expan> ἡ μὲν</seg>
                    <seg n="11" type="line" facs="z118v2_11">ΜΟ τῆι ΟΖ, ἡ δὲ ΘΕ τῆι ΕΖ, <w part="I"
                            >παράλλη</w></seg>
                    <seg n="12" type="line" facs="z118v2_12"><w part="F">λος</w>
                        <expan>ἄρα</expan>
                        <expan>ἐστὶν</expan> ἡ ΕΟ τῆι ΜΘ· διπλασία <expan>ἄρα</expan>
                        <expan>ἐστὶν</expan></seg>
                    <seg n="13" type="line" facs="z118v2_13">ἡ ΜΘ τῆι ΕΟ. ἀλλὰ καὶ ἡ ΓΔ <w part="I"
                            >διπλα</w></seg>
                    <seg n="14" type="line" facs="z118v2_14"><w part="F">σία</w>
                        <expan>ἐστὶν</expan> τῆς ΕΟ· ἴση ἄρα ἐστὶν ἡ ΜΘ</seg>
                    <seg n="15" type="line" facs="z118v2_15">τῆι ΓΜ, τὸ δὲ ὑπὸ τῶν ΓΔ ΔΞ ἴσον τῶι</seg>
                    <seg n="16" type="line" facs="z118v2_16">ἀπὸ τῆς ΑΔ· ἡ <expan>ἄρα</expan> τοῦ
                        σχήματος</seg>
                </seg>
            </ab>
        </body>
    </text>
