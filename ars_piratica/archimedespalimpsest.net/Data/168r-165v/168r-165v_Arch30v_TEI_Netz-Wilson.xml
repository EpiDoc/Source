                <seg n="168r2" type="folio">
                    <seg n="1" type="line" facs="z168r2_01">
                        <gap/>
                        <seg type="word">κεν<supplied reason="lost">τρον</supplied></seg>
                    </seg>
                    <seg n="2" type="line" facs="z168r2_02">δὲ αὐτοῦ τὸ Κ, καὶ ἐν αὐτῶι δεδόσθω</seg>
                    <seg n="3" type="line" facs="z168r2_03">εὐθεῖα ἐλάσσων τᾶς διαμέτρου ἁ</seg>
                    <seg n="4" type="line" facs="z168r2_04">ΓΑ, καὶ <expan>λόγον</expan> ἔχει ἁ Ζ
                            <expan>πρὸς</expan> Η, <choice>
                            <abbr>ἐλάσσω</abbr>
                            <expan>ἐλάσσων</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z168r2_05">τοῦ ὃν ἔχει ἁ ΓΘ ποτὶ τὰν ΚΘ, <seg
                            type="clearword">καθέ</seg></seg>
                    <seg n="6" type="line" facs="z168r2_06"><seg type="wordend">του</seg> οὔσας τᾶς
                        ΚΘ· ἄχθω δὲ ἀπὸ</seg>
                    <seg n="7" type="line" facs="z168r2_07">τοῦ κέντρου παρὰ τὰν ΑΓ ἁ ΚΝ καὶ</seg>
                    <seg n="8" type="line" facs="z168r2_08">τᾶ Κ<unclear>Γ</unclear> πρὸς ὀρθὰς ἁ
                        ΓΛ· ὁμοῖα δή <expan>ἐστι</expan></seg>
                    <seg n="9" type="line" facs="z168r2_09">τὰ ΓΘΚ ΓΚΛ τρίγωνα. ἔστιν οὖν ὡς</seg>
                    <seg n="10" type="line" facs="z168r2_10">ἁ ΓΘ ποτὶ τὰν ΘΚ οὕτως ἁ ΚΓ <seg
                            type="clearword">πο</seg></seg>
                    <seg n="11" type="line" facs="z168r2_11"><seg type="wordend">τὶ</seg> τὰν ΓΛ·
                        ἐλάσσονα ἄρα λόγον ἔχει ἁ</seg>
                    <seg n="12" type="line" facs="z168r2_12">Ζ ποτὶ τὰν Η <unclear>ἢ</unclear> ἁ ΚΓ
                        ποτὶ τὰν ΓΛ.</seg>
                    <seg n="13" type="line" facs="z168r2_13">ὃν δὴ λόγον ἔχει ἁ Ζ ποτὶ τὰν Η,</seg>
                    <seg n="14" type="line" facs="z168r2_14">τοῦτον ἐχέτω <expan>πρὸς</expan>
                        μείζονα τᾶς ΓΛ. <seg type="clearword">ἐ</seg></seg>
                    <seg n="15" type="line" facs="z168r2_15"><seg type="wordend">χέτω</seg> τὰν ΒΗ,
                        κείσθω δὲ ἁ ΒΝ <seg type="clearword">με</seg></seg>
                    <seg n="16" type="line" facs="z168r2_16"><seg type="wordend">ταξὺ</seg> τᾶς
                        περιφερείας καὶ τᾶς</seg>
                    <seg n="17" type="line" facs="z168r2_17">εὐθείας διὰ τοῦ Γ· δυνατὸν δέ <choice>
                            <abbr>ἐστι</abbr>
                            <expan>ἐστιν</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z168r2_18">οὕτως τέμνειν· καὶ πεσείτω <choice>
                            <abbr>ἐκτ</abbr>
                            <expan>ἐκτός</expan>
                        </choice>,</seg>
                    <seg n="19" type="line" facs="z168r2_19">ἐπὶ μείζων ἐστὶν τᾶς ΓΛ. ἐπεὶ οὖν</seg>
                    <seg n="20" type="line" facs="z168r2_20">ἁ ΚΓ ποτὶ ΒΝ τὸν αὐτὸν ἔχει <choice>
                            <abbr>λόγ</abbr>
                            <expan>λόγον</expan>
                        </choice>,</seg>
                    <seg n="21" type="line" facs="z168r2_21">ὃν ἁ Ζ ποτὶ Η, καὶ ἁ ΕΒ ποτὶ ΒΓ</seg>
                    <seg n="22" type="line" facs="z168r2_22">τὸν αὐτὸν ἕξει λόγον ὃν ἁ Ζ ποτὶ</seg>
                    <seg n="23" type="line" facs="z168r2_23">Η. <choice>
                            <abbr>ἑχ</abbr>
                            <expan>ἑχῆς</expan>
                        </choice> τὸ ΣΧΑΜΑ.</seg>
