    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="168r1" type="folio">
                    <seg n="1" type="line" facs="z168r1_01"><seg type="word">
                            <supplied reason="lost">ἔχε</supplied>
                            <unclear>ι</unclear>
                        </seg> ποτὶ τὰν ΘΚ, ὃν ἁ ΒΘ ποτὶ <seg type="word">
                            <choice>
                                <abbr>τὰ</abbr>
                                <expan>τὰ<supplied reason="lost">ν</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="2" type="line" facs="z168r1_02"><supplied reason="lost">ΘΗ</supplied>.
                            <unclear>ἁ</unclear>
                        <expan>ἄρα</expan> ΖΘ ποτὶ τὰν ΘΚ <seg type="clearword">ἐλάσσο</seg></seg>
                    <seg n="3" type="line" facs="z168r1_03"><seg type="wordend">
                            <supplied reason="lost">να</supplied>
                        </seg>
                        <seg type="word">
                            <unclear>λ</unclear>όγον</seg> ἔχει τοῦ ὃν ἁ ΒΘ <seg type="clearword"
                            >περιφέρει</seg></seg>
                    <seg n="4" type="line" facs="z168r1_04"><seg type="wordend">α</seg>
                        <seg type="word">
                            <unclear>π</unclear>οτὶ</seg> τὰν δοθεῖσαν <choice>
                            <abbr>περιφέρει</abbr>
                            <expan>περιφέρειαν</expan>
                        </choice>,</seg>
                    <seg n="5" type="line" facs="z168r1_05"><seg type="word">
                            <choice>
                                <abbr>δι</abbr>
                                <expan><supplied reason="lost">δι</supplied>ότι</expan>
                            </choice>
                        </seg>
                        <unclear>ἁ</unclear> μὲν ΒΘ εὐθεῖα ἐλάσσων <seg type="word"> ἐστ<supplied
                                reason="lost">ὶ</supplied>
                        </seg></seg>
                    <seg n="6" type="line" facs="z168r1_06"><supplied reason="lost">τᾶς</supplied>
                        ΒΘ περιφερείας, ἁ δὲ ΘΗ <seg type="clearword">μεί</seg></seg>
                    <seg n="7" type="line" facs="z168r1_07"><seg type="wordend"><supplied
                                reason="lost">ζω</supplied>ν</seg> τᾶς δοθείσας <choice>
                            <abbr>περιφερεία</abbr>
                            <expan>περιφερείας</expan>
                        </choice>·</seg>
                    <seg n="8" type="line" facs="z168r1_08"><seg type="word">
                            <supplied reason="lost">ἐλάσ</supplied>σονα</seg> οὖν λόγον ἔχει καὶ
                            <unclear>ἁ</unclear> ΖΘ</seg>
                    <seg n="9" type="line" facs="z168r1_09"><seg type="word">
                            <supplied reason="lost">πο</supplied>τὶ</seg> τὰν ἐκ τοῦ κέντρου ἢ ἁ ΒΘ</seg>
                    <seg n="10" type="line" facs="z168r1_10"><seg type="word">
                            <supplied reason="lost">περ</supplied>ιφέρεια</seg> ποτὶ τὰν δοθεῖσαν</seg>
                    <seg n="11" type="line" facs="z168r1_11"><seg type="word">
                            <supplied reason="lost">περ</supplied>ιφέρειαν</seg>. ἑξῆς τὸ
                    ΣΧΑΜΑ</seg>
                </seg>
                <seg n="168r1" type="folio">
                    <seg n="12" type="line" facs="z168r1_12"><seg type="word">
                            <supplied reason="lost">κύκλ</supplied>ου</seg> δοθέντος <seg
                            type="word">
                            <supplied reason="lost">κ</supplied>αὶ</seg> ἐν τῶι κύκλωι</seg>
                    <seg n="13" type="line" facs="z168r1_13"><seg type="word">
                            <supplied reason="lost">γραμμᾶ</supplied>ς</seg> ἐλάσσονος τᾶς <seg
                            type="clearword">διαμέ</seg></seg>
                    <seg n="14" type="line" facs="z168r1_14"><seg type="wordend">
                            <supplied reason="lost">τρου</supplied>
                        </seg>
                        <seg type="word">δ<unclear>υνα</unclear>τὸν</seg> ἀπὸ τοῦ κέντρου τοῦ</seg>
                    <seg n="15" type="line" facs="z168r1_15"><seg type="word">
                            <supplied reason="lost">κύ</supplied>κλου</seg> ποτὶ τὰν περιφέρειαν</seg>
                    <seg n="16" type="line" facs="z168r1_16"><seg type="word">
                            <supplied reason="lost">αὐτ</supplied>οῦ</seg> ποτιβαλεῖν εὐθεῖαν <seg
                            type="clearword">
                            <choice>
                                <abbr>τέμν</abbr>
                                <expan>τέμνου</expan>
                            </choice>
                        </seg></seg>
                    <seg n="17" type="line" facs="z168r1_17"><seg type="wordend">
                            <supplied reason="lost">σαν</supplied>
                        </seg> τὰν ἐν τῶι <seg type="word">κ<supplied reason="lost">ύ</supplied>κλωι</seg>
