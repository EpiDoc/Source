    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="128r1" type="folio">
                    <seg n="1" type="line" facs="z128r1_01">ἡ ϘΙ τῆς ΨΒ. τῆς δ’ ἐλάσσων ἄρα</seg>
                    <seg n="2" type="line" facs="z128r1_02">ἡ ΟΙ τῆς ΨΒ· ὥστε ἡ ΙΩ μείζων</seg>
                    <seg n="3" type="line" facs="z128r1_03">ἐστὶ <seg type="word">τῆ<supplied
                                reason="lost">ς</supplied></seg> ΨΡ. ἡ δὲ ΨΡ ἴση ἐστὶ τῆς</seg>
                    <seg n="4" type="line" facs="z128r1_04">Φ· μείζων ἄρα ἐστὶν ἡ ΠΗ τῆς Φ.</seg>
                    <seg n="5" type="line" facs="z128r1_05">καὶ ἐπεὶ ὑπόκειται τὸ τμῆμα</seg>
                    <seg n="6" type="line" facs="z128r1_06">τῶι βάρει πρὸς τὸ ὑγρὸν ἔχειν <seg
                            type="unclearword"><unclear>λ</unclear>ό</seg></seg>
                    <seg n="7" type="line" facs="z128r1_07"><seg type="wordend">γον,</seg> ὃν
                        τετράγωνον τὸ ἀπὸ τῆς</seg>
                    <seg n="8" type="line" facs="z128r1_08">ΦΧ πρὸς τὸ τετράγωνον τὸ ἀπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z128r1_09">ΒΔ, ὃν δὲ λόγον ἔχει τὸ τμῆμα</seg>
                    <seg n="10" type="line" facs="z128r1_10">τῶι βάρει πρὸς τὸ ὑγρόν, τοῦτον</seg>
                    <seg n="11" type="line" facs="z128r1_11">ἔχει τὸν λόγον τὸ δεδυκὸς <choice>
                            <abbr>αὐτ</abbr>
                            <expan>αὐτοῦ</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z128r1_12">πρὸς τὸ ὅλον τμῆμα, ὃν δὲ τὸ <seg
                            type="unclearword">δεδυ</seg></seg>
                    <seg n="13" type="line" facs="z128r1_13"><seg type="wordend"
                            >κὸ<unclear>ς</unclear></seg> πρὸς τὸ ὅλον, τοῦτον ἔχει τὸ <w part="I"
                            >τε</w></seg>
                    <seg n="14" type="line" facs="z128r1_14"><w part="F">τράγωνον</w> τὸ ἀπὸ τῆς ΠΜ <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z128r1_15">τὸ τετράγωνον τὸ ἀπὸ τῆς
                        Ο<unclear>Ν</unclear>,</seg>
                    <seg n="16" type="line" facs="z128r1_16">ὃν ἄρα λόγον ἔχει τὸ <choice>
                            <abbr>τετράγων</abbr>
                            <expan>τετράγωνον</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z128r1_17">τὸ ἀπὸ τῆς ΦΧ πρὸς τὸ <w part="I"
                        >τετρά</w></seg>
                    <seg n="18" type="line" facs="z128r1_18"><w part="F">γωνον</w> τὸ ἀπὸ τῆς ΒΔ,
                        τοῦτον</seg>
                    <seg n="19" type="line" facs="z128r1_19">ἔχει τὸν <seg type="word">λ<supplied
                                reason="lost">όγ</supplied>ον</seg> τὸ τετράγωνον</seg>
                </seg>
                <seg n="129v1" type="folio">
                    <seg n="1" type="line" facs="z129v1_01">τὸ ἀπὸ τῆς ΜΠ πρὸς τὸ <w part="I"
                        >τετρά</w></seg>
                    <seg n="2" type="line" facs="z129v1_02"><w part="F">γωνον</w> τὸ ἀπὸ τῆς ΟΝ· ἴση
                        ἄρα</seg>
                    <seg n="3" type="line" facs="z129v1_03"><expan>ἐστὶν</expan> ἡ ΦΧ τῆι ΠΜ. ἡ δὲ
                        ΠΗ ἐδείχθη</seg>
                    <seg n="4" type="line" facs="z129v1_04">μείζων οὖσα τῆς Φ· δῆλον οὖν,</seg>
                    <seg n="5" type="line" facs="z129v1_05"><expan>ὅτι</expan> ἡ ΠΜ ἐλάσσων ἡμιολία
                            <seg type="word">
                            <choice>
                                <abbr>ἐστι</abbr>
                                <expan>ἐστὶ<unclear>ν</unclear></expan>
                            </choice>
                        </seg></seg>
                    <seg n="6" type="line" facs="z129v1_06">τῆς ΠΗ, ἡ δὲ ΠΗ τῆς ΗΜ <seg type="word">
                            <choice>
                                <abbr>μειζ</abbr>
                                <expan>μεί<unclear>ζ</unclear>ων</expan>
                            </choice>
                        </seg></seg>
                    <seg n="7" type="line" facs="z129v1_07">ἢ διπλασίων. ἔστω οὖν ἡ ΠΖ <w part="I"
                            >δι</w></seg>
                    <seg n="8" type="line" facs="z129v1_08"><w part="F">πλασία</w> τῆς ΖΜ· ἔσται δὴ
                        τὸ</seg>
                    <seg n="9" type="line" facs="z129v1_09">μὲν Θ κέντρον τοῦ βάρους <w part="I"
                        >στε</w></seg>
                    <seg n="10" type="line" facs="z129v1_10"><w part="F">ρεοῦ,</w> τοῦ δ’ ἐν τῶι
                        ὑγρῶι τὸ Ζ· τοῦ</seg>
                    <seg n="11" type="line" facs="z129v1_11">λοιποῦ μεγέθους τὸ κέντρον</seg>
                    <seg n="12" type="line" facs="z129v1_12">τοῦ βάρους ἔσται ἐπὶ τῆς <seg
                            type="word">τᾶ<supplied reason="lost">ς</supplied></seg></seg>
                    <seg n="13" type="line" facs="z129v1_13">ΖΘ ἐπιζευγνυούσης, καὶ <w part="I"
                        >ἐκ</w></seg>
                    <seg n="14" type="line" facs="z129v1_14"><w part="F">βεβλήσθω</w> ἐπὶ τὸ ΕΓ· <w
                            part="I">δειχθή</w></seg>
                    <seg n="15" type="line" facs="z129v1_15"><w part="F">σεται</w> δὲ ὁμοίως ἡ ΘΗ
                            <seg type="word">κάθετο<supplied reason="lost">ς</supplied></seg></seg>
                    <seg n="16" type="line" facs="z129v1_16">οὖσα ἐπὶ τὴν τοῦ ὑγροῦ <seg type="word">
                            <choice>
                                <abbr>ἐπιφάνει</abbr>
                                <expan>ἐπιφάνει<supplied reason="lost">αν</supplied></expan>
                            </choice>
                        </seg>,</seg>
                </seg>
                <seg n="128r2" type="folio">
                    <seg n="1" type="line" facs="z128r2_01">καὶ τὸ μὲν ἐντὸς τοῦ <seg type="word"
                                >ὑγρο<unclear>ῦ</unclear></seg>
                        <seg type="word"><unclear>τ</unclear>μῆμ<supplied reason="lost"
                        >α</supplied></seg></seg>
                    <seg n="2" type="line" facs="z128r2_02">ἐνεχθήσεται εἰς τὸ ἐκτὸς τοῦ ὑγροῦ</seg>
                    <seg n="3" type="line" facs="z128r2_03">κατὰ τὴν διὰ τοῦ Ζ <seg type="word"
                                ><supplied reason="lost">ἀ</supplied>γομένην</seg>
                        <seg type="suppliedword">κά<supplied reason="lost">θ</supplied>ε</seg></seg>
                    <seg n="4" type="line" facs="z128r2_04"><seg type="wordend">τον</seg> ἐπὶ τὴν
                        τοῦ ὑγροῦ <choice>
                            <abbr>ἐπιφάνεια</abbr>
                            <expan>ἐπιφάνειαν</expan>
                        </choice>,</seg>
                    <seg n="5" type="line" facs="z128r2_05">τὸ δ’ ἐκτὸς τοῦ ὑγροῦ <choice>
                            <abbr>ἐνεχθήσετ</abbr>
                            <expan>ἐνεχθήσεται</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z128r2_06">ἐς τὸ ὑγρὸν κατὰ τὴν διὰ τοῦ Γ· <seg
                            type="word">ο<unclear>ὐ</unclear></seg></seg>
                    <seg n="7" type="line" facs="z128r2_07">μενεῖ δὲ τὸ τμῆμα κατὰ <seg type="word"
                                >τὴ<unclear>ν</unclear></seg>
                        <w part="I">ὑπο</w></seg>
                    <seg n="8" type="line" facs="z128r2_08"><w part="F">τεθεῖσαν</w> κλίσιν.</seg>
                </seg>
                <seg n="128r2" type="folio">
                    <seg n="8" type="line" facs="z128r2_08">οὐδὲ μὴν <seg type="word">ἐ<supplied
                                reason="lost">ς</supplied></seg>
                        <seg type="suppliedword">ὀ<unclear>ρ</unclear></seg></seg>
                    <seg n="9" type="line" facs="z128r2_09"><seg type="wordend">θ<supplied
                                reason="lost">ὸ</supplied>ν</seg> ἀποκαταστήσηται. δῆλόν <seg
                            type="word"><unclear>γ</unclear>ε</seg></seg>
                    <seg n="10" type="line" facs="z128r2_10">διὰ <seg type="word">τού<supplied
                                reason="lost">τ</supplied>ων·</seg>
                        <seg type="word">ἐπ<unclear>ὶ</unclear></seg> γὰρ τῶν <choice>
                            <abbr>ἠγμένω</abbr>
                            <expan>ἠγμένων</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z128r2_11">διὰ τῶν ΖΓ καθέτων ἡ μὲν διὰ</seg>
                    <seg n="12" type="line" facs="z128r2_12">τοῦ Ζ ἀγομένη τῆς ΓΖ ἐπὶ τὰ αὐτὰ</seg>
                    <seg n="13" type="line" facs="z128r2_13">μέρη πίπτει, ἐφ’ ἅ ἐστί κα τὸ Γ, ἡ δὲ</seg>
                    <seg n="14" type="line" facs="z128r2_14">διὰ τοῦ Γ ἐπὶ τὰ αὐτὰ τῆι Ζ<supplied
                            reason="lost">Γ,</supplied> δῆλον,</seg>
                    <seg n="15" type="line" facs="z128r2_15"><expan>ὅτι</expan>
                        <expan>διὰ</expan> τὰ προειρημένα τὸ μὲν Ζ <seg type="suppliedword"
                                ><supplied reason="lost">κ</supplied>έν</seg></seg>
                    <seg n="16" type="line" facs="z128r2_16"><seg type="wordend">τρον</seg>
                        ἀνοισθήσεται, τὸ δὲ Γ <seg type="word">κάτ<unclear>ω·</unclear></seg></seg>
                    <seg n="17" type="line" facs="z128r2_17">ὥστε τοῦ ὅλου μεγέθους τὰ ἐπὶ <seg
                            type="word">τ<unclear>ὰ</unclear></seg></seg>
                    <seg n="18" type="line" facs="z128r2_18"><seg type="word"
                            >α<unclear>ὐ</unclear>τὰ</seg>
                        <supplied reason="lost">μέρη</supplied> τοῦ Α κάτω οἰσθήσεται.</seg>
                </seg>
                <seg n="128r2" type="folio">
                    <seg n="19" type="line" facs="z128r2_19">τοῦ δ’ ἦν εὔχρηστον πρὸς τὸ
                    δεῖξαι.</seg>
                </seg>
                <seg n="129v2" type="folio">
                    <figure n="2.8.1">
                        <figDesc xml:lang="eng">Figure 2.8.1</figDesc>
                    </figure>
                    <seg n="1" type="line" facs="z129v2_01">Ὑποκείσθω πάλιν <seg type="word"
                                ><unclear>τ</unclear>ὰ</seg>
                        <seg type="word"><unclear>μ</unclear>ὲν</seg> ἄλλα τὰ</seg>
                    <seg n="2" type="line" facs="z129v2_02">αὐτά, ὁ δ’ ἄξων τοῦ τμήματος
                        <expan>πρὸς</expan></seg>
                    <seg n="3" type="line" facs="z129v2_03"><choice>
                            <abbr>τη</abbr>
                            <expan>τὴν</expan>
                        </choice> ἐπιφάνειαν τοῦ ὑγροῦ <w part="I">ποιεί</w></seg>
                    <seg n="4" type="line" facs="z129v2_04"><w part="F">τω</w> γωνίας <seg
                            type="word"><supplied reason="lost">τῆ</supplied>ς</seg> Β· <seg
                            type="word">ἔ<unclear>λ</unclear>ασ<unclear>σ</unclear>ον</seg>
                        <choice>
                            <abbr>δηλ</abbr>
                            <expan>δῆλον</expan>
                        </choice>
                    </seg>
                </seg>
            </ab>
        </body>
