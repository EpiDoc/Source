    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="138v-135r" type="folio">
                    <seg n="01" type="line" facs="z138v-135r_01"><unclear>ἂν</unclear>
                            ἀ<unclear>δελ</unclear>φ<unclear>ὰ</unclear>
                        <seg type="word">
                            <choice>
                                <abbr>παιδάρι</abbr>
                                <expan>παιδάρια</expan>
                            </choice>
                        </seg> π<unclear>ωλ</unclear>ῶσιν ἢ <unclear>μη</unclear>τέρα καὶ παιδία</seg>
                    <seg n="02" type="line" facs="z138v-135r_02"><gap extent="12"/>
                        <unclear>ὅτι</unclear> ζημιού<unclear>μ</unclear>ε<unclear>ν</unclear>οι
                        ἐλάττονος</seg>
                    <seg n="03" type="line" facs="z138v-135r_03"
                            >ἀ<unclear>π</unclear>οδίδοντα<unclear>ι</unclear>
                        <gap extent="3"/><unclear>ν</unclear>
                        <unclear>τι</unclear> τοῦτο τῶν <seg type="word">
                            <choice>
                                <abbr>δικαίω</abbr>
                                <expan>δικαίων</expan>
                            </choice>
                        </seg> ὄν. αἱ <seg type="word">
                            <choice>
                                <abbr>γ</abbr>
                                <expan>γὰρ</expan>
                            </choice>
                        </seg></seg>
                    <seg n="04" type="line" facs="z138v-135r_04">ε<unclear>ὔ</unclear>νοιαι
                            <unclear>τοῖς</unclear> ἀνθρώ<unclear>ποις</unclear> εἰσὶ διὰ τὴν <w
                            part="I">συνήθει</w></seg>
                    <seg n="05" type="line" facs="z138v-135r_05"><w part="F">αν</w> καὶ
                        τ<unclear>ὸ</unclear> συντρόφους αὐτοὺς εἶναι μᾶλλον ἢ <w part="I">δι</w></seg>
                    <seg n="06" type="line" facs="z138v-135r_06"><w part="F">ὰ</w> τὰς συγγενείας·
                            <unclear>τ</unclear>εκμήριον δὲ τούτου· <unclear>ο</unclear>ὔ<unclear>τε</unclear>
                        <unclear>γὰρ</unclear></seg>
                    <seg n="07" type="line" facs="z138v-135r_07">ἂ<unclear>ν</unclear>
                        <seg type="word">
                            <choice>
                                <abbr>πρς</abbr>
                                <expan><unclear>π</unclear>ατέ<unclear>ρ</unclear>ες</expan>
                            </choice>
                        </seg>
                        <supplied reason="lost">το</supplied><unclear>ὺ</unclear>ς
                        <unclear>αὑτῶν</unclear> παῖδ<unclear>ας</unclear>
                        <unclear>ἀσπάσαιντο</unclear>, εἰ μὴ <unclear>ὑπ’</unclear>
                        <w part="I">αὐ</w></seg>
                    <seg n="08" type="line" facs="z138v-135r_08"><w part="F">τῶν</w> ἐκ παιδαρίων <choice>
                            <sic>τραφεί<unclear>σ</unclear>αν</sic>
                            <corr>τραφείη<unclear>σ</unclear>αν</corr>
                        </choice>, εἰ εὐθύς τις <w part="I">αὐ</w></seg>
                    <seg n="09" type="line" facs="z138v-135r_09"><w part="F">τῶν</w>
                        <gap extent="8"/>
                        <unclear>ἀπο</unclear>σπ<unclear>ά</unclear>σ<unclear>α</unclear>ς, οὔτε
                            <unclear>ο</unclear>ἱ παῖδες τοὺς</seg>
                    <seg n="10" type="line" facs="z138v-135r_10"
                        >γ<unclear>ο</unclear>νέ<unclear>ας</unclear> εἰ μὴ ὑπ’ ἐκείνων
                        τ<unclear>ρα</unclear>φείησαν. <w part="I">Τί</w></seg>
                    <seg n="11" type="line" facs="z138v-135r_11"><w part="F">μανδρος</w> τοίνυν
                        τούτου <unclear>α</unclear>ὐ<unclear>τοῦ</unclear> γ<unclear>ε</unclear>
                        αἴτιος <seg type="word">
                            <choice>
                                <abbr>γέγον</abbr>
                                <expan>γέγονεν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="12" type="line" facs="z138v-135r_12">ὥστε
                            <unclear>τ</unclear>ὰ<unclear>ς</unclear> μὲν ἀδελφὰ<unclear>ς</unclear>
                        ἀλλήλας μὴ <seg type="word">
                            <choice>
                                <abbr>ἀναγνῶν</abbr>
                                <expan>ἀναγνῶν<unclear>αι</unclear></expan>
                            </choice>
                        </seg></seg>
                    <seg n="13" type="line" facs="z138v-135r_13">μήτε ἐν <unclear>ὁδῶι</unclear>
                        μήτε ἐν <unclear>ἱερῶι</unclear> ἰδούσας – <seg type="unclearword"
                        >πλεόν</seg></seg>
                    <seg n="14" type="line" facs="z138v-135r_14"><seg type="wordend">
                            <unclear>ων</unclear>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr>γ</abbr>
                                <expan>
                                    <unclear>γὰρ</unclear>
                                </expan>
                            </choice>
                        </seg> ἐτῶν ἢ τριῶν καὶ δέκα οὐχ ἑωράκασιν</seg>
                    <seg n="15" type="line" facs="z138v-135r_15">ἑαυτάς – τὸν δὲ ἀδελφὸν τουτονὶ
                        Ἀκάδημον</seg>
                    <seg n="16" type="line" facs="z138v-135r_16">ἀναγνωρίσαι τὴν ἑαυτοῦ ἀδελφὴν,
                            <seg type="word">
                            <choice>
                                <abbr>ἐλθόν</abbr>
                                <expan>ἐλθόντα</expan>
                            </choice>
                        </seg></seg>
                    <seg n="17" type="line" facs="z138v-135r_17">δ<unclear>ὲ</unclear>
                        <unclear>εἰς</unclear>
                        <unclear>Λῆμνον</unclear>
                        <unclear>μὴ</unclear>
                        <unclear>γνῶ</unclear>να<unclear>ι</unclear>
                        <seg type="word">
                            <choice>
                                <abbr>ἰδόν</abbr>
                                <expan>ἰδόντα</expan>
                            </choice>
                        </seg>. κα<unclear>ίτοι</unclear>
                        <unclear>ὁ</unclear>
                        <seg type="unclearword">
                            <unclear>νομο</unclear>
                        </seg></seg>
                    <seg n="18" type="line" facs="z138v-135r_18"><seg type="wordend"
                                >θέ<unclear>τ</unclear>η<unclear>ς</unclear></seg> τοὺς παῖδας
                            τοὺ<unclear>ς</unclear> ὀρφάνους οὐ χωρὶς <w part="I">ἕ</w></seg>
                    <seg n="19" type="line" facs="z138v-135r_19"><w part="F">καστον</w> τρέφεσθαι <choice>
                            <sic>ὠιήθην</sic>
                            <corr>ὠιήθη</corr>
                        </choice>
                        <unclear>δ</unclear>εῖν, οὐδ’ ὅ<unclear>π</unclear>ως ἂν <w part="I">τύ</w></seg>
                    <seg n="20" type="line" facs="z138v-135r_20"><w part="F">χωσιν,</w> ἀλλ’ ὅπου ἂν
                            ἄ<unclear>ρ</unclear>ι<unclear>στα</unclear>
                        <supplied reason="lost"><gap extent="5"/>τρ</supplied>έφεσθαι·</seg>
                    <seg n="21" type="line" facs="z138v-135r_21">καί μοι λέγε τὸν νόμον. ΝΟΜΟΣ</seg>
                    <seg n="22" type="line" facs="z138v-135r_22">εἰ τοίνυν παρά σοι εὖ ἐτρέφετο, ὦ
                        Τίμανδρε, ἡ μία,</seg>
                    <seg n="23" type="line" facs="z138v-135r_23">διὰ τί οὐ κα<unclear>ὶ</unclear>
                        οὗτοι εὖ ἐτρέφοντο παρά σοι καὶ ἐν</seg>
                    <seg n="24" type="line" facs="z138v-135r_24">τῶι αὐτῶι; εἰ δ’ οὗτοι εὖ, διὰ τί
                        οὐχὶ καὶ ἐκεινὴ</seg>
                    <seg n="25" type="line" facs="z138v-135r_25">εὖ καὶ ἐν τῶι αὐτῶι τοῖς ἀδελφοῖς
                        καὶ τῆι <w part="I">ἀδελ</w></seg>
                    <seg n="26" type="line" facs="z138v-135r_26"><w part="F">φῆι</w> τῆι
                        πρεσβυτέραι; ἀλλ’ οἶμαι <unclear>ἡ</unclear> τῶν <w part="I">χρημά</w></seg>
                    <seg n="27" type="line" facs="z138v-135r_27"><w part="F">των</w> ἐπιθυμία
                            τ<unclear>α</unclear>ῦτ<unclear>α</unclear> πάντα παρανομεῖν ἐποίει·</seg>
                    <seg n="28" type="line" facs="z138v-135r_28"
                            >τοι<unclear>γαρο</unclear>ῦ<unclear>ν</unclear> ἐκ <seg type="word">
                            <choice>
                                <abbr>πένητ</abbr>
                                <expan>πένητ<unclear>ος</unclear></expan>
                            </choice>
                        </seg> ἐπιτροπεύσας <seg type="word">
                            <choice>
                                <abbr>Ἀκάδημο</abbr>
                                <expan>Ἀκάδημον</expan>
                            </choice>
                        </seg></seg>
                    <seg n="29" type="line" facs="z138v-135r_29">τουτονὶ ἐκ τῶν τούτου πλέον ἢ πέντε
                            <seg type="unclearword"><unclear>τ</unclear>αλάν</seg></seg>
                    <seg n="30" type="line" facs="z138v-135r_30"><seg type="wordend">των</seg>
                        οὐσίαν ἔχει, ὡς ἐγὼ ὑμῖν ἐπιδείξω· <seg type="word">
                            <choice>
                                <abbr>πρῶτ</abbr>
                                <expan>πρῶτον</expan>
                            </choice>
                        </seg></seg>
                    <seg n="31" type="line" facs="z138v-135r_31">μ<unclear>ὲν</unclear> γὰρ εὐθὺς
                        τῶι πρώτωι ἐν<unclear>ι</unclear>αυτῶι <seg type="word">
                            <choice>
                                <abbr>ὡ</abbr>
                                <expan>
                                    <unclear>ὡς</unclear>
                                </expan>
                            </choice>
                        </seg>
                        <supplied reason="lost">ὁ</supplied>
                        <seg type="word">
                            <choice>
                                <abbr>
                                    <unclear>πρ</unclear>
                                </abbr>
                                <expan>
                                    <unclear>π</unclear>ατὴ<unclear>ρ</unclear>
                                </expan>
                            </choice>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr>αὐτῶ</abbr>
                                <expan>αὐτ<unclear>ῶ</unclear>ν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="32" type="line" facs="z138v-135r_32"><unclear>ἐτελεύ</unclear>τησεν τήν
                        τε παιδίσκην ἔλαβεν καὶ πέντε</seg>
                </seg>
            </ab>
