    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="94v1" type="folio">
                    <seg n="1" type="line" facs="z94v1_01"><expan>οὕτως</expan> τὸ ἀπὸ ΓΔ
                            <expan>πρὸς</expan> τὸ ἀπὸ ΗΘ, <choice>
                            <abbr><unclear>τ</unclear>ουτ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice> ἡ ΗΘ </seg>
                    <seg n="2" type="line" facs="z94v1_02"><expan>πρὸς</expan>
                        <seg type="word">
                            <unclear>Ε</unclear>
                            <supplied reason="lost">Ζ,</supplied>
                        </seg>
                        <seg type="word"><supplied reason="lost"
                        >κ</supplied><unclear>α</unclear>ὶ</seg> ἐναλλάξ, <seg type="word"
                                ><unclear>ὡ</unclear>ς</seg> ἡ ΓΔ <expan>πρὸς</expan> τὴν ΗΘ,</seg>
                    <seg n="3" type="line" facs="z94v1_03"><expan>οὕτως</expan> ἡ ΗΘ <expan>πρὸς</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice> ΜΝ καὶ ἡ ΜΝ <expan>πρὸς</expan>
                        <supplied reason="lost">τὴν</supplied>
                        <seg type="word"><supplied reason="lost">Ε</supplied>Ζ.</seg>
                    </seg>
                    <seg n="4" type="line" facs="z94v1_04"><expan>καί</expan>
                        <expan>ἐστιν</expan> δοθεῖσα <seg type="word"
                        >ἑ<unclear>κ</unclear>ατέρα</seg> τῶν ΓΔ, <seg type="word"><supplied
                                reason="lost">Ε</supplied>Ζ·</seg>
                    </seg>
                    <seg n="5" type="line" facs="z94v1_05">δύο ἄρα δοθεισῶν εὐθειῶν τῶν ΓΔ </seg>
                    <seg n="6" type="line" facs="z94v1_06">ΕΖ δύο μέσαι ἀνάλογόν
                        <expan>εἰσιν</expan> αἱ ΗΘ ΜΝ· </seg>
                    <seg n="7" type="line" facs="z94v1_07">δοθεῖσα <expan>ἄρα</expan> ἑκατέρα τῆς ΗΘ
                        ΜΝ.</seg>
                </seg>
                <seg n="94v1" type="folio">
                    <seg n="8" type="line" facs="z94v1_08">συντεθήσεται δὴ τὸ πρόβλημα οὕτως·</seg>
                    <seg n="9" type="line" facs="z94v1_09"><seg type="word">ἔ<supplied reason="lost"
                                >στ</supplied>ω</seg> δὴ ὁ δοθεὶς κῶνος ἢ <choice>
                            <abbr>κυλινδρο</abbr>
                            <expan>κύλινδρος</expan>
                        </choice>
                    </seg>
                    <seg n="10" type="line" facs="z94v1_10">ὁ Α· δεῖ δὴ τῶι Α κώνωι ἢ <choice>
                            <abbr>κυλινδρ</abbr>
                            <expan>κυλίνδρω</expan>
                        </choice>
                    </seg>
                    <seg n="11" type="line" facs="z94v1_11">ἴσην σφαῖραν εὑρεῖν.</seg>
                </seg>
                <seg n="94v1" type="folio">
                    <seg n="11" type="line" facs="z94v1_11">ἔστω τοῦ Α <w part="I">κώ</w></seg>
                    <seg n="12" type="line" facs="z94v1_12"><w part="F">νου</w> ἢ κυλίνδρου ἡμιόλιος
                        κύλινδρος, </seg>
                    <seg n="13" type="line" facs="z94v1_13">οὗ βάσις ὁ περὶ διάμετρον τὴν ΓΔ </seg>
                    <seg n="14" type="line" facs="z94v1_14">κύκλος, ἄξων δὲ ὁ ΕΖ, καὶ εἰλήφθω <choice>
                            <abbr>τ</abbr>
                            <expan>τῶν</expan>
                        </choice>
                    </seg>
                    <seg n="15" type="line" facs="z94v1_15">ΓΔ ΕΖ δύο μέσαι ἀνάλογον αἱ <seg
                            type="word">Η<unclear>Θ</unclear></seg> ΜΝ, </seg>
                    <seg n="16" type="line" facs="z94v1_16">ὥστε εἶναι <expan>ὡς</expan> τὴν ΓΔ
                            <expan>πρὸς</expan> τὴν ΗΘ, τὴν ΗΘ</seg>
                    <seg n="17" type="line" facs="z94v1_17"><expan>πρὸς</expan> τὴν ΜΝ
                        <expan>καὶ</expan> τὴν ΜΝ <expan>πρὸς</expan> τὴν ΕΖ, καὶ <w part="I">νο</w></seg>
                    <seg n="18" type="line" facs="z94v1_18"><w part="F">είσθω</w> κύλινδρος, οὗ
                        βάσις ὁ περὶ <seg type="expandedword">διά</seg></seg>
                    <seg n="19" type="line" facs="z94v1_19"><seg type="wordend">μετρον</seg> τὴν ΗΘ
                        κύκλος, ἄξων δὲ ὁ</seg>
                </seg>
                <seg n="91r1" type="folio">
                    <seg n="1" type="line" facs="z91r1_01">ΚΛ ἴσος τῆι ΗΘ <choice>
                            <abbr>μετρωι</abbr>
                            <expan>διαμέτρωι·</expan>
                        </choice> λέγω δὴ <expan>ὅτι</expan></seg>
                    <seg n="2" type="line" facs="z91r1_02">ἴσος <expan>ἐστὶν</expan> ὁ Ε κύλινδρος
                        τῶι Κ <w part="I">κυλίν</w></seg>
                    <seg n="3" type="line" facs="z91r1_03">
                        <w part="F">δρωι</w>
                    </seg>
                </seg>
                <seg n="91r1" type="folio">
                    <seg n="3" type="line" facs="z91r1_03"><expan>καὶ</expan> ἐπεί <expan>ἐστιν,</expan>
                        <expan>ὡς</expan> ἡ ΓΔ <expan>πρὸς</expan> ΗΘ, ἡ ΜΝ <expan>πρὸς</expan> ΕΖ,</seg>
                    <seg n="4" type="line" facs="z91r1_04"><expan>καὶ</expan> ἐναλλάξ,
                        <expan>καὶ</expan> ἴση ἡ ΗΘ τῆι ΚΛ ὡς <expan>ἄρα</expan>
                    </seg>
                    <seg n="5" type="line" facs="z91r1_05">ἡ ΓΔ <expan>πρὸς</expan> ΜΝ, <choice>
                            <abbr>τουτ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice> ὡς τὸ ἀπὸ τῆς ΓΔ </seg>
                    <seg n="6" type="line" facs="z91r1_06"><expan>πρὸς</expan> τὸ ἀπὸ ΗΘ,
                            <expan>οὕτως</expan> ὁ Ε κύκλος <expan>πρὸς</expan> τὸν Κ <w part="I"
                        >κύ</w></seg>
                    <seg n="7" type="line" facs="z91r1_07"><w part="F">κλον,</w>
                        <expan>ὡς</expan>
                        <expan>ἄρα</expan> ὁ <supplied reason="lost">Ε</supplied>
                        <choice>
                            <abbr>
                                <seg type="word"><supplied reason="lost">κ</supplied>υκλ</seg>
                            </abbr>
                            <expan>κύκλος</expan>
                        </choice>
                        <expan>πρὸς</expan> τὸν Κ κύκλον, <expan>οὕτως</expan>
                    </seg>
                    <seg n="8" type="line" facs="z91r1_08">ἡ ΚΔ <expan>πρὸς</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice> ΒΖ τῶν <ex>ἄρα</ex> ΕΚ κυλίνδρων</seg>
                    <seg n="9" type="line" facs="z91r1_09">ἀντιπεπόνθασιν αἱ βάσεις τοῖς <w part="I"
                            >ὕψε</w></seg>
                    <seg n="10" type="line" facs="z91r1_10"><w part="F">σιν·</w> ἴσος
                        <expan>ἄρα</expan> ὁ Ε κύλινδρος τῶι Κ <w part="I">κυ</w></seg>
                    <seg n="11" type="line" facs="z91r1_11"><w part="F">λίνδρωι.</w> ὁ δὲ Κ
                        κύλινδρος τῆς <seg type="expandedword">
                            <choice>
                                <abbr>σφ</abbr>
                                <expan>σφαί</expan>
                            </choice>
                        </seg></seg>
                    <seg n="12" type="line" facs="z91r1_12"><seg type="wordend">ρας,</seg> ἧς <choice>
                            <abbr>μετρος</abbr>
                            <expan>διάμετρος</expan>
                        </choice> ἡ ΗΘ, <seg type="word"
                            >ἡ<unclear>μ</unclear>ιόλι<unclear>ός</unclear></seg>
                    </seg>
                    <seg n="13" type="line" facs="z91r1_13"><expan>ἐστιν·</expan> καὶ ἡ σφαῖρα
                            <expan>ἄρα,</expan> ἧς ἡ <choice>
                            <abbr>
                                <seg type="word">με<supplied reason="lost">τ</supplied>ρος</seg>
                            </abbr>
                            <expan>διάμετρος</expan>
                        </choice>
                        <seg type="word"><supplied reason="lost">ἴ</supplied>ση</seg>
                        <expan>ἐστὶ</expan>
                    </seg>
                    <seg n="14" type="line" facs="z91r1_14"><seg type="word"
                        ><unclear>τῆ</unclear>ι</seg> ΗΘ, <choice>
                            <abbr>τουτ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice> ἡ Β, ἴση <expan>ἐστὶ</expan>
                        <supplied reason="lost">τῶι</supplied>
                        <supplied reason="lost">Α</supplied>
                        <choice>
                            <abbr>κων</abbr>
                            <expan>κώνω</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z91r1_15">ἢ κυλίνδρωι.</seg>
                </seg>
                <seg n="94v2" type="folio">
                    <figure n="2.1.1">
                        <figDesc>Figure 2.1.1.</figDesc>
                    </figure>
                </seg>
                <seg n="94v2" type="folio">
                    <seg n="1" type="line" facs="z94v2_01">παντὶ τμήματι τῆς σφαίρας ἴσος
                            <expan>ἐστὶ</expan>
                    </seg>
                    <seg n="2" type="line" facs="z94v2_02">κῶνος ὁ βάσιν μὲν ἔχων τὴν <choice>
                            <abbr>αυτη</abbr>
                            <expan>αὐτὴν</expan>
                        </choice>
                    </seg>
                    <seg n="3" type="line" facs="z94v2_03">τῶι τμήματι, ὕψος δὲ εὐθεῖαν, ἧτις </seg>
                    <seg n="4" type="line" facs="z94v2_04">πρὸς τὸ ὕψος τοῦ τμήματος τὸν <w part="I"
                            >αὐ</w></seg>
                    <seg n="5" type="line" facs="z94v2_05"><w part="F">τὸν</w> λόγον ἔχει, ὃν
                        συναμφότερος </seg>
                    <seg n="6" type="line" facs="z94v2_06">ἧ τε ἐκ τοῦ κέντρου <choice>
                            <abbr>τη</abbr>
                            <expan>τῆς</expan>
                        </choice> σφαίρας </seg>
                    <seg n="7" type="line" facs="z94v2_07">καὶ τὸ ὕψος τοῦ λοιποῦ τμήματος </seg>
                    <seg n="8" type="line" facs="z94v2_08"><expan>πρὸς</expan> τὸ ὕψος τοῦ λοιποῦ
                        τμήματος.</seg>
                </seg>
                <seg n="91r2" type="folio">
                    <seg n="1" type="line" facs="z91r2_01">ἔστω σφαῖρα, ἐν ᾗ μέγιστος <seg
                            type="word"><supplied reason="lost">κύκ</supplied>λος,</seg>
                    </seg>
                    <seg n="2" type="line" facs="z91r2_02">οὗ <choice>
                            <expan>διάμετρος</expan>
                            <abbr>μετρος</abbr>
                        </choice> ἡ ΑΓ, καὶ τετμήσθω <w part="I">ἐ</w></seg>
                    <seg n="3" type="line" facs="z91r2_03"><w part="F">πιπέδωι</w> ἡ σφαῖρα τῶι
                            <expan>διὰ</expan> τῶν ΒΖ </seg>
                    <seg n="4" type="line" facs="z91r2_04"><expan>πρὸς</expan> ὀρθὰς τῆι ΑΓ,
                            <expan>καὶ</expan> ἔστω κέντρον </seg>
                    <seg n="5" type="line" facs="z91r2_05">τῶι Θ, καὶ <seg type="word"
                                >πεποιή<unclear>σ</unclear>θω,</seg> ὡς <seg type="unclearword"
                                >συ<unclear>ν</unclear>αμ</seg></seg>
                    <seg n="6" type="line" facs="z91r2_06"><seg type="wordend">φότερος</seg> ἡ ΘΑ ΑΕ
                            <expan>πρὸς</expan> τὴν ΑΕ, <expan>οὕτως</expan> ἡ </seg>
                    <seg n="7" type="line" facs="z91r2_07">ΔΕ <expan>πρὸς</expan> ΓΕ, καὶ πάλιν <seg
                            type="word"><unclear>π</unclear><supplied reason="lost"
                                >ε</supplied><unclear>π</unclear>οιήσθω,</seg>
                        <expan>ὡς</expan>
                    </seg>
                    <seg n="8" type="line" facs="z91r2_08">συναμφότερος ἡ ΘΓ ΓΕ <expan>πρὸς</expan>
                        ΓΕ, <expan>οὕτως</expan> ἡ </seg>
                    <seg n="9" type="line" facs="z91r2_09">ΚΕ <expan>πρὸς</expan> ΕΑ, καὶ
                        ἀναγεγράφθωσαν </seg>
                    <seg n="10" type="line" facs="z91r2_10">κῶνοι ἀπὸ τοῦ κύκλου τοῦ περὶ <seg
                            type="expandedword">
                            <choice>
                                <expan>διάμε</expan>
                                <abbr>με</abbr>
                            </choice>
                        </seg></seg>
                    <seg n="11" type="line" facs="z91r2_11"><seg type="wordend">τρον</seg> τὴν ΒΖ
                        κορυφὰς ἔχοντες </seg>
                    <seg n="12" type="line" facs="z91r2_12">τὰ Κ Δ σημεῖα· λέγω <expan>ὅτι</expan>
                        ἴσος <expan>ἐστὶν</expan> ὁ <choice>
                            <abbr>μ</abbr>
                            <expan>μὲν</expan>
                        </choice>
                    </seg>
                    <seg n="13" type="line" facs="z91r2_13">ΒΔΖ κῶνος τῶι κατὰ τὸ Γ <w part="I"
                        >τμή</w></seg>
                    <seg n="14" type="line" facs="z91r2_14"><w part="F">ματι</w> τῆς σφαίρας, ὁ δὲ
                        ΒΚΖ </seg>
                    <seg n="15" type="line" facs="z91r2_15">τῶι κατὰ τὸ Α σημείωι.</seg>
                </seg>
                <seg n="91r2" type="folio">
                    <seg n="15" type="line" facs="z91r2_15">
                        <w part="I">ἐπεζεύχθω</w>
                    </seg>
                    <seg n="16" type="line" facs="z91r2_16"><w part="F">σαν</w>
                        <expan>γὰρ</expan> αἱ ΒΘ ΘΖ, <expan>καὶ</expan> νοείσθω <seg type="word"
                                >κ<unclear>ῶ</unclear>νος</seg></seg>
                    <seg n="17" type="line" facs="z91r2_17">βάσιν μὲν ἔχων τὸν περὶ <choice>
                            <expan>διάμετρον</expan>
                            <abbr>μετρο</abbr>
                        </choice></seg>
                </seg>
            </ab>
        </body>
    </text>
