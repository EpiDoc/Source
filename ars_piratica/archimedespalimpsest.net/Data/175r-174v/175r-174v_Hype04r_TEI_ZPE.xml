    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="175r-174v" type="folio">
                    <seg n="01" type="line" facs="z175r-174v_01">Ἵππαρχον καὶ Κλείταρχον, ἐν Ἄργει
                        δὲ Μνασίαν, ἐν</seg>
                    <seg n="02" type="line" facs="z175r-174v_02">δὲ <choice>
                            <sic>Μεγάλη</sic>
                            <corr>Μεγάληι</corr>
                        </choice> πόλει Ἱερώνυμον καὶ Κερκιδᾶν. <supplied reason="lost">καὶ</supplied>
                        <supplied reason="lost">μὲ</supplied><unclear>ν</unclear>
                        <unclear>δ</unclear>ὴ</seg>
                    <seg n="03" type="line" facs="z175r-174v_03">πάλιν ἐρῶτησον "διὰ τί
                        <unclear>αὐ</unclear>τοὺς μισεῖτε;" φή<unclear>σ</unclear>ειαν
                        <unclear>δ’</unclear> ἄν,</seg>
                    <seg n="04" type="line" facs="z175r-174v_04">διότι ἕκαστος αὐτῶν κατὰ τῆς ἑαυτοῦ
                        πατρίδος</seg>
                    <seg n="05" type="line" facs="z175r-174v_05">ὑπὲρ Φιλίππου ἔπραττεν.
                            εἶ<unclear>τ’</unclear> οἴει αὐτοὺς το<unclear>ῖ</unclear>ς μὲν</seg>
                    <seg n="06" type="line" facs="z175r-174v_06">ἔξω Φιλίππωι συναγωνιζομένοις
                        πολεμεῖν, <supplied reason="lost">τοὺς</supplied></seg>
                    <seg n="07" type="line" facs="z175r-174v_07">δὲ ἐνθάδε ὑπὲρ
                        <unclear>ἐ</unclear>κείνου <seg type="word">
                            <choice>
                                <abbr>λέγοντ</abbr>
                                <expan>λέγοντας</expan>
                            </choice>
                        </seg> εὔνους τῆι πόλει</seg>
                    <seg n="08" type="line" facs="z175r-174v_08">ὑπολαμβάνειν εἶναι; οὐκ ἔστι ταῦτα.
                        ἀλλὰ, οἶμαι,</seg>
                    <seg n="09" type="line" facs="z175r-174v_09">τὰ <seg type="word">
                            <choice>
                                <abbr>πράγμα</abbr>
                                <expan>πράγματα</expan>
                            </choice>
                        </seg> τῆι πόλει οὕτως νῦν περιέστηκεν, ὡς οὐκ <w part="I">ἂ</w></seg>
                    <seg n="10" type="line" facs="z175r-174v_10"><w part="F">ν</w> ἐβουλόμην. ἐπεὶ
                            τί<unclear>ς</unclear> οὐκ ἂν ἀλγήσειεν, ὅταν ἐν</seg>
                    <seg n="11" type="line" facs="z175r-174v_11"><choice>
                            <sic>τῆ</sic>
                            <corr>τῆι</corr>
                        </choice>
                        <choice>
                            <sic>ἐκκλησία</sic>
                            <corr>ἐκκλησίαι</corr>
                        </choice> ὑμῶν <choice>
                            <sic>καλευόντων</sic>
                            <corr>κελευόντων</corr>
                        </choice> ἀπαλεῖψαί τι τῶν</seg>
                    <seg n="12" type="line" facs="z175r-174v_12">γεγραμμένων μὴ <choice>
                            <sic>ἐθέλη</sic>
                            <corr>ἐθέληι</corr>
                        </choice> Δημάδη<unclear>ς</unclear> – ἄλ<unclear>λο</unclear>
                        <unclear>γρ</unclear>άψ<unclear>ου</unclear>σι<unclear>ν</unclear> αὖ</seg>
                    <seg n="13" type="line" facs="z175r-174v_13"><seg type="word">
                            <choice>
                                <abbr>ὑμ</abbr>
                                <expan>ὑμῖν</expan>
                            </choice>
                        </seg> φήσει ἀ<unclear>πι</unclear>έναι ἐκ τῆς πόλεως – παρελθὼν δέ</seg>
                    <seg n="14" type="line" facs="z175r-174v_14">τις <unclear>τ</unclear>ῶν κοινωνῶν
                        αὐτοῦ <choice>
                            <sic>εἴπ<unclear>η</unclear></sic>
                            <corr>εἴπ<unclear>η</unclear>ι</corr>
                        </choice> ὅτι ὑμεῖς, <supplied reason="lost">ὡς</supplied>
                        <unclear>ἂν</unclear>
                        <choice>
                            <sic>ἔχ<unclear>η</unclear></sic>
                            <corr>ἔχ<unclear>η</unclear>ι</corr>
                        </choice>,</seg>
                    <seg n="15" type="line" facs="z175r-174v_15">ψηφιεῖσθε ταῦτα,
                            <unclear>ὅ</unclear>π<unclear>ω</unclear><supplied reason="lost">ς</supplied>
                        <unclear>ἂν</unclear>
                        <unclear>δουλείαν</unclear>
                        <unclear>ὑπάγειν</unclear>
                        <w part="I">ὑ</w></seg>
                    <seg n="16" type="line" facs="z175r-174v_16"><w part="F">πὸ</w> Δημάδου
                            ἔχ<unclear>η</unclear>τε. τοῦτο γὰρ τῶν πάντων <seg type="unclearword"
                                ><unclear>παραλ</unclear>ογώ</seg></seg>
                    <seg n="17" type="line" facs="z175r-174v_17"><seg type="wordend"
                            ><unclear>τα</unclear>τόν</seg> ἐστιν, ὅτι ἐ<unclear>ν</unclear>
                        <unclear>μ</unclear>ὲν <unclear>τοῖς</unclear>
                        <unclear>ἔμπ</unclear>ροσθε<supplied reason="lost">ν</supplied>
                        <supplied reason="lost">χρόνοις</supplied>
                        <supplied reason="lost">οἱ</supplied></seg>
                    <seg n="18" type="line" facs="z175r-174v_18">ἰδιῶται, εἰκότως, οἶμαι, <choice>
                            <sic>ἐδεδεί<unclear>κ</unclear>εσαν</sic>
                            <corr>ἐδεδοί<unclear>κ</unclear>εσαν</corr>
                        </choice> μὴ <seg type="suppliedword">τι<unclear>μ</unclear><supplied
                                reason="lost">ωρί</supplied></seg></seg>
                    <seg n="19" type="line" facs="z175r-174v_19"><seg type="wordend">αν</seg> ὑπὸ
                        τοῦ δήμου ἔχωσι, νυνὶ δὲ τοὐναντίον· ὁ <seg type="word">
                            <choice>
                                <abbr>δῆμ</abbr>
                                <expan>δῆμος</expan>
                            </choice>
                        </seg></seg>
                    <seg n="20" type="line" facs="z175r-174v_20"><choice>
                            <sic>δέδειεν</sic>
                            <corr>δέδιεν</corr>
                        </choice>, μὴ ὑπό των ἰδιωτῶν αἰτίαν λάβηι. <supplied reason="lost"
                        >καὶ</supplied></seg>
                    <seg n="21" type="line" facs="z175r-174v_21">γὰρ νῦν τὰ μὲν ἄλλα ἐ<supplied
                            reason="lost">ά</supplied>σω τῆς ἀσελγείας αὐτοῦ,
                        ἔλεγ<unclear>ε</unclear></seg>
                    <seg n="22" type="line" facs="z175r-174v_22">δὲ ἐν τῆι πρ<unclear>ώ</unclear>ην
                        ἐκκλησίαι δεῖν ἡμᾶς τὴν Πάραλον</seg>
                    <seg n="23" type="line" facs="z175r-174v_23">πέμψαντας ὡς Ἀλεξάνδρον μέμφεσθαι <choice>
                            <sic>αὐτῶ</sic>
                            <corr>αὐτῶι</corr>
                        </choice>, ὅτι</seg>
                    <seg n="24" type="line" facs="z175r-174v_24">ὑστάτοις ὑμῖν ἐπέστειλεν περὶ τῶν
                        τριήρων, <unclear>ὅπ</unclear>ως</seg>
                    <seg n="25" type="line" facs="z175r-174v_25"><choice>
                            <sic>δῆλον ὅτι</sic>
                            <corr>δηλονότι</corr>
                        </choice> τὸ λοιπὸν πρώτοις ἡμῖν ἐπιτάττ<supplied reason="lost">ηι</supplied>
                        <supplied reason="lost">ἕ</supplied>κα<unclear>στα</unclear>,</seg>
                    <seg n="26" type="line" facs="z175r-174v_26"><unclear>εἰ</unclear> δ’
                        ἀναγκαζομένους καλῶς ἔχει ποιεῖν ταὐ<unclear>τ</unclear><supplied
                            reason="lost">ά</supplied>, ἀνα<supplied reason="lost"
                            >γ</supplied>κά<unclear>σ</unclear>ει,</seg>
                    <seg n="27" type="line" facs="z175r-174v_27"><unclear>εἰ</unclear> μή, πρῶτοι
                        ποιήσωμεν. ἢ πάλιν, ἐπειδὰν <w part="I">Διών</w></seg>
                    <seg n="28" type="line" facs="z175r-174v_28"><w part="F">δας</w> ἐπὶ τῶν
                        δικαστηρίων σεμνύνηται λέγων ὅτι</seg>
                    <seg n="29" type="line" facs="z175r-174v_29"><seg type="word">
                            <choice>
                                <abbr>πεντήκον</abbr>
                                <expan>πεντήκοντα</expan>
                            </choice>
                        </seg> γραφὰς ἐγράψατο, εἰ δέ τις αὐτὸν <w part="I">ἐ</w></seg>
                    <seg n="30" type="line" facs="z175r-174v_30"><w part="F">ρωτήσαι</w>· "ἔστιν οὖν
                        ἥντινα τούτων ἥιρηκας;", <w part="I">οὐδε</w></seg>
                    <seg n="31" type="line" facs="z175r-174v_31"><w part="F">μίαν</w> φανήσεται·
                        οὔκουν ἀνάγκη ἐστὶν <seg type="word">
                            <choice>
                                <abbr>πεντήκον</abbr>
                                <expan><unclear>π</unclear><supplied reason="lost"
                                        >εντ</supplied><unclear>ή</unclear>κοντα</expan>
                            </choice>
                        </seg></seg>
                    <seg n="32" type="line" facs="z175r-174v_32">ἀγῶνας ἠγωνίσθαι κατὰ τῶν ἐν τῆι
                        πόλει <unclear>σ</unclear>υμ­</seg>
                </seg>
            </ab>
        </body>
    </text>
