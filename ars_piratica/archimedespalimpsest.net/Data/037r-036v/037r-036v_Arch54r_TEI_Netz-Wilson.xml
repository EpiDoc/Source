    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="37r1" type="folio">
                    <seg n="1" type="line" facs="z37r1_01">κορυφῆς τοῦ ἑτέρου κώνου ἐπὶ</seg>
                    <seg n="2" type="line" facs="z37r1_02">τὴν πλευρὰν τοῦ <seg type="word"
                                ><supplied reason="lost">ἑτ</supplied>έρου</seg> κώνου <w part="I"
                            >κα</w></seg>
                    <seg n="3" type="line" facs="z37r1_03"><w part="F">θέτωι</w> ἠγμένηι.</seg>
                </seg>
                <seg n="37r1" type="folio">
                    <seg n="3" type="line" facs="z37r1_03">ἔστω ῥόμβος ἐξ <w part="I">ἰσο</w></seg>
                    <seg n="4" type="line" facs="z37r1_04"><w part="F">σκελῶν</w> κώνων <seg
                            type="word">συγκείμ<supplied reason="lost"
                            >ε</supplied>ν<unclear>ο</unclear>ς</seg></seg>
                    <seg n="5" type="line" facs="z37r1_05">ὁ <seg type="word"
                            >Α<unclear>Β</unclear>ΓΔ</seg>, καὶ τμηθήτω ὁ ἕτερος</seg>
                    <seg n="6" type="line" facs="z37r1_06">κῶνος ἐπιπέδωι παραλλήλωι</seg>
                    <seg n="7" type="line" facs="z37r1_07">τῆι βάσει, καὶ ποιείτω τομὴν <choice>
                            <abbr>τη</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z37r1_08">ΕΖ, ἀπὸ δὲ τοῦ περὶ διάμετρον <choice>
                            <abbr>τη</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z37r1_09">ΕΖ κύκλον κῶνος <seg type="word"
                                >ἀναγεγ<supplied reason="lost"
                        >ράφ</supplied>θ<unclear>ω</unclear></seg></seg>
                    <seg n="10" type="line" facs="z37r1_10">τὴν κορυφὴν ἔχων τὸ Δ σημεῖον·</seg>
                    <seg n="11" type="line" facs="z37r1_11"><seg type="word">ἔστ<supplied
                                reason="lost">α</supplied>ι</seg> δὴ γεγονὼς ῥόμβος ὁ ΕΒΔΖ.</seg>
                    <seg n="12" type="line" facs="z37r1_12">καὶ νοείσθω ἀφηρημένος ἀπὸ</seg>
                    <seg n="13" type="line" facs="z37r1_13">τοῦ ὅλου ῥόμβου, ἐκκείσθω δέ τις</seg>
                    <seg n="14" type="line" facs="z37r1_14">κῶνος ὁ ΘΚΛ τὴν μὲν βάσιν <w part="I"
                        >ἴ</w></seg>
                    <seg n="15" type="line" facs="z37r1_15"><w part="F">σην</w> ἔχων τῆι ἐπιφανείαι
                        τῆι <w part="I">με</w></seg>
                    <seg n="16" type="line" facs="z37r1_16"><w part="F">ταξὺ</w> τῶν ΑΓ, ΕΖ, τὸ δὲ
                        ὕψος ἴσον</seg>
                    <seg n="17" type="line" facs="z37r1_17">τῆι ἀπὸ τοῦ Δ σημείου καθέτω</seg>
                    <seg n="18" type="line" facs="z37r1_18">ἀγομένη ἐπὶ τὴν ΒΔ ἢ τὴν <seg
                            type="word"><unclear>ἐ</unclear><supplied reason="lost">π</supplied>᾽</seg>
                        <seg type="suppliedword">εὐ</seg></seg>
                </seg>
                <seg n="36v1" type="folio">
                    <seg n="1" type="line" facs="z36v1_01"><seg type="wordend">
                            <supplied reason="lost">θ</supplied>
                            <unclear>είας</unclear>
                        </seg>
                        <seg type="word"><unclear>αὐ</unclear>τῆι</seg>· λέγω <expan>ὅτι</expan> ὁ
                        ΘΚΛ <choice>
                            <abbr>κων</abbr>
                            <expan>κῶνος</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z36v1_02">ἴσος <expan>ἐστὶ</expan> τῶι εἰρημένῶι
                            <seg type="suppliedword">περι<supplied reason="lost"
                        >λ</supplied>είμ</seg></seg>
                    <seg n="3" type="line" facs="z36v1_03"><seg type="wordend">ματι</seg>.</seg>
                </seg>
                <seg n="36v1" type="folio">
                    <seg n="3" type="line" facs="z36v1_03">ἐκκείσθωσαν γὰρ δύο <w part="I">κῶ</w></seg>
                    <seg n="4" type="line" facs="z36v1_04"><w part="F">νοι</w> οἱ ΜΝΞ, ΟΠΡ, καὶ ἡ
                            <seg type="word">μὲ<unclear>ν</unclear></seg>
                        <seg type="word"><unclear>β</unclear>άσ<unclear>ις</unclear></seg></seg>
                    <seg n="5" type="line" facs="z36v1_05">τοῦ ΜΝΞ κώνου ἴση ἔστω τῆι <w part="I"
                            >ἐπι</w></seg>
                    <seg n="6" type="line" facs="z36v1_06"><w part="F">φανείαι</w> τοῦ ΑΒΓ, τὸ δὲ
                        ὕψος <choice>
                            <abbr>ισ</abbr>
                            <expan>ἴσον</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z36v1_07">τῆι ΔΗ <expan>διὰ</expan> δὴ τὰ
                        προδειχθέντα <w part="I">ἴ</w></seg>
                    <seg n="8" type="line" facs="z36v1_08"><w part="F">σος</w>
                        <expan>ἐστὶν</expan> ὁ ΜΝΞ κῶνος τῶι ΑΒΓΔ</seg>
                    <seg n="9" type="line" facs="z36v1_09">ῥόμβωι, τοῦ δὲ ΟΠΡ κώνου ἡ <choice>
                            <abbr>μ</abbr>
                            <expan>μὲν</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z36v1_10">βάσις ἴση ἔστω τῆι ἐπιφανείαι</seg>
                    <seg n="11" type="line" facs="z36v1_11">τοῦ ΕΒΖ κώνου, τὸ δὲ ὕψος ἴσον</seg>
                    <seg n="12" type="line" facs="z36v1_12">τῆι ΔΗ ὁμοίως δὴ ἴσος
                        <expan>ἐστὶν</expan> ὁ ΟΠΡ</seg>
                    <seg n="13" type="line" facs="z36v1_13">κῶνος τῶ ΕΒΔΖ ῥόμβῶι. ἐπεὶ δὲ</seg>
                    <seg n="14" type="line" facs="z36v1_14">ὁμοίως ἡ ἐπιφάνεια τοῦ ΑΒΓ</seg>
                    <seg n="15" type="line" facs="z36v1_15">κώνου <seg type="word"
                            >σύγ<unclear>κ</unclear>ειται</seg> ἔκ τε τῆς τοῦ ΕΒΖ</seg>
                    <seg n="16" type="line" facs="z36v1_16">καὶ τῆς μεταξὺ τῶν ΕΖ, ΑΓ, ἀλλὰ</seg>
                    <seg n="17" type="line" facs="z36v1_17">ἡ μὲν τοῦ ΑΒΓ κώνου ἐπιφάνεια</seg>
                </seg>
                <seg n="37r2" type="folio">
                    <seg n="1" type="line" facs="z37r2_01">ἴση <expan>ἐστὶ</expan> τῆι βάσει τοῦ
                        ΜΝΞ, ἡ δὲ τοῦ <seg type="word">ΕΒ<unclear>Ζ</unclear></seg></seg>
                    <seg n="2" type="line" facs="z37r2_02">κώνου ἐπιφάνεια <seg type="word"
                                >ἴσ<unclear>η</unclear></seg>
                        <expan>ἐστὶ</expan> τῆι <seg type="unclearword">
                            <unclear>βά</unclear>
                        </seg></seg>
                    <seg n="3" type="line" facs="z37r2_03"><seg type="wordend">σει</seg> τοῦ ΟΠΡ
                        κώνου, ἡ δὲ <seg type="word">μεταξ<supplied reason="lost">ὺ</supplied></seg>
                        <supplied reason="lost">τῶν</supplied></seg>
                    <seg n="4" type="line" facs="z37r2_04">ΕΖ, ΑΓ ἴση <expan>ἐστὶ</expan> τῆι βάσει <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> ΘΚΛ, <unclear>ἡ</unclear>
                        <supplied reason="lost">ἄρα</supplied></seg>
                    <seg n="5" type="line" facs="z37r2_05">βάσις τοῦ ΜΝΞ ἴση <expan>ἐστὶ</expan>
                        <seg type="word">
                            <choice>
                                <abbr>τς</abbr>
                                <expan><unclear>τ</unclear>αῖς</expan>
                            </choice>
                        </seg> βάσεσι</seg>
                    <seg n="6" type="line" facs="z37r2_06">τῶν ΟΠΡ, ΘΚΛ. καί <expan>εἰσιν</expan> οἱ
                            <seg type="word">κῶ<supplied reason="lost">νοι</supplied></seg>
                        <w part="I">ὑ</w></seg>
                    <seg n="7" type="line" facs="z37r2_07"><w part="F">πὸ</w> τὸ αὐτὸ <seg
                            type="word">ὕ<unclear>ψ</unclear>ος</seg>· καὶ ὁ ΜΝΞ <expan>ἄρα</expan>
                        <seg type="unclearword">κ<unclear>ῶ</unclear></seg></seg>
                    <seg n="8" type="line" facs="z37r2_08"><seg type="wordend">νος</seg> ἴσος
                            <expan>ἐστὶ</expan> τοῖς ΘΚΛ, ΟΠΡ <seg type="word"
                                ><unclear>κ</unclear>ώνο<supplied reason="lost">ις</supplied></seg>.</seg>
                    <seg n="9" type="line" facs="z37r2_09">ἀλλ᾽ ὁ μὲν <seg type="word"
                            >ΜΝ<unclear>Ξ</unclear></seg> κῶνος ἴσος <expan>ἐστὶ</expan> τῶι <w
                            part="I">Α</w></seg>
                    <seg n="10" type="line" facs="z37r2_10"><w part="F">ΒΓΔ</w> ῥόμβωι, ὁ δὲ ΟΠΡ
                        κῶνος τῶ <w part="I">Ε</w></seg>
                    <seg n="11" type="line" facs="z37r2_11"><w part="F">ΒΔΖ</w> ῥόμβωι· <seg
                            type="word">λο<supplied reason="lost">ι</supplied>πὸς</seg>
                        <expan>ἄρα</expan> ὁ κῶνος ὁ</seg>
                    <seg n="12" type="line" facs="z37r2_12">ΘΚΛ ἴσος <expan>ἐστὶ</expan> τῶι
                        περιλείμματι τῶι</seg>
                    <seg n="13" type="line" facs="z37r2_13"><seg type="word">λο<supplied
                                reason="lost">ι</supplied>πῶι</seg>.</seg>
                </seg>
                <seg n="36v2" type="folio">
                    <seg n="1" type="line" facs="z36v2_01">ἐὰν εἰς κύκλον πολύγωνον ἐγγραφῆι</seg>
                    <seg n="2" type="line" facs="z36v2_02">ἀρτιόπλευρόν τε καὶ <choice>
                            <abbr>ἰσόπλευρο</abbr>
                            <expan>ἰσόπλευρον</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z36v2_03">καὶ διαχθῶσιν εὐθεῖαι <seg
                            type="expandedword">
                            <choice>
                                <abbr>ἐπιζευγ</abbr>
                                <expan>ἐπιζευγνύουσαι</expan>
                            </choice>
                        </seg></seg>
                    <seg n="4" type="line" facs="z36v2_04"><seg type="wordend">ουσαι</seg>
                        <seg type="word">τὰ<unclear>ς</unclear></seg> πλευρὰς τοῦ κώνου</seg>
                    <seg n="5" type="line" facs="z36v2_05">ὥστε αὐτὰς παραλλήλους <seg type="word"
                                >εἶν<unclear>αι</unclear></seg></seg>
                </seg>
            </ab>
        </body>
