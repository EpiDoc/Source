    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="169v1" type="folio">
                    <seg n="1" type="line" facs="z169v1_01">τῶν ΑΙΔ ΑΠΟΛ τμημάτων τῆς</seg>
                    <seg n="2" type="line" facs="z169v1_02">Ψ,<gap extent="1"/> παράλληλος <seg
                            type="word"><supplied reason="lost">δ</supplied>ὲ</seg> τῆι ΒΔ ἡ ΦΙ <w
                            part="I">τέ</w></seg>
                    <seg n="3" type="line" facs="z169v1_03"><w part="F">μνουσα</w> τὴν <seg
                            type="word">μ<supplied reason="lost">ετ</supplied>α<supplied
                                reason="lost">ξ</supplied>ὺ</seg> τοῦ κώνου <choice>
                            <abbr>τομη</abbr>
                            <expan>τομὴν</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z169v1_04">κατὰ τὸ Υ· πάλιν δὴ ἡ ΙΦΥ ΔΙ τῆς</seg>
                    <seg n="5" type="line" facs="z169v1_05">ΥΙ δειχθήσεται, καθάπερ ἡ ΟϘ τῆς</seg>
                    <seg n="6" type="line" facs="z169v1_06"><supplied reason="lost">ΞΓ.</supplied>
                        ἤχθω δὲ ἀπὸ <seg type="word">το<supplied reason="lost">ῦ</supplied></seg> Φ
                        τῆς ΙΠ ΟΛ <w part="I">ἐ</w></seg>
                    <seg n="7" type="line" facs="z169v1_07"><w part="F">φαπτομένη</w> κατὰ τὸ Φ ἡ
                        ΦΩ· ὁμοίως</seg>
                    <seg n="8" type="line" facs="z169v1_08">δὴ τοῖς πρότερον δειχθήσεται ἡ
                            <expan>μὲν</expan>
                    </seg>
                    <seg n="9" type="line" facs="z169v1_09">ΑΙ <seg type="word">τῆ<supplied
                                reason="lost">ι</supplied></seg> ΧΙ τῆι Η, ἡ δὲ ΑΧ τῆι ΦΩ <w
                            part="I">παράλ</w></seg>
                    <seg n="10" type="line" facs="z169v1_10"><w part="F">ληλος.</w> δεικτέον δὴ, ὅτι
                        τὸ τμῆμα</seg>
                    <seg n="11" type="line" facs="z169v1_11"><seg type="word"
                                ><unclear>ἀφ</unclear><supplied reason="lost">ε</supplied>θὲν</seg>
                        ἐς τὸ ὑγρόν, ὥστε τὴν βάσιν</seg>
                    <seg n="12" type="line" facs="z169v1_12"><seg type="word"><supplied
                                reason="lost">α</supplied>ὐτοῦ</seg> μὴ θιγγάνειν τοῦ ὑγροῦ,
                            <expan>καὶ</expan></seg>
                    <seg n="13" type="line" facs="z169v1_13"><seg type="word"><supplied
                                reason="lost">τ</supplied><unclear>ε</unclear>θὲν</seg> κεκλιμένον
                        οὕτως <w part="I">κλιθή</w></seg>
                    <seg n="14" type="line" facs="z169v1_14"><w part="F">σεται</w>, ὥστε τὴν βάσιν
                            <seg type="word">αὐ<supplied reason="lost">τ</supplied>οῦ</seg>
                        <w part="I">κα</w></seg>
                    <seg n="15" type="line" facs="z169v1_15"><w part="F">τὰ</w> πλείονα τόπον
                        βρέχεσθαι <w part="I">ὑ</w></seg>
                    <seg n="16" type="line" facs="z169v1_16"><w part="F">πὸ</w> τοῦ ὑγροῦ.</seg>
                </seg>
                <seg n="169v1" type="folio">
                    <seg n="16" type="line" facs="z169v1_16">ἀφείσθω γὰρ εἰς τὸ</seg>
                    <seg n="17" type="line" facs="z169v1_17">ὑγρόν, ὡς εἴρηται, καὶ κείσθω τὸ</seg>
                    <seg n="18" type="line" facs="z169v1_18">πρῶτον καὶ οὕτως κεκλιμένον,</seg>
                    <seg n="19" type="line" facs="z169v1_19"><seg type="word"
                            >ὥσ<unclear>τ</unclear>ε</seg>
                        <seg type="word"><unclear>τ</unclear>ὴν</seg> βάσιν <seg type="word"
                                >αὐτο<unclear>ῦ</unclear></seg>
                        <seg type="word"><supplied reason="lost">μη</supplied>δὲ</seg> καθ’ ἓν</seg>
                </seg>
                <seg n="164r1" type="folio">
                    <seg n="1" type="line" facs="z164r1_01">ἅπτεσθαι τῆς τοῦ ὑγροῦ <seg type="word">
                            <choice>
                                <abbr>επιφανει</abbr>
                                <expan>ἐπιφανεί<supplied reason="lost">ας</supplied></expan>
                            </choice>
                        </seg>,</seg>
                    <seg n="2" type="line" facs="z164r1_02">τμηθέντος δὲ αὐτοῦ ἐπιπέδωι <w part="I"
                            >δι</w></seg>
                    <seg n="3" type="line" facs="z164r1_03"><w part="F">ὰ</w> τοῦ ἄξονος πρὸς τὴν
                        τοῦ ὑγροῦ</seg>
                    <seg n="4" type="line" facs="z164r1_04">ἐπιφάνειαν ἐν μὲν τῆι τοῦ <w part="I"
                            >τμήμα</w></seg>
                    <seg n="5" type="line" facs="z164r1_05"><w part="F">τος</w> ἐπιφανείαι γίνεται
                        τομὴ ἡ</seg>
                    <seg n="6" type="line" facs="z164r1_06">ΑΒΓ, ἐν δὲ τῆι τοῦ ὑγροῦ ἡ ΕΖ, ἄξων</seg>
                    <seg n="7" type="line" facs="z164r1_07">δ’ ἔστω τῆς τομῆς καὶ διάμετρος</seg>
                    <seg n="8" type="line" facs="z164r1_08">τοῦ τμήματος ἡ ΒΔ, καὶ τετμήσθω</seg>
                    <seg n="9" type="line" facs="z164r1_09">ἡ ΒΔ κατὰ τὸ ΚΡ ὁμοίως τοῖς <w part="I"
                            >πρότε</w></seg>
                    <seg n="10" type="line" facs="z164r1_10"><w part="F">ρον,</w> ἤχθω δὲ καὶ ἡ μὲν
                        ΗΛ παρὰ</seg>
                    <seg n="11" type="line" facs="z164r1_11">τὴν ΑΖ ἐφαπτομένη τῆς ἀπὸ</seg>
                    <seg n="12" type="line" facs="z164r1_12">τῆς ΑΒΓ τομῆς κα τὸ Η, ἡ δὲ ΗΘ</seg>
                    <seg n="13" type="line" facs="z164r1_13"><choice>
                            <abbr>π</abbr>
                            <expan>παρὰ</expan>
                        </choice> τὴν ΒΔ, ἡ δὲ ΗΓ κάθετος ἐπὶ <choice>
                            <abbr>τη</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z164r1_14">ΒΔ. ἐπὶ δὲ τὸ τμῆμα τῶι βάρει <choice>
                            <abbr>λογο</abbr>
                            <expan>λόγον</expan>
                        </choice>
                    </seg>
                    <seg n="15" type="line" facs="z164r1_15">ἔχει πρὸς τὸ ὑγρόν, ὃν τὸ ἀπὸ τῆς</seg>
                    <seg n="16" type="line" facs="z164r1_16">Ψ τετραγώνου πρὸς τὸ ἀπὸ τῆς ΒΔ,</seg>
                    <seg n="17" type="line" facs="z164r1_17">δῆλον, <expan>ὅτι</expan> ἡ Ψ ἴση ἐστὶν
                            <seg type="word">τῆ<supplied reason="lost">ι</supplied></seg> ΗΘ· <w
                            part="I">δειχθή</w></seg>
                    <seg n="18" type="line" facs="z164r1_18"><w part="F">σεται</w> γὰρ ὁμοίως τοῖς
                        πρότερον· ὥστε</seg>
                </seg>
                <seg n="169v2" type="folio">
                    <seg n="1" type="line" facs="z169v2_01">καὶ ἡ ΗΘ ἴση ἐστὶν <seg type="word"
                                ><supplied reason="lost">τ</supplied>ῆι</seg> ΦΙ· καὶ τὰ</seg>
                    <seg n="2" type="line" facs="z169v2_02">τμήματα ἄρα τὰ ΑΦ ΧΕ ΒΖ ἴσα</seg>
                    <seg n="3" type="line" facs="z169v2_03">ἐστὶν ἀλλήλοις. ἐπεὶ δ’ ἐν ἴσοις καὶ </seg>
                    <seg n="4" type="line" facs="z169v2_04">ὁμοίοις τμήμασι τοῖς <supplied
                            reason="lost">Α</supplied>Π ΟΛ <supplied reason="lost">Α</supplied>ΒΓ</seg>
                    <seg n="5" type="line" facs="z169v2_05">ἠγμέναι εἰσίν αἱ ΑΧ ΕΖ ἴσα τμή</seg>
                    <seg n="6" type="line" facs="z169v2_06">ματα ἀφαιροῦσαι, καὶ ἡ μὲν </seg>
                    <seg n="7" type="line" facs="z169v2_07"><seg type="word"
                        >ἀ<unclear>π’</unclear></seg> ἄκρας τῆς βάσεως, ἡ δὲ <w part="I">οὐ</w></seg>
                    <seg n="8" type="line" facs="z169v2_08"><w part="F">κ</w> ἀπ’ ἄκρας, ἐλάσσονα
                            <seg type="word">ποιήσ<supplied reason="lost">ει</supplied></seg></seg>
                    <seg n="9" type="line" facs="z169v2_09">τὴν ὀξεῖαν πρὸς τὴν διάμετρον</seg>
                    <seg n="10" type="line" facs="z169v2_10">τοῦ τμήματος ἡ ἀπ’ ἄκρας τῆς</seg>
                    <seg n="11" type="line" facs="z169v2_11">βάσεως <seg type="word"><supplied
                                reason="lost">ἠ</supplied>γμένη.</seg> καὶ <seg type="word"
                                >ἐπειδ<supplied reason="lost">ὴ</supplied></seg></seg>
                    <seg n="12" type="line" facs="z169v2_12">τοῦ ΗΛΓ τριγώνου ἡ Λ μείζων </seg>
                    <seg n="13" type="line" facs="z169v2_13"><seg type="word"
                        ><unclear>τ</unclear>ῆς</seg> Ω γωνίας τοῦ ΦΤΩ τριγώ</seg>
                    <seg n="14" type="line" facs="z169v2_14">νου, δῆλον, ὅτι ἐλάσσων ἐστὶν ἡ</seg>
                    <seg n="15" type="line" facs="z169v2_15">ΒΓ τῆς ΒΤ, ἡ δὲ ΓΡ τῆς ΡΤ <choice>
                            <abbr>μειζω</abbr>
                            <expan>μείζων</expan>
                        </choice>, </seg>
                    <seg n="16" type="line" facs="z169v2_16"><expan>καὶ</expan> ἡ ΗϠ μείζων τῆς ΦΗ·
                        ἡ δὲ ϠΘ </seg>
                    <seg n="17" type="line" facs="z169v2_17">ἄρα ἐλάσσων τῆς ΗΙ. καὶ ἐπειδὴ</seg>
                    <seg n="18" type="line" facs="z169v2_18">δέ ἐστιν ἡ ΦΥ τῆι ΥΙ, δῆλον, ὡς</seg>
                    <seg n="19" type="line" facs="z169v2_19">ἡ <unclear>Η</unclear>Ϡ <seg
                            type="word">μεί<supplied reason="lost">ζων</supplied></seg>
                        <supplied reason="lost">
                            <expan>ἐστὶν</expan>
                        </supplied> ἢ διπλασία τῆς</seg>
                    <seg n="20" type="line" facs="z169v2_20"><supplied reason="lost">Ϡ</supplied>Θ.
                        ἔστω οὖν <supplied reason="lost">ἡ</supplied> Η<supplied reason="lost">Α</supplied>
                        <seg type="word">δι<supplied reason="lost">π</supplied>λ<supplied
                                reason="lost">ασία</supplied></seg></seg>
                </seg>
                <seg n="164r2" type="folio">
                    <seg n="1" type="line" facs="z164r2_01">τῆς Αθ· δῆλον δὴ ἐκ τούτων,
                        <expan>ὅτι</expan></seg>
                    <seg n="2" type="line" facs="z164r2_02">οὐ μενεῖ τὸ τμῆμα, ἀλλὰ <w part="I"
                            >ἐπικλι</w></seg>
                    <seg n="3" type="line" facs="z164r2_03"><w part="F">θήσεται,</w> ἕως ἂν ἡ βάσις <choice>
                            <abbr>αυτ</abbr>
                            <expan>αὐτοῦ</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z164r2_04">θίγηι καθ’ ἓν σημεῖον τῆς τοῦ</seg>
                    <seg n="5" type="line" facs="z164r2_05">ὑγροῦ ἐπιφανείας.</seg>
                </seg>
                <seg n="164r2" type="folio">
                    <seg n="5" type="line" facs="z164r2_05">ἁπτέσθω δὴ</seg>
                    <seg n="6" type="line" facs="z164r2_06">καθ’ ἓν σημεῖον, ὡς ἐν τῶι τρίτωι</seg>
                    <seg n="7" type="line" facs="z164r2_07">σχήματι γεγράφθω, καὶ τὰ ἄλλα</seg>
                    <seg n="8" type="line" facs="z164r2_08">τὰ αὐτὰ κατασκευάσθω· <w part="I"
                        >δειχθή</w></seg>
                    <seg n="9" type="line" facs="z164r2_09"><w part="F">σεται</w> δὲ πάλιν μή τε ΘΜ
                        ἴση οὖσα</seg>
                    <seg n="10" type="line" facs="z164r2_10">τῆι ΦΙ καὶ τὰ ΑΦΧ ΑΒΥ <w part="I"
                        >τμή</w></seg>
                    <seg n="11" type="line" facs="z164r2_11"><w part="F">ματα</w> ἴσα ἀλλήλοις. καὶ
                        ἐπειδὴ</seg>
                    <seg n="12" type="line" facs="z164r2_12">ἐν ἴσοις καὶ ὁμοίοις τμήμασι</seg>
                    <seg n="13" type="line" facs="z164r2_13">τοῖς Α ΠΟ ΛΑ ΒΓ ἠγμέναι εἰσὶν</seg>
                    <seg n="14" type="line" facs="z164r2_14">αἱ ΑΧ ΑΚ ἴσα τμήματα <w part="I"
                        >ἀφαι</w></seg>
                    <seg n="15" type="line" facs="z164r2_15"><w part="F">ροῦσαι,</w> ἴσας ποιῶσι
                        γωνίας <expan>πρὸς</expan></seg>
                    <seg n="16" type="line" facs="z164r2_16">ταῖς διαμέτροις τῶν <w part="I"
                        >τμημά</w></seg>
                    <seg n="17" type="line" facs="z164r2_17"><w part="F">των·</w> τῶν ἄρα
                            <unclear>Δ</unclear>Β<unclear>Σ</unclear> ΦΤΩ αἱ
                    <expan>πρὸς</expan></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
