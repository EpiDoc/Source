    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="17r1" type="folio">
                    <seg n="1" type="line" facs="z17r1_01"><w part="F">φανείας</w> τοῦ ὑγροῦ ἁ ΑΒ ΓΔ
                            <w part="I">πε</w></seg>
                    <seg n="2" type="line" facs="z17r1_02"><w part="F">ριφέρεια,</w> τοῦ δὲ σχάματος
                        ἁ ΕΖΗ</seg>
                    <seg n="3" type="line" facs="z17r1_03">περιφέρεια καὶ ἁ ΕΗ εὐθεῖα, <w part="I"
                        >ἄ</w></seg>
                    <seg n="4" type="line" facs="z17r1_04"><w part="F">ξων</w> δὲ ἔστω τοῦ τμάματος
                        ἁ ΖΘ.</seg>
                    <seg n="5" type="line" facs="z17r1_05">εἰ οὖν δυνατόν, μὴ κατὰ ὀρθὸν</seg>
                    <seg n="6" type="line" facs="z17r1_06">ἔστω ἁ ΖΘ· <sic>εικται</sic> οὖν,
                            <expan>ὅτι</expan> οὐ μενεῖ</seg>
                    <seg n="7" type="line" facs="z17r1_07">τὸ σχῆμα, ἀλλὰ ἐπ᾽ ὀρθὸν <w part="I"
                        >κατα</w></seg>
                    <seg n="8" type="line" facs="z17r1_08">
                        <w part="F">τασεῖται.</w>
                    </seg>
                </seg>
                <seg n="17r1" type="folio">
                    <seg n="8" type="line" facs="z17r1_08">ἔστι δὴ τὸ κέντρον τᾶς</seg>
                    <seg n="9" type="line" facs="z17r1_09">σφαίρας ἐπὶ τῆς ΖΘ· πάλιν
                        <expan>γὰρ</expan></seg>
                    <seg n="10" type="line" facs="z17r1_10">ἡμισφαιρίου ἔστω <choice>
                            <abbr>πρω</abbr>
                            <expan>πρῶτον</expan>
                        </choice> τὸ σχᾶμα·</seg>
                    <seg n="11" type="line" facs="z17r1_11">καὶ ἔστω τὸ Κ· διὰ δὲ τοῦ Κ καὶ τοῦ</seg>
                    <seg n="12" type="line" facs="z17r1_12">κέντρου τᾶς γᾶς τοῦ Λ <seg type="word"
                                >ἄχθ<unclear>ω</unclear></seg></seg>
                    <seg n="13" type="line" facs="z17r1_13">δὲ κατὰ τὸ σχῆμα τὸ ἐκτὸς τοῦ <w
                            part="I">ὑ</w></seg>
                    <seg n="14" type="line" facs="z17r1_14"><w part="F">γροῦ</w> ἀπολαμβανόμενον ὑπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τᾶς</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z17r1_15">τοῦ ὑγροῦ ἐπιφανείας τὸν ἄξονα</seg>
                    <seg n="16" type="line" facs="z17r1_16">ἔχει ἐπὶ τᾶς διὰ τοῦ Κ, διὰ ταὐτὰ</seg>
                    <seg n="17" type="line" facs="z17r1_17">τοῖς πρότερον ἔστιν αὐτοῦ τὸ <w part="I"
                            >κέν</w></seg>
                    <seg n="18" type="line" facs="z17r1_18"><w part="F">τρον</w> τοῦ βάρεος ἐπὶ τᾶσι
                        ΙΒ· ἔστω</seg>
                    <seg n="19" type="line" facs="z17r1_19"><expan>γὰρ</expan> τὸ Ρ. τοῦ δὲ ὅλου
                        τμάματος τὸ <choice>
                            <abbr>κ</abbr>
                            <expan>
                                <w part="I">κέν</w>
                            </expan>
                        </choice></seg>
                    <seg n="20" type="line" facs="z17r1_20"><w part="F">τρον</w>
                        <unclear>ἐ</unclear><supplied reason="lost">στὶ</supplied> τοῦ βάρεος <seg
                            type="word">ἐ<unclear>π</unclear>ὶ</seg>
                        <seg type="word">τᾶ<supplied reason="lost">ς</supplied></seg>
                        <unclear>Ζ</unclear>Θ</seg>
                </seg>
                <seg n="16v1" type="folio">
                    <seg n="1" type="line" facs="z16v1_01">μεταξὺ τῶν Κ Ζ· ἔστω τὸ Τ. τοῦ ἄρα</seg>
                    <seg n="2" type="line" facs="z16v1_02">λοιποῦ σχάματος τοῦ ἐν τῶι <w part="I"
                        >ὑ</w></seg>
                    <seg n="3" type="line" facs="z16v1_03"><w part="F">γρῶι</w> τὸ κέντρον ἐσσεῖται
                        ἐπὶ <choice>
                            <abbr>τ</abbr>
                            <expan>τᾶς</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z16v1_04">Τ εὐθείας ἐκβληθείσας τινός,</seg>
                    <seg n="5" type="line" facs="z16v1_05">δείξει <expan>περὶ</expan> τὸν ΤΡ τὸν
                        αὐτὸν λόγον,</seg>
                    <seg n="6" type="line" facs="z16v1_06">ἔχει τὸ μέρος τοῦ τμάματος <w part="I"
                        >ἐκ</w></seg>
                    <seg n="7" type="line" facs="z16v1_07"><w part="F">τὸς</w> τοῦ Υ ποτὶ τὸ βάρος
                        τοῦ <w part="I">σχά</w></seg>
                    <seg n="8" type="line" facs="z16v1_08"><w part="F">ματος</w> τοῦ ἐν τῶι ὑγρῶι·
                        κατὰ</seg>
                    <seg n="9" type="line" facs="z16v1_09">τὸ <unclear>Σ</unclear> κέντρου εἰρημένου <choice>
                            <abbr>σχηματ</abbr>
                            <expan>σχήματος</expan>
                        </choice>,</seg>
                    <seg n="10" type="line" facs="z16v1_10">διὰ τοῦ κάθετος ἔστω τὸ ΘΣΛ· <w part="I"
                            >οἰ</w></seg>
                    <seg n="11" type="line" facs="z16v1_11"><w part="F">σεῖται</w> οὖν τὸ βάρος τοῦ
                        μὲν <w part="I">τμά</w></seg>
                    <seg n="12" type="line" facs="z16v1_12"><w part="F">ματος</w>, ὅ ἐστιν ἐκτὸς τοῦ
                        ὑγροῦ,</seg>
                    <seg n="13" type="line" facs="z16v1_13">κατὰ τὰς εὐθεῖας τὰς ΡΛ ἔστω</seg>
                    <seg n="14" type="line" facs="z16v1_14">κάτω, τοῦ δ᾽ ἐν τῶι ὑγρῶι <choice>
                            <abbr>σχαματ</abbr>
                            <expan><unclear>σ</unclear>χάματος</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z16v1_15">κατὰ τὰς εὐθεῖας τὰς ΕΛ ἔστω</seg>
                    <seg n="16" type="line" facs="z16v1_16"><sic>αν ει ω.</sic> οὐκ ἄρα μὲν εἰς τὸ
                        σχᾶμα,</seg>
                    <seg n="17" type="line" facs="z16v1_17">ἀλλὰ τὸ <choice>
                            <abbr>μ</abbr>
                            <expan>μὲν</expan>
                        </choice>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> σχάματος τὰ μὲν</seg>
                </seg>
                <seg n="17r2" type="folio">
                    <seg n="1" type="line" facs="z17r2_01">ποτὶ τῶι η μέρει οἷς οὔτε ἐσ <seg
                            type="word">τ<unclear>ὼ</unclear></seg>
                        <supplied reason="lost">κάτω</supplied>,</seg>
                    <seg n="2" type="line" facs="z17r2_02">τὰ δὲ ποτὶ τὸ Ε <seg type="word"
                                >ἔστα<unclear>ι</unclear></seg> τὸ ἄνω, <seg type="word">
                            <unclear>κ</unclear>
                            <supplied reason="lost">αὶ</supplied>
                        </seg>
                        <seg type="word"><supplied reason="lost">ἀ</supplied>εὶ</seg></seg>
                    <seg n="3" type="line" facs="z17r2_03">τοῦτο ἐσσεῖται, καὶ ὁ ΕΖ <seg type="word"
                                ><unclear>κα</unclear>τὰ</seg>
                        <w part="I">κά</w></seg>
                    <seg n="4" type="line" facs="z17r2_04"><w part="F">θετον</w> γένηται.</seg>
                    <seg n="5" type="line" facs="z17r2_05">ΣΥΡΑΚΟΥΣΊΟΥ <w part="I">ἈΡΧΙ</w></seg>
                    <seg n="6" type="line" facs="z17r2_06">
                        <w part="F">ΜΉΔΟΥΣ</w>
                        <choice>
                            <abbr>ὈΧΟΥΜΈΝ</abbr>
                            <expan>ὈΧΟΥΜΈΝων</expan>
                        </choice>
                        <num>Α</num>
                    </seg>
                    <figure n="1.9.1">
                        <figDesc xml:lang="eng">Figure 1.9.1</figDesc>
                    </figure>
                </seg>
                <seg n="16v2" type="folio">
                    <num>β</num>
                </seg>
                <seg n="16v2" type="folio">
                    <num>α</num>
                </seg>
                <seg n="16v2" type="folio">
                    <seg n="1" type="line" facs="z16v2_01">εἴ κά τι μέγεθος κουφότερον ἐὸν</seg>
                    <seg n="2" type="line" facs="z16v2_02">τοῦ ὑγροῦ ἀφεθῆ ἐς τὸ ὑγρόν, <seg
                            type="word">
                            <choice>
                                <abbr>τουτο</abbr>
                                <expan>τοῦτο<supplied reason="lost">ν</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="3" type="line" facs="z16v2_03">ἕξει τὸν λόγον τῶι βάρει ποτὶ τὸ</seg>
                    <seg n="4" type="line" facs="z16v2_04">ὑγρόν, ὃν ἔχει τὸ δεδυκὸς <choice>
                            <abbr>μεγεθ</abbr>
                            <expan>μέγεθος</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z16v2_05">ποτὶ τὸ ὅλον <seg type="word"
                                >μέγε<unclear>θ</unclear>ος</seg></seg>
                </seg>
                <seg n="16v2" type="folio">
                    <seg n="5" type="line" facs="z16v2_05">
                        <seg type="word">ἀφεί<supplied reason="lost">σ</supplied>θω</seg>
                    </seg>
                    <seg n="6" type="line" facs="z16v2_06">γάρ τι εἰς τὸ ὑγρὸν μέγεθος <w part="I"
                            >στερε</w></seg>
                    <seg n="7" type="line" facs="z16v2_07"><w part="F">ὸν</w> τὸ ΦΑ κουφότερον τοῦ
                        ὑγροῦ,</seg>
                    <seg n="8" type="line" facs="z16v2_08">ἔστω δὲ τὸ μὲν δεδυκὸς αὐτοῦ τὸ Α,</seg>
                    <seg n="9" type="line" facs="z16v2_09">τὸ δὲ ἐκτὸς τοῦ <seg type="word"
                                >ὑγ<unclear>ρ</unclear>οῦ</seg> τὸ Φ. <seg type="word">
                            <choice>
                                <abbr>δεικτ</abbr>
                                <expan>δεικτ<unclear>έον</unclear></expan>
                            </choice>, </seg></seg>
                </seg>
            </ab>
