    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="139r1" type="folio">
                    <seg n="1" type="line" facs="z139r1_01">
                        <sic>πίπεδα</sic>
                        <seg type="word">τμήματ<supplied reason="lost">α</supplied></seg>
                        <seg type="word"><supplied reason="lost">μεί</supplied>ζονά</seg>
                        <choice>
                            <abbr>εστι</abbr>
                            <expan>ἐστιν</expan>
                        </choice>
                    </seg>
                    <seg n="2" type="line" facs="z139r1_02"><seg type="word"><unclear>τ</unclear>ῶν</seg>
                        <seg type="word">παρ<unclear>α</unclear>λληλογράμμων</seg>, ὧν <w part="I"
                            >βά</w></seg>
                    <seg n="3" type="line" facs="z139r1_03"><w part="F">σεις</w> μὲν αἱ ΑΕ, ΕΒ, ὕψος
                        δὲ τὸ <seg type="word">αὐ<supplied reason="lost">τὸ</supplied></seg>
                    </seg>
                    <seg n="4" type="line" facs="z139r1_04"><seg type="word"
                            >κυλίν<unclear>δ</unclear>ρωι</seg>. τὰ δὲ <w part="I">παραλληλόγραμ</w></seg>
                    <seg n="5" type="line" facs="z139r1_05"><w part="F">μα</w>, ὧν βάσεις μὲν αἱ ΑΕ
                        ΕΒ, ὕψος </seg>
                    <seg n="6" type="line" facs="z139r1_06">δὲ τὸ αὐτὸ τῶι κυλίνδρωι, ἴσα εἶναι </seg>
                    <seg n="7" type="line" facs="z139r1_07">τῶι ΑΒ ΓΒ παραλληλογράμμωι</seg>
                    <seg n="8" type="line" facs="z139r1_08"><expan>καὶ</expan> τῶι Η χωρίωι· καὶ ἡ
                            <w part="I">ἀποτεμνο</w></seg>
                    <seg n="9" type="line" facs="z139r1_09"><w part="F">μένη</w> ἄρα κυλινδρικὴ <w
                            part="I">ἐπιφά</w></seg>
                    <seg n="10" type="line" facs="z139r1_10"><w part="F">νεια</w> ὑπὸ τῶν ΑΓ ΒΔ
                        εὐθειῶν καὶ </seg>
                    <seg n="11" type="line" facs="z139r1_11">τὰ ΑΘ ΘΕ ΕΚ ΚΒ ΓΛ ΛΖ ΖΜ ΜΔ <w part="I"
                            >ἐπί</w></seg>
                    <seg n="12" type="line" facs="z139r1_12"><w part="F">πεδα</w> τμήματα μείζονά
                        ἐστιν</seg>
                    <seg n="13" type="line" facs="z139r1_13">τῶν ΑΓΔΒ παραλληλογράμμων</seg>
                    <seg n="14" type="line" facs="z139r1_14">καὶ τοῦ Η χωρίου ἀφαιρεθέντα δὲ </seg>
                    <seg n="15" type="line" facs="z139r1_15">τὰ ΑΘ ΘΕ ΕΚ ΚΒ ΓΛ ΛΖ ΖΜ ΜΔ <w part="I"
                            >τμή</w></seg>
                    <seg n="16" type="line" facs="z139r1_16"><w part="F">ματα</w> τοῦ Η χωρίου
                        ἐλάσσονα <seg type="suppliedword"><supplied reason="lost"
                        >λ</supplied>οι</seg></seg>
                    <seg n="17" type="line" facs="z139r1_17"><seg type="wordend">πὴ</seg> ἄρα ἡ
                        ἀποτεμνομένη <seg type="expandedword">
                            <choice>
                                <abbr>κυλι</abbr>
                                <expan>κυλινδρικὴ</expan>
                            </choice>
                        </seg></seg>
                    <seg n="18" type="line" facs="z139r1_18"><seg type="wordend">
                            <abbr>δρικη</abbr>
                        </seg> ἐπιφάνεια ὑπὸ τῶν ΑΓ ΒΔ</seg>
                    <seg n="19" type="line" facs="z139r1_19"><seg type="word"><supplied
                                reason="lost">ε</supplied><unclear>ὐ</unclear>θει<supplied
                                reason="lost">ῶν</supplied></seg>
                        <seg type="word"><supplied reason="lost">μ</supplied>είζων</seg> ἐστὶν τοῦ
                        ΑΓ <supplied reason="lost">Β</supplied>Δ <seg type="suppliedword"
                    >πα</seg></seg>
                </seg>
                <seg n="134v1" type="folio">
                    <seg n="1" type="line" facs="z134v1_01"><seg type="wordend">ραλληλ<supplied
                                reason="lost">ογρ</supplied>άμμου</seg>.</seg>
                </seg>
                <seg n="134v1" type="folio">
                    <seg n="2" type="line" facs="z134v1_02">ἐὰν ἐν ἐπιφανείαι κυλίνδρου τινὸς</seg>
                    <seg n="3" type="line" facs="z134v1_03">ὀρθοῦ δύο εὐθεῖαι ὦσιν, ἀπὸ δὲ τῶν</seg>
                    <seg n="4" type="line" facs="z134v1_04">περάτων τῶν εὐθειῶν ἀχθῶσίν</seg>
                    <seg n="5" type="line" facs="z134v1_05">τινες ἐπιψαύουσαι <seg type="word"
                                ><unclear>τ</unclear>ῶ<supplied reason="lost">ν</supplied></seg>
                        <seg type="word"><supplied reason="lost"
                                >κύ</supplied>κλ<unclear>ω</unclear><supplied reason="lost"
                            >ν</supplied></seg>,</seg>
                </seg>
                <seg n="139r2" type="folio">
                    <seg n="1" type="line" facs="z139r2_01">αἵ <seg type="word"
                            >ε<unclear>ἰσ</unclear>ιν</seg>
                        <seg type="word">βάσ<unclear>ε</unclear>ις</seg> τοῦ κυλίνδρου, ἐν τῶι</seg>
                    <seg n="2" type="line" facs="z139r2_02"><seg type="word"
                            ><unclear>ἐπι</unclear>πέδωι</seg> αὐτῶν οὖσαι καὶ <seg
                            type="suppliedword">συμπέ</seg></seg>
                    <seg n="3" type="line" facs="z139r2_03"><seg type="wordend"><supplied
                                reason="lost">σ</supplied>ωσιν</seg>, τὰ <seg type="word"
                            ><unclear>π</unclear>αραλληλόγραμμα</seg> τὰ <seg type="expandedword"
                        > </seg></seg>
                    <seg n="4" type="line" facs="z139r2_04"><seg type="wordend">
                            <choice>
                                <abbr>εχόμενα</abbr>
                                <expan>περιεχόμενα</expan>
                            </choice>
                        </seg>
                        <unclear>ὑπό</unclear>
                        <seg type="word"><unclear>τ</unclear>ε</seg>
                        <seg type="word">τῶ<unclear>ν</unclear></seg> ἐπιψαυουσῶν <choice>
                            <abbr>κ</abbr>
                            <expan>καὶ</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z139r2_05">τῶν <seg type="word"
                            >πλ<unclear>ευ</unclear>ρῶν</seg> τοῦ κυλίνδρου <w part="I">μείζο</w></seg>
                    <seg n="6" type="line" facs="z139r2_06"><w part="F">να</w> ἔσται τῆς ἐπιφανείας
                        τοῦ <w part="I">κυλίν</w></seg>
                    <seg n="7" type="line" facs="z139r2_07"><w part="F">δρου</w> τῆς μεταξὺ τῶν
                        εὐθειῶν τῶν</seg>
                    <seg n="8" type="line" facs="z139r2_08">ἐν τῆι ἐπιφανείαι <seg type="word"
                                ><unclear>τ</unclear>οῦ</seg>
                        <seg type="word"><unclear>κυ</unclear>λίνδρου</seg>. </seg>
                </seg>
                <seg n="139r2" type="folio">
                    <seg n="9" type="line" facs="z139r2_09">ἔστω κυλίνδρου τινὸς ὀρθοῦ βάσις</seg>
                    <seg n="10" type="line" facs="z139r2_10">ὁ ΑΒΓ κύκλος, καὶ ἔστωσαν ἐν τῆι</seg>
                    <seg n="11" type="line" facs="z139r2_11">ἐπιφανείαι αὐτοῦ <seg type="word"
                                ><unclear>δύ</unclear>ο</seg> εὐθεῖαι, ὧν</seg>
                    <seg n="12" type="line" facs="z139r2_12">πέρατα τὰ Α, Γ, ἀπὸ δὲ τῶν Α, Γ <seg
                            type="unclearword">ἤχθω</seg></seg>
                    <seg n="13" type="line" facs="z139r2_13"><seg type="wordend"
                            ><unclear>σ</unclear>αν</seg> ἐπιψαύουσαι τοῦ κύκλου ἐν τῶι</seg>
                    <seg n="14" type="line" facs="z139r2_14">αὐτῶι ἐπιπέδωι οὖσαι <choice>
                            <expan>καὶ</expan>
                        </choice>
                        <w part="I">συμπιπτέ</w></seg>
                    <seg n="15" type="line" facs="z139r2_15"><w part="F">τωσαν</w> κατὰ τὸ Η,
                        νοείσθωσαν <unclear>δὲ</unclear>
                        <choice>
                            <expan>καὶ</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z139r2_16">ἐν τῆι <seg type="word"><supplied
                                reason="lost">ἑτ</supplied>έραι</seg> βάσει τοῦ κυλίνδρου <w
                            part="I">ἀ</w></seg>
                    <seg n="17" type="line" facs="z139r2_17"><w part="F">πὸ</w> τῶν περάτων ἐν τῆι
                            <seg type="suppliedword">ἐπι<unclear>φ</unclear>αν<supplied
                                reason="lost">εί</supplied></seg></seg>
                    <seg n="18" type="line" facs="z139r2_18"><seg type="wordend">αι</seg>
                        <seg type="unclearword">ε<unclear>ὐ</unclear>θεῖαι</seg> ἠγμέναι ἐπιψαύουσαι</seg>
                    <seg n="19" type="line" facs="z139r2_19">τοῦ <seg type="word"
                                >κύκ<unclear>λ</unclear><supplied reason="lost">ο</supplied>υ</seg>·
                        δεικτέον <choice>
                            <expan>ὅτι</expan>
                        </choice> τὰ <seg type="suppliedword">π<unclear>αρ</unclear><supplied
                                reason="lost">αλλη</supplied></seg></seg>
                </seg>
                <seg n="134v2" type="folio">
                    <seg n="1" type="line" facs="z134v2_01"><seg type="wordend">λόγραμμα</seg> τὰ
                        περιεχόμενα ὑπὸ τῶν</seg>
                    <seg n="2" type="line" facs="z134v2_02">ἐπιψαυουσῶν καὶ τῶν πλευρῶν τοῦ</seg>
                    <seg n="3" type="line" facs="z134v2_03">κυλίνδρου μείζονά ἐστι τῆς κατὰ <seg
                            type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">ὴν</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="4" type="line" facs="z134v2_04">ΑΒΓ περιφέρειαν <seg type="word"
                                >ἐπι<supplied reason="lost">φα</supplied>νείας</seg> τοῦ</seg>
                    <seg n="5" type="line" facs="z134v2_05">κυλίνδρου.</seg>
                </seg>
                <seg n="134v2" type="folio">
                    <seg n="5" type="line" facs="z134v2_05">ἤχθω γὰρ ἡ ΕΖ <w part="I">ἐπιψαύου</w></seg>
                    <seg n="6" type="line" facs="z134v2_06"><w part="F">σα</w>, καὶ ἀπὸ τῶν Ε, Ζ
                        σημείων <w part="I">ἤχθω</w></seg>
                    <seg n="7" type="line" facs="z134v2_07"><w part="F">σάν</w> τινες εὐθεῖαι παρὰ
                        τὸν ἄξονα</seg>
                    <seg n="8" type="line" facs="z134v2_08">τοῦ κυλίνδρου ἕως τῆς <choice>
                            <abbr>ἐπιφανεί</abbr>
                            <expan>ἐπιφανείας</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z134v2_09">τῆς ἑτέρας βάσεως· τὰ δὴ <w part="I"
                            >παραλ</w></seg>
                    <seg n="10" type="line" facs="z134v2_10"><w part="F">ληλόγραμμα</w> τὰ
                        περιεχόμενα ὑπὸ</seg>
                    <seg n="11" type="line" facs="z134v2_11">τῶν ΑΗ, ΗΓ καὶ τῶν πλευρῶν τοῦ <w
                            part="I">κυ</w></seg>
                    <seg n="12" type="line" facs="z134v2_12"><w part="F">λίνδρου</w> μείζονά ἐστιν
                        τῶν <w part="I">παραλ</w></seg>
                    <seg n="13" type="line" facs="z134v2_13"><w part="F">ληλογράμμων</w> τῶν <choice>
                            <abbr>περιεχομένω</abbr>
                            <expan>περιεχομένων</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z134v2_14">ὑπό τε τῶν ΑΕ, ΕΖ, ΖΓ <seg type="word"
                                >κα<supplied reason="lost">ὶ</supplied></seg>
                        <seg type="word"><unclear>τ</unclear>ῆς</seg>
                        <seg type="word"><supplied reason="lost">π</supplied>λευρ<supplied
                                reason="lost">ᾶς</supplied></seg></seg>
                    <seg n="15" type="line" facs="z134v2_15">τοῦ κυλίνδρου ἐπεὶ γὰρ αἱ ΕΗ, ΗΖ τῆς</seg>
                    <seg n="16" type="line" facs="z134v2_16">ΕΖ μείζους <choice>
                            <expan>
                                <unclear>εἰσίν</unclear>
                            </expan>
                        </choice>, κοιναὶ <seg type="word">
                            <choice>
                                <abbr>κείσθωσαν</abbr>
                                <expan>προσκείσθω<supplied reason="lost"
                                    >σ</supplied>α<unclear>ν</unclear></expan>
                            </choice>
                        </seg></seg>
                    <seg n="17" type="line" facs="z134v2_17">αἱ ΑΕ, ΖΓ. <seg type="word"><supplied
                                reason="lost">ὅλ</supplied>αι</seg>
                        <choice>
                            <expan>
                                <unclear>ἄρα</unclear>
                            </expan>
                        </choice>
                        <supplied reason="lost">αἱ</supplied> ΗΑ, <supplied reason="lost">ΗΓ μείζους <choice>
                                <expan>εἰσίν</expan>
                            </choice></supplied></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
