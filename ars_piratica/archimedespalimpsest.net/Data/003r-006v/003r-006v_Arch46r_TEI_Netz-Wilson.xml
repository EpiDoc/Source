                <seg n="3r1" type="folio">
                    <seg n="1" type="line" facs="z3r1_01">
                        <supplied reason="lost">ἐπὶ τὰ αὐτὰ ἔσονται ἢ οὐδὲν ἔχουσιν</supplied>
                    </seg>
                    <seg n="2" type="line" facs="z3r1_02">
                        <supplied reason="lost">ἐπὶ τὰ ἕτερα</supplied>
                    </seg>
                </seg>
                <seg n="3r1" type="folio">
                    <seg n="2" type="line" facs="z3r1_02">
                        <supplied reason="lost">ἐπὶ τὰ αὐτὰ δὴ</supplied>
                    </seg>
                    <seg n="3" type="line" facs="z3r1_03"><seg type="word">κοίλ<unclear>ας</unclear></seg>
                        <seg type="word"><supplied reason="lost"
                        >κ</supplied><unclear>α</unclear>λῶ</seg> τὰς <seg type="word"
                            >το<unclear>ι</unclear>αύτας</seg>
                        <seg type="unclearword">ἐπιφα</seg></seg>
                    <seg n="4" type="line" facs="z3r1_04"><seg type="wordend"
                            >νεί<unclear>ας</unclear></seg>, ἐν αἷς ἂν <seg type="word"
                            ><unclear>δ</unclear>ύο</seg>
                        <seg type="word">σημεί<supplied reason="lost"
                            >ω</supplied><unclear>ν</unclear></seg>
                        <w part="I">λαμ</w></seg>
                    <seg n="5" type="line" facs="z3r1_05"><w part="F">βανομένων</w> αἱ <seg
                            type="word">μ<unclear>ετ</unclear><supplied reason="lost"
                            >αξυ</supplied>̀</seg>
                        <seg type="word">τ<supplied reason="lost">ῶν</supplied></seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">σημεί</supplied>
                        </seg></seg>
                    <seg n="6" type="line" facs="z3r1_06"><seg type="wordend">ων</seg> εὐθεῖαι <seg
                            type="word">ἤ<supplied reason="lost">τ</supplied>οι</seg>
                        <seg type="word">π<supplied reason="lost">ᾶσαι</supplied></seg>
                        <supplied reason="lost">ἐπὶ τὰ</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">αὐ</supplied>
                        </seg></seg>
                    <seg n="7" type="line" facs="z3r1_07"><seg type="wordend">τὰ</seg>
                        <seg type="word">π<unclear>ί</unclear>πτ<supplied reason="lost"
                            >ουσιν</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τῆς</supplied>
                        </seg>
                        <seg type="word">ἐ<unclear>πι</unclear>φ<supplied reason="lost"
                                >α</supplied><unclear>ν</unclear><supplied reason="lost"
                            >είας</supplied></seg>,</seg>
                    <seg n="8" type="line" facs="z3r1_08">ἢ τινὲς <seg type="word"
                            >μ<unclear>ὲν</unclear></seg>
                        <supplied reason="lost">ἐπὶ τὰ</supplied>
                        <seg type="word"><supplied reason="lost">α</supplied>ὐτ<supplied
                                reason="lost">ὰ</supplied></seg>, <supplied reason="lost">τινὲς δὲ
                        </supplied></seg>
                    <seg n="9" type="line" facs="z3r1_09">κατ’ <seg type="word">
                            <unclear>α</unclear>
                            <supplied reason="lost">ὐτῆς</supplied>
                        </seg>, <seg type="word"><unclear>ἐπ</unclear>ὶ</seg>
                        <seg type="word">τ<unclear>ὰ</unclear></seg>
                        <seg type="word"><unclear>ἕτ</unclear><supplied reason="lost"
                        >ερ</supplied>α</seg> δὲ <seg type="word">
                            <supplied reason="lost">μ</supplied>
                            <unclear>έρ</unclear>
                            <supplied reason="lost">η</supplied>
                        </seg>
                        <seg type="suppliedword"><unclear>μ</unclear>η</seg></seg>
                    <seg n="10" type="line" facs="z3r1_10"><seg type="wordend"
                                ><unclear>δε</unclear>μ<supplied reason="lost"
                    >ία</supplied></seg>.</seg>
                </seg>
                <seg n="3r1" type="folio">
                    <seg n="10" type="line" facs="z3r1_10"><seg type="word">
                            <supplied reason="lost">τομέ</supplied>
                            <unclear>α</unclear>
                        </seg>
                        <seg type="word"><unclear>δ</unclear>ὲ</seg>
                        <seg type="word">στε<supplied reason="lost"
                                >ρ</supplied><unclear>ε</unclear><supplied reason="lost"
                            >ὸν</supplied></seg>
                        <seg type="word"><unclear>κα</unclear>λῶ</seg>,</seg>
                    <seg n="11" type="line" facs="z3r1_11">
                        <seg type="word"><supplied reason="lost"
                            >ἐπ</supplied><unclear>ειδ</unclear>ὰν</seg>
                        <seg type="word"><supplied reason="lost">σφαῖ</supplied>ρ<supplied
                                reason="lost">αν</supplied></seg>
                        <seg type="word"><unclear>κῶ</unclear>ν<unclear>ος</unclear></seg>
                        <seg type="word">τέμη<supplied>ι</supplied></seg>
                    </seg>
                    <seg n="12" type="line" facs="z3r1_12"><seg type="word">κορ<supplied
                                reason="lost">υφὴν</supplied></seg>
                        <seg type="word">
                            <supplied reason="lost">ἔχω</supplied>
                            <unclear>ν</unclear>
                        </seg>
                        <seg type="word">πρ<supplied reason="lost">ὸς</supplied></seg>
                        <seg type="word"><unclear>τ</unclear>ῶι</seg> κέντρωι</seg>
                    <seg n="13" type="line" facs="z3r1_13">τῆς <seg type="word"
                                ><unclear>σφ</unclear>αί<unclear>ρ</unclear>ας</seg>, τὸ <seg
                            type="suppliedword">ἐμ<supplied reason="lost">περιεχ</supplied>όμε</seg></seg>
                    <seg n="14" type="line" facs="z3r1_14"><seg type="wordend">
                            <supplied reason="lost">νον</supplied>
                        </seg>
                        <supplied reason="lost">σχῆμα ὑπό</supplied> τε <seg type="word">τῆ<supplied
                                reason="lost">ς</supplied></seg>
                        <seg type="suppliedword">ἐπιφα</seg></seg>
                    <seg n="15" type="line" facs="z3r1_15"><seg type="wordend">
                            <unclear>νεί</unclear>
                            <supplied reason="lost">ας</supplied>
                        </seg> τοῦ <seg type="word">κών<supplied reason="lost">ου</supplied></seg>
                        <seg type="word"><supplied reason="lost">κ</supplied>αὶ</seg> τῆς <w
                            part="I">ἐπιφα</w></seg>
                    <seg n="16" type="line" facs="z3r1_16"><w part="F">νείας</w> τῆς σφαίρας ἐντὸς
                        τοῦ</seg>
                    <seg n="17" type="line" facs="z3r1_17">κώνου.</seg>
                </seg>
                <seg n="3r1" type="folio">
                    <seg n="17" type="line" facs="z3r1_17">
                        <seg type="word"><supplied reason="lost"
                                >ῥ</supplied>ό<unclear>μ</unclear>β<supplied reason="lost"
                            >ον</supplied></seg>
                        <seg type="word">
                            <unclear>δ</unclear>
                            <supplied reason="lost">ὲ</supplied>
                        </seg>
                        <seg type="word"><unclear>κα</unclear>λῶ</seg>
                        <seg type="suppliedword">στερε</seg>
                    </seg>
                    <seg n="18" type="line" facs="z3r1_18"><seg type="wordend">
                            <supplied reason="lost">όν</supplied>
                        </seg>, <seg type="word">
                            <supplied reason="lost">ἐπειδ</supplied>
                            <unclear>ὰν</unclear>
                        </seg>
                        <unclear>δύο</unclear>
                        <supplied reason="lost">κῶνοι τὴν αὐτὴν</supplied></seg>
                </seg>
                <seg n="6v1" type="folio">
                    <seg n="1" type="line" facs="z6v1_01">βάσιν ἔχοντες τὰς κορυφὰς</seg>
                    <seg n="2" type="line" facs="z6v1_02">ἔχωσιν ἐφ’ ἑκάτερα τοῦ <w part="I"
                        >ἐπιπέ</w></seg>
                    <seg n="3" type="line" facs="z6v1_03"><w part="F">δου</w> τῆς βάσεως, ὅπως οἱ <w
                            part="I">ἄξο</w></seg>
                    <seg n="4" type="line" facs="z6v1_04"><w part="F">νες</w> αὐτῶν ἐπ’ εὐθείας ὦσι
                            <w part="I">κείμε</w></seg>
                    <seg n="5" type="line" facs="z6v1_05"><w part="F">νοι</w>, τὸ ἐξ ἀμφοῖν τοῖν
                        κώνοιν</seg>
                    <seg n="6" type="line" facs="z6v1_06">συγκείμενον τὸ στερεὸν σχῆμα.</seg>
                </seg>
                <seg n="6v1" type="folio">
                    <seg n="7" type="line" facs="z6v1_07">λαμβάνω δὲ ταῦτα·</seg>
                </seg>
                <seg n="6v1" type="folio">
                    <seg n="7" type="line" facs="z6v1_07">τῶν τὰ αὐτὰ </seg>
                    <seg n="8" type="line" facs="z6v1_08">πέρατα ἐχουσῶν γραμμῶν <w part="I">ἐ</w></seg>
                    <seg n="9" type="line" facs="z6v1_09"><w part="F">λαχίστην</w> εἶναι τὴν
                        εὐθεῖαν.</seg>
                </seg>
                <seg n="6v1" type="folio">
                    <seg n="9" type="line" facs="z6v1_09">τῶν</seg>
                    <seg n="10" type="line" facs="z6v1_10">δὲ ἄλλων γραμμῶν, εἶναι τὰς <w part="I"
                            >τοιαύ</w></seg>
                    <seg n="11" type="line" facs="z6v1_11"><w part="F">τας</w>, ἐπειδ’ ἂν ὦσιν
                        ἀμφότεραι</seg>
                    <seg n="12" type="line" facs="z6v1_12">ἐπὶ τὰ αὐτὰ κοῖλαι, καὶ ἤτοι ὅλη </seg>
                    <seg n="13" type="line" facs="z6v1_13">περιλαμβάνηται ἡ ἑτέρα <choice>
                            <abbr>αυτ</abbr>
                            <expan>αὐτῶν</expan>
                        </choice>
                    </seg>
                    <seg n="14" type="line" facs="z6v1_14">ὑπὸ τῆς ἑτέρας ἐπιφανείας </seg>
                    <seg n="15" type="line" facs="z6v1_15"><expan>καὶ</expan> τῆς εὐθείας τῆς τὰ
                        αὐτὰ <w part="I">πέρα</w></seg>
                    <seg n="16" type="line" facs="z6v1_16"><w part="F">τα</w> ἐχούσης αὐτῆ, ἢ τινὰ
                        μὲν <seg type="expandedword"/>
                    </seg>
                </seg>
                <seg n="3r2" type="folio">
                    <seg n="1" type="line" facs="z3r2_01"><seg type="wordend">
                            <choice>
                                <expan>περιλαμβάνηται</expan>
                                <abbr>
                                    <supplied reason="lost">λαμβάνηται</supplied>
                                </abbr>
                            </choice>
                        </seg>, <supplied reason="lost">τινὰ δὲ κοινὰ ἔχη</supplied>, </seg>
                    <seg n="2" type="line" facs="z3r2_02">
                        <supplied reason="lost">καὶ ἐλάσσονα εἶναι τὴν </supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">περιλαμ</supplied>
                        </seg>
                    </seg>
                    <seg n="3" type="line" facs="z3r2_03"><seg type="wordend">βαν<supplied
                                reason="lost">ο</supplied>μέ<supplied reason="lost"
                        >νην</supplied></seg>.</seg>
                </seg>
                <seg n="3r2" type="folio">
                    <seg n="3" type="line" facs="z3r2_03">ὁμοίως δὲ καὶ τῶν <seg type="suppliedword"
                            >ἐπιφα</seg></seg>
                    <seg n="4" type="line" facs="z3r2_04"><seg type="wordend">ν<supplied
                                reason="lost">ειῶ</supplied>ν</seg> τῶν <seg type="word">τ<supplied
                                reason="lost">ὰ</supplied></seg>
                        <supplied reason="lost">αὐτὰ πέρατα</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἐχου</supplied>
                        </seg></seg>
                    <seg n="5" type="line" facs="z3r2_05"><seg type="wordend"><supplied
                                reason="lost">σῶ</supplied>ν</seg>, <unclear>ἐὰν</unclear>
                        <supplied reason="lost">ἐν</supplied>
                        <seg type="word">
                            <supplied reason="lost">ἐ</supplied>πιπέ<unclear>δω</unclear><supplied
                                reason="lost">ι</supplied></seg>
                        <supplied reason="lost">τὰ</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">πέρα</supplied>
                        </seg></seg>
                    <seg n="6" type="line" facs="z3r2_06"><seg type="wordend">
                            <supplied reason="lost">τα</supplied>
                        </seg>
                        <seg type="word"><supplied reason="lost">ἔχωσι</supplied>ν</seg>, ἐλάσσονα
                        εἶναι <supplied reason="lost">τὴν</supplied></seg>
                    <seg n="7" type="line" facs="z3r2_07">
                        <supplied reason="lost">ἐπίπεδον</supplied>.</seg>
                </seg>
                <seg n="3r2" type="folio">
                    <seg n="7" type="line" facs="z3r2_07">
                        <supplied reason="lost">τῶν δὲ ἄλλων</supplied>
                        <seg type="suppliedword"><supplied reason="lost"
                            >ἐ</supplied><unclear>πι</unclear>φα</seg>
                    </seg>
                    <seg n="8" type="line" facs="z3r2_08">
                        <seg type="wordend">
                            <supplied reason="lost">νειῶν</supplied>
                        </seg>
                        <supplied reason="lost">καὶ τὰ αὐτὰ πέρατα ἐχουσῶν,</supplied>
                    </seg>
                    <seg n="9" type="line" facs="z3r2_09">
                        <supplied reason="lost"> ἐὰν ἐν ἐπιπέδωι τὰ πέρατα ἦι,</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἀ</supplied>
                        </seg>
                    </seg>
                    <seg n="10" type="line" facs="z3r2_10">
                        <seg type="wordend">ν<supplied reason="lost">ίσους</supplied></seg>
                        <supplied reason="lost">εἶναι τὰς τοιαύτας, ἐπειδὰν</supplied>
                    </seg>
                    <seg n="11" type="line" facs="z3r2_11">ὦσιν <seg type="word">ἀ<supplied
                                reason="lost">μ</supplied>φ<unclear>ότερ</unclear><supplied
                                reason="lost">αι</supplied></seg>
                        <supplied reason="lost">ἐπὶ τὰ αὐτὰ</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">κοῖ</supplied>
                        </seg></seg>
                    <seg n="12" type="line" facs="z3r2_12"><seg type="wordend">λαι</seg>, καὶ ἤτοι
                        ὅλη <seg type="word">περιλαμβάνητ<supplied reason="lost">αι</supplied></seg></seg>
                    <seg n="13" type="line" facs="z3r2_13">ὑπὸ τῆς ἑτέρας ἐπιφανείας <seg
                            type="word"><unclear>κα</unclear>ὶ</seg></seg>
                    <seg n="14" type="line" facs="z3r2_14">τῆς <seg type="word">ἐπι<supplied
                                reason="lost">πέ</supplied>δου</seg> τῆς τὰ <seg type="word"
                                >αὐτ<unclear>ὰ</unclear></seg>
                        <seg type="suppliedword"><supplied reason="lost">πέ</supplied>ρα</seg></seg>
                    <seg n="15" type="line" facs="z3r2_15"><seg type="wordend">τα</seg> ἐχούσης
                        αὐτῆ, ἢ τινὰ μὲν <w part="I">πε</w></seg>
                    <seg n="16" type="line" facs="z3r2_16"><w part="F">ριλαμβάνηται</w>, <seg
                            type="word">τιν<supplied reason="lost">ὰ</supplied></seg>
                        <seg type="word">
                            <unclear>δ</unclear>
                            <supplied reason="lost">ὲ</supplied>
                        </seg>
                        <supplied reason="lost">κοινὰ</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἔ</supplied>
                        </seg>
                    </seg>
                    <seg n="17" type="line" facs="z3r2_17"><seg type="wordend">
                            <supplied reason="lost">χη</supplied>
                        </seg>, <seg type="word"><supplied reason="lost"
                            >κ</supplied><unclear>α</unclear>ὶ</seg> ἐλάσσονα εἶναι <supplied
                            reason="lost">τὴν</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">περι</supplied>
                        </seg></seg>
                    <seg n="18" type="line" facs="z3r2_18"><seg type="wordend"><supplied
                                reason="lost">λαμβανομ</supplied>ένην</seg>.</seg>
                </seg>
                <seg n="3r2" type="folio">
                    <seg n="18" type="line" facs="z3r2_18">ἔτι δὲ τῶν <seg type="word">ἀν<supplied
                                reason="lost">ίσων</supplied></seg></seg>
                </seg>
                <seg n="6v2" type="folio">
                    <seg n="1" type="line" facs="z6v2_01">γραμμῶν <expan>καὶ</expan> τῶν ἀνίσων <w
                            part="I">ἐπιφα</w></seg>
                    <seg n="2" type="line" facs="z6v2_02"><w part="F">νειῶν</w> καὶ τῶν ἀνίσων
                        στερεῶν</seg>
                    <seg n="3" type="line" facs="z6v2_03">τὸ μεῖζον τοῦ ἐλάσσονος <choice>
                            <abbr>υπερεχει</abbr>
                            <expan>ὑπερέχειν</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z6v2_04">τοιούτω, ὃ συντιθέμενον αὐτὸ <w part="I"
                        >ἑ</w></seg>
                    <seg n="5" type="line" facs="z6v2_05"><w part="F">αυτῶι</w> δυνατόν ἐστιν
                        ὑπερέχειν <w part="I">παν</w></seg>
                    <seg n="6" type="line" facs="z6v2_06"><w part="F">τὸς</w> τοῦ προτεθέντος τῶν
                            <expan>πρὸς</expan>
                        <w part="I">ἄλλη</w></seg>
                    <seg n="7" type="line" facs="z6v2_07"><w part="F">λα</w> λεγομένων.</seg>
                </seg>
                <seg n="6v2" type="folio">
                    <seg n="7" type="line" facs="z6v2_07">τούτων δὲ <w part="I">ὑποκει</w></seg>
                    <seg n="8" type="line" facs="z6v2_08"><w part="F">μένων</w>, ἐὰν εἰς κύκλον
                        πολύγωνον</seg>
                    <seg n="9" type="line" facs="z6v2_09">ἐγγραφῆ, φανερὸν <expan>ὅτι</expan> ἡ
                        περίμετρος</seg>
                    <seg n="10" type="line" facs="z6v2_10">τοῦ ἐγγραφέντος πολυγώνου <w part="I"
                            >ἐλάσ</w></seg>
                    <seg n="11" type="line" facs="z6v2_11"><w part="F">σων</w> ἐστὶν τῆς τοῦ κύκλου <choice>
                            <abbr>περφερει</abbr>
                            <expan>περιφερείας</expan>
                        </choice>·</seg>
                    <seg n="12" type="line" facs="z6v2_12">ἑκάστη γὰρ τῶν τοῦ πολυγώνου <seg
                            type="suppliedword">πλ<supplied reason="lost">ευ</supplied></seg></seg>
                    <seg n="13" type="line" facs="z6v2_13"><seg type="wordend">ρῶν</seg> ἐλάσσων
                        ἐστὶ τῆς τοῦ κύκλου <seg type="expandedword">περι</seg></seg>
                    <seg n="14" type="line" facs="z6v2_14"><seg type="wordend">
                            <choice>
                                <abbr>φερειας</abbr>
                                <expan>περιφερείας</expan>
                            </choice>
                        </seg> τῆς ὑπὸ τῆς αὐτῆς <w part="I">ἀπο</w></seg>
                    <seg n="15" type="line" facs="z6v2_15"><w part="F">τεμνομένης</w>.</seg>
                </seg>
                <seg n="6v2" type="folio">
                    <num>Α</num>
                </seg>
                <seg n="6v2" type="folio">
                    <seg n="15" type="line" facs="z6v2_15">ἐάνπερ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλον</expan>
                        </choice>
                    </seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
