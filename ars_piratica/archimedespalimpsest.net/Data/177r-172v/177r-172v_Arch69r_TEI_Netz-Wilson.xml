            <ab>
                <seg n="177r1" type="folio">
                    <seg n="1" type="line" facs="z177r1_01">ΗΑΓ, <unclear>καὶ</unclear> ἡ ὑπὸ
                            <supplied reason="lost">ΗΓΒ</supplied>
                        <seg type="word">
                            <unclear>τ</unclear>
                            <supplied reason="lost">ῆι</supplied>
                        </seg> ὑπὸ ΗΑΓ </seg>
                    <seg n="2" type="line" facs="z177r1_02"><supplied reason="lost">
                            <expan>ἐστὶν</expan>
                        </supplied>
                        <seg type="word"><supplied reason="lost">ἴ</supplied>ση</seg>. <seg
                            type="word">
                            <supplied reason="lost">κ</supplied>
                            <unclear>αὶ</unclear>
                        </seg>
                        <seg type="word">κ<supplied reason="lost">οινὴ</supplied></seg>
                        <supplied reason="lost">ἡ</supplied> ὑπὸ ΑΗΓ ὀρθή· </seg>
                    <seg n="3" type="line" facs="z177r1_03"><seg type="word"><supplied reason="lost"
                                >κ</supplied>αὶ</seg>
                        <seg type="word">τρ<unclear>ί</unclear>τη</seg>
                        <unclear>
                            <expan>ἄρα</expan>
                        </unclear>
                        <supplied reason="lost">ἡ</supplied>
                        <supplied reason="lost">ὑπὸ</supplied>
                        <supplied reason="lost">ΗΖΓ</supplied>
                        <seg type="word"><supplied reason="lost">τ</supplied>ρίτη</seg> τῆι </seg>
                    <seg n="4" type="line" facs="z177r1_04"><supplied reason="lost">ὑπὸ</supplied>
                        <supplied reason="lost">ΑΓΗ.</supplied>
                        <seg type="word"><supplied reason="lost"
                                >ἰσ</supplied><unclear>ο</unclear>γ<unclear>ών</unclear>ιο<unclear>ν</unclear></seg>
                        ἄρα τὸ ΑΗΓ </seg>
                    <seg n="5" type="line" facs="z177r1_05"><supplied reason="lost">τῶι</supplied>
                        <supplied reason="lost">ΓΗΖ</supplied>
                        <seg type="word"><supplied reason="lost"
                                >τ</supplied><unclear>ρι</unclear>γώνω<supplied reason="lost"
                            >ι</supplied>·</seg> ἔστιν ἄρα, ὡς </seg>
                    <seg n="6" type="line" facs="z177r1_06">ἡ <supplied reason="lost">ΑΗ</supplied>
                        <seg type="word"><supplied reason="lost">πρὸ</supplied>ς</seg> ΗΓ, ἡ ΓΗ πρὸς
                        ΗΖ καὶ ἡ ΑΓ </seg>
                    <seg n="7" type="line" facs="z177r1_07"><expan>πρὸς</expan> ΓΖ. ἀλλ' <seg
                            type="word"><unclear>ὡ</unclear>ς</seg> ἡ ΑΓ <expan>πρὸς</expan> ΓΖ,
                            <expan>καὶ</expan>
                        <seg type="clearword">συναμ</seg></seg>
                    <seg n="8" type="line" facs="z177r1_08"><seg type="wordend">φότερος</seg> ἡ ΓΑΒ
                            <expan>πρὸς</expan> ΒΓ· καὶ ὡς <seg type="suppliedword">συ</seg></seg>
                    <seg n="9" type="line" facs="z177r1_09"><seg type="wordend">ν<supplied
                                reason="lost">α</supplied>μφό<unclear>τε</unclear>ρος</seg>
                        <expan>ἄρα</expan> ἡ <seg type="word">Β<supplied reason="lost">Α</supplied>Γ</seg>
                        <expan>πρὸς</expan> ΒΓ, ἡ </seg>
                    <seg n="10" type="line" facs="z177r1_10"><seg type="word"><supplied
                                reason="lost">Α</supplied>Η</seg>
                        <expan>πρὸς</expan> ΗΓ. διὰ τοῦτο <expan>οὖν</expan>
                        <unclear>ἡ</unclear> ΑΗ <expan>πρὸς</expan> τὴν ΗΓ </seg>
                    <seg n="11" type="line" facs="z177r1_11">ἐλάσσονα λόγον ἔχει <choice>
                            <abbr>η</abbr>
                            <expan>ἤπερ</expan>
                        </choice>
                        <num type="cardinal">ΒϡΙΑ</num>
                    </seg>
                    <seg n="12" type="line" facs="z177r1_12"><expan>πρὸς</expan>
                        <seg type="word">
                            <num>Ψ<unclear>Π</unclear></num>
                        </seg>, ἡ δὲ ΑΓ <expan>πρὸς</expan> τὴν ΓΗ ἐλάσσονα </seg>
                    <seg n="13" type="line" facs="z177r1_13"><supplied reason="lost">ἢ</supplied>
                        <supplied reason="lost">ὃν</supplied>
                        <num type="cardinal">ΓΙΓ</num>
                        <num type="fraction">Γ</num>
                        <num type="fraction">Δ</num>
                        <expan>πρὸς</expan>
                        <num>ΨΠ</num>. δίχα ἡ ὑπὸ <seg type="suppliedword">Γ</seg></seg>
                    <seg n="14" type="line" facs="z177r1_14"><seg type="wordend">
                            <supplied reason="lost">ΑΗ</supplied>
                        </seg> τῆι ΑΘ· ἡ ΑΘ <expan>ἄρα</expan> διὰ τὰ αὐτὰ </seg>
                    <seg n="15" type="line" facs="z177r1_15"><expan>πρὸς</expan> τὴν ΘΓ
                            <sic>ἐλάσσονο</sic> λόγον ἔχει ἢ </seg>
                    <seg n="16" type="line" facs="z177r1_16"><supplied reason="lost">ὃν</supplied>
