                        </seg>
                        <num type="fraction">Ε</num>
                        <num type="fraction">Δ</num>
                        <expan>πρὸς</expan>
                        <num>ΨΠ</num> ἢ ὃν <seg type="word">
                            <num type="cardinal">Α<supplied reason="lost">Ω</supplied>ΚΓ</num>
                        </seg></seg>
                    <seg n="17" type="line" facs="z177r1_17"><supplied reason="lost">
