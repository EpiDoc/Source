    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="39v1" type="folio">
                    <seg n="1" type="line" facs="z39v1_01"><seg type="word"><supplied reason="lost"
                                >σ</supplied><unclear>χῆ</unclear>μα</seg> τοῦ τῆς Δ <expan>πρὸς</expan>
                        <unclear>Ζ</unclear>. ἡ δὲ Δ <expan>πρὸς</expan> Ε <w part="I">μεί</w></seg>
                    <seg n="2" type="line" facs="z39v1_02"><w part="F">ζονα</w>
                        <seg type="word">λ<supplied reason="lost">όγον</supplied></seg> ἔχει ἢ
                        τριπλάσιον <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z39v1_03">τῆς Δ <expan>πρὸς</expan> Ζ· τὸ
                            <expan>ἄρα</expan>
                        <seg type="word">περιγεγραμμ<unclear>έ</unclear><supplied reason="lost"
                            >ν</supplied>ον</seg>
                        <expan>σχῆμα</expan></seg>
                    <seg n="4" type="line" facs="z39v1_04">στερεὸν τῶι τομεῖ <expan>πρὸς</expan> τὸ
                            <seg type="suppliedword">ἐγγ<supplied reason="lost"
                        >εγρα</supplied>μμέ</seg></seg>
                    <seg n="5" type="line" facs="z39v1_05"><seg type="wordend">νον</seg> σχῆμα
                        ἐλάσσονα <sic>λογο ει</sic> ἡ </seg>
                    <seg n="6" type="line" facs="z39v1_06">Δ <expan>πρὸς</expan> Ε. ἡ δὲ Δ
                            <expan>πρὸς</expan> Ε ἐλάσσονα λόγον ἔχει</seg>
                    <seg n="7" type="line" facs="z39v1_07">ἢ ὁ στερεὸς τομεὺς πρὸς <seg type="word"
                                >τὸ<unclear>ν</unclear></seg>
                        <supplied reason="lost">Θ</supplied>
                        <w part="I">κῶ</w></seg>
                    <seg n="8" type="line" facs="z39v1_08"><w part="F">νον</w>· ἢ τὸ <seg
                            type="word">περιγεγραμμέν<supplied reason="lost">ον</supplied></seg>
                        <seg type="word"><supplied reason="lost">τ</supplied>ῶι</seg>
                        <w part="I">το</w></seg>
                    <seg n="9" type="line" facs="z39v1_09"><w part="F">μεῖ</w> σχῆμα
                        <expan>πρὸς</expan> τὸ <seg type="word">ἐγγεγραμμ<supplied reason="lost"
                                >ένον</supplied></seg>. <expan>καὶ</expan>
                    </seg>
                    <seg n="10" type="line" facs="z39v1_10">ἐναλλάξ· μεῖζον δέ <expan>ἐστι</expan>
                        τὸ <w part="I">περιγε</w></seg>
                    <seg n="11" type="line" facs="z39v1_11"><w part="F">γραμμένον</w> στερεὸν σχῆμα
                            <seg type="word"><supplied reason="lost"
                            >το</supplied><unclear>υ</unclear>͂</seg>
                        <w part="I">τμή</w></seg>
                    <seg n="12" type="line" facs="z39v1_12"><w part="F">ματος</w>·
                        <expan>καὶ</expan> τὸ ἐγγεγραμμένον <supplied reason="lost">
                            <expan>ἄρα</expan>
                        </supplied>
                        <choice>
                            <abbr>σχ</abbr>
                            <expan>σχῆμα</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z39v1_13">ἐν τῶι <choice>
                            <abbr>το</abbr>
                            <expan>τομεῖ</expan>
                        </choice> μεῖζόν <abbr>ἐστι</abbr> τοῦ Θ κώνου·</seg>
                    <seg n="14" type="line" facs="z39v1_14">
                        <choice>
                            <abbr>ο</abbr>
                            <expan>ὅπερ</expan>
                        </choice> ἀδύνατον· δέδεικται γὰρ ἐν</seg>
                    <seg n="15" type="line" facs="z39v1_15">τοῖς ἄνω ἔλασσον ὂν τοῦ <seg
                            type="suppliedword">τηλικο<supplied reason="lost">ύ</supplied></seg></seg>
                    <seg n="16" type="line" facs="z39v1_16"><seg type="wordend">του</seg> κώνου <choice>
                            <abbr>τουτ</abbr>
                            <expan>τουτέστι</expan>
                        </choice> τοῦ ἔχοντος <seg type="word"><supplied reason="lost"
                                >β</supplied>ά<supplied reason="lost">σιν</supplied></seg></seg>
                    <seg n="17" type="line" facs="z39v1_17">μὲν <choice>
                            <abbr>κυκλ</abbr>
                            <expan>κύκλον</expan>
                        </choice>, οὗ ἡ ἐκ τοῦ κέντρου</seg>
                    <seg n="18" type="line" facs="z39v1_18">ἴση <expan>ἐστὶ</expan> τῆι ἀπὸ τῆς
                        κορυφῆς τοῦ </seg>
                    <seg n="19" type="line" facs="z39v1_19">τμήματος ἐπὶ τὴν <choice>
                            <abbr>περιφερεια</abbr>
                            <expan>περιφέρειαν</expan>
                        </choice></seg>
                    <seg n="20" type="line" facs="z39v1_20">ἐπεζευγνυμένη εὐθείαι <seg type="word"
                                >το<supplied reason="lost">ῦ</supplied></seg> κύκλου,</seg>
                    <seg n="21" type="line" facs="z39v1_21">ὅς <expan>ἐστιν</expan> βάσις τοῦ
                        τμήματος, ὕψος δὲ</seg>
                    <gap/>
                </seg>
                <seg n="39v2" type="folio">
                    <gap/>
                    <seg n="1" type="line" facs="z39v2_01">γεγραμμένου περὶ τὸν <choice>
                            <abbr>επιπεδο</abbr>
                            <expan>ἐπίπεδον</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z39v2_02"><seg type="word"><supplied reason="lost"
                                >τ</supplied><unclear>ο</unclear>μέα</seg>
                        <seg type="word">πολυγώνο<unclear>υ</unclear></seg> ἀρτιογώνου </seg>
                    <seg n="3" type="line" facs="z39v2_03">ἡ <seg type="word">πλευ<supplied
                                reason="lost">ρὰ</supplied></seg>
                        <expan>πρὸς</expan> τὴν τοῦ <w part="I">ἐγγεγραμμέ</w></seg>
                    <seg n="4" type="line" facs="z39v2_04"><w part="F">νου</w> ἐλάσσονα
                        <expan>λόγον</expan> ἐχέτω τοῦ ὃν <w part="I">ἔ</w></seg>
                    <seg n="5" type="line" facs="z39v2_05"><w part="F">χει</w> ἡ Δ
                        <expan>πρὸς</expan> Ζ καὶ γεγενήσθω τὸ <expan>περὶ</expan></seg>
                    <seg n="6" type="line" facs="z39v2_06">τὸν στερεὸν τομέα στερεὰ <choice>
                            <abbr>σχημα</abbr>
                            <expan>σχήματα</expan>
                        </choice>·</seg>
                    <seg n="7" type="line" facs="z39v2_07">ὁμοίως <seg type="word"
                            >οὖ<unclear>ν</unclear></seg> δείξομεν <expan>ὅτι</expan> τὸ <w part="I"
                            >περιγε</w></seg>
                    <seg n="8" type="line" facs="z39v2_08"><w part="F">γραμμένον</w> περὶ τὸν τομέα
                            <seg type="unclearword"><unclear>στ</unclear>ε</seg></seg>
                    <seg n="9" type="line" facs="z39v2_09"><seg type="wordend">ρεὸν</seg> σχῆμα
                            <expan>πρὸς</expan> τὸ <seg type="word">ἐγγεγραμ<supplied reason="lost"
                                >μένον</supplied></seg></seg>
                    <seg n="10" type="line" facs="z39v2_10">ἐλάσσονα <expan>λόγον</expan> ἔχει τοῦ
                        ὃν ἔχει ἡ Δ</seg>
                    <seg n="11" type="line" facs="z39v2_11"><expan>πρὸς</expan> Ε <expan>καὶ</expan>
                        τοῦ ὃν ἔχει ὁ Θ κῶνος <expan>πρὸς</expan> τὸν</seg>
                    <seg n="12" type="line" facs="z39v2_12">τομέα ὥστε καὶ ὁ τομεὺς
                        <expan>πρὸς</expan> τὸν</seg>
                    <seg n="13" type="line" facs="z39v2_13">κῶνον ἐλάσσονα <choice>
                            <abbr>λογο</abbr>
                            <expan>λόγον</expan>
                        </choice> ἔχει <choice>
                            <abbr>η</abbr>
                            <expan>ἤπερ</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z39v2_14">τὸ ἐγγεγραμμένον στερεὸν ἐν <choice>
                            <abbr>τ</abbr>
                            <expan>τῶι</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z39v2_15">τμήματι πρὸς τὸ <w part="I">περιγε</w></seg>
                    <seg n="16" type="line" facs="z39v2_16"><w part="F">γραμμένον</w>. μείζων δέ
                            <expan>ἐστιν</expan> ὁ <w part="I">το</w></seg>
                    <seg n="17" type="line" facs="z39v2_17"><w part="F">μεὺς</w> τοῦ ἐγγεγραμμένου
                        εἰς <w part="I">αὐ</w></seg>
                    <seg n="18" type="line" facs="z39v2_18"><w part="F">τὸν</w> σχήματος· μείζων
                            <expan>ἄρα</expan> ὁ Θ <w part="I">κῶ</w></seg>
                    <seg n="19" type="line" facs="z39v2_19"><w part="F">νος</w> τοῦ περιγεγραμμένου
                            <sic>σχη</sic>
                    </seg>
                    <gap/>
                </seg>
            </ab>
        </body>
    </text>
