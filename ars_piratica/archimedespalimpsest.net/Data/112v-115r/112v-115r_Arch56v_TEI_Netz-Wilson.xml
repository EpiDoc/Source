    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="112v1" type="folio">
                    <seg n="1" type="line" facs="z112v1_01">καὶ ἐπεὶ <sic>εδει</sic>
                        <expan>ὅτι</expan>
                        <expan>ἐστίν</expan>, ὡς ἡ ἴση πάσαις</seg>
                    <seg n="2" type="line" facs="z112v1_02">ταῖς ΕΙ, ΖΚ, ΒΔ, ΗΛ, ΘΜ
                        <expan>πρὸς</expan> τὴν <w part="I">διά</w></seg>
                    <seg n="3" type="line" facs="z112v1_03"><w part="F">μετρον</w> τοῦ <seg
                            type="word">κύ<unclear>κ</unclear>λου</seg> τὴν ΑΓ, οὕτως ἡ</seg>
                    <seg n="4" type="line" facs="z112v1_04">ΓΕ <expan>πρὸς</expan> ΕΑ, <seg
                            type="word"><unclear>τ</unclear>ὸ</seg> ἄρα ὑπὸ τῆς ἴσης <choice>
                            <abbr>πασς</abbr>
                            <expan>πάσαις</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z112v1_05"><choice>
                            <abbr>τς</abbr>
                            <expan>ταῖς</expan>
                        </choice> εἰρημέναις καὶ τῆς ΕΑ, <choice>
                            <abbr>τουτ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z112v1_06">τὸ ἀπὸ τῆς ἐκ τοῦ κέντρου τοῦ Ρ <w
                            part="I">κύ</w></seg>
                    <seg n="7" type="line" facs="z112v1_07"><w part="F">κλου</w>, ἴσον ἐστὶν τὸ ὑπὸ
                        τῶν <seg type="word">Α<supplied reason="lost">Γ</supplied></seg>, ΓΕ.</seg>
                    <seg n="8" type="line" facs="z112v1_08">ἀλλὰ καὶ τὸ ὑπὸ ΑΓ, ΓΕ ἔλασσόν
                            <expan>ἐστιν</expan></seg>
                    <seg n="9" type="line" facs="z112v1_09">τοῦ ἀπὸ τῆς ΑΓ· ἔλασσον ἄρα <choice>
                            <abbr>εστι</abbr>
                            <expan>ἐστὶν</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z112v1_10">τὸ ἀπὸ τῆς ἐκ τοῦ κέντρου τοῦ Ρ</seg>
                    <seg n="11" type="line" facs="z112v1_11">τοῦ ἀπὸ τῆς ΑΓ ἐλάσσων ἄρα
                        <expan>ἐστὶν</expan></seg>
                    <seg n="12" type="line" facs="z112v1_12">ἡ ἐκ τοῦ κέντρου τοῦ Ρ τῆς ΑΓ· <w
                            part="I">ὥσ</w></seg>
                    <seg n="13" type="line" facs="z112v1_13"><w part="F">τε</w> ἡ διάμετρος τοῦ
                            <unclear>Ο</unclear> κύκλου <w part="I">ἐλάσ</w></seg>
                    <seg n="14" type="line" facs="z112v1_14"><w part="F">σων</w> ἐστὶν ἢ διπλασία
                        τῆς <w part="I">δια</w></seg>
                    <seg n="15" type="line" facs="z112v1_15"><w part="F">μέτρου</w> τοῦ ΑΒ ΓΔ
                        κύκλου, <expan>καὶ</expan> δύο <expan>ἄρα</expan></seg>
                    <seg n="16" type="line" facs="z112v1_16">τοῦ ΑΒΓΔ κύκλου διάμετροι <choice>
                            <abbr>μειζ</abbr>
                            <expan>μείζους</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z112v1_17"><expan>εἰσὶ</expan> τῆς διαμέτρου τοῦ Ρ
                        κύκλου, <expan>καὶ</expan> τὸ</seg>
                    <seg n="18" type="line" facs="z112v1_18"><seg type="word">τετρά<supplied
                                reason="lost">κι</supplied>ς</seg> ἀπὸ τῆς διαμέτρου τοῦ</seg>
                </seg>
                <seg n="115r1" type="folio">
                    <seg n="1" type="line" facs="z115r1_01"><seg type="word">ΑΒ<supplied
                                reason="lost">Γ</supplied>Δ</seg> κύκλου, <choice>
                            <abbr>το<unclear>υ</unclear>τ</abbr>
                            <expan>τουτέστι</expan>
                        </choice> τῆς ΑΓ, μεῖζόν</seg>
                    <seg n="2" type="line" facs="z115r1_02"><expan>ἐστι</expan>
                        <seg type="word">τ<supplied reason="lost">ο</supplied>ῦ</seg> ἀπὸ τῆς τοῦ Ρ
                        κύκλου <w part="I">διαμέ</w></seg>
                    <seg n="3" type="line" facs="z115r1_03"><w part="F">τρου</w>. ὡς δὲ τὸ τετράκις
                        ἀπὸ τῆς ΑΓ</seg>
                    <seg n="4" type="line" facs="z115r1_04"><expan>πρὸς</expan> τὸ ἀπὸ τῆς τοῦ Ρ
                        κύκλου <choice>
                            <abbr>μετρ</abbr>
                            <expan>διαμέτρου</expan>
                        </choice>,</seg>
                    <seg n="5" type="line" facs="z115r1_05">οὕτως τέσσαρες κύκλοι οἱ ΑΒ ΓΔ</seg>
                    <seg n="6" type="line" facs="z115r1_06"><expan>
                            <unclear>πρὸς</unclear>
                        </expan>
                        <seg type="word">
                            <unclear>τὸ</unclear>
                            <supplied reason="lost">ν</supplied>
                        </seg>
                        <supplied reason="lost">Ρ</supplied>
                        <seg type="word"><supplied reason="lost">κύκ</supplied>λον</seg>· τέσσαρες
                            <expan>ἄρα</expan>
                        <seg type="word">κύκ<unclear>λ</unclear>οι</seg></seg>
                    <seg n="7" type="line" facs="z115r1_07">οἱ ΑΒ ΓΔ μείζους <expan>εἰσὶν</expan>
                        τοῦ Ρ κύκλου· ὁ <expan>ἄρα</expan></seg>
                    <seg n="8" type="line" facs="z115r1_08">κύκλος ὁ Ρ ἐλάσσων <expan>ἐστὶν</expan>
                        ἢ <w part="I">τετραπλά</w></seg>
                    <seg n="9" type="line" facs="z115r1_09"><w part="F">σιος</w> τοῦ μεγίστου
                        κύκλου. ὁ δὲ Ρ <seg type="unclearword">κ<unclear>ύ</unclear></seg></seg>
                    <seg n="10" type="line" facs="z115r1_10"><seg type="wordend">κλος</seg> ἴσος
                        ἐδείχθη τῆι <seg type="word">εἰρημέν<unclear>αι</unclear></seg></seg>
                    <seg n="11" type="line" facs="z115r1_11"><seg type="word"
                            >ἐπιφα<unclear>ν</unclear>εία</seg>
                        <seg type="word"><unclear>τ</unclear>ο<supplied reason="lost"
                        >ῦ</supplied></seg> σχήματος·</seg>
                </seg>
                <seg n="112v2" type="folio">
                    <seg n="1" type="line" facs="z112v2_01">ἐλάσσων <expan>ἐστὶν</expan> ἢ <seg
                            type="word">τε<supplied reason="lost"
                            >τ</supplied>ραπλασ<unclear>ί</unclear>α</seg> τοῦ</seg>
                    <seg n="2" type="line" facs="z112v2_02">μεγίστου κύκλου <seg type="word"
                                ><supplied reason="lost">τ</supplied>ῶν</seg>
                        <unclear>ἐν</unclear>
                        <seg type="word"><unclear>τῆ</unclear>ι</seg>
                        <w part="I">σφαί</w></seg>
                    <seg n="3" type="line" facs="z112v2_03"><w part="F">ραι</w>
                        <choice>
                            <abbr>εξ</abbr>
                            <expan>ἑξ<supplied reason="lost">ῆς</supplied></expan>
                        </choice>
                        <seg type="word">τ<unclear>ὸ</unclear></seg> ΣΧΗΜΑ.</seg>
                </seg>
                <seg n="112v2" type="folio">
                    <seg n="4" type="line" facs="z112v2_04">τῶι ἐγγραφομένωι ἐν τῆι <choice>
                            <abbr>σφαιρ</abbr>
                            <expan>σφαίραι</expan>
                        </choice>
                    </seg>
                    <seg n="5" type="line" facs="z112v2_05">σχήματι τῶι περιεχομένωι ὑπὸ</seg>
                    <seg n="6" type="line" facs="z112v2_06">τῶν ἐπιφανειῶν τῶν κωνικῶν </seg>
                    <seg n="7" type="line" facs="z112v2_07">ἴσος <expan>ἐστὶν</expan> κῶνος ὁ βάσιν
                        μὲν ἔχων</seg>
                    <seg n="8" type="line" facs="z112v2_08">τὸν κύκλον τὸν ἴσον τῆι <w part="I"
                            >ἐπιφα</w></seg>
                    <seg n="9" type="line" facs="z112v2_09"><w part="F">νείαι</w> τοῦ <seg
                            type="word">σχ<unclear>ή</unclear>ματος</seg> τοῦ <seg
                            type="suppliedword">ἐγ<supplied reason="lost">γρ</supplied>α</seg></seg>
                    <seg n="10" type="line" facs="z112v2_10"><seg type="wordend">φέντος</seg> ἐν
                            <seg type="word">τῆ<supplied reason="lost">ι</supplied></seg>
                        <unclear>σφαίραι</unclear>, ὕψος δὲ <choice>
                            <abbr>ισ</abbr>
                            <expan>ἴσον</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z112v2_11">τῆι ἀπὸ τοῦ <seg type="word"
                                >κέντρο<supplied reason="lost">υ</supplied></seg> τῆς <choice>
                            <abbr>σφαιρ</abbr>
                            <expan>σφαίρας</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z112v2_12">ἐπὶ μίαν πλευρὰν <seg type="word"
                                >το<supplied reason="lost">υ</supplied>͂</seg>
                        <choice>
                            <abbr><unclear>π</unclear><supplied reason="lost">ολυ</supplied>γων</abbr>
                            <expan>πολυγώνου</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z112v2_13">καθέτωι ἠγμένη.</seg>
                </seg>
                <seg n="112v2" type="folio">
                    <seg n="13" type="line" facs="z112v2_13">ἔστω ἡ <seg type="word"><supplied
                                reason="lost">σ</supplied><unclear>φ</unclear>αῖρα</seg></seg>
                    <seg n="14" type="line" facs="z112v2_14">καὶ ὁ ἐν αὐτῆι <seg type="word"
                                >μέγι<supplied reason="lost">σ</supplied>τος</seg>
                        <seg type="word"><supplied reason="lost">κύ</supplied>κλος</seg> ὁ ΑΒ</seg>
                    <seg n="15" type="line" facs="z112v2_15">Γ<unclear>Δ</unclear>
                        <expan>καὶ</expan> τὰ ἄλλα τὰ αὐτὰ <supplied reason="lost">τῶ</supplied>
                        <seg type="suppliedword"><supplied reason="lost"
                            >π</supplied><unclear>ρ</unclear>ότε</seg></seg>
                    <seg n="16" type="line" facs="z112v2_16"><seg type="wordend">
                            <supplied reason="lost">ρον</supplied>
                        </seg>, ἔστω δὲ κῶνος <seg type="word"><supplied reason="lost"
                                >ὀ</supplied>ρ<unclear>θ</unclear>ὸς</seg> ὁ Ρ <seg
                            type="unclearword"><unclear>β</unclear>ά</seg></seg>
                    <seg n="17" type="line" facs="z112v2_17"><seg type="wordend">σιν</seg> μὲν ἔχων
                        τῆι ἐπιφανείαι τοῦ </seg>
                    <seg n="18" type="line" facs="z112v2_18">σχήματος τοῦ ἐγγεγραμμένου</seg>
                    <seg n="19" type="line" facs="z112v2_19">ἐν <seg type="word"
                            ><unclear>τ</unclear>ῆι</seg>
                        <seg type="word"><supplied reason="lost">σφ</supplied>αίραι</seg>, <seg
                            type="word">ὕ<supplied reason="lost">ψος</supplied></seg>
                        <supplied reason="lost">δὲ ἴσον τῆι</supplied>
                    </seg>
                </seg>
                <seg n="115r2" type="folio">
                    <seg n="1" type="line" facs="z115r2_01">ἀπὸ τοῦ κέντρου τῆς σφαίρας</seg>
                    <seg n="2" type="line" facs="z115r2_02">ἐπὶ μίαν πλευρὰν τοῦ <choice>
                            <abbr>πολυγων</abbr>
                            <expan>πολυγώνου</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z115r2_03">καθέτωι ἠγμένη· δεικτέον
                        <expan>ὅτι</expan> ὁ <w part="I">κῶ</w></seg>
                    <seg n="4" type="line" facs="z115r2_04"><w part="F">νος</w> ὁ Ρ ἴσος
                            <expan>ἐστὶν</expan> τῶι <seg type="word">ἐγγεγρ<supplied reason="lost"
                                >α</supplied>μ<supplied reason="lost">μ</supplied>ένωι</seg></seg>
                    <seg n="5" type="line" facs="z115r2_05">ἐν τῆι σφαίραι σχήματι.</seg>
                </seg>
                <seg n="115r2" type="folio">
                    <seg n="5" type="line" facs="z115r2_05">ἀπὸ <expan>γὰρ</expan></seg>
                    <seg n="6" type="line" facs="z115r2_06">τῶν κύκλων, ὧν <expan>εἰσι</expan>
                        <seg type="word">διάμετρο<supplied reason="lost">ι</supplied></seg> αἱ ΖΝ</seg>
                    <seg n="7" type="line" facs="z115r2_07">ΗΜ ΘΛ ΙΚ, κῶνοι <w part="I"
                        >ἀναγεγράφθω</w></seg>
                    <seg n="8" type="line" facs="z115r2_08"><w part="F">σαν</w> κορυφὴν ἔχοντες τὸ
                        τῆς <seg type="unclearword"><unclear>σ</unclear>φαί</seg></seg>
                    <seg n="9" type="line" facs="z115r2_09"><seg type="wordend">ρας</seg> κέντρον·
                        ἔσται δὴ ῥόμβος</seg>
                    <seg n="10" type="line" facs="z115r2_10">στερεὸς ἔκ τε τοῦ <seg type="word"
                                >κώνο<unclear>υ</unclear></seg>, οὗ βάσις <choice>
                            <abbr>μ</abbr>
                            <expan>μέν</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z115r2_11">ἔστω ὁ κύκλος ὁ περὶ τὴν ΖΝ,</seg>
                    <seg n="12" type="line" facs="z115r2_12"><seg type="word"
                            >κ<unclear>ο</unclear>ρυφὴ</seg> δὲ τὸ Α σημεῖον, καὶ τοῦ</seg>
                    <seg n="13" type="line" facs="z115r2_13"><seg type="word"
                            ><unclear>κώ</unclear>νου</seg>, οὗ βάσις ὁ αὐτὸς <choice>
                            <abbr>κυκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice>,</seg>
                    <seg n="14" type="line" facs="z115r2_14">κορυφὴ δὲ τὸ Χ σημεῖον· ἴσος <choice>
                            <abbr>εστι</abbr>
                            <expan>ἐστὶν</expan>
                        </choice>
                    </seg>
                    <seg n="15" type="line" facs="z115r2_15">τῶι <seg type="word"
                            >κω<unclear>́ν</unclear>ωι</seg> τῶι βάσιν μὲν <seg type="word"
                                >ἔ<unclear>χ</unclear>ον</seg></seg>
                    <seg n="16" type="line" facs="z115r2_16">τι τὴν ἐπιφάνειαν τοῦ
                        ΝΑ<unclear>Ζ</unclear>, <choice>
                            <abbr>υψ</abbr>
                            <expan>ὕψος</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z115r2_17">δὲ ἴσον τῆι ἀπὸ τοῦ Χ καθέτωι</seg>
                </seg>
            </ab>
        </body>
    </text>
