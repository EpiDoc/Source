    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="38r1" type="folio">
                    <seg n="1" type="line" facs="z38r1_01">ἐὰν περὶ κῶνον ἰσοσκελῆ <seg
                            type="suppliedword">πυ</seg></seg>
                    <seg n="2" type="line" facs="z38r1_02"><seg type="wordend">ραμ<supplied
                                reason="lost">ὶς</supplied></seg> περιγραφῆι, ἡ <choice>
                            <abbr>επιφανει</abbr>
                            <expan>ἐπιφάνεια</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z38r1_03">τῆς πυραμίδος χωρὶς τῆς <w part="I"
                        >βά</w></seg>
                    <seg n="4" type="line" facs="z38r1_04"><w part="F">σεως</w> μείζων ἐστὶν τῆς <w
                            part="I">ἐπιφα</w></seg>
                    <seg n="5" type="line" facs="z38r1_05"><w part="F">νεία</w> τοῦ κώνου χωρὶς τῆς
                            <w part="I">βά</w></seg>
                    <seg n="6" type="line" facs="z38r1_06"><w part="F">σεως</w> κατὰ τὸ συνεχὲς
                        ἐκείνωι. </seg>
                </seg>
                <seg n="38r1" type="folio">
                    <seg n="7" type="line" facs="z38r1_07">φανερὸν δὲ ἐκ τῶν <w part="I"
                        >ἀποδεδειγ</w></seg>
                    <seg n="8" type="line" facs="z38r1_08"><w part="F">μένων</w>
                        <expan>ὅτι</expan> τε, ἐὰν εἰς κύλινδρον</seg>
                    <seg n="9" type="line" facs="z38r1_09">ὀρθὸν πρίσμα ἐγγραφῆι, ἡ <w part="I"
                        >ἐπι</w></seg>
                    <seg n="10" type="line" facs="z38r1_10"><w part="F">φάνεια</w> τοῦ πρίσματος ἡ
                        ἐκ <choice>
                            <abbr>τω</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z38r1_11">παραλληλογράμμων <w part="I"
                        >συγκειμέ</w></seg>
                    <seg n="12" type="line" facs="z38r1_12"><w part="F">νη</w> ἐλάσσων
                        <expan>ἐστὶ</expan> τῆς ἐπιφανείας</seg>
                    <seg n="13" type="line" facs="z38r1_13">τοῦ κυλίνδρου χωρὶς τῆς <w part="I"
                        >βάσε</w></seg>
                    <seg n="14" type="line" facs="z38r1_14"><w part="F">ως</w> ἔλασσον γὰρ ἕκαστον
                            <w part="I">παραλ</w></seg>
                    <seg n="15" type="line" facs="z38r1_15"><w part="F">ληλόγραμμον</w> τοῦ
                        πρίσματός <expan>ἐστι</expan></seg>
                    <seg n="16" type="line" facs="z38r1_16">τῆς καθ᾽ αὑτὸ τοῦ κυλίνδρου <w part="I"
                            >ἐ</w></seg>
                    <seg n="17" type="line" facs="z38r1_17"><w part="F">πιφανείας</w>, <expan>καὶ</expan>
                        <expan>ὅτι</expan>, ἐὰν <seg type="word">πε<supplied reason="lost"
                            >ρὶ</supplied></seg>
                        <seg type="suppliedword"><supplied reason="lost">κ</supplied>ύλ<supplied
                                reason="lost">ι</supplied>ν</seg></seg>
                    <seg n="18" type="line" facs="z38r1_18"><seg type="wordend">
                            <unclear>δρο</unclear>
                            <supplied reason="lost">ν</supplied>
                        </seg> ὀρθὸν πρίσμα <choice>
                            <abbr>γραφη</abbr>
                            <expan>περιγραφῆ</expan>
                        </choice>,</seg>
                </seg>
                <seg n="35v1" type="folio">
                    <seg n="1" type="line" facs="z35v1_01">ἡ ἐπιφάνεια τοῦ πρίσματος</seg>
                    <seg n="2" type="line" facs="z35v1_02">ἡ ἐκ τῶν <choice>
                            <abbr>παραλληλογραμμω</abbr>
                            <expan>παραλληλογράμμων</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z35v1_03">συγκειμένη μείζων ἐστὶ τῆς</seg>
                    <seg n="4" type="line" facs="z35v1_04">ἐπιφανείας τοῦ κυλίνδρου</seg>
                    <seg n="5" type="line" facs="z35v1_05">χωρὶς τῆς βάσεως.</seg>
                </seg>
                <seg n="35v1" type="folio">
                    <seg n="6" type="line" facs="z35v1_06">παντὸς κυλίνδρου ὀρθοῦ ἡ <w part="I"
                        >ἐπι</w></seg>
                    <seg n="7" type="line" facs="z35v1_07"><w part="F">φάνεια</w> χωρὶς τῆς βάσεως
                            <w part="I">ἴ</w></seg>
                    <seg n="8" type="line" facs="z35v1_08"><w part="F">ση</w>
                        <expan>ἐστὶ</expan> κύκλωι, οὗ ἡ ἐκ τοῦ <choice>
                            <abbr>κέντρ</abbr>
                            <expan>κέντρου</expan>
                        </choice></seg>
                </seg>
                <seg n="38r2" type="folio">
                    <seg n="1" type="line" facs="z38r2_01">μέσον λόγον <seg type="word">ἔ<supplied
                                reason="lost">χ</supplied>ει</seg>
                        <seg type="word">τ<unclear>ῆ</unclear>ς</seg>
                        <supplied reason="lost">πλευρᾶς τοῦ</supplied></seg>
                    <seg n="2" type="line" facs="z38r2_02">κυλίνδρου καὶ τῆς διαμέτρου τῆς</seg>
                    <seg n="3" type="line" facs="z38r2_03">βάσεως τοῦ κυλίνδρου. ἔστω</seg>
                    <seg n="4" type="line" facs="z38r2_04">κυλίνδρου τινὸς ὀρθοῦ βάσις ὁ <supplied
                            reason="lost">Α</supplied></seg>
                    <seg n="5" type="line" facs="z38r2_05">κύκλος, καὶ ἔστω τῆι μὲν <w part="I"
                            >διαμέ</w></seg>
                    <seg n="6" type="line" facs="z38r2_06"><w part="F">τρωι</w> τοῦ Α κύκλου ἴση ἡ
                        ΓΔ, τῆι δὲ</seg>
                    <seg n="7" type="line" facs="z38r2_07">πλευρᾶι τοῦ κυλίνδρου ἡ ΕΖ, ἐχέτω</seg>
                    <seg n="8" type="line" facs="z38r2_08">δὲ μέσον λόγον τῶν ΔΓ, ΕΖ ἡ Η, καὶ</seg>
                    <seg n="9" type="line" facs="z38r2_09">κείσθω κύκλος, οὗ ἡ ἐκ τοῦ κέντρου ἴση</seg>
                    <seg n="10" type="line" facs="z38r2_10">
                        <expan>ἐστὶ</expan> τῆι Η, ὁ Β· δεικτέον <expan>ὅτι</expan> ὁ Β κύκλος</seg>
                    <seg n="11" type="line" facs="z38r2_11">ἴσος <expan>ἐστὶ</expan> τῆι ἐπιφανείαι
                        τοῦ <w part="I">κυλίν</w></seg>
                    <seg n="12" type="line" facs="z38r2_12"><w part="F">δρου</w> χωρὶς τῆς βάσεως.
                        εἰ γὰρ μή</seg>
                    <seg n="13" type="line" facs="z38r2_13">
                        <expan>ἐστιν</expan> ἴσος, ἤτοι μείζων <expan>ἐστὶ</expan> ἢ <choice>
                            <abbr>ἐλάσσω</abbr>
                            <expan>ἐλάσσων</expan>
                        </choice>.</seg>
                    <seg n="14" type="line" facs="z38r2_14">ἔστω πρότερον, εἰ δυνατόν, <choice>
                            <abbr>ἐλάσσω</abbr>
                            <expan>ἐλάσσων</expan>
                        </choice>.</seg>
                    <seg n="15" type="line" facs="z38r2_15">δύο δὴ μεγεθῶν ὄντων ἀνίσων</seg>
                    <seg n="16" type="line" facs="z38r2_16">τῆς τε ἐπιφανείας τοῦ <choice>
                            <abbr>κυλίνδρ</abbr>
                            <expan>κυλίνδρου</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z38r2_17">καὶ τοῦ Β κύκλου δυνατόν ἐστιν εἰς</seg>
                    <seg n="18" type="line" facs="z38r2_18">τὸν Β κύκλον ἰσόπλευρον <w part="I"
                            >πολύγω</w></seg>
                </seg>
                <seg n="35v2" type="folio">
                    <seg n="1" type="line" facs="z35v2_01"><w part="F">νον</w> ἐγγράψαι καὶ ἄλλο
                            <seg type="unclearword">περι<unclear>γρά</unclear></seg></seg>
                    <seg n="2" type="line" facs="z35v2_02"><seg type="wordend">ψαι</seg>, ὥστε τὸ
                        περιγραφὲν <expan>πρὸς</expan> τὸ <w part="I">ἐγ</w></seg>
                    <seg n="3" type="line" facs="z35v2_03"><w part="F">γραφὲν</w> ἐλάσσονα λόγον
                        ἔχει ἡ</seg>
                    <seg n="4" type="line" facs="z35v2_04">ἐπιφάνεια τοῦ κυλίνδρου
                        <expan>πρὸς</expan> τὸν</seg>
                    <seg n="5" type="line" facs="z35v2_05">Β κύκλον.</seg>
                </seg>
                <seg n="35v2" type="folio">
                    <seg n="5" type="line" facs="z35v2_05">νοείσθω δὴ <w part="I">περιγεγραμ</w></seg>
                    <seg n="6" type="line" facs="z35v2_06"><w part="F">μένον</w> καὶ ἐγγεγραμμένον,
                        καὶ <expan>περὶ</expan></seg>
                    <seg n="7" type="line" facs="z35v2_07">τὸν Α κύκλον περιγεγράφθω <w part="I"
                            >εὐθύ</w></seg>
                    <seg n="8" type="line" facs="z35v2_08"><w part="F">γραμμον</w> ὅμοιον τῶι περὶ
                        τὸν Β</seg>
                    <seg n="9" type="line" facs="z35v2_09">περιγεγραμμένω, καὶ <w part="I"
                        >ἀναγεγρά</w></seg>
                    <seg n="10" type="line" facs="z35v2_10"><w part="F">φθω</w> ἀπὸ τοῦ εὐθυγράμμου
                            <w part="I">πρίσ</w></seg>
                    <seg n="11" type="line" facs="z35v2_11"><w part="F">μα</w>· ἔσται δὴ περὶ τὸν
                        κύλινδρον</seg>
                    <seg n="12" type="line" facs="z35v2_12">περιγεγραμμένον. ἔστω δὲ καὶ τῆι</seg>
                    <seg n="13" type="line" facs="z35v2_13">περιμέτρω τοῦ εὐθυγράμμου τοῦ
                            <expan>περὶ</expan></seg>
                    <seg n="14" type="line" facs="z35v2_14">τὸν Α κύκλον ἴση ἡ ΚΔ καὶ τῆι ΚΔ</seg>
                    <seg n="15" type="line" facs="z35v2_15">ἴση ἡ ΛΞ, τῆς δὲ ΓΔ ἡμίσεια ἔστω ἡ</seg>
                    <seg n="16" type="line" facs="z35v2_16">ΓΔΤ· ἔσται δὴ τὸ ΚΔΤ τρίγωνον <seg
                            type="word">
                            <choice>
                                <abbr><supplied reason="lost">ἴσ</supplied>ο</abbr>
                                <expan><supplied reason="lost">ἴσ</supplied>ον</expan>
                            </choice>
                        </seg></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
