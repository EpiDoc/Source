    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="145v-144r" type="folio">
                    <seg n="01" type="line" facs="z145v-144r_01">πεπολιτευμένοις εἰ μηδὲ τῶν χρόνων
                        ἔστιν <seg type="word">
                            <choice>
                                <abbr>ἐκείνω</abbr>
                                <expan>ἐκείνων</expan>
                            </choice>
                        </seg></seg>
                    <seg n="02" type="line" facs="z145v-144r_02">μνησθῆναι. ἐγὼ δ’ ἐβουλόμη<supplied
                            reason="lost">ν</supplied> ἄν, ὦ ἄνδρες <w part="I">δικα</w></seg>
                    <seg n="03" type="line" facs="z145v-144r_03"><w part="F">σταί</w>, ὥσπερ
                        χρήσιμός ἐστιν ὁ καιρός, οὕτως ῥάιδιον</seg>
                    <seg n="04" type="line" facs="z145v-144r_04">αὐτὸν ὑμῖν <choice>
                            <sic>συνειδεῖν</sic>
                            <corr>συνιδεῖν</corr>
                        </choice> εἶναι, καὶ ἡγοῦμαι <seg type="word">
                            <choice>
                                <abbr>προσήκει</abbr>
                                <expan>προσήκειν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="05" type="line" facs="z145v-144r_05">τοὐναντίον ἢ Διώνδας, ὃς λέγει
                        ὀργὴν ἄκαιρον</seg>
                    <seg n="06" type="line" facs="z145v-144r_06"><seg type="word">
                            <choice>
                                <abbr>κ</abbr>
                                <expan>καὶ</expan>
                            </choice>
                        </seg>
                        <unclear>τ</unclear>οὺς κρινομένους ἀναιρεῖν, οὐκ ἀπολογίας, <w part="I"
                        >κα</w></seg>
                    <seg n="07" type="line" facs="z145v-144r_07"><w part="F">τηγορεῖν</w> εἰς καιρὸν
                        ἀναφερόμενος <seg type="word">
                            <choice>
                                <abbr>ἐγνωσμέν</abbr>
                                <expan><unclear>ἐγν</unclear>ωσμέν<unclear>ον</unclear></expan>
                            </choice>
                        </seg>.</seg>
                    <seg n="08" type="line" facs="z145v-144r_08"><unclear>ν</unclear>ῦν εἰκότως
                        Διώνδας τὸν καιρὸν ἀποδοκιμάζει</seg>
                    <seg n="09" type="line" facs="z145v-144r_09">οὐδὲν <seg type="word">
                            <choice>
                                <abbr>αὐτὸ</abbr>
                                <expan>αὐτὸς</expan>
                            </choice>
                        </seg> ἐν καιρῶι πράττων, <unclear>ὅσπερ</unclear> ἐμ<unclear>ο</unclear>ῦ
                            <w part="I">κατη</w></seg>
                    <seg n="10" type="line" facs="z145v-144r_10"><w part="F">γορεῖ</w> ὡς οὐκ ἴση ἡ
                            <seg type="word">
                            <choice>
                                <abbr>συμμαχί</abbr>
                                <expan>συμμαχία</expan>
                            </choice>
                        </seg> ἐγένετο, <unclear>καὶ</unclear> ἡμεῖς <w part="I">Θηβαί</w></seg>
                    <seg n="11" type="line" facs="z145v-144r_11"><w part="F">ων</w> διπλάσια
                        συνεβαλόμεθα εἰς τὸν πόλεμον καὶ <seg type="clearword">χρή</seg></seg>
                    <seg n="12" type="line" facs="z145v-144r_12"><seg type="wordend">
                            <choice>
                                <abbr>μα</abbr>
                                <expan>ματα</expan>
                            </choice>
                        </seg> καὶ ἵππους καὶ <seg type="word">
                            <choice>
                                <abbr>στρατιώτ</abbr>
                                <expan>στρατιώτας</expan>
                            </choice>
                        </seg>. εἰς δὲ τὴν ἐν <w part="I">Σαλα</w></seg>
                    <seg n="13" type="line" facs="z145v-144r_13"><w part="F">μῖνι</w>
                        <seg type="word">
                            <choice>
                                <abbr>ναυμαχία</abbr>
                                <expan>ναυμαχίαν</expan>
                            </choice>
                        </seg>, ὦ Διώνδα, <seg type="word">
                            <choice>
                                <abbr>ἑξήκον</abbr>
                                <expan>ἑξήκοντα</expan>
                            </choice>
                        </seg> καὶ <w part="I">τριακο</w></seg>
                    <seg n="14" type="line" facs="z145v-144r_14"><w part="F">σίων</w>
                        <seg type="word">
                            <choice>
                                <abbr>οὐσῶ</abbr>
                                <expan>οὐσῶν</expan>
                            </choice>
                        </seg> τῶν τριήρων <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τῶν</expan>
                            </choice>
                        </seg> Ἑλληνίδων, τούτων</seg>
                    <seg n="15" type="line" facs="z145v-144r_15">ἡ πόλις ἡ ἡμετέρα διακοσίας καὶ
                        εἴκοσι <w part="I">πα</w></seg>
                    <seg n="16" type="line" facs="z145v-144r_16"><w part="F">ρέσχεν</w> καὶ <seg
                            type="word">
                            <choice>
                                <abbr>τἀναλώμα</abbr>
                                <expan>τἀναλώματα</expan>
                            </choice>
                        </seg> εἰς ταύτας, αἱ δ’ ἄλλαι</seg>
                    <seg n="17" type="line" facs="z145v-144r_17">πόλεις
                            <unclear>σύμπα</unclear>σ<unclear>αι</unclear>
                        <seg type="word">
                            <choice>
                                <abbr>τετταράκον</abbr>
                                <expan>τετταράκοντα</expan>
                            </choice>
                        </seg> καὶ ἑκατόν. ἐν <w part="I">Μαρα</w></seg>
                    <seg n="18" type="line" facs="z145v-144r_18"><w part="F">θῶνι</w> δ’ οἱ πρόγονοι
                        οἱ ἡμέτεροι αὐτοὶ μόνοι <w part="I">ὑ</w></seg>
                    <seg n="19" type="line" facs="z145v-144r_19"><w part="F">πὲρ</w> ἁπάντων τῶν
                        Ἑλλήνων ἐμαχέσαντο· τῆς</seg>
                    <seg n="20" type="line" facs="z145v-144r_20">δ’ ἐπ’ Ἀρτεμισίωι στρατείας οὐδὲ τὸ
                        πέμπτον <w part="I">μέ</w></seg>
                    <seg n="21" type="line" facs="z145v-144r_21"><w part="F">ρος</w> τῶν τριήρων οἱ
                            ἄ<unclear>λλοι</unclear>
                        <unclear>Ἕ</unclear>λ<unclear>λ</unclear>ηνες <w part="I">συνεβάλον</w></seg>
                    <seg n="22" type="line" facs="z145v-144r_22"><w part="F">το</w>. τοῦτο
                            <unclear>γὰρ</unclear> π<unclear>ρ</unclear>ὸς τοῖς ἄλλ<supplied
                            reason="lost">οις</supplied> ἡ πόλις ἡμῶν ἔχει</seg>
                    <seg n="23" type="line" facs="z145v-144r_23">ἀγαθο<unclear>ῖ</unclear>ς·
                            <unclear>οὐ</unclear>
                        <choice>
                            <sic>φιλονεικεῖ</sic>
                            <corr>φιλονικεῖ</corr>
                        </choice>
                        <unclear>ἐν</unclear>
                        <unclear>τ</unclear><supplied reason="lost">οῖ</supplied><unclear>ς</unclear>
                        <unclear>τοι</unclear>ούτοις καιροῖς,</seg>
                    <seg n="24" type="line" facs="z145v-144r_24">ὅπως ταὐτὰ ἕκάστ<unclear>οι</unclear>
                        <unclear>συμβαλοῦνται</unclear>, ἀλλὰ τὰ</seg>
                    <seg n="25" type="line" facs="z145v-144r_25">μὲν διδόμενα
                        π<unclear>α</unclear>ρ’ ἑκάστω<unclear>ν</unclear>
                        <supplied reason="lost">λαβο</supplied>ῦσα καὶ τοῦ ὅλου</seg>
                    <seg n="26" type="line" facs="z145v-144r_26">ἡγεμὼν γενομένη πο<unclear>λέμου</unclear>
                        <unclear>δέ</unclear>δωκε τὰ <seg type="unclearword">ὑπάρ</seg></seg>
                    <seg n="27" type="line" facs="z145v-144r_27"><seg type="wordend">
                            <choice>
                                <abbr>χον</abbr>
                                <expan>χον<unclear>τα</unclear></expan>
                            </choice>
                        </seg> αὐτῆι <seg type="word">
                            <choice>
                                <abbr>πάν</abbr>
                                <expan>πάντα</expan>
                            </choice>
                        </seg> εἰς τὴν κοιν<unclear>ὴν</unclear> τῶν Ἑλλήνων <w part="I">σω</w></seg>
                    <seg n="28" type="line" facs="z145v-144r_28"><w part="F">τηρίαν</w>. <supplied
                            reason="lost">τ</supplied><unclear>οιγαρ</unclear><supplied
                            reason="lost">ο</supplied><unclear>ῦν</unclear> ὅ<unclear>ταν
                            </unclear><gap extent="8"/>
                        <choice>
                            <sic>κατορθωθῆ</sic>
                            <corr>κατορθωθῆι</corr>
                        </choice></seg>
                    <seg n="29" type="line" facs="z145v-144r_29"><unclear>τ</unclear><gap extent="8"/>
                        <unclear>δόξας</unclear>
                        <unclear>ἐ</unclear>ξ αὐτῶν μεγίστ<unclear>α</unclear>ς</seg>
                    <seg n="30" type="line" facs="z145v-144r_30">εἴλ<unclear>η</unclear>φεν, καὶ τὴν
                            ἡγεμονί<unclear>α</unclear>ν τότε ἤδη παρ’ <w part="I">ἑ</w></seg>
                    <seg n="31" type="line" facs="z145v-144r_31"><w part="F">κόντων</w> τῶν Ἑλλήνων
                        εἰκότως ἐλά<unclear>βο</unclear>μεν.</seg>
                    <seg n="32" type="line" facs="z145v-144r_32">Διώνδας δὲ νῦν μὲν οὐκ
                            <unclear>ἀ</unclear>γ<unclear>α</unclear>ν<unclear>ακ</unclear>τεῖ εἰ
                            ὑπὲ<unclear>ρ</unclear>
                        <unclear>τ</unclear>οῦ</seg>
                </seg>
            </ab>
        </body>
    </text>
