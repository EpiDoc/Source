            <ab>
                <seg n="59r1" type="folio">
                    <seg n="1" type="line" facs="z59r1_01">ΞΓ <expan>πρὸς</expan>
                        <supplied reason="lost">Κ</supplied>Β, καὶ λοιπὰ ἁ ΙΓ ποτὶ</seg>
                    <seg n="2" type="line" facs="z59r1_02"><seg type="word">
                            <unclear>λοι</unclear>πὰν</seg> τὰν ΒΕ <supplied reason="lost">
                            <expan>ἐστὶν</expan>
                        </supplied>
                        <seg type="word">
                            <supplied reason="lost">ὡ</supplied>ς</seg> ἁ ΞΓ ποτὶ ΓΚ.</seg>
                    <seg n="3" type="line" facs="z59r1_03"><supplied reason="lost">ὃν</supplied>
                        <supplied reason="lost">δὲ</supplied>
                        <seg type="word">
                            <unclear>λόγ</unclear>ον</seg>
                        <seg type="word">ἔ<unclear>χ</unclear>ει</seg> ἁ Ξ<supplied reason="lost"
                        >Γ</supplied> ποτὶ ΓΚ, <seg type="clearword">τοῦ</seg></seg>
                    <seg n="4" type="line" facs="z59r1_04"><seg type="wordend"
                            >το<unclear>ν</unclear></seg> ἔχει ἁ Η <supplied reason="lost">ποτὶ</supplied>
                        <supplied reason="lost">Ζ</supplied>· ποτιπέπτωκε</seg>
                    <seg n="5" type="line" facs="z59r1_05"><unclear>δὴ</unclear> ἁ Κ<unclear>Ε</unclear>
                        <supplied reason="lost">ποτὶ</supplied>
                        <seg type="word">τ<supplied reason="lost">ὰ</supplied>ν</seg>
                        <seg type="word">
                            <choice>
                                <abbr>ἐκβεβλημένα</abbr>
                                <expan><unclear>ἐ</unclear>κβεβλημέναν</expan>
                            </choice>
                        </seg>,</seg>
                    <seg n="6" type="line" facs="z59r1_06"><supplied reason="lost">
                            <expan>καὶ</expan>
                        </supplied> ἁ <seg type="word">μετα<supplied reason="lost">ξὺ</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τᾶ</supplied>ς</seg>
                        <seg type="word">
                            <supplied reason="lost">ἐκβ</supplied>εβλημένας</seg></seg>
                    <seg n="7" type="line" facs="z59r1_07"><supplied reason="lost">
                            <expan>καὶ</expan>
                        </supplied> τᾶς <seg type="word">
                            π<unclear>ε</unclear>ρι<unclear>φερ</unclear>ειὰς</seg> ἁ ΒΕ ποτὶ <choice>
                            <abbr>τὰ</abbr>
                            <expan>τὰν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z59r1_08"><seg type="word">
                            <unclear>Γ</unclear>
                            <supplied reason="lost">Ι</supplied>
                        </seg>
                        <supplied reason="lost">τὰν</supplied>
                        <supplied reason="lost">ἀπὸ</supplied>
                        <supplied reason="lost">τᾶς</supplied> ἐπιψαυούσας <seg type="clearword"
                        >ἀ</seg></seg>
                    <seg n="9" type="line" facs="z59r1_09"><seg type="wordend">πολαφθεῖσαν</seg> τὸν
                        αὐτὸν ἔχει <choice>
                            <abbr>λόγ</abbr>
                            <expan>λόγον</expan>
                        </choice>,</seg>
                    <seg n="10" type="line" facs="z59r1_10"><seg type="word">
                            <supplied reason="lost">ὃ</supplied>ν</seg> ἁ <unclear>Ζ</unclear>
                        <seg type="word">
                            <unclear>π</unclear>οτὶ</seg> τὰν Η. <choice>
                            <abbr>ἑξ</abbr>
                            <expan>ἑξῆς</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z59r1_11"><supplied reason="lost">Ἁ</supplied>
                        <seg type="word">
                            <supplied reason="lost">Κ</supplied>ΑΤΑΓΡΑΦΆ</seg>.</seg>
                </seg>
                <seg n="59r1" type="folio">
                    <seg n="12" type="line" facs="z59r1_12">εἴ κα γραμμαὶ ἑξῆς τεθέωντι <seg
                            type="clearword">ὁπο</seg></seg>
                    <seg n="13" type="line" facs="z59r1_13"><seg type="wordend">
                            <supplied reason="lost">σ</supplied>
                            <unclear>αιοῦν</unclear>
                        </seg>
                        <supplied reason="lost">τῶι</supplied>
                        <supplied reason="lost">ἴσωι</supplied> ἀλλαλᾶν <seg type="clearword"
                        >ὑπερ</seg></seg>
                    <seg n="14" type="line" facs="z59r1_14"><seg type="wordend">έχουσα<supplied
                                reason="lost">ι,</supplied></seg>
                        <supplied reason="lost">ἦ</supplied>
                        <seg type="word">
                            <supplied reason="lost">δ</supplied>ὲ</seg> ἁ ὑπεροχὰ ἴσα τᾶι</seg>
                    <seg n="15" type="line" facs="z59r1_15"><seg type="word"
                                >ἐ<unclear>λ</unclear><supplied reason="lost">αχίστα</supplied>
                        </seg>, καὶ ἄλλαι γραμμαὶ</seg>
                    <seg n="16" type="line" facs="z59r1_16"><seg type="word">
                            <supplied reason="lost">τεθέω</supplied>ντι</seg> τῶι μὲν πλήθει ἴσα</seg>
                    <seg n="17" type="line" facs="z59r1_17"><seg type="word">
                            <supplied reason="lost">ταύτα</supplied>ις</seg>, τῶι δὲ μεγέθει ἑκάστα</seg>
                    <seg n="18" type="line" facs="z59r1_18"><supplied reason="lost"
                        >μεγίστα</supplied>, τετράγωνα τὰ ἀπὸ τᾶν</seg>
                    <seg n="19" type="line" facs="z59r1_19"><supplied reason="lost">ἰσᾶν</supplied>
                        τὰ μέγιστα <seg type="clearword">ποτιλαμ<unclear>β</unclear>ά</seg>
                    </seg>
                </seg>
                <seg n="62v1" type="folio">
                    <seg n="1" type="line" facs="z62v1_01"><seg type="wordend">νοντα</seg> τό τε ἀπὸ
                        τᾶς μεγίστας</seg>
                    <seg n="2" type="line" facs="z62v1_02">τετράγωνον καὶ τὸ <seg type="clearword"
                            >περιεχόμε</seg></seg>
                    <seg n="3" type="line" facs="z62v1_03"><seg type="wordend">νον</seg>
                        <seg type="word">
                            <supplied reason="lost">ὑ</supplied>πό</seg> τε τᾶς <seg type="word"
                                >ἐλαχίστ<unclear>α</unclear>ς</seg> καὶ</seg>
                    <seg n="4" type="line" facs="z62v1_04">τᾶς ἴσας πάσαις <supplied reason="lost"
                            >ταῖς</supplied> τῶι <choice>
                            <abbr>ἴσ</abbr>
                            <expan>ἴσω</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z62v1_05"><seg type="word"
                            >ἀλλ<unclear>α</unclear>λᾶν </seg> ὑπερεχούσαις <seg type="clearword">
                            <choice>
                                <abbr>τριπλ</abbr>
                                <expan>τριπλά</expan>
                            </choice>
                        </seg></seg>
                    <seg n="6" type="line" facs="z62v1_06"><seg type="wordend">σια</seg> ἐσσοῦνται
                        τῶν <choice>
                            <abbr>τετραγώνω</abbr>
                            <expan>τετραγώνων</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z62v1_07">πάντων τῶν ἀπὸ τᾶν τῶι <supplied
                            reason="lost">ἴσωι</supplied></seg>
                    <seg n="8" type="line" facs="z62v1_08">ἀλλαλᾶν <seg type="word">
                                ὑπερεχου<unclear>σ</unclear>ᾶ<supplied reason="lost">ν.</supplied>
                        </seg></seg>
                    <figure n="10.1">
                        <figDesc>Figure 10.1</figDesc>
                    </figure>
                </seg>
                <seg n="59r2" type="folio">
                    <seg n="1" type="line" facs="z59r2_01">ἔστωσαν γραμμαὶ ὁποσαιοῦν <seg
                            type="clearword">
                            <supplied reason="lost">ἐφε</supplied>
                        </seg></seg>
                    <seg n="2" type="line" facs="z59r2_02"><seg type="wordend">ξῆς</seg> κείμεναι
                            <seg type="word">τ<supplied reason="lost">ῶι</supplied>
                        </seg>
                        <supplied reason="lost">ἴσωι</supplied>
                        <seg type="word">
                            <supplied reason="lost">ἀ</supplied>λλαλ<supplied reason="lost"
                            >ᾶ</supplied>ν</seg>
                        <seg type="clearword">ὑ</seg></seg>
                    <seg n="3" type="line" facs="z59r2_03"><seg type="wordend">περέχουσαι</seg> αἱ
                        ΑΒ ΓΔ <seg type="word">
                            <unclear>Ε</unclear>
                            <supplied reason="lost">Ζ</supplied>
                        </seg>
                        <supplied reason="lost">Η</supplied>Θ, <unclear>ἁ</unclear> δὲ Θ</seg>
                    <seg n="4" type="line" facs="z59r2_04"><seg type="word">
                            <unclear>ἴ</unclear>σα</seg> ἔστω τῆι <seg type="word"> ὑπερ<supplied
                                reason="lost">ο</supplied><unclear>χ</unclear><supplied
                                reason="lost">ᾶι,</supplied>
                        </seg>
                        <seg type="clearword">
                            <supplied reason="lost">ποτικεί</supplied>
                        </seg></seg>
                    <seg n="5" type="line" facs="z59r2_05"><seg type="wordend">σθω</seg> δὲ ποτὶ
                            <seg type="word">τ<unclear>ὰ</unclear><supplied reason="lost"
                            >ν</supplied>
                        </seg>
                        <supplied reason="lost">Β</supplied>
                        <supplied reason="lost">ἴσα</supplied>
                        <supplied reason="lost">τᾶι</supplied>
                        <supplied reason="lost">Θ</supplied>
                        <supplied reason="lost">ἁ</supplied>
                        <supplied reason="lost">Ι,</supplied>
                        <seg type="clearword">
                            <supplied reason="lost">πο</supplied>
                        </seg></seg>
                    <seg n="6" type="line" facs="z59r2_06"><seg type="wordend">τί</seg> τε δὲ τὰν Γ
