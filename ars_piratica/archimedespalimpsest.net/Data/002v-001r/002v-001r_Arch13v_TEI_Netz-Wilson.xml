                    <seg n="2" type="line" facs="z2v1_02"><supplied reason="lost">ἡ</supplied>
                        <supplied reason="lost">Π</supplied>Υ <seg type="word"
                                ><unclear>δι</unclear>πλ<supplied reason="lost">ῆ</supplied></seg>
                        τῆς <unclear>ΥΙ</unclear>, <seg type="word">
                            <unclear>κ</unclear>
                            <supplied reason="lost">αθάπερ</supplied>
                        </seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">δέδει</supplied>
                        </seg></seg>
                    <seg n="3" type="line" facs="z2v1_03"><seg type="wordend">
