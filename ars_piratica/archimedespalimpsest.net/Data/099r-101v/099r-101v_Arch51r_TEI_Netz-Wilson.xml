    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="99r1" type="folio">
                    <seg n="1" type="line" facs="z99r1_01"><seg type="word"
                                ><unclear>ἀ</unclear>δύ<unclear>ν</unclear><supplied reason="lost"
                                >ατον</supplied></seg>
                        <unclear>ἡ</unclear>
                        <seg type="word"><unclear>μὲ</unclear>ν</seg> γὰρ <seg type="word"
                                >ἐπι<supplied reason="lost">φάνεια</supplied></seg></seg>
                    <seg n="2" type="line" facs="z99r1_02">τοῦ <seg type="word"><supplied
                                reason="lost">π</supplied><unclear>ρ</unclear><supplied
                                reason="lost">ίσματ</supplied>ος</seg> τοῦ <seg type="suppliedword"
                                >περιγεγ<supplied reason="lost"
                        >ρ</supplied>α<unclear>μ</unclear></seg></seg>
                    <seg n="3" type="line" facs="z99r1_03"><seg type="wordend">μένου</seg> περὶ τὸν
                        κύλινδρον <choice>
                            <abbr>μείζω</abbr>
                            <expan>μείζων</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z99r1_04">οὖσα <seg type="word"
                                ><unclear>δ</unclear>έδ<unclear>ει</unclear>κται</seg> τῆς
                        ἐπιφανείας</seg>
                    <seg n="5" type="line" facs="z99r1_05">τοῦ κυλίνδρου, τὸ δὲ <seg
                            type="unclearword">ἐγγεγρ<unclear>α</unclear>μμέ</seg></seg>
                    <seg n="6" type="line" facs="z99r1_06"><seg type="wordend">νον</seg> εὐθύγραμμον
                        ἐν τῶι Β <seg type="word">κ<unclear>ύκ</unclear>λωι</seg></seg>
                    <seg n="7" type="line" facs="z99r1_07">ἔλασσόν ἐστιν τοῦ Β κύκλου. οὐκ
                            <expan>ἄρα</expan>
                        <expan>ἐστὶν</expan></seg>
                    <seg n="8" type="line" facs="z99r1_08">ὁ Β κύκλος ἐλάσσων τῆς <w part="I"
                        >ἐπιφα</w></seg>
                    <seg n="9" type="line" facs="z99r1_09"><w part="F">νείας</w> τοῦ
                    κυλίνδρου.</seg>
                </seg>
                <seg n="99r1" type="folio">
                    <seg n="9" type="line" facs="z99r1_09">ἔστω δή, εἰ <w part="I">δυ</w></seg>
                    <seg n="10" type="line" facs="z99r1_10"><w part="F">νατόν</w>, μείζων. πάλιν δὴ
                        νοείσθω</seg>
                    <seg n="11" type="line" facs="z99r1_11">εἰς τὸν Β κύκλον εὐθύγραμμον <w part="I"
                            >ἐγ</w></seg>
                    <seg n="12" type="line" facs="z99r1_12"><w part="F">γεγραμμένον</w>, ὥστε <seg
                            type="word">τ<supplied reason="lost">ὸ</supplied></seg>
                        <seg type="suppliedword"><supplied reason="lost">π</supplied>εριγεγραμ</seg></seg>
                    <seg n="13" type="line" facs="z99r1_13"><seg type="wordend">μένον</seg>
                        <expan>πρὸς</expan> τὸ ἐγγεγραμμένον <w part="I">ἐλάσ</w></seg>
                    <seg n="14" type="line" facs="z99r1_14"><w part="F">σονα</w>
                        <expan>λόγον</expan> ἔχειν ἤπερ τὸν Β κύκλον</seg>
                    <seg n="15" type="line" facs="z99r1_15"><expan>πρὸς</expan> τὴν ἐπιφάνειαν τοῦ <choice>
                            <abbr>κυλίνδρ</abbr>
                            <expan>κυλίνδρου</expan>
                        </choice>,</seg>
                    <seg n="16" type="line" facs="z99r1_16"><expan>καὶ</expan> ἐγγεγράφθω <seg
                            type="word">εἰ<unclear>ς</unclear></seg>
                        <seg type="word"><unclear>τ</unclear>ὸν</seg> Α κύκλον</seg>
                    <seg n="17" type="line" facs="z99r1_17">πολύγωνον ὅμοιον τῶι εἰς τὸν Β <seg
                            type="suppliedword">κύ</seg></seg>
                </seg>
                <seg n="101v1" type="folio">
                    <seg n="1" type="line" facs="z101v1_01"><seg type="wordend">
                            <supplied reason="lost">κλον</supplied>
                        </seg>
                        <seg type="word">ἐγ<unclear>γ</unclear><supplied reason="lost"
                                >εγ</supplied><unclear>ρ</unclear><supplied reason="lost"
                                >α</supplied>μμέ<unclear>νον</unclear></seg>, <seg type="word"
                                >κα<supplied reason="lost">ὶ</supplied></seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">πρί</supplied>
                            <unclear>σ</unclear>
                        </seg></seg>
                    <seg n="2" type="line" facs="z101v1_02"><seg type="wordend">μα</seg> ἀναγεγράφθω
                        ἀπὸ τοῦ ἐν</seg>
                    <seg n="3" type="line" facs="z101v1_03"><seg type="word">τ<unclear>ῶι</unclear></seg>
                        <seg type="word"><supplied reason="lost"
                            >κ</supplied><unclear>ύ</unclear>κλωι</seg> ἐγγεγραμμένου <w part="I"
                        >πο</w></seg>
                    <seg n="4" type="line" facs="z101v1_04"><w part="F">λυγώνου</w>· καὶ πάλιν ἡ ΚΔ
                        ἴση</seg>
                    <seg n="5" type="line" facs="z101v1_05">τῆι περιμέτρωι τοῦ <choice>
                            <abbr>εὐθυγράμμ</abbr>
                            <expan>εὐθυγράμμου</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z101v1_06">τοῦ ἐν τῶι Α κύκλωι <w part="I"
                            >ἐγγεγραμμέ</w></seg>
                    <seg n="7" type="line" facs="z101v1_07"><w part="F">νου</w>, καὶ ἡ ΖΑ ἴση αὐτῆι
                        ἔστω. ἔσται</seg>
                    <seg n="8" type="line" facs="z101v1_08">δὴ τὸ μὲν ΚΤΔ τρίγωνον <choice>
                            <abbr>μεῖζο</abbr>
                            <expan>μεῖζον</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z101v1_09">τοῦ εὐθυγράμμου τοῦ ἐν τῶι Α <seg
                            type="unclearword"><unclear>κ</unclear>ύ</seg></seg>
                    <seg n="10" type="line" facs="z101v1_10"><seg type="wordend">κλωι</seg>
                        ἐγγεγραμμένου <choice>
                            <abbr>δι</abbr>
                            <expan>διότι</expan>
                        </choice> βάσιν</seg>
                    <seg n="11" type="line" facs="z101v1_11">μὲν ἔχει τὴν περίμετρον αὐτοῦ,</seg>
                    <seg n="12" type="line" facs="z101v1_12">ὕψος δὲ μεῖζον τῆς ἀπὸ τοῦ <w part="I"
                            >κέν</w></seg>
                    <seg n="13" type="line" facs="z101v1_13"><w part="F">τρου</w> πλευρᾶς ἐπὶ μίαν
                        πλευρὰν</seg>
                    <seg n="14" type="line" facs="z101v1_14">τοῦ πολυγώνου <seg type="word"
                                ><supplied reason="lost">ἀγ</supplied>ομένης</seg>
                        <choice>
                            <abbr><unclear>καθ</unclear>έτ</abbr>
                            <expan><unclear>καθ</unclear>έτου</expan>
                        </choice>,</seg>
                    <seg n="15" type="line" facs="z101v1_15">τὸ δὲ ΕΛ <seg type="word"
                                >παραλληλόγραμμ<supplied reason="lost">ον</supplied></seg>
                        <choice>
                            <abbr>ἴσ</abbr>
                            <expan>ἴσ<supplied reason="lost">ον</supplied></expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z101v1_16">ἐν τῆι <seg type="word"><supplied
                                reason="lost">ἐπ</supplied>ιφανείαι</seg> τοῦ πρίσματος</seg>
                    <seg n="17" type="line" facs="z101v1_17">
                        <seg type="word">τ<unclear>ῆ</unclear></seg>
                        <supplied reason="lost">ἐκ</supplied>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ῶν</unclear>
                        </seg>
                        <seg type="word"><supplied reason="lost">π</supplied>αρα<supplied
                                reason="lost">λληλ</supplied>ογράμμων</seg>
                    </seg>
                </seg>
                <seg n="99r2" type="folio">
                    <seg n="1" type="line" facs="z99r2_01">συγκειμένηι <choice>
                            <abbr>δι</abbr>
                            <expan>διότι</expan>
                        </choice> περιέχεται ὑπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τ<supplied reason="lost">ῆς</supplied></expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z99r2_02">πλευρᾶς τοῦ κυλίνδρου καὶ τῆς <choice>
                            <abbr>ἴσ</abbr>
                            <expan>ἴσης</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z99r2_03">τῆι περιμέτρωι <seg type="word"
                                >το<supplied reason="lost">ῦ</supplied></seg>
                        <seg type="word"><supplied reason="lost"
                            >εὐ</supplied><unclear>θυ</unclear>γράμμου</seg>, ὅς</seg>
                    <seg n="4" type="line" facs="z99r2_04"><expan>ἐστιν</expan> βάσις τοῦ <seg
                            type="word"><unclear>π</unclear>ρίσματος</seg>· <seg type="word"
                                ><supplied reason="lost">ὥ</supplied>στ<supplied reason="lost"
                            >ε</supplied></seg> καὶ</seg>
                    <seg n="5" type="line" facs="z99r2_05">τὸ ΡΛΖ <seg type="word"
                            >τρίγω<unclear>ν</unclear>ον</seg>
                        <seg type="word">ἴ<unclear>σ</unclear><supplied reason="lost">ον</supplied></seg>
                        <expan>
                            <supplied reason="lost">ἐστὶ</supplied>
                        </expan>
                        <seg type="word"><supplied reason="lost">τ</supplied>ῆι</seg>
                        <seg type="suppliedword">ἐπιφ<supplied reason="lost">α</supplied></seg></seg>
                    <seg n="6" type="line" facs="z99r2_06"><seg type="wordend">νείαι</seg> τοῦ <seg
                            type="word">πρί<unclear>σμ</unclear><supplied reason="lost"
                            >ατος</supplied></seg>. <seg type="word"><supplied reason="lost"
                            >κ</supplied>αὶ</seg>
                        <seg type="word">
                            <supplied reason="lost">ἐ</supplied>
                            <unclear>πεὶ</unclear>
                        </seg>
                        <choice>
                            <abbr>α</abbr>
                            <expan><unclear>ἴσ</unclear>α</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z99r2_07"><expan>ἐστι</expan> τὰ <seg type="word"
                                ><supplied reason="lost"
                                >ε</supplied><unclear>ὐ</unclear>θύγ<unclear>ρ</unclear>αμμ<supplied
                                reason="lost">α</supplied></seg>
                        <supplied reason="lost">τὰ</supplied>
                        <seg type="word"><supplied reason="lost">ἐ</supplied>ν</seg> τοῖς ΑΒΓ</seg>
                    <seg n="8" type="line" facs="z99r2_08">κύκλοις <seg type="word"
                                >ἐγγεγραμμ<unclear>έ</unclear><supplied reason="lost"
                        >να</supplied></seg>, τὸν αὐτὸν</seg>
                    <seg n="9" type="line" facs="z99r2_09">ἔχει λόγον <expan>πρὸς</expan> ἄλληλα ὃν
                        αἱ ἐκ <seg type="word"><unclear>τ</unclear>ῶν</seg></seg>
                    <seg n="10" type="line" facs="z99r2_10">κέντρων αὐτῶν δυνάμει. <seg type="word"
                                >ε<unclear>π</unclear>εὶ</seg>
                        <expan>καὶ</expan></seg>
                    <seg n="11" type="line" facs="z99r2_11">τὰ ΚΤΔ, ΖΡΛ τρίγωνα <expan>πρὸς</expan>
                        ἄλληλα <expan>λόγον</expan>,</seg>
                    <seg n="12" type="line" facs="z99r2_12">ὃν αἱ ἐκ τῶν κέντρων <seg type="word"
                                >τ<unclear>ῶ</unclear>ν</seg>
                        <choice>
                            <abbr>κύ<unclear>κ</unclear>λω</abbr>
                            <expan>κύ<unclear>κ</unclear>λων</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z99r2_13">δυνάμει· τὸν αὐτὸν ἄρα <seg type="word"
                                >λ<unclear>ό</unclear>γον</seg> ἔχει</seg>
                    <seg n="14" type="line" facs="z99r2_14">τὸ εὐθύγραμμον τὸ ἐν τῶι Β κύκλωι <w
                            part="I">ἐγγε</w></seg>
                    <seg n="15" type="line" facs="z99r2_15"><w part="F">γραμμένον</w>
                        <expan>καὶ</expan> τὸ ΚΤΔ <choice>
                            <abbr><unclear>τ</unclear><supplied reason="lost">ρίγ</supplied>ωνο</abbr>
                            <expan><unclear>τ</unclear><supplied reason="lost"
                            >ρίγ</supplied>ωνον</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z99r2_16"><expan>πρὸς</expan> τὸ ΛΖΡ τρίγωνον.
                            <seg type="word">ἔλασσο<supplied reason="lost">ν</supplied></seg>
                        <seg type="word">
                            <supplied reason="lost">δ</supplied>
                            <unclear>έ</unclear>
                        </seg>
                        <choice>
                            <abbr>ἐστι</abbr>
                            <expan>ἐστιν</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z99r2_17">τὸ εὐθύγραμμον τὸ <seg type="word"
                                >ἐ<unclear>ν</unclear></seg>
                        <seg type="word"><supplied reason="lost">τ</supplied>ῶι</seg>
                        <supplied reason="lost">Α</supplied>
                        <seg type="word">
                            <supplied reason="lost">κύκλ</supplied>
                            <unclear>ω</unclear>
                        </seg></seg>
                </seg>
                <seg n="101v2" type="folio">
                    <seg n="1" type="line" facs="z101v2_01"><seg type="word">ἐγγεγρ<supplied
