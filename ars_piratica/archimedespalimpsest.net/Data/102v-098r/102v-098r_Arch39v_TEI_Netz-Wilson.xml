    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="102v1" type="folio">
                    <seg n="1" type="line" facs="z102v1_01">λαβόντα τὸ χωρίον τὸ <seg
                            type="suppliedword">περιεχό</seg></seg>
                    <seg n="2" type="line" facs="z102v1_02"><seg type="wordend">μ<supplied
                                reason="lost">ε</supplied>νον</seg>
                        <seg type="word">ὑπ<supplied reason="lost">ὸ</supplied>
                        </seg> τᾶς ἕλικος τᾶς ἐν τᾶ</seg>
                    <seg n="3" type="line" facs="z102v1_03">
                        <seg type="word">δευτέρ<supplied reason="lost">αι</supplied>
                        </seg>
                        <seg type="word">περι<supplied reason="lost">φ</supplied>ορᾶι</seg>
                        <seg type="clearword">γεγραμμέ</seg>
                    </seg>
                    <seg n="4" type="line" facs="z102v1_04"><seg type="wordend">νας</seg> καὶ τᾶς
                            <seg type="word">
                            <supplied reason="lost">εὐ</supplied>θείας</seg>, ἅ ἐστι <seg
                            type="clearword">δευ</seg></seg>
                    <seg n="5" type="line" facs="z102v1_05"><seg type="wordend">τέρα</seg> τᾶν ἐν
                            <seg type="word">τ<supplied reason="lost">ᾶι</supplied>
                        </seg> ἀρχᾶι τᾶς <seg type="clearword">πε</seg></seg>
                    <seg n="6" type="line" facs="z102v1_06"><seg type="wordend">ριφορᾶς</seg>, <seg
                            type="word">δυνα<supplied reason="lost">τ</supplied>όν</seg> ἐστιν περὶ
                            <seg type="clearword">αὐ</seg></seg>
                    <seg n="7" type="line" facs="z102v1_07"><seg type="wordend">τὸ</seg> σχᾶμα
                        ἐπίπεδον <seg type="clearword">περιγρά</seg></seg>
                    <seg n="8" type="line" facs="z102v1_08"><seg type="wordend">ψαι</seg> ἐξ ὁμοίων
                        τομέων <seg type="clearword">συγκεί</seg></seg>
                    <seg n="9" type="line" facs="z102v1_09"><seg type="wordend">μενον</seg>
                        <seg type="word">κα<supplied reason="lost">ὶ</supplied>
                        </seg> ἄλλο ἐγγράψαι, ὥστε</seg>
                    <seg n="10" type="line" facs="z102v1_10">τὸ περιγραφὲν τοῦ <choice>
                            <abbr>ἐγγραφέντ</abbr>
                            <expan>ἐγγραφέντος</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z102v1_11">μεῖζον εἶμεν ἐλάσσονι παντὸς</seg>
                    <seg n="12" type="line" facs="z102v1_12">τοῦ προτεθέντος χωρίου.</seg>
                </seg>
                <seg n="102v1" type="folio">
                    <seg n="12" type="line" facs="z102v1_12">ἔστω <seg type="clearword">ἕ</seg></seg>
                    <seg n="13" type="line" facs="z102v1_13"><seg type="wordend">λιξ</seg>, ἐφ’ ἇι ἁ
                        ΑΒ ΓΔΕ, ἐν τᾶι <seg type="clearword">δευτέ</seg></seg>
                    <seg n="14" type="line" facs="z102v1_14"><seg type="wordend">ραι</seg> περιφορᾶι
                        γεγραμμένα,</seg>
                    <seg n="15" type="line" facs="z102v1_15">καὶ ἔστω τὸ μὲν Θ σαμεῖον ἀρχὰ</seg>
                    <seg n="16" type="line" facs="z102v1_16">τᾶς ἕλικος, ἁ δὲ ΑΘ ἀρχὰ τᾶς <seg
                            type="clearword">πε</seg></seg>
                    <seg n="17" type="line" facs="z102v1_17"><seg type="wordend">ριφορᾶς</seg>, ἁ δὲ
                        ΕΑ δευτέρα <seg type="clearword">εὐθεῖ</seg></seg>
                    <seg n="18" type="line" facs="z102v1_18">
                        <seg type="wordend">αι</seg> τᾶν ἐν τᾶι ἀρχᾶι <seg type="word"> τᾶ<supplied
                                reason="lost">ς</supplied>
                        </seg>
                        <choice>
                            <abbr>περιφορ</abbr>
                            <expan>περιφορᾶς</expan>
                        </choice>,</seg>
                </seg>
                <seg n="98r1" type="folio">
                    <seg n="1" type="line" facs="z98r1_01">ὁ δὲ ΑΖΗ κύκλος ἔστω δεύτερος καὶ</seg>
                    <seg n="2" type="line" facs="z98r1_02"><supplied reason="lost">αἱ</supplied> ΑΓ
                            <unclear>Η</unclear>ΖΙ <seg type="word">
                            <supplied reason="lost">δ</supplied>ιάμ<unclear>ε</unclear>τρ<supplied
                                reason="lost">ο</supplied>ι</seg> αὐτοῦ πρὸς <seg type="clearword"
                            >ὀρ</seg></seg>
                    <seg n="3" type="line" facs="z98r1_03"><seg type="wordend">θὰς</seg>
                        <seg type="word">ἀλλ<supplied reason="lost">ά</supplied>λαις </seg>. πάλιν
                        οὖν δίχα</seg>
                    <seg n="4" type="line" facs="z98r1_04">τεμνομένας τᾶς ὀρθᾶς <choice>
                            <abbr>γωνί</abbr>
                            <expan>γωνίας</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z98r1_05">καὶ τοῦ τομέως τοῦ τὴν ὀρθὴν</seg>
                    <seg n="6" type="line" facs="z98r1_06">γωνίαν περιέχοντος ἐσσεῖται</seg>
                    <seg n="7" type="line" facs="z98r1_07">τὸ καταλειπόμενον ἔλασσον</seg>
                    <seg n="8" type="line" facs="z98r1_08">τοῦ προτεθέντος· καὶ ἔστω <seg
                            type="clearword">γεγε</seg></seg>
                    <seg n="9" type="line" facs="z98r1_09"><seg type="wordend">νημένος</seg> ὁ
                            ΘΚ<supplied reason="lost">Α</supplied> τομεὺς <choice>
                            <abbr>ἐλάσσω</abbr>
                            <expan>ἐλάσσων</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z98r1_10">τοῦ προτεθέντος <seg type="word">
                                χ<unclear>ω</unclear>ρίου</seg>. <seg type="clearword">διαι</seg></seg>
                    <seg n="11" type="line" facs="z98r1_11"><seg type="wordend">ρεθεισᾶν</seg> δὴ
                        τᾶν ὀρθᾶν <seg type="clearword">γωνι</seg></seg>
                    <seg n="12" type="line" facs="z98r1_12"><seg type="wordend">ᾶν</seg> εἰς τὰς
                        ἴσας γωνίας τᾶ <seg type="clearword">ὑ</seg></seg>
                    <seg n="13" type="line" facs="z98r1_13"><seg type="wordend">πὸ</seg> τῶν ΚΘΑ καὶ
                        τῶν ἄλλων <seg type="clearword">
                            <expan>κατα</expan>
                        </seg></seg>
                    <seg n="14" type="line" facs="z98r1_14"><seg type="wordend">σκευασθέντων</seg>
                        κατὰ τὰ <seg type="word">α<unclear>ὐτὰ</unclear>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>
                                    <supplied reason="lost">τοῖς</supplied>
                                </expan>
                            </choice>
                        </seg></seg>
                    <seg n="15" type="line" facs="z98r1_15">πρότερον ἐσσεῖται τὸ <seg
                            type="suppliedword">περι<supplied reason="lost">γ</supplied>ε</seg></seg>
                    <seg n="16" type="line" facs="z98r1_16"><seg type="wordend"><supplied
                                reason="lost">γραμ</supplied>μένον</seg> σχᾶμα τοῦ <seg
                            type="clearword">ἐγγεγραμ</seg></seg>
                </seg>
                <seg n="102v2" type="folio">
                    <seg n="1" type="line" facs="z102v2_01"><seg type="wordend">μένου</seg> σχάματος
                        μείζων <sic>
                            <seg type="clearword">ἐλάσ</seg>
                        </sic></seg>
                    <seg n="2" type="line" facs="z102v2_02"><sic>
                            <seg type="wordend">σωνι</seg>
                        </sic> ἢ ὁ τομεὺς ὁ ΘΚΑ· μείζων</seg>
                    <seg n="3" type="line" facs="z102v2_03">γὰρ ἐσσεῖται α υπεροχα, ἇ <seg
                            type="clearword">ὑπερ</seg></seg>
                    <seg n="4" type="line" facs="z102v2_04"><seg type="wordend">έχει</seg> ὁ ΘΚΑ
                        τομεὺς τοῦ ΘΕΡ.</seg>
                </seg>
                <seg n="102v2" type="folio">
                    <seg n="4" type="line" facs="z102v2_04">δῆλον</seg>
                    <seg n="5" type="line" facs="z102v2_05">οὖν, <expan>ὅτι</expan> δυνατόν ἐστιν
                        καὶ τὸ <seg type="clearword">περιγρα</seg></seg>
                    <seg n="6" type="line" facs="z102v2_06"><seg type="wordend">φὲν</seg> σχᾶμα τοῦ
                        λαφθέντος <seg type="clearword">χωρί</seg></seg>
                    <seg n="7" type="line" facs="z102v2_07"><seg type="wordend">ου</seg> μείζων
                        εἶμεν ἐλάσσονι παντὸς</seg>
                    <seg n="8" type="line" facs="z102v2_08">τοῦ προτεθέντος χωρίου, καὶ</seg>
                    <seg n="9" type="line" facs="z102v2_09">πάλιν τὸ λαφθὲν χωρίον <choice>
                            <abbr>μείζω</abbr>
                            <expan>μείζων</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z102v2_10">εἶμεν τοῦ ἐγγραφέντος <seg
                            type="clearword">σχάμα</seg></seg>
                    <seg n="11" type="line" facs="z102v2_11"><seg type="wordend">τος</seg> ἐλάσσονι
                        παντὸς τοῦ <seg type="clearword">προ</seg></seg>
                    <seg n="12" type="line" facs="z102v2_12"><seg type="wordend">τεθέντος</seg>
                        χωρίου.</seg>
                    <figure n="23.1">
                        <figDesc>Figure 23.1</figDesc>
                    </figure>
                </seg>
                <seg n="98r2" type="folio">
                    <seg n="1" type="line" facs="z98r2_01">Διὰ δὲ τοῦ αὐτοῦ τρόπου φανερὸν <seg
                            type="clearword">δι</seg></seg>
                    <seg n="2" type="line" facs="z98r2_02"><seg type="wordend">
                            <expan>ότι</expan>
                        </seg> δυνατὸν λαβόντα τὸ χωρίον τὸ</seg>
                    <seg n="3" type="line" facs="z98r2_03">περιεχόμενον ὑπό <seg type="word"
                                >τ<supplied reason="lost">ε</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ᾶς</seg>
                        <choice>
                            <abbr>ἕλικ</abbr>
                            <expan>ἕλικος</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z98r2_04">τᾶν ἐν ὁποιαοῦν περιφορᾶι</seg>
                    <seg n="5" type="line" facs="z98r2_05">γεγραμμένας καὶ τᾶς εὐθείας</seg>
                    <seg n="6" type="line" facs="z98r2_06">τᾶς ἐν τᾶ ἀρχᾶι τᾶς περιφορᾶς</seg>
                    <seg n="7" type="line" facs="z98r2_07">κατὰ τὸν αὐτὸν <seg type="word"
                                >ἀρ<supplied reason="lost">ι</supplied>θμὸν</seg>
                        <seg type="clearword">λεγομέ</seg></seg>
                    <seg n="8" type="line" facs="z98r2_08"><seg type="wordend">νας</seg> περιγράψαι
                        σχᾶμα, οἷον <seg type="clearword">εἴρη</seg></seg>
                    <seg n="9" type="line" facs="z98r2_09"><seg type="wordend">ται</seg>, ἐπίπεδον,
                        ὥστε τὸ <seg type="clearword">περιγρα</seg></seg>
                    <seg n="10" type="line" facs="z98r2_10"><seg type="wordend">φὲν</seg> σχᾶμα
                        μεῖζον εἶμεν τοῦ <seg type="clearword">λα</seg></seg>
                    <seg n="11" type="line" facs="z98r2_11"><seg type="wordend">φθέντος</seg> χωρίου
                        ἐλάσσονι <seg type="clearword">παν</seg></seg>
                    <seg n="12" type="line" facs="z98r2_12"><seg type="wordend">τὸς</seg> τοῦ
                        προτεθέντος χωρίου, <expan>καὶ</expan></seg>
                    <seg n="13" type="line" facs="z98r2_13">πάλιν ἐγγράψαι, <seg type="word"
                                >ὥσ<supplied reason="lost">τ</supplied>ε</seg> τὸ λαφθὲν</seg>
                    <seg n="14" type="line" facs="z98r2_14">χωρίον <seg type="word">
                            μ<unclear>εῖ</unclear>ζον</seg> εἶμεν <seg type="word">
                            <unclear>τ</unclear>
                            <supplied reason="lost">οῦ</supplied>
                        </seg>
                        <sic>τοῦ</sic>
                        <seg type="clearword">ἐγ</seg></seg>
                    <seg n="15" type="line" facs="z98r2_15">
                        <seg type="wordend">γραφέντος</seg>
                        <seg type="word">σχάμ<supplied reason="lost">ατος</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">ἐλά</supplied>σσονι</seg>
                    </seg>
                </seg>
            </ab>
        </body>
    </text>
