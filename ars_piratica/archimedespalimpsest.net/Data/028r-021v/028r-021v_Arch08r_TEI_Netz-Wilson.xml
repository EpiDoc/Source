    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="28r1" type="folio">
                    <seg n="1" type="line" facs="z28r1_01">Ρ, ὥστε καὶ ΟΡ τῆς ΡΝ διπλῆν</seg>
                    <seg n="2" type="line" facs="z28r1_02">εἶναι· ἔσται δὴ τοῦ μείζονος <w part="I"
                            >ὅ</w></seg>
                    <seg n="3" type="line" facs="z28r1_03"><w part="F">λου</w> τμάματος τοῦ στερεοῦ
                            <w part="I">κέν</w></seg>
                    <seg n="4" type="line" facs="z28r1_04"><w part="F">τρον</w> τοῦ βάρους τὸ Ρ, τοῦ
                        δὲ <expan>κατὰ</expan></seg>
                    <seg n="5" type="line" facs="z28r1_05">τὴν ΙΠΟΣ τὸ Β· δέδεικται γὰρ</seg>
                    <seg n="6" type="line" facs="z28r1_06">ἐν ταῖς ἰσορροπείαις, ὅτι <w part="I"
                        >παν</w></seg>
                    <seg n="7" type="line" facs="z28r1_07"><w part="F">τὸς</w> ὀρθογωνίου κώνου <choice>
                            <abbr>ειδς</abbr>
                            <expan>εἴδους</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z28r1_08">τμάματος τὸ κέντρον τοῦ <w part="I"
                        >βά</w></seg>
                    <seg n="9" type="line" facs="z28r1_09"><w part="F">ρους</w> ἐστὶν ἐπὶ τοῦ ἄξονος
                            <w part="I">διη</w></seg>
                    <seg n="10" type="line" facs="z28r1_10"><w part="F">ρήσθω</w> οὕτως, ὥστε τὸ
                        πρὸς τῆι</seg>
                    <seg n="11" type="line" facs="z28r1_11">κορυφῆι τοῦ ἄξονος τμᾶμα</seg>
                    <seg n="12" type="line" facs="z28r1_12">διπλάσιον εἶμεν τοῦ λοιποῦ. <w part="I"
                            >ἀ</w></seg>
                    <seg n="13" type="line" facs="z28r1_13"><w part="F">φαιρεθέντος</w> δὲ τοῦ κατὰ
                        τὴν</seg>
                    <seg n="14" type="line" facs="z28r1_14">ΙΠΟΣ τμάματος στερεοῦ <w part="I">ἀ</w></seg>
                    <seg n="15" type="line" facs="z28r1_15"><w part="F">πὸ</w> τοῦ ὅλου τοῦ λοιποῦ
                            <w part="I">κέν</w></seg>
                    <seg n="16" type="line" facs="z28r1_16"><w part="F">τρου</w> ἔσται τοῦ βάρους ὁ
                        ἐπὶ <choice>
                            <abbr>τ</abbr>
                            <expan>
                                <seg type="word">τ<supplied reason="lost">ῆς</supplied></seg>
                            </expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z28r1_17">ΒΓ εὐθείας· δέδεικται γὰρ <w part="I"
                            >τοῦ</w></seg>
                    <seg n="18" type="line" facs="z28r1_18"><w part="F">το</w> ἐν τοῖς στοιχείοις
                        τῶν <w part="I">μηχα</w></seg>
                    <seg n="19" type="line" facs="z28r1_19"><w part="F">νικῶν</w>,
                        <expan>ὅτι</expan>, <supplied reason="lost">ἐὰν</supplied> ἀπό τινος <seg
                            type="suppliedword">μεγέ</seg></seg>
                </seg>
                <seg n="21v1" type="folio">
                    <seg n="1" type="line" facs="z21v1_01">
                        <seg type="wordend">
                            <supplied reason="lost">θους</supplied>
                        </seg>
                        <supplied reason="lost">ἀφαιρεθῆι</supplied>
                        <supplied reason="lost">τι</supplied>
                        <supplied reason="lost">μέγεθος</supplied>
                    </seg>
                    <seg n="2" type="line" facs="z21v1_02">τὸ αὐτὸ κέντρον ἔχον τοῦ βάρους</seg>
                    <seg n="3" type="line" facs="z21v1_03">τῶι ὅλωι μεγέθει, τοῦ λοιποῦ τὸ</seg>
                    <seg n="4" type="line" facs="z21v1_04">κέντρον ἔσται τοῦ βάρους ἐπὶ τῆς</seg>
                    <seg n="5" type="line" facs="z21v1_05">εὐθείας τῆς ἐπιζευγνούσης</seg>
                    <seg n="6" type="line" facs="z21v1_06">τὰ κέντρα τοῦ τε ὅλου μεγέθεος</seg>
                    <seg n="7" type="line" facs="z21v1_07"><expan>καὶ</expan> τοῦ ἀφηρημένου ἐπὶ τὰ
                        αὐτά,</seg>
                    <seg n="8" type="line" facs="z21v1_08">ἐφ’ οὗ τὸ κέντρον τοῦ ὅλου <w part="I"
                            >μεγέ</w></seg>
                    <seg n="9" type="line" facs="z21v1_09"><w part="F">θους</w>
                        <unclear>
                            <expan>ἐστίν.</expan>
                        </unclear> ἐκβεβλήσθω δὴ ἡ ΒΡ ἐπὶ</seg>
                    <seg n="10" type="line" facs="z21v1_10">τὸ <supplied reason="lost">Γ,</supplied>
                        καὶ ἔστω τὸ Γ τοῦ βάρους τοῦ</seg>
                    <seg n="11" type="line" facs="z21v1_11">λοιποῦ μεγέθους. ἐπεὶ οὖν ἡ ΝΟ</seg>
                    <seg n="12" type="line" facs="z21v1_12">τῆς μὲν ΟΡ <sic>η μη δια τις</sic> δὲ
                        μέχρι</seg>
                    <seg n="13" type="line" facs="z21v1_13">τοῦ ἄξονος οὐ μεῖζον εἰ <seg
                            type="suppliedword">ἡμιολ<supplied reason="lost">ί</supplied></seg></seg>
                    <seg n="14" type="line" facs="z21v1_14"><seg type="wordend">α,</seg> δῆλον, ὅτι
                        ἡ ΡΟ τῆς μέχρι τοῦ</seg>
                    <seg n="15" type="line" facs="z21v1_15">ἄξονος οὐκ ἐστὶ μείζων· ἡ ΠΡ ἄρα</seg>
                    <seg n="16" type="line" facs="z21v1_16">πρὸς τὴν ΚΩ γωνίας ἀνίσους</seg>
                    <seg n="17" type="line" facs="z21v1_17">ποιεῖ, καὶ ἡ ὑπὸ τῶν ΡΠΩ <choice>
                            <abbr>γινετ</abbr>
                            <expan>γίνεται</expan>
                        </choice></seg>
                </seg>
                <seg n="28r2" type="folio">
                    <seg n="1" type="line" facs="z28r2_01">ὀξείη· ἀπὸ τοῦ Ρ ἄρα κάθετος ἐπὶ</seg>
                    <seg n="2" type="line" facs="z28r2_02">τὴν ΠΩ ἀγομένη μεταξὺ <choice>
                            <abbr>πεσειτ</abbr>
                            <expan>πεσεῖται</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z28r2_03">τῶν ΠΩ. πιπτέτω ὡς ἡ ΡΘ· ἡ ΡΘ</seg>
                    <seg n="4" type="line" facs="z28r2_04">ἄρα ὀρθή ἐστι καὶ πρὸς τὸ τοῦ <seg
                            type="suppliedword">ὕ<supplied reason="lost">δ</supplied>α</seg></seg>
                    <seg n="5" type="line" facs="z28r2_05"><seg type="wordend">τος</seg> ἐπίπεδον,
                        ἐν ὧι ἐστιν ἡ ΣΙ, ὅ</seg>
                    <seg n="6" type="line" facs="z28r2_06">ἐστιν ἡ ἐπὶ τῆς ἐπιφανείας τοῦ</seg>
                    <seg n="7" type="line" facs="z28r2_07">ὑγροῦ. ἤχθωσαν δέ τινες ἀπὸ <choice>
                            <abbr>τω</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z28r2_08">ΒΓ παρὰ τὰν ΡΘ· ἐνεχθήσεται δὴ</seg>
                    <seg n="9" type="line" facs="z28r2_09">τὸ μὲν ἐκτὸς τοῦ ὑγροῦ οὗ <w part="I"
                            >μεγέ</w></seg>
                    <seg n="10" type="line" facs="z28r2_10"><w part="F">θους</w> εἰς τὸ κάτω κατὰ
                        τὴν διὰ τοῦ</seg>
                    <seg n="11" type="line" facs="z28r2_11">Γ ἀγομένην κάθετον· ὑπόκειται</seg>
                    <seg n="12" type="line" facs="z28r2_12">ἕκαστον τῶν βαρέων <seg type="word"
                                >εἴ<unclear>ς</unclear></seg>
                        <seg type="word"><unclear>τ</unclear>ε</seg> κάτω</seg>
                    <seg n="13" type="line" facs="z28r2_13">φέρεσθαι κατὰ τὴν κάθετον τὴν</seg>
                    <seg n="14" type="line" facs="z28r2_14">διὰ τοῦ κέντρου ἀγομένην· τὸ δὲ</seg>
                    <seg n="15" type="line" facs="z28r2_15">ἐν τῶι ὑγρῶι μέγεθος, ἐπὶ <w part="I"
                            >κουφό</w></seg>
                    <seg n="16" type="line" facs="z28r2_16"><w part="F">τερον</w> γίνεται τοῦ ὑγροῦ,
                            <w part="I">ἐνεχθή</w></seg>
                    <seg n="17" type="line" facs="z28r2_17"><w part="F">σεται</w> εἰς τὸ ἄνω κατὰ
                        τὴν <w part="I">κάθε</w></seg>
                    <seg n="18" type="line" facs="z28r2_18"><w part="F">τον</w> τὴν διὰ <seg
                            type="word"><supplied reason="lost">το</supplied>ῦ</seg> Β ἀγομένην. <w
                            part="I">ἐπι</w></seg>
                    <seg n="19" type="line" facs="z28r2_19"><w part="F">πέδου</w> κατὰ τὴν αὐτὴν
                            <seg type="word">
                            <choice>
                                <abbr>καθετο</abbr>
                                <expan>κάθε<supplied reason="lost">το</supplied>ν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="20" type="line" facs="z28r2_20">ἀλλὰ <seg type="word">ἀλλήλο<supplied
                                reason="lost">ι</supplied>ς</seg>
                        <seg type="word">ἀντιθλίβ<supplied reason="lost">ονται</supplied></seg>,</seg>
                    <seg n="21" type="line" facs="z28r2_21">δῆλον, <seg type="word"><supplied
                                reason="lost">ὡ</supplied>ς</seg> οὐ <seg type="word">με<supplied
                                reason="lost">ν</supplied>εῖ</seg> τὸ <seg type="word">τμᾶμ<supplied
                                reason="lost">α</supplied></seg></seg>
                </seg>
                <seg n="21v2" type="folio">
                    <seg n="1" type="line" facs="z21v2_01"><seg type="word"><supplied reason="lost"
                                >ἐ</supplied>ν</seg> τῶι ὑγρῶι <supplied reason="lost">ἀλλὰ</supplied>
                        <supplied reason="lost">τὰ</supplied>
                        <supplied reason="lost">μὲν</supplied>
                        <supplied reason="lost">κατὰ</supplied></seg>
                    <seg n="2" type="line" facs="z21v2_02">τὸ Α εἰς τὸ ἄνω ἐνεχθήσεται, τὰ</seg>
                    <seg n="3" type="line" facs="z21v2_03">δὲ κατὰ τὸ Λ εἰς τὸ κάτω, ἀεὶ <sic>
                            <choice>
                                <abbr>εστε</abbr>
                                <expan>ἕστεν</expan>
                            </choice>
                        </sic>,</seg>
                    <seg n="4" type="line" facs="z21v2_04">ἕως ἂν ὀρθὸν ἀποκατασταθῆι.</seg>
                    <seg n="5" type="line" facs="z21v2_05">ΕΞΗΣ ΤΟ ΣΧΗΜΑ</seg>
                    <figure n="2.2.1">
                        <figDesc xml:lang="eng">Figure 2.2.1</figDesc>
                    </figure>
                </seg>
                <seg n="21v2" type="folio">
                    <num>γ</num>
                </seg>
                <seg n="21v2" type="folio">
                    <seg n="6" type="line" facs="z21v2_06">ὀρθὸν τμᾶμα τοῦ ὀρθογωνίου <w part="I"
                        >κω</w></seg>
                    <seg n="7" type="line" facs="z21v2_07"><w part="F">νοειδοῦς,</w> ὅταν τὸν ἄξονα
                        ἔχη</seg>
                    <seg n="8" type="line" facs="z21v2_08">μὴ μείζονα ἡμιόλιον τῆς μέχρι</seg>
                    <seg n="9" type="line" facs="z21v2_09">τοὺς ἄξονας, πάντα λόγον <seg type="word"
                                >ἔχο<supplied reason="lost">ν</supplied></seg></seg>
                    <seg n="10" type="line" facs="z21v2_10">πρὸς τὸ ὑγρὸν τῶι βάρει, <seg
                            type="word">ἀφεθὲ<supplied reason="lost">ν</supplied></seg></seg>
                    <seg n="11" type="line" facs="z21v2_11">εἰς τὸ ὑγρὸν οὕτως, ὥστε τὴν <seg
                            type="word">βάσι<supplied reason="lost">ν</supplied></seg></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
