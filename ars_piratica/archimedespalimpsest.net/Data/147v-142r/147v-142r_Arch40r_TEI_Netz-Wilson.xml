    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="147v1" type="folio">
                    <seg n="1" type="line" facs="z147v1_01"><seg type="word">
                            <supplied reason="lost">παν</supplied>τὸς</seg> τοῦ <seg type="word"
                                >προτε<supplied reason="lost">θέντος</supplied>
                        </seg>
                        <seg type="word">χ<supplied reason="lost">ωρίου.</supplied>
                        </seg></seg>
                </seg>
                <seg n="147v1" type="folio">
                    <seg n="2" type="line" facs="z147v1_02">λαβόντα τὸ χωρίον <supplied
                            reason="lost">τὸ</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">περιεχό</supplied>
                        </seg></seg>
                    <seg n="3" type="line" facs="z147v1_03"><seg type="wordend">μενον</seg> ὑπό τε
                        τᾶς ἕλικος, ἅ ἐστιν</seg>
                    <seg n="4" type="line" facs="z147v1_04">ἐλάσσων τᾶς ἐν μιᾶι περιφορᾶι</seg>
                    <seg n="5" type="line" facs="z147v1_05">γεγραμμένας, οὐκ ἐχούσας <choice>
                            <abbr>πέρ</abbr>
                            <expan>πέρας</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z147v1_06">τὰν ἀρχὰν τᾶς ἕλικος, καὶ τᾶν</seg>
                    <seg n="7" type="line" facs="z147v1_07">εὐθειᾶν τᾶν ἀπὸ τῶν περάτων</seg>
                    <seg n="8" type="line" facs="z147v1_08">τᾶς ἕλικος ἀγομενᾶν δυνατόν
                        <expan>ἐστι</expan></seg>
                    <seg n="9" type="line" facs="z147v1_09">περὶ τὸ χωρίον σχᾶμα <choice>
                            <abbr>ἐπίπεδ</abbr>
                            <expan>ἐπίπεδον</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z147v1_10">περιγράψαι ἐξ ὁμοίων τομέων</seg>
                    <seg n="11" type="line" facs="z147v1_11">συγκείμενον καὶ ἄλλο ἐγγράψαι,</seg>
                    <seg n="12" type="line" facs="z147v1_12">ὥστε τὸ περιγραφὲν σχᾶμα <seg
                            type="word"> το<unclear>ῦ</unclear>
                        </seg></seg>
                    <seg n="13" type="line" facs="z147v1_13">ἐγγραφέντος μείζονι μὲν <seg
                            type="clearword">ἐλάσ</seg></seg>
                    <seg n="14" type="line" facs="z147v1_14"><seg type="wordend">σονι</seg> παντὸς
                        τοῦ προτεθέντος <seg type="clearword">χω</seg></seg>
                    <seg n="15" type="line" facs="z147v1_15">
                        <seg type="wordend">ρίου</seg>. </seg>
                </seg>
                <seg n="147v1" type="folio">
                    <seg n="15" type="line" facs="z147v1_15">ἔστω ἕλιξ, ἐφ’ ἇς ἁ ΑΒΓΔΕ,</seg>
                    <seg n="16" type="line" facs="z147v1_16">πέρατα δὲ αὐτᾶς τὰ αΕ, ἔστω</seg>
                    <seg n="17" type="line" facs="z147v1_17">δὲ ἀρχὰ τᾶς ἕλικος τὸ Θ, καὶ</seg>
                    <seg n="18" type="line" facs="z147v1_18"><seg type="word">ἐπεζ<supplied
                                reason="lost">εύ</supplied>χθωσαν</seg> αἱ αΘ ΘΕ. <seg
                            type="suppliedword">
                            <supplied reason="lost">γεγρά</supplied>
                        </seg>
                    </seg>
                </seg>
                <seg n="142r1" type="folio">
                    <seg n="1" type="line" facs="z142r1_01"><seg type="wordend">φθω</seg> δὴ κύκλος
                        κέντρωι μὲν τῶι</seg>
                    <seg n="2" type="line" facs="z142r1_02">Θ, διαστάματι δὲ τῶι ΘΑ, καὶ</seg>
                    <seg n="3" type="line" facs="z142r1_03">συμπιπτέτω τᾶ ΘΕ κατὰ τὸ ΖΑ.</seg>
                    <seg n="4" type="line" facs="z142r1_04">εἰ δὲ τᾶς γωνίας τᾶς ποτὶ τὸ Θ</seg>
                    <seg n="5" type="line" facs="z142r1_05">καὶ τοῦ τομέως τοῦ ΘΑΖ δίχα</seg>
                    <seg n="6" type="line" facs="z142r1_06">τεμνομένων ἔσται τὸ <seg
                            type="clearword">καταλει</seg></seg>
                    <seg n="7" type="line" facs="z142r1_07"><seg type="wordend">πόμενον</seg> τοῦ
                        προτεθέντος <seg type="clearword">ἐ</seg></seg>
                    <seg n="8" type="line" facs="z142r1_08"><seg type="wordend">λάσσων</seg>. ἔστω
                        ἐλάσσων τομεὺς</seg>
                    <seg n="9" type="line" facs="z142r1_09">ὁ ΘΑΚ τοῦ προτεθέντος. ὁμοίως</seg>
                    <seg n="10" type="line" facs="z142r1_10">δὴ τοῖς πρότερον <choice>
                            <abbr>γεγράφθωσ</abbr>
                            <expan>γεγράφθωσαν</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z142r1_11">κύκλοι διὰ τῶν σαμείων, καθ’ ἃ</seg>
                    <seg n="12" type="line" facs="z142r1_12">τέμνουσι τὰν ἕλικα οἱ τὰς <choice>
                            <abbr>ἴσ</abbr>
                            <expan>ἴσας</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z142r1_13">γωνίας <choice>
                            <abbr>ποιοῦσ</abbr>
                            <expan>ποιοῦσαι</expan>
                        </choice> ποτὶ <seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ῶι</unclear>
                        </seg> Θ, <seg type="clearword">ὥσ</seg></seg>
                    <seg n="14" type="line" facs="z142r1_14"><seg type="wordend">τε</seg> τᾶν
                        περιφερειᾶν ἑκάστα</seg>
                    <seg n="15" type="line" facs="z142r1_15">συμπίπτειν τᾶ τε προαγευμένα</seg>
                    <seg n="16" type="line" facs="z142r1_16"><expan>καὶ</expan> τᾶ ἑπομένα· ἐσσεῖται
                        δή τι τὸ</seg>
                </seg>
                <seg n="147v2" type="folio">
                    <seg n="1" type="line" facs="z147v2_01"><seg type="word">
                            <unclear>π</unclear><supplied reason="lost">ε</supplied>ριεχόμενον</seg>
                        χωρίον ὑπό τε τᾶς</seg>
                    <seg n="2" type="line" facs="z147v2_02"><seg type="word">
                            <supplied reason="lost">Α</supplied>Β<supplied reason="lost"
                            >Γ</supplied>ΔΕ</seg> ἕλικος καὶ τᾶν αΘ ΘΕ <seg type="clearword"
                        >εὐθει</seg></seg>
                    <seg n="3" type="line" facs="z147v2_03"><seg type="wordend">ᾶν</seg>
                        περιγεγραμμένον σχᾶμα <seg type="clearword">ἐ</seg></seg>
                    <seg n="4" type="line" facs="z147v2_04"><seg type="wordend">πίπεδον</seg> ἐξ
                        ὁμοίων τομέων <seg type="clearword">συγκεί</seg></seg>
                    <seg n="5" type="line" facs="z147v2_05"><seg type="wordend">μενον</seg> καὶ ἄλλο
                        ἐγγεγραμμένον, <expan>καὶ</expan></seg>
                    <seg n="6" type="line" facs="z147v2_06">τὸ περιγεγραμμένον τοῦ <seg
                            type="clearword">ἐγγεγραμ</seg></seg>
                    <seg n="7" type="line" facs="z147v2_07"><seg type="wordend">μένου</seg> ἐλάσσονι
                        ὑπερέχει τοῦ <seg type="clearword">προ</seg></seg>
                    <seg n="8" type="line" facs="z147v2_08"><seg type="wordend">τεθέντος</seg>
                        χωρίου· ἐλάσσων γάρ <choice>
                            <abbr>ἐστι</abbr>
                            <expan>ἐστιν</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z147v2_09">ὁ ΘΑΚ τομεύς.</seg>
                </seg>
                <seg n="147v2" type="folio">
                    <seg n="9" type="line" facs="z147v2_09">ἐκ τούτου φανερόν <expan>ἐστιν</expan>,</seg>
                    <seg n="10" type="line" facs="z147v2_10"><expan>ὅτι</expan> δυνατόν ἐστιν περὶ
                        τὸ εἰρημένον</seg>
                    <seg n="11" type="line" facs="z147v2_11">χωρίον ἐπίπεδον, οἷον εἴρηται, <seg
                            type="clearword">περι</seg></seg>
                    <seg n="12" type="line" facs="z147v2_12"><seg type="wordend">γράψαι</seg>, ὥστε
                        τὸ περιγραφὲν σχᾶμα</seg>
                    <seg n="13" type="line" facs="z147v2_13">μεῖζον εἶμεν τοῦ <seg type="word"
                                >χωρ<supplied reason="lost">ί</supplied>ου</seg> ἐλάσσονι</seg>
                    <seg n="14" type="line" facs="z147v2_14">παντὸς τοῦ προτεθέντος <choice>
                            <abbr>χωρί</abbr>
                            <expan>χωρίου</expan>
                        </choice>,</seg>
                    <figure n="24.1">
                        <figDesc>Figure 24.1</figDesc>
                    </figure>
                </seg>
                <seg n="142r2" type="folio">
                    <seg n="1" type="line" facs="z142r2_01">Τὸ περιλαφθὲν χωρίον ὑπό τε τᾶς</seg>
                    <seg n="2" type="line" facs="z142r2_02">ἕλικος τᾶς ἐν τᾶι πρώται <seg
                            type="clearword">περι</seg></seg>
                    <seg n="3" type="line" facs="z142r2_03"><seg type="wordend">φορᾶι</seg>
                        γεγραμμένας καὶ τᾶς <seg type="clearword">εὐ</seg></seg>
                    <seg n="4" type="line" facs="z142r2_04"><seg type="wordend">θείας</seg> τᾶς
                        πρώτας τᾶς ἐν τᾶι</seg>
                    <seg n="5" type="line" facs="z142r2_05">ἀρχᾶι τᾶς περιφορᾶς τρίτον</seg>
                    <seg n="6" type="line" facs="z142r2_06">μέρος <expan>ἐστὶ</expan> τοῦ κύκλου τοῦ
                        πρώτου.</seg>
                </seg>
                <seg n="142r2" type="folio">
                    <seg n="7" type="line" facs="z142r2_07">ἔστω ἕλιξ, ἐφ’ ἇς ἁ ΑΒΓΔΕΘ, ἐν</seg>
                    <seg n="8" type="line" facs="z142r2_08">τᾶι <seg type="word">πρώτα<supplied
                                reason="lost">ι</supplied>
                        </seg>
                        <seg type="word">περιφορᾶ<supplied reason="lost">ι</supplied>
                        </seg>
                        <seg type="clearword">γεγραμ</seg></seg>
                    <seg n="9" type="line" facs="z142r2_09"><seg type="wordend">μένα</seg>, ἔστω δὲ
                        τὸ μὲν Θ σαμεῖον</seg>
                    <seg n="10" type="line" facs="z142r2_10">ἀρχὰ τᾶς ἕλικος, ἁ δὲ ΘΑ <seg
                            type="clearword">εὐ</seg></seg>
                    <seg n="11" type="line" facs="z142r2_11"><seg type="wordend">θεῖα</seg> πρώτα ἐν
                        τᾶι ἀρχᾶι <choice>
                            <abbr>τ</abbr>
                            <expan>τᾶς</expan>
                        </choice></seg>
                </seg>
            </ab>
        </body>
    </text>
