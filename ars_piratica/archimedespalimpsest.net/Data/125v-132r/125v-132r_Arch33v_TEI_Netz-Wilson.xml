    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="125v1" type="folio">
                    <seg n="1" type="line" facs="z125v1_01"><seg type="word">
                            <supplied reason="lost">φε</supplied>ρόμενον</seg>, πρώτα καλείσθω, ἃν</seg>
                    <seg n="2" type="line" facs="z125v1_02"><supplied reason="lost">δ’</supplied>
                        <seg type="word">
                            <supplied reason="lost">ἐ</supplied>ν</seg> τᾶι δευτέραι περιφορᾶι τὸ
                            <seg type="suppliedword">αὐ</seg></seg>
                    <seg n="3" type="line" facs="z125v1_03"><seg type="wordend">τ<supplied
                                reason="lost">ὸ</supplied></seg> σαμεῖον διανύση, δευτέρα,
                            <expan>καὶ</expan></seg>
                    <seg n="4" type="line" facs="z125v1_04">ἄλλαι ὁμοίως ταύταις <choice>
                            <abbr>ὁμωνύμ</abbr>
                            <expan>ὁμωνύμως</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z125v1_05"><seg type="word">
                            <supplied reason="lost">τα</supplied>ῖς</seg> περιφοραῖς
                    καλείσθωσαν.</seg>
                </seg>
                <seg n="125v1" type="folio">
                    <seg n="6" type="line" facs="z125v1_06">τὸ δὲ χωρίον τὸ περιλαφθὲν <seg
                            type="clearword">ὑ</seg></seg>
                    <seg n="7" type="line" facs="z125v1_07"><seg type="wordend">πό</seg> τε τᾶς
                        ἕλικος τᾶς ἐν τᾶι <seg type="clearword">πρώ</seg></seg>
                    <seg n="8" type="line" facs="z125v1_08"><seg type="wordend">ται</seg> περιφορᾶι
                        γραφείσας καὶ</seg>
                    <seg n="9" type="line" facs="z125v1_09">τᾶς εὐθείας, ἅ ἐστι πρώτα, <choice>
                            <abbr>πρῶ</abbr>
                            <expan>πρῶτον</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z125v1_10">καλείσθω, τὸ δὲ περιλαφθὲν ὑπό τε</seg>
                    <seg n="11" type="line" facs="z125v1_11">τᾶς ἕλικος τᾶς ἐν τᾶι δευτέραι</seg>
                    <seg n="12" type="line" facs="z125v1_12">περιφορᾶι γραφείσας καὶ τᾶς</seg>
                    <seg n="13" type="line" facs="z125v1_13">εὐθείας τᾶς δευτέρας <choice>
                            <abbr>δεύτερο</abbr>
                            <expan>δεύτερον</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z125v1_14">καλείσθω.</seg>
                </seg>
                <seg n="125v1" type="folio">
                    <seg n="14" type="line" facs="z125v1_14">καὶ εἴ κα ἀπὸ τοῦ <seg type="clearword"
                            >σα</seg></seg>
                    <seg n="15" type="line" facs="z125v1_15"><seg type="wordend">μείου</seg>, ὅ
                        ἐστιν ἀρχὰ τᾶς ἕλικος,</seg>
                    <seg n="16" type="line" facs="z125v1_16">ἀχθῆ τις εὐθεῖα γραμμά, τᾶς</seg>
                    <seg n="17" type="line" facs="z125v1_17">εὐθείας ταύτας ἐπὶ τὰ αὐτά,</seg>
                    <seg n="18" type="line" facs="z125v1_18">ἁ περιφορὰ γεγένηται, <seg
                            type="suppliedword">προαγό</seg>
                    </seg>
                </seg>
                <seg n="132r1" type="folio">
                    <seg n="1" type="line" facs="z132r1_01">
                        <seg type="wordend">
                            <supplied reason="lost">μενα</supplied>
                        </seg>
                        <supplied reason="lost">καλείσθω,</supplied>
                        <supplied reason="lost">τὰ</supplied>
                        <supplied reason="lost">δὲ</supplied>
                        <supplied reason="lost">ἐπὶ</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">θά</supplied>
                        </seg>
                    </seg>
                    <seg n="2" type="line" facs="z132r1_02"><seg type="wordend">τερα</seg>
                    ἑπόμενα.</seg>
                </seg>
                <seg n="132r1" type="folio">
                    <seg n="2" type="line" facs="z132r1_02">ὅ τε γραφθεὶς</seg>
                    <seg n="3" type="line" facs="z132r1_03">κύκλος κέντρωι μὲν τῶι σαμείωι,</seg>
                    <seg n="4" type="line" facs="z132r1_04">ὅ ἐστιν ἀρχὰ τᾶς ἕλικος, <seg
                            type="clearword">διαστάμα</seg></seg>
                    <seg n="5" type="line" facs="z132r1_05"><seg type="wordend">τι</seg> δὲ τᾶι
                        εὐθείαι, ἅ ἐστιν πρώται,</seg>
                    <seg n="6" type="line" facs="z132r1_06">πρώτως καλείσθω, ὁ δὲ γραφθεὶς</seg>
                    <seg n="7" type="line" facs="z132r1_07">κέντρωι μὲν τῶι αὐτῶι, <seg
                            type="clearword">διαστάμα</seg></seg>
                    <seg n="8" type="line" facs="z132r1_08"><seg type="wordend">τι</seg> δὲ τᾶι
                        διπλασίαι εὐθείαι <seg type="clearword">δεύ</seg></seg>
                    <seg n="9" type="line" facs="z132r1_09"><seg type="wordend">τερος</seg>
                        καλείσθω, καὶ οἱ ἄλλοι δὲ ἑξῆς</seg>
                    <seg n="10" type="line" facs="z132r1_10">τούτοις τὸν αὐτὸν τρόπον.</seg>
                </seg>
                <seg n="132r1" type="folio">
                    <seg n="11" type="line" facs="z132r1_11">εἴ κα ποτὶ τὰν ἕλικα τᾶι μὲν μιᾶι</seg>
                    <seg n="12" type="line" facs="z132r1_12">περιφορᾶ ὁποιαοῦν <seg
                            type="suppliedword">γεγραμμέ</seg></seg>
                    <seg n="13" type="line" facs="z132r1_13"><seg type="wordend">να<supplied
                                reason="lost">ν</supplied></seg> ἀπὸ τᾶς ἀρχᾶς τᾶς <choice>
                            <abbr>ἕλικ</abbr>
                            <expan>ἕλικος</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z132r1_14">εὐθεῖαι ἐμπεσῶντι ὁποιαιοῦν</seg>
                    <seg n="15" type="line" facs="z132r1_15">ἴσας ποιοῦσαι γωνίας ποτ’ <seg
                            type="clearword">ἀλλά</seg></seg>
                    <seg n="16" type="line" facs="z132r1_16"><seg type="wordend">λας</seg>, τῶι ἴσω
                        ὑπερέχοντι <seg type="word">
                            <choice>
                                <abbr>ἀλλαλᾶ</abbr>
                                <expan>ἀλλα<supplied reason="lost">λᾶ</supplied>ν</expan>
                            </choice>
                        </seg>.</seg>
                </seg>
                <seg n="132r1" type="folio">
                    <seg n="17" type="line" facs="z132r1_17">ἔστω <seg type="word">
                            <unclear>ἕ</unclear>λιξ</seg>, ἐφ’ ἇς αἱ ΑΒ Α<unclear>Γ</unclear> ΑΔ ΑΕ</seg>
                    <seg n="18" type="line" facs="z132r1_18">ΑΖ ἴσας <seg type="word">
                            γων<unclear>ί</unclear>ας</seg> ποιοῦσαι <seg type="clearword"
                    >πο</seg></seg>
                </seg>
                <seg n="125v2" type="folio">
                    <seg n="1" type="line" facs="z125v2_01"><seg type="wordend">τ’</seg> ἀλλάλας.
                            <seg type="word">δεικτ<supplied reason="lost"
                            >έ</supplied><unclear>ο</unclear>ν</seg> ὅτι τῶι ἴσωι</seg>
                    <seg n="2" type="line" facs="z125v2_02">ὑπερέχει ἁ ΑΓ τᾶς ΑΒ καὶ ἁ ΑΔ</seg>
                    <seg n="3" type="line" facs="z125v2_03">τᾶς ΑΓ καὶ αἱ ἄλλαι ὁμοίως.</seg>
                </seg>
                <seg n="125v2" type="folio">
                    <seg n="3" type="line" facs="z125v2_03">ἐν ὧι</seg>
                    <seg n="4" type="line" facs="z125v2_04"><expan>γὰρ</expan> χρόνωι ἁ περιαγομένα
                        γραμμὰ</seg>
                    <seg n="5" type="line" facs="z125v2_05">ἀπὸ τᾶς ΑΒ ἐπὶ τὰν ΑΓ <seg type="word">
                            <choice>
                                <abbr>ἀφικνεῖτ</abbr>
                                <expan>ἀφι<supplied reason="lost">κ</supplied>νεῖται</expan>
                            </choice>
                        </seg>,</seg>
                    <seg n="6" type="line" facs="z125v2_06">ἐν τούτω τῶι χρόνωι τὸ σαμεῖον</seg>
                    <seg n="7" type="line" facs="z125v2_07">τὸ κατὰ τᾶς εὐθείας <choice>
                            <abbr>φερόμεν</abbr>
                            <expan>φερόμενον</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z125v2_08">τὰν ὑπεροχὰν διαπορεύεται, ἇ</seg>
                    <seg n="9" type="line" facs="z125v2_09">ὑπερέχει ἁ ΓΑ ταν ΑΒ, ἐν ὧι δὲ</seg>
                    <seg n="10" type="line" facs="z125v2_10">χρόνωι ἀπὸ τᾶς ΑΓ ἐπὶ τὰν ΑΔ,</seg>
                    <seg n="11" type="line" facs="z125v2_11">ἐν τούτωι διαπορεύεται τὰν ὑπερο</seg>
                    <seg n="12" type="line" facs="z125v2_12">χάν, ἇ ὑπερέχει ἁ ΑΔ τᾶς ΑΓ. <seg
                            type="clearword">ἐ</seg></seg>
                    <seg n="13" type="line" facs="z125v2_13"><seg type="wordend">ν</seg> ἴσωι δὲ
                        χρόνωι ἁ <seg type="clearword">περιαγομέ</seg></seg>
                    <seg n="14" type="line" facs="z125v2_14"><seg type="wordend">να</seg> γραμμὰ ἀπό
                        τε τᾶς ΑΒ <seg type="clearword">ἐ</seg></seg>
                    <seg n="15" type="line" facs="z125v2_15"><seg type="wordend">πὶ</seg> τὰν ΑΓ
                        ἀφικνεῖται καὶ ἀπὸ</seg>
                    <seg n="16" type="line" facs="z125v2_16">τᾶς ΑΓ ἐπὶ τὰν ΑΔ, ἐπειδὴ αἱ <seg
                            type="clearword">γω</seg></seg>
                    <seg n="17" type="line" facs="z125v2_17"><seg type="wordend">νίαι</seg> ἴσαι
                        ἐντί· ἐν ἴσωι ἄρα <seg type="clearword">χρό</seg></seg>
                    <seg n="18" type="line" facs="z125v2_18"><seg type="wordend">νωι</seg> τὸ κατὰ
                        τᾶς εὐθείας <seg type="suppliedword">φερό</seg></seg>
                </seg>
                <seg n="132r2" type="folio">
                    <seg n="1" type="line" facs="z132r2_01">
                        <seg type="wordend">μ<supplied reason="lost"
                            >ενο</supplied><unclear>ν</unclear></seg>
                        <seg type="word">σαμ<unclear>εῖον</unclear>
                        </seg>
                        <seg type="word">διαπο<unclear>ρ</unclear><supplied reason="lost"
                                >εύ</supplied><unclear>ε</unclear>ται</seg>
                    </seg>
                    <seg n="2" type="line" facs="z132r2_02">τὰν ὑπεροχάν, ἇ ὑπερέχει ἁ <supplied
                            reason="lost">ΓΑ</supplied></seg>
                    <seg n="3" type="line" facs="z132r2_03">τᾶς ΑΒ, καὶ τὰν ὑπεροχάν, <seg
                            type="clearword">ὑπε</seg></seg>
                    <seg n="4" type="line" facs="z132r2_04"><seg type="wordend">ρέχει</seg> ἁ ΑΔ τᾶς
                        ΑΓ. τῶι ἴσωι <expan>ἄρα</expan></seg>
                    <seg n="5" type="line" facs="z132r2_05">ὑπερέχει ἅ τε ΑΓ τᾶς ΑΒ καὶ ἁ</seg>
                    <seg n="6" type="line" facs="z132r2_06"><supplied reason="lost">Α</supplied>Δ
                        τᾶς ΑΓ, καὶ αἱ λοιπαί.</seg>
                    <figure n="12.1">
                        <figDesc>Figure 12.1</figDesc>
                    </figure>
                </seg>
                <seg n="132r2" type="folio">
                    <seg n="7" type="line" facs="z132r2_07">εἴ κα εὐθεῖα γραμμὰ τᾶς ἕλικος</seg>
                    <seg n="8" type="line" facs="z132r2_08">ἐπιψαύη, καθ’ ἓν μόνον <seg
                            type="clearword">ἐπιψαύ</seg></seg>
                    <seg n="9" type="line" facs="z132r2_09"><seg type="wordend">σει</seg>
                    σαμεῖον.</seg>
                </seg>
                <seg n="132r2" type="folio">
                    <seg n="9" type="line" facs="z132r2_09">ἔστω ἕλιξ, ἐφ’ ἇς τὰ</seg>
                    <seg n="10" type="line" facs="z132r2_10">ΑΒ <unclear>Γ</unclear>Δ, ἔστω δὲ καὶ
                        ἀρχὰ μὲν τᾶς</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
