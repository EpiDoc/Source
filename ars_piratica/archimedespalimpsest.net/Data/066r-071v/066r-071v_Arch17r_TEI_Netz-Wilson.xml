    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="66r1" type="folio">
                    <seg n="1" type="line" facs="z66r1_01">ἐν τοῖς ἰσορροπικοῖς. ἔσται οὖν <w
                            part="I">ἰ</w></seg>
                    <seg n="2" type="line" facs="z66r1_02"><w part="F">σόρροπον</w> τὸ ΖΑΓ τρίγωνον
                            <w part="I">αὐ</w></seg>
                    <seg n="3" type="line" facs="z66r1_03"><w part="F">τοῦ</w> μένον τῶι ΒΑΓ τμήματι
                            <expan>κατὰ</expan></seg>
                    <seg n="4" type="line" facs="z66r1_04">τὸ Κ τεθέντι περὶ τὸ Θ κέντρον</seg>
                    <seg n="5" type="line" facs="z66r1_05">τοῦ βάρους, καί ἐστιν τοῦ ΖΑΓ <w part="I"
                            >τρι</w></seg>
                    <seg n="6" type="line" facs="z66r1_06"><w part="F">γώνου</w> κέντρον βάρους τὸ
                        Χ, <choice>
                            <abbr>ἔστι</abbr>
                            <expan>ἔστιν</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z66r1_07">ἄρα ὡς τὸ ΑΖΓ τρίγωνον πρὸς</seg>
                    <seg n="8" type="line" facs="z66r1_08">τὸ ΑΒΓ τμῆμα κείμενον περὶ τὸ</seg>
                    <seg n="9" type="line" facs="z66r1_09">Θ κέντρον, οὕτως ἡ ΘΚ πρὸς ΧΚ.</seg>
                    <seg n="10" type="line" facs="z66r1_10">τριπλασία δέ <expan>ἐστιν</expan> ἡ ΘΚ
                        τῆς ΚΧ· <seg type="suppliedword">τρ<supplied reason="lost"
                        >ι</supplied></seg></seg>
                    <seg n="11" type="line" facs="z66r1_11"><seg type="wordend">πλάσιον</seg> ἄρα
                        καὶ τὸ ΑΖΓ <choice>
                            <abbr>τρίγων</abbr>
                            <expan>τρίγωνον</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z66r1_12">τοῦ ΑΒΓ τμήματος· ἔστι δὲ καὶ</seg>
                    <seg n="13" type="line" facs="z66r1_13">τὸ ΖΑΓ τρίγωνον <choice>
                            <abbr>τετραπλάσιο</abbr>
                            <expan>τετραπλάσιον</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z66r1_14">τοῦ ΑΒΓ τριγώνου διὰ τὸ ἴσην <choice>
                            <abbr>εἶν</abbr>
                            <expan>εἶναι</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z66r1_15">τὴν μὲν ΖΚ τῆι ΚΑ, τὴν δὲ ΑΔ <seg
                            type="word">τῆ<supplied reason="lost">ι</supplied></seg></seg>
                    <seg n="16" type="line" facs="z66r1_16">ΔΓ· ἐπίτριτον ἄρα ἐστὶν τὸ ΑΒΓ <seg
                            type="suppliedword">τμῆ</seg></seg>
                    <seg n="17" type="line" facs="z66r1_17"><seg type="wordend"><supplied
                                reason="lost">μ</supplied>α</seg> τοῦ <seg type="word">Α<supplied
                                reason="lost">Β</supplied><unclear>Γ</unclear></seg> τριγώνου. τοῦτο
                        γοῦν</seg>
                    <seg n="18" type="line" facs="z66r1_18"><seg type="word"
                                ><unclear>φ</unclear>α<supplied reason="lost"
                        >νε</supplied>ρόν</seg>.</seg>
                    <figure n="1.1">
                        <figDesc xml:lang="eng">Figure 1.1</figDesc>
                    </figure>
                </seg>
                <seg n="71v1" type="folio">
                    <sic>Τ</sic>
                </seg>
                <seg n="71v1" type="folio">
                    <seg n="1" type="line" facs="z71v1_01">τοῦτο δὴ <choice>
                            <abbr>δ</abbr>
                            <expan>διὰ</expan>
                        </choice> μὲν τῶν νῦν <choice>
                            <abbr>εἰρημένω</abbr>
                            <expan>εἰρημένων</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z71v1_02">οὐκ ἀποδέδεικται, ἔμφασιν δέ</seg>
                    <seg n="3" type="line" facs="z71v1_03">τινα πεποίηκε τὸ συμπέρασμα</seg>
                    <seg n="4" type="line" facs="z71v1_04">ἀληθὲς εἶναι· διόπερ ἡμεῖς <w part="I"
                        >ὁ</w></seg>
                    <seg n="5" type="line" facs="z71v1_05"><w part="F">ρῶντες</w> μὲν οὐκ <w
                            part="I">ἀποδεδειγμέ</w></seg>
                    <seg n="6" type="line" facs="z71v1_06">
                        <w part="F">νον</w>, ὑπονοοῦντες δὲ τὸ <w part="I">συμπέ</w></seg>
                    <seg n="7" type="line" facs="z71v1_07"><w part="F">ρασμα</w> ἀληθὲς εἶναι, <w
                            part="I">τάξο</w></seg>
                    <seg n="8" type="line" facs="z71v1_08"><w part="F">μεν</w> τὴν γεωμετρουμένην <w
                            part="I">ἀ</w></seg>
                    <seg n="9" type="line" facs="z71v1_09"><w part="F">πόδειξιν</w> ἐξευρόντες <seg
                            type="word">αὐτο<unclear>ὶ</unclear></seg>
                        <choice>
                            <abbr>τὴ</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z71v1_10">ἐδοθεῖσαν πρότερον.</seg>
                </seg>
                <seg n="71v1" type="folio">
                    <seg n="10" type="line" facs="z71v1_10">ὅτι δὲ <w part="I">πᾶ</w></seg>
                    <seg n="11" type="line" facs="z71v1_11"><w part="F">σα</w> σφαῖρα διπλασία ἐστὶν
                        τοῦ</seg>
                    <seg n="12" type="line" facs="z71v1_12"><seg type="word"
                            ><unclear>κ</unclear>ώνου</seg> τοῦ βάσιν μὲν ἔχοντος</seg>
                </seg>
                <seg n="66r2" type="folio">
                    <seg n="1" type="line" facs="z66r2_01">ἴσην τῶι μεγίστωι κύκλωι τῶν ἐν</seg>
                    <seg n="2" type="line" facs="z66r2_02">τῆι σφαίραι, ὕψος δὲ ἴσον τῆι ἐκ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z66r2_03">κέντρου τῆς σφαίρας, καὶ ὁ <w part="I"
                            >κύλιν</w></seg>
                    <seg n="4" type="line" facs="z66r2_04"><w part="F">δρος</w> ὁ βάσιν μὲν ἔχων
                        ἴσην τῶι</seg>
                    <seg n="5" type="line" facs="z66r2_05">μεγίστωι κύκλωι τῶν ἐν τῆι σφαίραι,</seg>
                    <seg n="6" type="line" facs="z66r2_06">ὕψος δὲ ἴσον τῶι <choice>
                            <abbr>δμέτρωι</abbr>
                            <expan>διαμέτρωι</expan>
                        </choice> τῆς <seg type="expandedword">
                            <choice>
                                <abbr>σφ</abbr>
                                <expan>σφαί</expan>
                            </choice>
                        </seg></seg>
                    <seg n="7" type="line" facs="z66r2_07"><seg type="wordend">ρας</seg>, ἡμιόλιος
                        τῆς σφαίρας ἐστίν,</seg>
                    <seg n="8" type="line" facs="z66r2_08">ὧδε θεωρεῖται διὰ τοῦ τρόπου <seg
                            type="word">
                            <choice>
                                <abbr>το<unclear>ύ</unclear>τ</abbr>
                                <expan>το<unclear>ύ</unclear>του</expan>
                            </choice>
                        </seg>.</seg>
                </seg>
                <seg n="66r2" type="folio">
                    <seg n="9" type="line" facs="z66r2_09">ἔστω γάρ τις σφαῖρα, ἐν ἧι μέγιστος</seg>
                    <seg n="10" type="line" facs="z66r2_10">κύκλος ὁ ΑΒΓΔ, διάμετροι δὲ αἱ</seg>
                    <seg n="11" type="line" facs="z66r2_11">ΑΓ ΒΔ πρὸς ὀρθὰς ἀλλήλαις <w part="I"
                        >οὖ</w></seg>
                    <seg n="12" type="line" facs="z66r2_12"><w part="F">σαις</w>, ἔστω δὲ κύκλος ἐν
                        τῆι <w part="I">σφαί</w></seg>
                    <seg n="13" type="line" facs="z66r2_13"><w part="F">ραι</w> περὶ διάμετρον τὴν
                        ΒΔ <choice>
                            <abbr>ὀρθ</abbr>
                            <expan>ὀρθὸς</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z66r2_14">πρὸς τὸν ΑΒ ΓΔ κύκλον, καὶ ἀπὸ</seg>
                    <seg n="15" type="line" facs="z66r2_15">τοῦ ὀρθοῦ τούτου κῶνος <w part="I"
                        >ἀναγε</w></seg>
                    <seg n="16" type="line" facs="z66r2_16"><w part="F">γράφθω</w>
                        <seg type="word">κορ<unclear>υ</unclear>φὴν</seg>
                        <seg type="word">ἔχ<unclear>ω</unclear>ν</seg> τὸ Α <w part="I">ση</w></seg>
                    <seg n="17" type="line" facs="z66r2_17"><w part="F">μεῖον</w>, καὶ ἐκβληθείσης
                        τῆς <w part="I">ἐπιφα</w></seg>
                    <seg n="18" type="line" facs="z66r2_18"><w part="F">νείας</w> αὐτοῦ τετμήσθω ὁ
                        κῶνος <w part="I">ἐπι</w></seg>
                    <seg n="19" type="line" facs="z66r2_19"><w part="F">πέδωι</w>
                        <choice>
                            <abbr>δ</abbr>
                            <expan>διὰ</expan>
                        </choice> τοῦ Γ παρὰ τὴν βάσιν·</seg>
                    <seg n="20" type="line" facs="z66r2_20">ἔστιν <seg type="word">δ<supplied
                                reason="lost">ὴ</supplied></seg> ἡ τομὴ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice> ὀρθὸς <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice></seg>
                </seg>
                <seg n="71v2" type="folio">
                    <seg n="1" type="line" facs="z71v2_01">τὴν ΑΓ, καὶ διάμετρος αὐτοῦ ἡ ΕΖ.</seg>
                    <seg n="2" type="line" facs="z71v2_02">ἀπὸ δὲ τοῦ κύκλου τούτου κύλινδρος</seg>
                    <seg n="3" type="line" facs="z71v2_03">ἀναγεγράφθω ἄξονα ἔχων <choice>
                            <abbr>τὴ</abbr>
                            <expan>τὴν</expan>
                        </choice>
                    </seg>
                    <seg n="4" type="line" facs="z71v2_04">ΑΓ, πλευραὶ δὲ ἔστωσαν τοῦ <seg
                            type="suppliedword">κυλίν</seg></seg>
                    <seg n="5" type="line" facs="z71v2_05"><seg type="wordend"
                            ><unclear>δ</unclear>ρου</seg> αἱ ΕΛ ΖΥ· καὶ ἐκβεβλήσθω</seg>
                    <seg n="6" type="line" facs="z71v2_06">ἡ ΓΑ, καὶ κείσθω αὐτῆ ἴση ἡ ΑΘ,
                            <expan>καὶ</expan></seg>
                    <seg n="7" type="line" facs="z71v2_07">νοείσθω ὁ ζυγὸς ὁ ΓΘ, μέσον δὲ <w
                            part="I">αὐ</w></seg>
                    <seg n="8" type="line" facs="z71v2_08"><w part="F">τοῦ</w> τὸ Α, καὶ ἤχθω τις <choice>
                            <abbr>παράλληλ</abbr>
                            <expan>παράλληλος</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z71v2_09"><choice>
                            <abbr>τυχσα</abbr>
                            <expan>τυχοῦσα</expan>
                        </choice> τῆι ΒΔ ἡ ΜΝ, τεμνέτω</seg>
                    <seg n="10" type="line" facs="z71v2_10"><seg type="word"
                        ><unclear>δ</unclear>ὴ</seg> αὕτη τὸν μὲν Α<unclear>Β</unclear>ΓΔ κύκλον
                            <expan>κατὰ</expan></seg>
                    <seg n="11" type="line" facs="z71v2_11">τὰ ΞΟ, τὴν δὲ ΑΓ <choice>
                            <abbr>δμετρο</abbr>
                            <expan>διάμετρον</expan>
                        </choice>
                        <choice>
                            <abbr>κ</abbr>
                            <expan>κατὰ</expan>
                        </choice> τὸ Σ,</seg>
                    <seg n="12" type="line" facs="z71v2_12">τὴν δὲ ΑΕ εὐθεῖαν κατὰ τὸ
                        <unclear>Τ</unclear>, <choice>
                            <abbr>τὴ</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z71v2_13">δὲ ΑΖ <choice>
                            <abbr>κα</abbr>
                            <expan>κατὰ</expan>
                        </choice> τὸ Ρ, καὶ ἀπὸ τῆς ΜΝ</seg>
                    <seg n="14" type="line" facs="z71v2_14">εὐθείας ἐπίπεδον ἀνεστάτω</seg>
                    <seg n="15" type="line" facs="z71v2_15">ὀρθὸν πρὸς τὴν ΑΓ· <seg type="word"
                                >ποιήσ<unclear>ει</unclear></seg>
                        <seg type="word">δ<supplied reason="lost">ὴ</supplied></seg>
                        <seg type="suppliedword"><supplied reason="lost">τ</supplied>οῦ</seg></seg>
                    <seg n="16" type="line" facs="z71v2_16"><seg type="wordend">το</seg> ἐν μὲν τῶι
                        κυλίνδρωι <seg type="word">το<supplied reason="lost"
                    >μὴν</supplied></seg></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
