    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="56r1" type="folio">
                    <seg n="1" type="line" facs="z56r1_01">ἢ κατὰ τὰν ΟΠ· ὥστε <seg type="clearword">
                            <choice>
                                <abbr>ἐξωθήσο</abbr>
                                <expan>ἐξωθήσον</expan>
                            </choice>
                        </seg></seg>
                    <seg n="2" type="line" facs="z56r1_02"><seg type="wordend">ται</seg> τὰ ἧσσον
                        θλιβόμενα ὑπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z56r1_03">μᾶλλον θλιβομένων· οὐ μένει
                        <expan>ἄρα</expan></seg>
                    <seg n="4" type="line" facs="z56r1_04">τὸ ὑγρόν. ὑπέκειτο δὲ <w part="I"
                        >καθεστα</w></seg>
                    <seg n="5" type="line" facs="z56r1_05"><w part="F">κὸς</w> εἶμεν ὥστε μένειν <w
                            part="I">ἀκίνη</w></seg>
                    <seg n="6" type="line" facs="z56r1_06"><w part="F">τον·</w> ἀναγκαῖον ἄρα τὰν
                        ΑΒΓΔ</seg>
                    <seg n="7" type="line" facs="z56r1_07">γραμμὰν κύκλου περιφέρειαν <w part="I"
                        >εἶ</w></seg>
                    <seg n="8" type="line" facs="z56r1_08"><w part="F">μεν</w> καὶ κέντρον αὐτᾶς τὸ
                        Κ. <w part="I">ὁμοί</w></seg>
                    <seg n="9" type="line" facs="z56r1_09"><w part="F">ως</w> δὴ δειχθήσεται καί,
                            <sic>πως</sic> καὶ</seg>
                    <seg n="10" type="line" facs="z56r1_10">ἄλλως ἁ ἐπιφάνεια τοῦ ὑγροῦ <w part="I"
                            >ἐ</w></seg>
                    <seg n="11" type="line" facs="z56r1_11"><w part="F">πιπέδω</w> τμαθῆ διὰ τοῦ <choice>
                            <abbr>κεντρ</abbr>
                            <expan>κέντρου</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z56r1_12">τᾶς γᾶς, <expan>ὅτι</expan> ἁ τομὰ
                        ἐσσεῖται <w part="I">κύ</w></seg>
                    <seg n="13" type="line" facs="z56r1_13"><w part="F">κλου</w> περιφέρεια, καὶ
                        κέντρον</seg>
                    <seg n="14" type="line" facs="z56r1_14">αὐτᾶς ἐσσεῖται, ὃ καὶ τᾶς γᾶς</seg>
                    <seg n="15" type="line" facs="z56r1_15">ἐστι κέντρον. δῆλον οὖν, ὅτι ἁ <w
                            part="I">ἐπιφά</w></seg>
                    <seg n="16" type="line" facs="z56r1_16"><w part="F">νεια</w> τοῦ ὑγροῦ
                        καθεστακότος</seg>
                    <seg n="17" type="line" facs="z56r1_17">ἀκινήτου σφαίρας ἔχει τὸ <w part="I"
                        >σχᾶ</w></seg>
                    <seg n="18" type="line" facs="z56r1_18"><w part="F">μα</w> τὸ αὐτὸ κέντρον
                        ἐχούσας <choice>
                            <abbr>τ</abbr>
                            <expan>τᾶς</expan>
                        </choice></seg>
                    <seg n="19" type="line" facs="z56r1_19">γᾶς, ἐπειδὴ τοιαύτα ἐστίν, ὥστε</seg>
                    <seg n="20" type="line" facs="z56r1_20">
                        <seg type="word"><supplied reason="lost">τ</supplied>ε<supplied
                                reason="lost">μνομέναν</supplied></seg>
                        <seg type="word">δ<supplied reason="lost">ιὰ</supplied></seg>
                        <seg type="word">τούτ<supplied reason="lost">ου</supplied></seg>
                        <seg type="suppliedword"><supplied reason="lost">σ</supplied>αμ<supplied
                                reason="lost">εί</supplied></seg>
                    </seg>
                </seg>
                <seg n="49v1" type="folio">
                    <seg n="1" type="line" facs="z49v1_01"><seg type="wordend">ου</seg> τὰν τομὰν
                        ποιεῖν <w part="I">περιφέρει</w></seg>
                    <seg n="2" type="line" facs="z49v1_02"><w part="F">αν</w> κύκλου κέντρον ἔχοντα
                        τὸ</seg>
                    <seg n="3" type="line" facs="z49v1_03">σαμεῖον, δι᾽ οὗ τέμνεται τῶ <w part="I"
                            >ἐπιπέ</w></seg>
                    <seg n="4" type="line" facs="z49v1_04">
                        <w part="F">δω.</w>
                    </seg>
                    <figure n="1.2.1">
                        <figDesc>Figure 1.2.1</figDesc>
                    </figure>
                </seg>
                <seg n="49v1" type="folio">
                    <seg n="5" type="line" facs="z49v1_05">τῶν στερεῶν μεγεθέων τὰ</seg>
                    <seg n="6" type="line" facs="z49v1_06">ἰσοβαροῦντα τῶι ὑγρῶι <choice>
                            <abbr>αφεθ</abbr>
                            <expan>
                                <w part="I">ἀφεθέν</w>
                            </expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z49v1_07"><w part="F">τα</w> εἰς τὸ ὑγρὸν
                        καταβαροῦνται,</seg>
                    <seg n="8" type="line" facs="z49v1_08">ὥστε τᾶς ἐπιφανείας τᾶς τοῦ <w part="I"
                        >ὑ</w></seg>
                    <seg n="9" type="line" facs="z49v1_09"><w part="F">γροῦ</w> μὴ ὑπερέχειν μηθέν,
                            <expan>καὶ</expan></seg>
                    <seg n="10" type="line" facs="z49v1_10"><seg type="word"><supplied reason="lost"
                                >ο</supplied>ὐκέτι</seg> οἰσθήσονται ἐπὶ τὰ <seg n="κάτω"
                            type="suppliedword">κά</seg></seg>
                    <seg n="11" type="line" facs="z49v1_11">
                        <seg type="wordend">
                            <supplied reason="lost">τω.</supplied>
                        </seg>
                    </seg>
                </seg>
                <seg n="49v1" type="folio">
                    <seg n="11" type="line" facs="z49v1_11">ἀφείσθω γάρ τι στερεὸν <w part="I"
                        >μέ</w></seg>
                </seg>
                <seg n="56r2" type="folio">
                    <seg n="1" type="line" facs="z56r2_01"><w part="F">γεθος</w> εἰς τὸ ὑγρὸν τῶν <choice>
                            <abbr>ισοβαρεω</abbr>
                            <expan>ἰσοβαρέων</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z56r2_02">τῶι ὐγρῶι, <expan>καί,</expan> εἰ
                        δυνατόν, <w part="I">ὑπερεχέ</w></seg>
                    <seg n="3" type="line" facs="z56r2_03"><w part="F">τω</w> τι αὐτοῦ τᾶς
                            <unclear>τοῦ</unclear> ὑγροῦ <w part="I">ἐπιφα</w></seg>
                    <seg n="4" type="line" facs="z56r2_04"><w part="F">νείας,</w> καθεστάτω δὲ τὸ
                        ὑγρόν, ὥστε</seg>
                    <seg n="5" type="line" facs="z56r2_05">μένειν ἀκίνητον. νοείσθω δή τι <w
                            part="I">ἐ</w></seg>
                    <seg n="6" type="line" facs="z56r2_06"><w part="F">πίπεδον</w> ἐκβεβλημένον διά
                        τε</seg>
                    <seg n="7" type="line" facs="z56r2_07">τοῦ κέντρου τᾶς γᾶς καὶ τοῦ ὑγροῦ</seg>
                    <seg n="8" type="line" facs="z56r2_08">καὶ διὰ τοῦ στερεοῦ μεγέθεος, τομὰ</seg>
                    <seg n="9" type="line" facs="z56r2_09">ἔστω τᾶς μὲν ἐπιφανείας τοῦ <w part="I"
                        >ὑ</w></seg>
                    <seg n="10" type="line" facs="z56r2_10"><w part="F">γροῦ</w> ἁ ΑΒΓΔ περιφέρεια,
                        τοῦ</seg>
                    <seg n="11" type="line" facs="z56r2_11">δὲ στερεοῦ μεγέθεος τὸ ΕΖΗΘ <w part="I"
                            >σχᾶ</w></seg>
                    <seg n="12" type="line" facs="z56r2_12"><w part="F">μα,</w> κέντρον τε τᾶς γᾶς
                        τὸ Κ. ἔστω</seg>
                    <seg n="13" type="line" facs="z56r2_13">δὴ τοῦ μὲν στερεοῦ τὸ μὲν ΒΓ ΗΘ</seg>
                    <seg n="14" type="line" facs="z56r2_14">ἐν τῶι ὑγρῶι, τὸ δὲ ΒΕ ΖΓ ἐκτός. <w
                            part="I">νο</w></seg>
                    <seg n="15" type="line" facs="z56r2_15"><w part="F">είσθω</w> δὴ τὸ στερεὸν
                        σχῆμα <w part="I">περιλαμ</w></seg>
                    <seg n="16" type="line" facs="z56r2_16">
                        <w part="F">βανόμενον</w>
                        <sic>πυραμοειδῆ</sic>
                        <choice>
                            <abbr>βάσι</abbr>
                            <expan>βάσιν</expan>
                        </choice>
                    </seg>
                    <seg n="17" type="line" facs="z56r2_17">μὲν <seg type="word">ἔχουσ<supplied
                                reason="lost">α</supplied></seg>
                        <supplied reason="lost">τὸ</supplied>
                        <w part="I">παραλληλόγραμ</w></seg>
                    <seg n="18" type="line" facs="z56r2_18"><w part="F">μον</w> τὸ <seg type="word"
                                ><supplied reason="lost">ἐ</supplied>ν</seg> τᾶι ἐπιφανείαι τοῦ <w
                            part="I">ὑ</w></seg>
                    <seg n="19" type="line" facs="z56r2_19"><w part="F">γροῦ</w>, <seg type="word"
                                >κορυφ<unclear>ὰ</unclear><supplied reason="lost">ν</supplied></seg>
                        <supplied reason="lost">δὲ</supplied>
                        <supplied reason="lost">τὸ</supplied> κέντρον τᾶς γᾶς,</seg>
                    <seg n="20" type="line" facs="z56r2_20">τομὴ δὲ <seg type="word">
                            <supplied reason="lost">ἔσ</supplied>
                            <unclear>τ</unclear>ω</seg> τοῦ τε ἐπιπέδου, <seg type="word">ἐ<supplied
                                reason="lost">ν</supplied></seg> ὧι</seg>
                </seg>
                <seg n="49v2" type="folio">
                    <seg n="1" type="line" facs="z49v2_01">ἐστὶν ΑΒ ΓΔ περιφέρεια, καὶ <seg
                            type="word">
                            <choice>
                                <abbr>τῶ</abbr>
                                <expan>τῶ<supplied reason="lost">ν</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="2" type="line" facs="z49v2_02">τᾶς πυραμίδας ἐπιπέδων αἱ</seg>
                    <seg n="3" type="line" facs="z49v2_03">ΚΛ ΚΝ. γεγράφθω τις ἄλλας <w part="I"
                            >σφαί</w></seg>
                    <seg n="4" type="line" facs="z49v2_04"><w part="F">ρας</w> ἐπιφανείας περὶ
                        κέντρον</seg>
                    <seg n="5" type="line" facs="z49v2_05">τὸ Κ ἐν τῶι ὑγρῶι, ὧι ὑπὸ τοῦ ΕΖ ΗΘ</seg>
                    <seg n="6" type="line" facs="z49v2_06"><seg type="word">
                            <supplied reason="lost">μ</supplied>
                            <unclear>ὴ</unclear>
                        </seg> τέμνεσθαι ἐπιπέδου, λελάφθω</seg>
                    <seg n="7" type="line" facs="z49v2_07">τις <choice>
                            <abbr>και</abbr>
                            <expan>καὶ</expan>
                        </choice> ἄλλα πυραμὶς ἴσα καὶ <w part="I">ὁ</w></seg>
                    <seg n="8" type="line" facs="z49v2_08"><w part="F">μοία</w> τᾶ περιλαμβανούσαι
                        τὸ</seg>
                    <seg n="9" type="line" facs="z49v2_09">στερεὸν συνεχὴς αὐτᾶς, τομὰ δὲ</seg>
                    <seg n="10" type="line" facs="z49v2_10">ἔστω τῶν ἐπίπεδον αὐτᾶς αἱ</seg>
                    <seg n="11" type="line" facs="z49v2_11">ΚΜ ΚΝ, καὶ τῶι ὑγρῶι νοείσθω</seg>
                    <seg n="12" type="line" facs="z49v2_12">τι μέγεθος τοῦ ὑγροῦ <w part="I"
                        >ἀπολαμ</w></seg>
                    <seg n="13" type="line" facs="z49v2_13"><w part="F">βανόμενον</w> τὸ ΡΣ ΤΥ ἴσον
                        καὶ <w part="I">ὅ</w></seg>
                    <seg n="14" type="line" facs="z49v2_14"><w part="F">μοιον</w> τῶν στερεῶν κατὰ
                        τὰ</seg>
                    <seg n="15" type="line" facs="z49v2_15">ΒΗ ΘΓ, ὅ ἐστιν αὐτοῦ ἐν τῶι ὑγρῶι·</seg>
                    <seg n="16" type="line" facs="z49v2_16">τὰ δὴ μέρεα τοῦ ὑγροῦ τό τε ἐν</seg>
                    <seg n="17" type="line" facs="z49v2_17">τᾶι πρώται πυραμίδι τὰ ὑπὸ</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
