    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="149v1" type="folio">
                    <seg n="1" type="line" facs="z149v1_01"><seg type="wordend"
                            ><unclear>φανει</unclear>αι</seg> τοῦ <choice>
                            <abbr>ἐγγεγραμμέ<unclear>ν</unclear></abbr>
                            <expan>ἐγγεγραμμένου</expan>
                        </choice>·</seg>
                    <seg n="2" type="line" facs="z149v1_02">περὶ τὴν σφαῖραν ὁ δὲ Ν ἴσος τῆι</seg>
                    <seg n="3" type="line" facs="z149v1_03">ἐπιφανείαι <seg type="word"
                                >τ<unclear>ο</unclear><supplied reason="lost">ῦ</supplied></seg>
                        <seg type="word">γεγραμμένο<supplied reason="lost">υ</supplied></seg>
                        <seg type="expandedword">δύ</seg></seg>
                    <seg n="4" type="line" facs="z149v1_04"><seg type="wordend">
                            <choice>
                                <abbr>δυν<supplied reason="lost">α</supplied>τ</abbr>
                                <expan>δυν<supplied reason="lost">α</supplied>τ<supplied
                                        reason="lost">αι</supplied></expan>
                            </choice>
                        </seg>
                        <expan>ἄρα</expan> τοῦ μὲν Μ <supplied reason="lost">ἡ</supplied>
                        <seg type="word">ἐ<unclear>κ</unclear></seg>
                        <supplied reason="lost">τοῦ</supplied>
                        <choice>
                            <abbr>κέν<unclear>τ</unclear>ρ</abbr>
                            <expan>κέν<unclear>τ</unclear>ρου</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z149v1_05">τὸ <seg type="word"
                                >περιε<unclear>χ</unclear>όμεν<unclear>ον</unclear></seg>
                        <seg type="word"><unclear>ὑ</unclear>πὸ</seg> τῆς ΕΑ</seg>
                    <seg n="6" type="line" facs="z149v1_06">καὶ τῆς ἴσης <seg type="word"
                                >πά<unclear>σ</unclear>αις</seg>
                        <seg type="word">τ<unclear>αῖ</unclear><supplied reason="lost">ς</supplied></seg>
                        <w part="I">ἐπι</w></seg>
                    <seg n="7" type="line" facs="z149v1_07"><w part="F">γνυούσαις</w>
                        <seg type="word"><supplied reason="lost">τ</supplied>ὰς</seg>
                        <seg type="word">γων<supplied reason="lost">ί</supplied>ας</seg> τοῦ <w
                            part="I">πολυγώ</w></seg>
                    <seg n="8" type="line" facs="z149v1_08"><w part="F">νου</w> τοῦ περιγεγραμμένου,
                        ἡ δὲ</seg>
                    <seg n="9" type="line" facs="z149v1_09">ἐκ τοῦ <seg type="word"
                            >κέν<unclear>τ</unclear>ρου</seg> τοῦ <unclear>Ν</unclear> τὸ <seg
                            type="word">ὑπ<supplied reason="lost">ὸ</supplied></seg> τῆς ΑΚ</seg>
                    <seg n="10" type="line" facs="z149v1_10">
                        <expan>καὶ</expan> τῆς ἴσης πάσαις <choice>
                            <abbr>τσ</abbr>
                            <expan>ταῖς</expan>
                        </choice>
                        <w part="I">ἐπιζευγνυ</w></seg>
                    <seg n="11" type="line" facs="z149v1_11"><w part="F">ούσαις</w> τὰς <seg
                            type="word">γω<supplied reason="lost"
                        >ν</supplied><unclear>ί</unclear>ας</seg>. <expan>καὶ</expan>
                        <seg type="word"><unclear>ἐ</unclear>πεὶ</seg> ὅμοιά <choice>
                            <abbr>ἐστ<unclear>ι</unclear></abbr>
                            <expan>ἐστιν</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z149v1_12">τὰ πολύγωνα, ὅμοια ἂν εἴη καὶ τὰ</seg>
                    <seg n="13" type="line" facs="z149v1_13">περιεχόμενα χωρία ὑπὸ τῶν <seg
                            type="unclearword">εἰρ<unclear>η</unclear></seg></seg>
                    <seg n="14" type="line" facs="z149v1_14"><seg type="wordend">μένων</seg> γραμμῶν
                        τουτέστι τῶν <seg type="suppliedword">
                            <supplied reason="lost">ἐ</supplied>
                        </seg></seg>
                    <seg n="15" type="line" facs="z149v1_15"><seg type="wordend">πὶ</seg> τὰς γωνίας
                        ἢ τὰς πλευρὰς τῶν</seg>
                    <seg n="16" type="line" facs="z149v1_16">πολυγώνων, ὥστε τὸν αὐτὸν λόγον</seg>
                    <seg n="17" type="line" facs="z149v1_17">ἔχειν <expan>πρὸς</expan> ἄλληλας, ὃν
                        ἔχουσιν αἱ <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z149v1_18">πολυγώνων πλευραὶ δυνάμει. <choice>
                            <abbr>ἀλλ</abbr>
                            <expan>ἀλλὰ</expan>
                        </choice></seg>
                </seg>
                <seg n="154r1" type="folio">
                    <seg n="1" type="line" facs="z154r1_01"><supplied reason="lost"
                            ><expan>καὶ</expan> ὃν ἔχει</supplied> λόγον <seg type="word">τ<supplied
                                reason="lost">ὰ</supplied></seg>
                        <seg type="word"><supplied reason="lost"
                            >πε</supplied>ριεχ<unclear>ό</unclear>μενα</seg></seg>
                    <seg n="2" type="line" facs="z154r1_02">ὑπὸ τῶν εἰρημένων <seg type="word"
                                >γραμμ<unclear>ῶ</unclear>ν</seg>, <w part="I">τοῦ</w></seg>
                    <seg n="3" type="line" facs="z154r1_03"><w part="F">τον</w> ἔχουσιν αἱ ἐκ τῶν
                            <seg type="word">κέντ<unclear>ρω</unclear>ν</seg>
                        <seg type="word">τ<supplied reason="lost">ῶν</supplied></seg></seg>
                    <seg n="4" type="line" facs="z154r1_04">ΜΝ <seg type="word">κ<supplied
                                reason="lost">ύ</supplied>κλων</seg> πρὸς ἀλλήλαις δυνάμει·</seg>
                    <seg n="5" type="line" facs="z154r1_05">ὥστε καὶ αἱ τῶν ΜΝ διάμετροι τὸν</seg>
                    <seg n="6" type="line" facs="z154r1_06">αὐτὸν ἔχουσι λόγον ταῖς τῶν <w part="I"
                            >πολυγώ</w></seg>
                    <seg n="7" type="line" facs="z154r1_07"><w part="F">νων</w> πλευραῖς. οἱ δὲ
                        κύκλοι πρὸς <w part="I">ἀλ</w></seg>
                    <seg n="8" type="line" facs="z154r1_08"><w part="F">λήλους</w> διπλασίονα λόγον
                        ἔχουσιν</seg>
                    <seg n="9" type="line" facs="z154r1_09">τῶν διαμέτρων, οἵτινες ἴσοι <expan>εἰσὶν</expan>
                        <choice>
                            <abbr>τς</abbr>
                            <expan>ταῖς</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z154r1_10">
                        <seg type="word">ἐπιφ<supplied reason="lost">αν</supplied>είαις</seg> τοῦ <choice>
                            <abbr>περιγεγραμμέν</abbr>
                            <expan>περιγεγραμμένου</expan>
                        </choice>·</seg>
                    <seg n="11" type="line" facs="z154r1_11">δῆλον οὖν <expan>ὅτι</expan> ἡ
                        ἐπιφάνεια τοῦ <w part="I">περι</w></seg>
                    <seg n="12" type="line" facs="z154r1_12"><w part="F">γεγραμμένου</w> σχήματος
                        περὶ <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z154r1_13">σφαῖραν <expan>πρὸς</expan> τὴν
                        ἐπιφάνειαν τοῦ <w part="I">ἐγ</w></seg>
                    <seg n="14" type="line" facs="z154r1_14"><w part="F">γεγραμμένου</w> σχήματος
                        εἰς τὴν <seg type="suppliedword"><unclear>σφ</unclear>α<supplied
                                reason="lost">ῖ</supplied></seg></seg>
                    <seg n="15" type="line" facs="z154r1_15"><seg type="wordend">ραν</seg>
                        διπλασίονα λόγον ἔχει <choice>
                            <abbr>ἤ</abbr>
                            <expan>ἤπερ</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z154r1_16">ἡ ΕΛ <expan>πρὸς</expan> ΑΚ.</seg>
                </seg>
                <seg n="154r1" type="folio">
                    <seg n="16" type="line" facs="z154r1_16">εἰλήφθωσαν δὴ δύο</seg>
                </seg>
                <seg n="149v2" type="folio">
                    <seg n="1" type="line" facs="z149v2_01">κῶνοι οἱ ΟΞ καὶ ἔστω ὁ μὲν Ξ <choice>
                            <abbr>κῶν</abbr>
                            <expan>κῶνος</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z149v2_02">βάσιν ἔχων τοῦ Ξ κύκλον ἴσον τῶι Μ,</seg>
                    <seg n="3" type="line" facs="z149v2_03">ὁ δὲ Ο βάσιν ἔχων τὸν Ο κύκλον <choice>
                            <abbr>ἴσο</abbr>
                            <expan>ἴσον</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z149v2_04">τῶι Ν, ὕψος δὲ ὁ μὲν Ξ κῶνος τὴν</seg>
                    <seg n="5" type="line" facs="z149v2_05">ἐκ τοῦ κέντρου τῆς σφαίρας, ὁ δὲ</seg>
                    <seg n="6" type="line" facs="z149v2_06">
                        <unclear>Ο</unclear> τὴν ἀπὸ τοῦ κέντρου ἐπὶ τὴν ΑΚ</seg>
                    <seg n="7" type="line" facs="z149v2_07">κάθετον ἠγμένην· ἴσος <expan>ἄρα</expan>
                        ὁ μὲν <supplied reason="lost">Ξ</supplied></seg>
                    <seg n="8" type="line" facs="z149v2_08">
                        <seg type="word"><supplied reason="lost">κ</supplied>ῶνος</seg>
                        <seg type="word">τῶ<unclear>ι</unclear></seg> σχήματι τῶι <seg
                            type="suppliedword">περιγε</seg></seg>
                    <seg n="9" type="line" facs="z149v2_09"><seg type="wordend"><supplied
                                reason="lost">γ</supplied>ραμμένωι</seg> περὶ τὴν <seg type="word"
                                >σφαῖρ<unclear>αν</unclear></seg>,</seg>
                    <seg n="10" type="line" facs="z149v2_10">ὁ δὲ Ο τῶι ἐγγεγραμμένωι <choice>
                            <abbr>δέδεικτ</abbr>
                            <expan>δέδεικται</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z149v2_11">οὖν ταῦτα. καὶ ἐπεὶ ὅμοιά ἐστι τὰ <w
                            part="I">πο</w></seg>
                    <seg n="12" type="line" facs="z149v2_12"><w part="F">λύγωνα</w>, τὸν αὐτὸν <seg
                            type="word"><unclear>ἔχ</unclear>ει</seg> λόγον ἡ ΕΛ</seg>
                    <seg n="13" type="line" facs="z149v2_13">
                        <expan>πρὸς</expan> ΑΚ, ὃν ἡ <seg type="word">
                            <unclear>ἐ</unclear>
                            <supplied reason="lost">κ</supplied>
                        </seg> τοῦ κέντρου <seg type="word">τ<supplied reason="lost">ῆ</supplied>ς</seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">σφ</supplied>
                            <unclear>α</unclear>
                            <supplied reason="lost">ί</supplied>
                        </seg></seg>
                    <seg n="14" type="line" facs="z149v2_14"><seg type="wordend">ρας</seg>
                        <expan>πρὸς</expan> τὴν ἀπὸ τοῦ κέντρου τῆς</seg>
                    <seg n="15" type="line" facs="z149v2_15">σφαίρας ἐπὶ τὴν ΑΚ <seg type="word"
                                >κά<supplied reason="lost">θετ</supplied><unclear>ο</unclear>ν</seg>
                        <w part="I">ἀ</w></seg>
                    <seg n="16" type="line" facs="z149v2_16"><w part="F">γομένην</w>· τὸν αὐτὸν
                            <expan>ἄρα</expan> λόγον ἔχει</seg>
                    <seg n="17" type="line" facs="z149v2_17">τὸ ὕψος τοῦ Ξ κώνου <expan>πρὸς</expan>
                        <seg type="word"><supplied reason="lost">τ</supplied>ὸ</seg> ὕψος <seg
                            type="word">το<unclear>ῦ</unclear></seg>
                        <supplied reason="lost">Ο</supplied></seg>
                    <seg n="18" type="line" facs="z149v2_18">κώνου, ὃν ἡ ΕΛ <expan>πρὸς</expan> ΑΚ.
                        ἔχει δὲ καὶ <supplied reason="lost">ἡ</supplied></seg>
                    <seg n="19" type="line" facs="z149v2_19">
                        <seg type="word">διάμε<supplied reason="lost">τ</supplied>ρος</seg> τοῦ Μ
                        κύκλου <expan>πρὸς</expan>
                        <supplied reason="lost">τὴν</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">διά</supplied>
                        </seg></seg>
                </seg>
                <seg n="154r2" type="folio">
                    <seg n="1" type="line" facs="z154r2_01"><seg type="wordend">μετρον</seg> τοῦ Ν
                        κύκλου λόγον, ὃν ἔχει</seg>
                    <seg n="2" type="line" facs="z154r2_02">ἡ <supplied reason="lost">ΕΛ</supplied>
                        <expan>πρὸς</expan> ΑΚ· τῶν <expan>ἄρα</expan> ΞΟ κώνων αἱ <seg
                            type="expandedword"/></seg>
                    <seg n="3" type="line" facs="z154r2_03"><seg type="wordend">
                            <choice>
                                <abbr>μετροι</abbr>
                                <expan>διάμετροι</expan>
                            </choice>
                        </seg>
                        <seg type="word">τ<supplied reason="lost">ῶ</supplied>ν</seg>
                        <seg type="word">βάσ<supplied reason="lost">ε</supplied>ων</seg> τοῖς ὕψεσι <choice>
                            <abbr>τὸ</abbr>
                            <expan>τὸν</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z154r2_04">αὐτὸν ἔχουσι λόγον ὅμοιοι <expan>ἄρα</expan>
                        <expan>εἰσίν</expan>, <expan>καὶ</expan></seg>
                    <seg n="5" type="line" facs="z154r2_05">διὰ <seg type="word"
                            ><unclear>τ</unclear>ὸ</seg> αὐτὸ τριπλασίονα λόγον <w part="I">ἕ</w></seg>
                    <seg n="6" type="line" facs="z154r2_06"><w part="F">ξει</w>
                        <supplied reason="lost">ὁ</supplied> Ξ κῶνος <expan>πρὸς</expan> τὸν Ο κῶνον
                            <seg type="expandedword">
                            <choice>
                                <abbr>ἤ</abbr>
                                <expan>ἤπερ</expan>
                            </choice>
                        </seg></seg>
                    <seg n="7" type="line" facs="z154r2_07"><seg type="wordend"/> ἡ <choice>
                            <abbr>μετρος</abbr>
                            <expan>διάμετρος</expan>
                        </choice>
                        <seg type="word">το<unclear>ῦ</unclear></seg> Μ κύκλου <expan>πρὸς</expan>
                        <choice>
                            <abbr>τὴ</abbr>
                            <expan>τὴν</expan>
                        </choice>
                    </seg>
                    <seg n="8" type="line" facs="z154r2_08"><choice>
                            <abbr>μετρον</abbr>
                            <expan>διάμετρον</expan>
                        </choice> τοῦ <unclear>Ν</unclear> κύκλου. δῆλον <seg type="word"
                                >ο<unclear>ὖν</unclear></seg>
                        <expan>ὅτι</expan></seg>
                    <seg n="9" type="line" facs="z154r2_09">
                        <expan>καὶ</expan> τὸ σχῆμα τὸ <seg type="word">
                            <unclear>π</unclear>
                            <supplied reason="lost">εριγεγραμμένον</supplied>
                        </seg></seg>
                    <seg n="10" type="line" facs="z154r2_10">πρὸς τὸ <seg type="word"><supplied
                                reason="lost"
                            >ἐγγε</supplied>γρ<unclear>α</unclear>μμ<unclear>έ</unclear>νον</seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">τριπλα</supplied>
                        </seg></seg>
                    <seg n="11" type="line" facs="z154r2_11"><seg type="wordend">σίονα</seg>
                        <seg type="word"><unclear>λό</unclear>γο<unclear>ν</unclear></seg> ἕξει <choice>
                            <abbr>ἤ</abbr>
                            <expan>ἤπερ</expan>
                        </choice> ἡ ΕΛ <expan>πρὸς</expan> ΑΚ.</seg>
                    <seg n="12" type="line" facs="z154r2_12">
                        <choice>
                            <abbr>ἑξ</abbr>
                            <expan>ἑξῆς</expan>
                        </choice> Ἡ <choice>
                            <abbr>Κ</abbr>
                            <expan>Κατὰ</expan>
                        </choice> ΓΡΑΦΉ</seg>
                </seg>
                <seg n="154r2" type="folio">
                    <seg n="13" type="line" facs="z154r2_13">πάσης <seg type="word"
                                ><unclear>σ</unclear>φ<unclear>α</unclear>ίρ<unclear>α</unclear>ς</seg>
                        ἡ ἐπιφάνεια </seg>
                    <seg n="14" type="line" facs="z154r2_14">τετραπαλσία ἐστὶ τοῦ μεγίστου <seg
                            type="suppliedword">κύ</seg></seg>
                    <seg n="15" type="line" facs="z154r2_15">
                        <seg type="wordend"><supplied reason="lost">κ</supplied>λ<supplied
                                reason="lost">ου</supplied></seg> τῶν <seg type="word"
                            >ἐ<unclear>ν</unclear></seg>
                        <supplied reason="lost">αὐτῆι</supplied>. </seg>
                </seg>
                <seg n="154r2" type="folio">
                    <seg n="15" type="line" facs="z154r2_15">ἔστω γὰρ <w part="I">σφαῖ</w></seg>
                    <seg n="16" type="line" facs="z154r2_16"><w part="F">ρά</w> τις <supplied
                            reason="lost">καὶ</supplied>
                        <seg type="word">ἔστ<supplied reason="lost">ω</supplied></seg>
                        <seg type="word"><supplied reason="lost">τ</supplied>ετραπλάσιος</seg></seg>
                </seg>
            </ab>
        </body>
    </text>
