    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="116r1" type="folio">
                    <seg n="1" type="line" facs="z116r1_01">σὺν τῶι κώνωι τριπλασίονα</seg>
                    <seg n="2" type="line" facs="z116r1_02">λόγον ἔχει τοῦ αὐτοῦ.</seg>
                </seg>
                <seg n="116r1" type="folio">
                    <seg n="2" type="line" facs="z116r1_02">ἔστω <expan>γὰρ</expan> ὁ Μ</seg>
                    <seg n="3" type="line" facs="z116r1_03">κύκλος, οὗ ἡ ἐκ τοῦ κέντρου <choice>
                            <abbr>ισ</abbr>
                            <expan>ἴσον</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z116r1_04">δύναται τῶι ὑπό τε μιᾶς <w part="I"
                        >πλευ</w></seg>
                    <seg n="5" type="line" facs="z116r1_05"><w part="F">ρᾶς</w> τοῦ περιγεγραμμένον
                            <w part="I">πο</w></seg>
                    <seg n="6" type="line" facs="z116r1_06"><w part="F">λυγώνον</w> καὶ πασῶν τῶν <w
                            part="I">ἐπι</w></seg>
                    <seg n="7" type="line" facs="z116r1_07"><w part="F">ζευγνυουσῶν</w> τὰς γωνίας
                        καὶ</seg>
                    <seg n="8" type="line" facs="z116r1_08">ἔτι τῆς ἡμισείας τῆς ΕΖ· ἔσται δὴ</seg>
                    <seg n="9" type="line" facs="z116r1_09">ὁ Μ κύκλος ἴσος τῆι ἐπιφανείαι</seg>
                    <seg n="10" type="line" facs="z116r1_10">τοῦ περιγεγραμμένου <choice>
                            <abbr>σχημ<unclear>α</unclear><supplied reason="lost">τ</supplied></abbr>
                            <expan>σχήματος</expan>
                        </choice>.</seg>
                    <seg n="11" type="line" facs="z116r1_11">εἰλήφθω <seg type="word">
                            <unclear>δ</unclear>
                            <supplied reason="lost">ὴ</supplied>
                        </seg> καὶ ὁ Ν κύκλος, οὗ ἡ</seg>
                    <seg n="12" type="line" facs="z116r1_12">ἐκ τοῦ κέντρου ἴσον <choice>
                            <abbr>δυνατ</abbr>
                            <expan>δύναται</expan>
                        </choice> τῶι</seg>
                    <seg n="13" type="line" facs="z116r1_13">περιεχομένωι ὑπό τε μιᾶς <w part="I"
                            >πλευ</w></seg>
                    <seg n="14" type="line" facs="z116r1_14"><w part="F">ρᾶς</w> τοῦ ἐγγεγραμμένου
                            <w part="I">πολυγώ</w></seg>
                    <seg n="15" type="line" facs="z116r1_15"><w part="F">νου</w> καὶ πασῶν τῶν <seg
                            type="unclearword">ἐπιζευγνυ</seg></seg>
                    <seg n="16" type="line" facs="z116r1_16"><seg type="wordend"
                            ><unclear>ο</unclear>υσῶν</seg> τὰς γωνίας σὺν τῆι <w part="I">ἡμι</w></seg>
                    <seg n="17" type="line" facs="z116r1_17"><w part="F">σείαι</w> τῆς ΑΓ· <choice>
                            <abbr>εστ</abbr>
                            <expan>ἔσται</expan>
                        </choice> δὴ καὶ οὗτος <choice>
                            <abbr>ισ</abbr>
                            <expan>ἴσος</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z116r1_18">τῆι ἐπιφανείαι τοῦ <w part="I"
                            >ἐγγεγραμμέ</w></seg>
                    <seg n="19" type="line" facs="z116r1_19"><w part="F">νου</w>
                        <seg type="word">σ<unclear>χ</unclear>ήματος</seg>. ἀλλὰ τὰ <w part="I"
                            >εἰρημέ</w></seg>
                </seg>
                <seg n="111v1" type="folio">
                    <seg n="1" type="line" facs="z111v1_01"><w part="F">να</w>
                        <seg type="word">χωρ<unclear>ί</unclear>α</seg> ἐστὶ <expan>πρὸς</expan>
                        ἄλληλα <expan>ὡς</expan> τὸ <seg type="word"><supplied reason="lost"
                            >ἀ</supplied>πὸ</seg></seg>
                    <seg n="2" type="line" facs="z111v1_02">τῆς ΕΚ πλευρᾶς <expan>πρὸς</expan> τὸ
                        ἀπὸ τῆς</seg>
                    <seg n="3" type="line" facs="z111v1_03">ΑΛ πλευρᾶς <expan>καὶ</expan>
                        <expan>ὡς</expan>
                        <choice>
                            <abbr>π</abbr>
                            <expan>παρὰ</expan>
                        </choice> τὸ <choice>
                            <abbr>πολυγων</abbr>
                            <expan>πολύγωνον</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z111v1_04"><expan>πρὸς</expan> τὸ πολύγωνον, ὁ Μ
                            <expan>κύκλος</expan>
                        <expan>πρὸς</expan> τὸν Ν <expan>κύκλον</expan>·</seg>
                    <seg n="5" type="line" facs="z111v1_05">φανερὸν οὖν <expan>ὅτι</expan>
                        <expan>καὶ</expan> ἡ ἐπιφάνεια</seg>
                    <seg n="6" type="line" facs="z111v1_06">τοῦ περιγεγραμμένου <choice>
                            <abbr>σχημα</abbr>
                            <expan>σχήματος</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z111v1_07">διπλασίονα λόγον ἔχει <choice>
                            <abbr>η</abbr>
                            <expan>ἤπερ</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z111v1_08">ἡ ΕΚ <expan>πρὸς</expan> ΑΛ τὸν δὲ
                        αὐτόν, ὃν <expan>καὶ</expan> τὸ</seg>
                    <seg n="9" type="line" facs="z111v1_09">πολύγωνον.</seg>
                </seg>
                <seg n="111v1" type="folio">
                    <seg n="9" type="line" facs="z111v1_09">ἔστω πάλιν κῶνος</seg>
                    <seg n="10" type="line" facs="z111v1_10">ὁ Ξ βάσιν μὲν ἔχων τῶι Μ ἴσην,</seg>
                    <seg n="11" type="line" facs="z111v1_11">ὕψος δὲ τὴν ἐκ τοῦ κέντρου τῆς <seg
                            type="suppliedword">ἐ</seg></seg>
                    <seg n="12" type="line" facs="z111v1_12"><seg type="wordend"><supplied
                                reason="lost">λάσ</supplied>σονος</seg> σφαίρας· ἴσος δὴ <seg
                            type="suppliedword">οὗ</seg></seg>
                    <seg n="13" type="line" facs="z111v1_13"><seg type="wordend">
                            <supplied reason="lost">τός</supplied>
                        </seg>
                        <supplied reason="lost">
                            <expan>ἐστιν</expan>
                        </supplied> ὁ κῶνος τῶι <w part="I">περιγεγραμ</w></seg>
                    <seg n="14" type="line" facs="z111v1_14"><w part="F">μένωι</w> σχήματι σὺν τῶι
                        κώνωι,</seg>
                    <seg n="15" type="line" facs="z111v1_15">οὗ βάσις ὁ περὶ τὴν ΕΖ κύκλος,</seg>
                    <seg n="16" type="line" facs="z111v1_16">κορυφὴ δὲ τὸ Δ. καὶ ἔστω <choice>
                            <abbr>αλλ</abbr>
                            <expan>ἄλλος</expan>
                        </choice></seg>
                </seg>
                <seg n="116r2" type="folio">
                    <seg n="1" type="line" facs="z116r2_01">κῶνος ὁ Ο βάσιν <seg type="word"
                                >μ<supplied reason="lost">ὲν</supplied></seg>
                        <supplied reason="lost">ἴσην ἔχων</supplied></seg>
                    <seg n="2" type="line" facs="z116r2_02">τῶι Ν, ὕψος δὲ τὴν ἀπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> Δ <supplied reason="lost">ἐπὶ τὴν</supplied></seg>
                    <seg n="3" type="line" facs="z116r2_03">ΑΛ κάθετον ἠγμένην· <choice>
                            <abbr>εστ</abbr>
                            <expan>ἔσται</expan>
                        </choice>
                        <seg type="word">δ<supplied reason="lost">ὴ</supplied></seg>
                        <supplied reason="lost">καὶ</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">οὗ</supplied>
                        </seg></seg>
                    <seg n="4" type="line" facs="z116r2_04"><seg type="wordend">τος</seg> ἴσος τῶι
                            <seg type="word">ἐγγεγραμμένω<supplied reason="lost">ι</supplied></seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">σχή</supplied>
                        </seg></seg>
                    <seg n="5" type="line" facs="z116r2_05"><seg type="wordend">ματι</seg> σὺν τῶι
                        κώνωι, οὗ βάσις <supplied reason="lost">ὁ</supplied></seg>
                    <seg n="6" type="line" facs="z116r2_06"><seg type="word"
                                ><unclear>π</unclear>ε<unclear>ρὶ</unclear></seg> διάμετρον τὴν ΑΓ
                        κύκλος,</seg>
                    <seg n="7" type="line" facs="z116r2_07">κορυφὴ δὲ τὸ Δ κέντρον· <seg type="word"
                                >ταῦ<unclear>τ</unclear><supplied reason="lost">α</supplied></seg>
                        <supplied reason="lost">
                            <expan>γὰρ</expan>
                        </supplied></seg>
                    <seg n="8" type="line" facs="z116r2_08">πάντα <choice>
                            <abbr>προγεγραπτ</abbr>
                            <expan>προγέγραπται</expan>
                        </choice>. <expan>καὶ</expan> ἐπεί <expan>ἐστιν</expan>
                        <seg type="word">ὡ<supplied reason="lost">ς</supplied></seg></seg>
                    <seg n="9" type="line" facs="z116r2_09">ἡ ΕΚ <expan>πρὸς</expan> τὴν ἐκ τοῦ
                        κέντρου τῆς</seg>
                    <seg n="10" type="line" facs="z116r2_10">ἐλάσσονος σφαίρας <expan>οὕτως</expan>
                        ἡ ΑΛ <supplied reason="lost">
                            <expan>πρὸς</expan>
                        </supplied></seg>
                    <seg n="11" type="line" facs="z116r2_11">τὴν ἀπὸ τοῦ κέντρου τοῦ Δ ἐπὶ <choice>
                            <abbr>
                                <supplied reason="lost">τ</supplied>
                            </abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z116r2_12">ΑΛ κάθετον ἠγμένην, ἐδείχθη <supplied
                            reason="lost">δὲ</supplied></seg>
                    <seg n="13" type="line" facs="z116r2_13">ὡς ἡ ΕΚ <expan>πρὸς</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice> ΑΛ <expan>οὕτως</expan> ἡ ἐκ τοῦ <choice>
                            <abbr>κεν<supplied reason="lost">τρ</supplied></abbr>
                            <expan>κέντρου</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z116r2_14"><choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice>
                        <unclear>Μ</unclear>
                        <seg type="word"><supplied reason="lost">κύκ</supplied>λου</seg> καὶ ἡ <choice>
                            <abbr>μετρος</abbr>
                            <expan>διάμετρος</expan>
                        </choice>
                        <expan>πρὸς</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z116r2_15"><choice>
                            <abbr>μετρον</abbr>
                            <expan>διάμετρον</expan>
                        </choice>· ἔσται <expan>ἄρα</expan> ὡς ἡ <choice>
                            <abbr>μετρο<supplied reason="lost">ς</supplied></abbr>
                            <expan>διάμετρος</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z116r2_16">τοῦ κύκλου, <choice>
                            <abbr>ο</abbr>
                            <expan>ὅς</expan>
                        </choice>
                        <expan>ἐστι</expan> βάσις τοῦ Ξ, <supplied reason="lost">
                            <expan>πρὸς</expan>
                        </supplied>
                        <choice>
                            <abbr>
                                <supplied reason="lost">τ</supplied>
                            </abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z116r2_17"><choice>
                            <abbr>μετρον</abbr>
                            <expan>διάμετρον</expan>
                        </choice>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> κύκλου, ὅς <expan>ἐστι</expan>
                        <seg type="word">βάσι<supplied reason="lost">ς</supplied></seg></seg>
                    <seg n="18" type="line" facs="z116r2_18">τοῦ Ο, <expan>οὕτως</expan> τὸ ὕψος <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> Ξ κώνου <expan>πρὸς</expan>
                        <seg type="word">τ<supplied reason="lost">ὸ</supplied></seg></seg>
                    <seg n="19" type="line" facs="z116r2_19">ὕψος τὸ <supplied reason="lost"
                        >Ο</supplied> κώνου ὅμοιοι <expan>ἄρα</expan>
                        <expan>εἰσὶν</expan>
                        <supplied reason="lost">οἱ</supplied></seg>
                </seg>
                <seg n="111v2" type="folio">
                    <seg n="1" type="line" facs="z111v2_01"><seg type="word"><supplied reason="lost"
                                >κ</supplied>ῶνοι</seg>. ὁ Ξ <expan>ἄρα</expan> κῶνος
                        <expan>πρὸς</expan> τὸν Ο <seg type="suppliedword">κ<supplied reason="lost"
                                >ῶ</supplied></seg></seg>
                    <seg n="2" type="line" facs="z111v2_02"><seg type="wordend">νον</seg>
                        τριπλασίονα λόγον ἔχει <choice>
                            <abbr>η</abbr>
                            <expan>ἤπερ</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z111v2_03">ἡ <choice>
                            <abbr>μετρος</abbr>
                            <expan>διάμετρος</expan>
                        </choice>
                        <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice> τὴν διάμετρον· <w part="I">φα</w></seg>
                    <seg n="4" type="line" facs="z111v2_04"><w part="F">νερὸν</w> οὖν
                        <expan>ὅτι</expan> καὶ τὸ σχῆμα τὸ <w part="I">περι</w></seg>
                    <seg n="5" type="line" facs="z111v2_05"><w part="F">γεγραμμένον</w> σὺν τῶι
                        κώνωι</seg>
                    <seg n="6" type="line" facs="z111v2_06">
                        <expan>πρὸς</expan> τὸ ἐγγεγραμμένον σὺν τῶι <w part="I">κώ</w></seg>
                    <seg n="7" type="line" facs="z111v2_07"><w part="F">νωι</w> τριπλασίονα <choice>
                            <abbr>λογο</abbr>
                            <expan>λόγον</expan>
                        </choice> ἔχει <choice>
                            <abbr>η</abbr>
                            <expan>ἤπερ</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z111v2_08">ἡ ΕΚ <expan>πρὸς</expan> ΑΛ.</seg>
                </seg>
                <seg n="111v2" type="folio">
                    <num>Μ</num>
                </seg>
                <seg n="111v2" type="folio">
                    <seg n="9" type="line" facs="z111v2_09">παντὸς τμήματος σφαίρας <w part="I"
                        >ἐλάσ</w></seg>
                    <seg n="10" type="line" facs="z111v2_10"><w part="F">σονος</w> ἡμισφαιρίου ἡ <w
                            part="I">ἐπιφά</w></seg>
                    <seg n="11" type="line" facs="z111v2_11"><w part="F">νεια</w> ἴση
                        <expan>ἐστὶ</expan> κύκλωι, οὗ ἡ ἐκ <seg type="suppliedword">τού</seg></seg>
                    <figure n="1.41.1">
                        <figDesc>Figure 1.41.1.</figDesc>
                    </figure>
                </seg>
            </ab>
