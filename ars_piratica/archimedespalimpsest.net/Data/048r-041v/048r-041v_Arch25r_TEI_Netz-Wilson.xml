    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="48r1" type="folio">
                    <seg n="1" type="line" facs="z48r1_01">Μ κύλινδρον, <expan>οὕτως</expan> ἡ ΘΑ
                            <expan>πρὸς</expan> ΑΦ, <choice>
                            <abbr>τουτ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z48r1_02">ἡ ΓΑ <expan>πρὸς</expan> ΑΦ. καί
                            <expan>ἐστι</expan> τῶ Ε<unclear>Α</unclear>Ζ κώνω <w part="I">ἴ</w></seg>
                    <seg n="3" type="line" facs="z48r1_03"><w part="F">σος</w> ὁ ΜΝ κύλινδρος
                            <expan>πρὸς</expan> τὸν Ν <seg type="unclearword"
                        >κ<unclear>ύ</unclear></seg></seg>
                    <seg n="4" type="line" facs="z48r1_04"><seg type="wordend">λινδρον</seg>, οὕτως
                        ἡ ΑΓ <expan>πρὸς</expan> ΓΦ. καί <expan>ἐστιν</expan></seg>
                    <seg n="5" type="line" facs="z48r1_05">ἴσος ὁ ΜΝ κύλινδρος τῶι ΕΑΖ <seg
                            type="unclearword">κ<unclear>ώ</unclear></seg></seg>
                    <seg n="6" type="line" facs="z48r1_06"><seg type="wordend">νωι</seg>· <seg
                            type="word">
                            <unclear>ὡ</unclear>
                            <supplied reason="lost">ς</supplied>
                        </seg>
                        <unclear>
                            <expan>ἄρα</expan>
                        </unclear> ὁ ΕΑΖ κῶνος <expan>πρὸς</expan> τὸν Ν</seg>
                    <seg n="7" type="line" facs="z48r1_07">κύλινδρον, οὕτως ἡ ΓΑ <expan>πρὸς</expan>
                        ΓΦ, <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>του</expan>
                            </choice>
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τέστιν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="8" type="line" facs="z48r1_08"><supplied reason="lost">ἡ</supplied>
                        <supplied reason="lost">Θ</supplied>Α <expan>πρὸς</expan>
                        <supplied reason="lost">Γ</supplied><unclear>Φ</unclear>. ἐδείχθη δὲ καὶ ὡς
                        τὸ Β</seg>
                    <seg n="9" type="line" facs="z48r1_09">ΑΔ τμῆμα πρὸς τὸν ΕΑΖ κῶνον,
                        <expan>οὕτως</expan></seg>
                    <seg n="10" type="line" facs="z48r1_10">ἡ ΓΦ <expan>πρὸς</expan> ΧΑ· δι᾽ <seg
                            type="word">
                            <choice>
                                <abbr><supplied reason="lost">ο</supplied>υ</abbr>
                                <expan><supplied reason="lost">ἴσο</supplied>υ</expan>
                            </choice>
                        </seg>
                        <expan>ἄρα</expan> ἔσται ὡς τὸ ΑΒΔ</seg>
                    <seg n="11" type="line" facs="z48r1_11">τμῆμα <expan>πρὸς</expan> τὸν Ν
                        κύλινδρον, <expan>οὕτως</expan> ἡ</seg>
                    <seg n="12" type="line" facs="z48r1_12">ΘΑ <expan>πρὸς</expan> ΑΧ. καὶ ἐδείχθη
                        ἰσόρροπον</seg>
                    <seg n="13" type="line" facs="z48r1_13">τὸ ΒΑΔ τμῆμα τῶι Ν κυλίνδρωι</seg>
                    <seg n="14" type="line" facs="z48r1_14">κατὰ τὸ Α, καί ἐστι τοῦ Ν <choice>
                            <abbr>κυλίνδρ</abbr>
                            <expan>κυλίνδρου</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z48r1_15">κέντρον βάρους τὸ Θ· καὶ τοῦ ΒΑΔ</seg>
                    <seg n="16" type="line" facs="z48r1_16">τμήματος κέντρον τὸ Χ. ἑξῆς</seg>
                    <seg n="17" type="line" facs="z48r1_17">τὸ <choice>
                            <abbr>ΣΧ</abbr>
                            <expan>ΣΧΗΜΑ</expan>
                        </choice>.</seg>
                    <figure n="9.1">
                        <figDesc xml:lang="eng">Figure 9.1.</figDesc>
                    </figure>
                </seg>
                <seg n="48r2" type="folio">
                    <seg n="1" type="line" facs="z48r2_01">ὁμοίως δὲ τούτοις θεωρεῖται <supplied
                            reason="lost">καὶ</supplied>
                        <supplied reason="lost">
                            <expan>ὅτι</expan>
                        </supplied></seg>
                    <seg n="2" type="line" facs="z48r2_02">παντὸς τμήματος <seg type="word"
                                >σφαιροειδ<supplied reason="lost"
                        >έ</supplied>ο<unclear>ς</unclear></seg></seg>
                    <seg n="3" type="line" facs="z48r2_03">τὸ κέντρον ἐστὶν τοῦ βάρους <supplied
                            reason="lost">ἐπὶ</supplied>
                        <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">ῆς</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="4" type="line" facs="z48r2_04">εὐθείας, ἥ <expan>ἐστιν</expan> ἄξων τοῦ
                            <seg type="word">
                            <choice>
                                <abbr>τμήματ</abbr>
                                <expan>τμήματ<unclear>ος</unclear></expan>
                            </choice>
                        </seg>,</seg>
                    <seg n="5" type="line" facs="z48r2_05">διηρημένης τῆς εὐθείας, <seg type="word"
                                ><supplied reason="lost">ὥ</supplied>στε</seg></seg>
                    <seg n="6" type="line" facs="z48r2_06">τὸ μέρος αὐτῆς τὸ πρὸς τῆ <seg
                            type="suppliedword"><supplied reason="lost">κ</supplied>ο</seg></seg>
                    <seg n="7" type="line" facs="z48r2_07"><seg type="wordend">ρυφῆι</seg> τοῦ
                        τμήματος <expan>πρὸς</expan> τὸ <seg type="word">
                            <choice>
                                <abbr>λοι<supplied reason="lost">π</supplied></abbr>
                                <expan>λοι<supplied reason="lost">πὸν</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="8" type="line" facs="z48r2_08">τοῦτον ἔχειν τὸν λόγον, ὃν ἔχει <w
                            part="I">συ</w></seg>
                    <seg n="9" type="line" facs="z48r2_09"><w part="F">ναμφότερος</w> ὅ τε ἄξων τοῦ
                            <w part="I">τμή</w></seg>
                    <seg n="10" type="line" facs="z48r2_10"><w part="F">ματος</w> καὶ ἡ τετραπλασία <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z48r2_11">ἄξονος <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> ἐν τῶι <seg type="word">ἀντικειμέν<unclear>ωι</unclear></seg></seg>
                    <seg n="12" type="line" facs="z48r2_12">τμήματι <expan>πρὸς</expan> συναμφότερον
                        τόν</seg>
                    <seg n="13" type="line" facs="z48r2_13">τε ἄξονα τοῦ τμήματος καὶ <choice>
                            <abbr>τὴ</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z48r2_14">διπλασίαν <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> ἄξονος τοῦ ἐν <choice>
                            <abbr>τ</abbr>
                            <expan>τῶ</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z48r2_15">ἀντικειμένωι τμήματι ἐν <seg type="word"
                                >τῆ<supplied reason="lost">ι</supplied>
                            <seg type="suppliedword">
                                <supplied reason="lost">ἐ</supplied>
                            </seg></seg>
                    </seg>
                    <seg n="16" type="line" facs="z48r2_16"><seg type="wordend">χομένηι</seg>.</seg>
                </seg>
                <seg n="48r2" type="folio">
                    <seg n="16" type="line" facs="z48r2_16">θεωρεῖται δὲ <choice>
                            <abbr>δ</abbr>
                            <expan>διὰ</expan>
                        </choice>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice>
                        <choice>
                            <abbr>τρόπ</abbr>
                            <expan>τρόπου</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z48r2_17">τούτου καὶ διότι παντὸς <seg
                            type="suppliedword">ἀμβλυγω</seg>
                    </seg>
                </seg>
                <seg n="41v2" type="folio">
                    <seg n="1" type="line" facs="z41v2_01"><seg type="wordend">ν<supplied
                                reason="lost">ίου</supplied></seg>
                        <unclear>
                            <expan>πρὸς</expan>
                        </unclear>
                        <seg type="word">
                            <unclear>τ</unclear>
                            <supplied reason="lost">ὸ</supplied>
                            <unclear>ν</unclear>
                        </seg>
                        <seg type="word"><supplied reason="lost"
                        >κῶ</supplied>ν<unclear>ο</unclear>ν</seg> τὸν βάσιν <seg
                            type="suppliedword">ἔχον</seg></seg>
                    <seg n="2" type="line" facs="z41v2_02"><seg type="wordend">
                            <unclear>τ</unclear>
                            <supplied reason="lost">α</supplied>
                        </seg> τὰν αὐτὰν τῶι τμήματι <unclear>
                            <expan>καὶ</expan>
                        </unclear></seg>
                    <seg n="3" type="line" facs="z41v2_03"><seg type="word"
                        ><unclear>ἄ</unclear>ξονα</seg> τὸν <seg type="word"
                            >α<unclear>ὐ</unclear>τὸν</seg> τοῦτον ἔχει <seg type="word">
                            <choice>
                                <abbr>λόγο</abbr>
                                <expan>λόγο<unclear>ν</unclear></expan>
                            </choice>
                        </seg>,</seg>
                    <seg n="4" type="line" facs="z41v2_04">ὃν ἔχει συναμφότερος ὅ τε <choice>
                            <abbr>ἄξω</abbr>
                            <expan>ἄξων</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z41v2_05">τοῦ τμήματος καὶ ἡ τριπλασία</seg>
                    <seg n="6" type="line" facs="z41v2_06">τῆς προσούσης τῶι ἄξονι <expan>πρὸς</expan>
                        <w part="I">συ</w></seg>
                    <seg n="7" type="line" facs="z41v2_07"><w part="F">ναμφότερον</w> τόν τε ἄξονα
                        τοῦ <w part="I">τμή</w></seg>
                    <seg n="8" type="line" facs="z41v2_08"><w part="F">ματος</w> τοῦ κωνοειδοῦς καὶ
                        τὴν <w part="I">δι</w></seg>
                    <seg n="9" type="line" facs="z41v2_09"><w part="F">πλασίαν</w> τῆς προσούσης τῶι
                            <w part="I">ἄξο</w></seg>
                    <seg n="10" type="line" facs="z41v2_10"><w part="F">νι</w>, κέντρον δὲ <seg
                            type="word">τ<unclear>ο</unclear>ῦ</seg> βάρους τοῦ <w part="I"
                        >ἀμβλυ</w></seg>
                    <seg n="11" type="line" facs="z41v2_11"><w part="F">γωνίου</w> κωνοειδέος
                        τμηθέντος</seg>
                    <seg n="12" type="line" facs="z41v2_12"><seg type="word">τ<supplied
                                reason="lost">οῦ</supplied></seg>
                        <seg type="word"><unclear>ἄ</unclear><supplied reason="lost"
                                >ξ</supplied><unclear>ον</unclear>ος</seg> αὐτοῦ <seg type="word"
                                >ὥ<unclear>στ</unclear>ε</seg> τὸ <expan>πρὸς</expan> τῆι</seg>
                    <seg n="13" type="line" facs="z41v2_13"><seg type="word"
                            >κορ<unclear>υ</unclear>φῆι</seg>
                        <seg type="word"><unclear>τμ</unclear><supplied reason="lost">ῆ</supplied>μα</seg>
                        <expan>πρὸς</expan> τὸν λοιπὸν <w part="I">λό</w></seg>
                    <seg n="14" type="line" facs="z41v2_14"><w part="F">γον</w> ἔχει, <seg
                            type="word">ὃ<supplied reason="lost">ν</supplied></seg>
                        <supplied reason="lost">ἔχει</supplied> ἡ τετραπλασία</seg>
                    <seg n="15" type="line" facs="z41v2_15">τοῦ <seg type="word"
                                >ἄξο<unclear>ν</unclear><supplied reason="lost">ος</supplied></seg>
                        <seg type="word"><supplied reason="lost">κ</supplied>αὶ</seg>
                    ὀκταπλασία</seg>
                </seg>
            </ab>
