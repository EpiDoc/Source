    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="45r1" type="folio">
                    <seg n="1" type="line" facs="z45r1_01"><seg type="wordend"
                            >ειδέο<unclear>ς</unclear></seg>
                        <seg type="word"><supplied reason="lost"
                                >με</supplied>τενεχθ<unclear>έν</unclear><supplied reason="lost"
                            >το</supplied>ς</seg> τοῦ ζυγοῦ</seg>
                    <seg n="2" type="line" facs="z45r1_02">κατὰ τὸ Θ οὕτως, ὥστε κέντρον <choice>
                            <abbr>εἶν</abbr>
                            <expan>εἶναι</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z45r1_03">αὐτοῦ τοῦ βάρους τὸ Θ. <w part="I"
                            >συμπληρω</w></seg>
                    <seg n="4" type="line" facs="z45r1_04"><w part="F">θέντος</w> οὖν τοῦ κυλίνδρου
                        καὶ τοῦ</seg>
                    <seg n="5" type="line" facs="z45r1_05">τμήματος τοῦ ὀρθογωνίου <seg
                            n="κωνοειδέος" type="suppliedword">κωνο</seg></seg>
                    <seg n="6" type="line" facs="z45r1_06"><seg type="wordend">ειδέ<supplied
                                reason="lost">ο</supplied>ς</seg> ἰσορροπήσει περὶ τὸ Α <w part="I"
                            >ση</w></seg>
                    <seg n="7" type="line" facs="z45r1_07"><w part="F">μεῖον</w> ὁ κύλινδρος αὐτοῦ
                        μένων <choice>
                            <abbr>τ</abbr>
                            <expan>τῶ</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z45r1_08">τμήματι τοῦ ὀρθογωνίου <w part="I"
                        >κωνοει</w></seg>
                    <seg n="9" type="line" facs="z45r1_09"><w part="F">δέος</w> μετενεχθέντι καὶ
                        τεθέντι</seg>
                    <seg n="10" type="line" facs="z45r1_10">τοῦ ζυγοῦ κατὰ τὸ Θ
                        <expan>οὕτως</expan>, ὥστε <seg type="expandedword">
                            <choice>
                                <abbr>κέ</abbr>
                                <expan>κέν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="11" type="line" facs="z45r1_11"><seg type="wordend">τρον</seg> εἶναι
                        αὐτοῦ τοῦ βάρους τὸ Θ.</seg>
                    <seg n="12" type="line" facs="z45r1_12">ἐπεὶ δὲ ἰσορροπεῖ περὶ τὸ Α <w part="I"
                            >σημεῖ</w></seg>
                    <seg n="13" type="line" facs="z45r1_13"><w part="F">ον</w> τὰ εἰρημένα
                            με<unclear>γ</unclear>έθη, καί ἐστὶ</seg>
                    <seg n="14" type="line" facs="z45r1_14">τοῦ μὲν κυλίνδρου κέντρον <w part="I"
                        >βά</w></seg>
                    <seg n="15" type="line" facs="z45r1_15"><w part="F">ρους</w> τὸ Κ σημεῖον δίχα
                            <w part="I">τεμνομέ</w></seg>
                    <seg n="16" type="line" facs="z45r1_16"><w part="F">νης</w>
                        <unclear>τ</unclear>ῆς ΑΔ κατὰ τὸ Κ σημεῖον,</seg>
                    <seg n="17" type="line" facs="z45r1_17">τοῦ τμήματος <choice>
                            <abbr>μετηνεγμέν</abbr>
                            <expan>μετηνεγμένου</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z45r1_18">κέντρον ἐστὶ τοῦ βάρεος τὸ Θ, <w
                            part="I">ἀντι</w></seg>
                    <seg n="19" type="line" facs="z45r1_19"><w part="F">πεπονθότως</w> τὸν αὐτὸν
                        ἕξει <w part="I">λό</w></seg>
                </seg>
                <seg n="44v1" type="folio">
                    <seg n="1" type="line" facs="z44v1_01"><w part="F">γον</w> ἡ <supplied
                            reason="lost">Θ</supplied>Α πρὸς ΑΚ, ὃν ὁ <choice>
                            <abbr>κύλινδρ</abbr>
                            <expan>κύλινδρος</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z44v1_02">πρὸς τὸ τμῆμα. διπλασία δὲ ἡ</seg>
                    <seg n="3" type="line" facs="z44v1_03"><unclear>Θ</unclear>Α τῆς ΑΚ· διπλάσιος
                        ἄρα καὶ</seg>
                    <seg n="4" type="line" facs="z44v1_04">ὁ κύλινδρος τοῦ τμήματος. ὁ δὲ</seg>
                    <seg n="5" type="line" facs="z44v1_05">αὐτὸς κύλινδρος τριπλάσιός
                        <expan>ἐστι</expan></seg>
                    <seg n="6" type="line" facs="z44v1_06">τοῦ κώνου τοῦ βάσιν ἔχοντος</seg>
                    <seg n="7" type="line" facs="z44v1_07">τὸν <choice>
                            <abbr>κύκλο</abbr>
                            <expan>κύκλον</expan>
                        </choice>,</seg>
                    <figure n="4.1">
                        <figDesc xml:lang="eng">Figure 4.1</figDesc>
                    </figure>
                    <seg n="8" type="line" facs="z44v1_08">οὗ <w part="I">διάμε</w></seg>
                    <seg n="9" type="line" facs="z44v1_09"><w part="F">τρος</w> ἡ ΒΓ,</seg>
                    <seg n="10" type="line" facs="z44v1_10">κορυφὴ δὲ</seg>
                    <seg n="11" type="line" facs="z44v1_11">τὸ Α <w part="I">σημεῖ</w></seg>
                    <seg n="12" type="line" facs="z44v1_12"><unclear>
                            <w part="F">ον</w>
                        </unclear>· δῆλον</seg>
                    <seg n="13" type="line" facs="z44v1_13">οὖν, <expan>ὅτι</expan> τὸ <w part="I"
                            >τμῆ</w></seg>
                    <seg n="14" type="line" facs="z44v1_14">
                        <w part="F">μα</w>
                        <choice>
                            <abbr>ἡμιόλιό</abbr>
                            <expan>ἡμιόλιόν</expan>
                        </choice>
                    </seg>
                    <seg n="15" type="line" facs="z44v1_15">ἐστιν τοῦ <w part="I">αὐ</w></seg>
                    <seg n="16" type="line" facs="z44v1_16"><w part="F">τοῦ</w> κώνου.</seg>
                    <seg n="17" type="line" facs="z44v1_17">ΟΙ.</seg>
                </seg>
                <seg n="44v1" type="folio">
                    <seg n="18" type="line" facs="z44v1_18">ὅτι δὲ τοῦ τμήματος τοῦ <w part="I"
                            >ὀρθογω</w></seg>
                    <seg n="19" type="line" facs="z44v1_19"><w part="F">νίου</w> κωνοειδέος τοῦ <choice>
                            <abbr>ἀποτεμνομέν</abbr>
                            <expan>ἀποτεμνομένου</expan>
                        </choice></seg>
                </seg>
                <seg n="45r2" type="folio">
                    <seg n="1" type="line" facs="z45r2_01">ἐπιπέδωι ὀρθῶι πρὸς <sic>
                            <seg type="word">τῶ<unclear>ν</unclear></seg>
                        </sic>
                        <seg type="word"><unclear>ἄ</unclear>ξ<unclear>ο</unclear>ν<supplied
                                reason="lost">α</supplied></seg></seg>
                    <seg n="2" type="line" facs="z45r2_02">τὸ κέντρον τοῦ βάρους ἐστὶν ἐπὶ <seg
                            type="word">
                            <choice>
                                <abbr>
                                    <supplied reason="lost">τ</supplied>
                                </abbr>
                                <expan>
                                    <supplied reason="lost">τῆς</supplied>
                                </expan>
                            </choice>
                        </seg>
                    </seg>
                    <seg n="3" type="line" facs="z45r2_03">εὐθείας, ἥ ἐστιν ἄξων τοῦ τμήματος,</seg>
                    <seg n="4" type="line" facs="z45r2_04">τμηθείσης οὕτως τῆς <seg type="word"
                                >εἰρημέν<unclear>η</unclear>ς</seg></seg>
                    <seg n="5" type="line" facs="z45r2_05">εὐθείας, ὥστε διπλασίονα εἶναι</seg>
                    <seg n="6" type="line" facs="z45r2_06">τὸ μέρος αὐτοῦ <add>τὸ</add> πρὸς τῆι
                        κορυφῆι <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z45r2_07">λοιποῦ τμήματος, ὧδε διὰ τοῦ <w part="I"
                            >τρό</w></seg>
                    <seg n="8" type="line" facs="z45r2_08"><w part="F">που</w> θεωρεῖται·</seg>
                </seg>
                <seg n="45r2" type="folio">
                    <seg n="8" type="line" facs="z45r2_08">ἔστω τμῆμα</seg>
                    <seg n="9" type="line" facs="z45r2_09"><seg type="word">ὀρθογών<supplied
                                reason="lost">ιο</supplied>ν</seg> κωνοειδὲς <seg
                            type="suppliedword">ἀποτε</seg></seg>
                    <seg n="10" type="line" facs="z45r2_10"><seg type="wordend">μνόμενο<supplied
                                reason="lost">ν</supplied></seg> ἐπιπέδωι ὀρθῶι πρὸς</seg>
                    <seg n="11" type="line" facs="z45r2_11">τὸν ἄξονα καὶ τετμήσθω <w part="I"
                        >ἐπιπέ</w></seg>
                    <seg n="12" type="line" facs="z45r2_12"><w part="F">δωι</w> ἑτέρωι διὰ τοῦ
                        ἄξονος, καὶ <seg type="unclearword">ποι</seg></seg>
                    <seg n="13" type="line" facs="z45r2_13"><seg type="wordend"
                            ><unclear>εί</unclear>τω</seg> τομὴν ἐν τῆι ἐπιφανείαι τὴν</seg>
                    <seg n="14" type="line" facs="z45r2_14">ΑΒΓ ὀρθογωνίου κώνου τομήν, τοῦ</seg>
                    <seg n="15" type="line" facs="z45r2_15">δὲ ἀποτετμηκότος τὸ τμῆμα <w part="I"
                            >ἐπι</w></seg>
                    <seg n="16" type="line" facs="z45r2_16"><w part="F">πέδου</w> καὶ τοῦ τμήματος
                        κοινὴ</seg>
                    <seg n="17" type="line" facs="z45r2_17">τομὴ ἔστω ἡ Β<supplied reason="lost"
                        >Γ</supplied>, ἄξων δὲ ἔστω τοῦ</seg>
                    <seg n="18" type="line" facs="z45r2_18">τμήματος καὶ διάμετρος τῆς</seg>
                    <seg n="19" type="line" facs="z45r2_19">ΑΒΓ τομῆς ἡ ΑΔ εὐθεῖα, καὶ τῆι</seg>
                </seg>
                <seg n="44v2" type="folio">
                    <seg n="1" type="line" facs="z44v2_01">ΔΑ <seg type="word">εὐθεί<supplied
                                reason="lost">αι</supplied></seg> ἴση κείσθω ἡ ΑΘ, <seg type="word"
                                >κ<supplied reason="lost">αὶ</supplied></seg>
                    </seg>
                    <seg n="2" type="line" facs="z44v2_02">νοείσθω ζυγὸς ὁ ΔΘ, μέσον δὲ <w part="I"
                            >αὐ</w></seg>
                    <seg n="3" type="line" facs="z44v2_03"><w part="F">τῆς</w> τὸ Α, ἔστω δὲ καὶ
                        κῶνος <w part="I">γε</w></seg>
                    <seg n="4" type="line" facs="z44v2_04"><w part="F">γραμμένος</w> ἐν τῶι τμήματι,
                            <seg type="clearword">πλευ</seg></seg>
                    <seg n="5" type="line" facs="z44v2_05"><seg type="wordend">ραὶ</seg> δὲ αὐτοῦ αἱ
                        ΒΑ ΑΓ, ἤχθω δέ τις</seg>
                    <seg n="6" type="line" facs="z44v2_06">ἐν τῆι τοῦ ὀρθογωνίου κώνου <w part="I"
                            >το</w></seg>
                    <seg n="7" type="line" facs="z44v2_07"><w part="F">μῆι</w> ἡ ΞΟ παράλληλος οὖσα
                        τῆι</seg>
                    <seg n="8" type="line" facs="z44v2_08">ΒΓ, τεμνέτω δὲ αὕτη τὴν μὲν τοῦ <w
                            part="I">ὀρ</w></seg>
                    <seg n="9" type="line" facs="z44v2_09"><w part="F">θογωνίου</w> κώνου τομὴν κατὰ
                        τὸ</seg>
                    <seg n="10" type="line" facs="z44v2_10">Ξ Ο, τὰς δὲ τοῦ κώνου πλευρὰς
                            <expan>κατὰ</expan></seg>
                    <seg n="11" type="line" facs="z44v2_11">τὸ ΠΡ σημεῖα.</seg>
                </seg>
                <seg n="44v2" type="folio">
                    <seg n="11" type="line" facs="z44v2_11">ἐπεὶ οὖν <seg type="word"
                            >ἐ<unclear>ν</unclear></seg>
                        <w part="I">ὀρθογωνί</w></seg>
                    <seg n="12" type="line" facs="z44v2_12"><w part="F">ου</w>
                        <seg type="word"><unclear>κώ</unclear>νου</seg> τομῆι κάθετοι ἠγμέναι</seg>
                    <seg n="13" type="line" facs="z44v2_13">εἰσὶν ἐπὶ τὴν διάμετρον αἱ
                        Ξ<unclear>Ο</unclear> ΒΔ,</seg>
                    <seg n="14" type="line" facs="z44v2_14"><expan>ἔστιν</expan>, <expan>ὡς</expan>
                        ἡ ΔΑ <expan>πρὸς</expan> ΑΣ, οὕτως τὸ ἀπὸ ΒΔ <expan>πρὸς</expan>
                    </seg>
                    <seg n="15" type="line" facs="z44v2_15">τὸ ἀπὸ ΞΣ. ὡς δὲ ἡ ΔΑ
                        <expan>πρὸς</expan> ΑΣ, <expan>οὕτως</expan> ἡ ΒΔ</seg>
                    <seg n="16" type="line" facs="z44v2_16">
                        <expan>πρὸς</expan>
                        <unclear>Π</unclear>Σ, ὡς δὲ ἡ ΒΔ <expan>πρὸς</expan> ΠΣ, οὕτως τὸ ἀπὸ</seg>
                    <seg n="17" type="line" facs="z44v2_17">ΒΔ <expan>πρὸς</expan> τὸ ὑπὸ τῶν ΒΔ ΠΣ·
                        ἔσται ἄρα</seg>
                    <seg n="18" type="line" facs="z44v2_18">καὶ, ὡς τὸ ἀπὸ ΒΔ πρὸς τὸ ἀπὸ ΞΣ,
                    οὕτως</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
