                    <seg n="8" type="line" facs="z57r1_08"><seg type="word">το<supplied
                                reason="lost">ῦ</supplied></seg>
                        <seg type="word">ἀπ<supplied reason="lost">οδει</supplied>χθέν<supplied
                                reason="lost">το</supplied>ς</seg> τρόπου καὶ</seg>
                    <seg n="9" type="line" facs="z57r1_09">ἄλλα <seg type="word"
