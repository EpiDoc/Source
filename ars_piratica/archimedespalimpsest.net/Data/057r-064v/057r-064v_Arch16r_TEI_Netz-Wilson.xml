                    <seg n="8" type="line" facs="z64v1_08"><seg type="wordend"><supplied
                                reason="lost">θ</supplied>ους</seg>
                        <seg type="word">μέγ<supplied reason="lost">εθος</supplied></seg>
                        <seg type="word">ἀφαιρ<supplied reason="lost">ε</supplied>θ<supplied
                                reason="lost">ῆι</supplied></seg>, <supplied reason="lost">ἦι</supplied>
                        <supplied reason="lost">δὲ</supplied></seg>
                    <seg n="9" type="line" facs="z64v1_09"><supplied reason="lost">μὴ</supplied> τὸ
                            <supplied reason="lost">αὐτὸ</supplied>
                        <supplied reason="lost">σημεῖον</supplied>
                        <seg type="word"><supplied reason="lost">κ</supplied>έ<supplied
                                reason="lost">ν</supplied>τρ<supplied reason="lost"
                        >ον</supplied></seg></seg>
                    <seg n="10" type="line" facs="z64v1_10"><seg type="word">το<supplied
                                reason="lost">ῦ</supplied></seg>
                        <seg type="word"><supplied reason="lost">βάρου</supplied>ς</seg>
                        <seg type="word">το<supplied reason="lost">ῦ</supplied></seg> τε <seg
                            type="word"><supplied reason="lost">ὅ</supplied>λ<supplied reason="lost"
                                >ου</supplied></seg>
                        <seg type="word">μεγέθ<supplied reason="lost">ους</supplied></seg></seg>
                    <seg n="11" type="line" facs="z64v1_11">
                        <seg type="word"><supplied reason="lost">κ</supplied>αὶ</seg>
                        <seg type="word">το<supplied reason="lost">ῦ</supplied></seg>
                        <seg type="word"><supplied reason="lost">ἀφ</supplied>αιρο<supplied
                                reason="lost">υ</supplied>μένου</seg>
                        <seg type="word">μεγέ<supplied reason="lost">θους,</supplied></seg>
                    </seg>
                    <seg n="12" type="line" facs="z64v1_12"><supplied reason="lost">τὸ</supplied>
                        <seg type="word"><supplied reason="lost">κέ</supplied>ν<supplied
                                reason="lost">τρον</supplied></seg> ἐστὶ τοῦ βάρους τοῦ</seg>
                    <seg n="13" type="line" facs="z64v1_13"><supplied reason="lost">λοιποῦ</supplied>
                        <seg type="word">μ<supplied reason="lost">εγέ</supplied>θους</seg> ἐπὶ <seg
                            type="word"><supplied reason="lost">τ</supplied>ῆ<supplied reason="lost"
                                >ς</supplied></seg>
                        <supplied reason="lost">εὐθείας</supplied></seg>
                    <seg n="14" type="line" facs="z64v1_14">
                        <supplied reason="lost">τῆς</supplied>
                        <seg type="word"><supplied reason="lost">ἐπιζευγ</supplied>νούσ<supplied
                                reason="lost">η</supplied>ς</seg>
                        <seg type="word"><supplied reason="lost">τ</supplied>ὰ</seg>
                        <seg type="word">κ<supplied reason="lost">έντρα</supplied></seg>
                    </seg>
                    <seg n="15" type="line" facs="z64v1_15">
                        <supplied reason="lost">τοῦ</supplied>
                        <supplied reason="lost">βάρους</supplied>
                        <supplied reason="lost">τοῦ</supplied> τε ὅλου <seg type="word"
                                >μεγέ<supplied reason="lost">θους</supplied></seg>
                    </seg>
                    <seg n="16" type="line" facs="z64v1_16"><supplied reason="lost">καὶ</supplied>
                        <supplied reason="lost">τοῦ</supplied>
                        <supplied reason="lost">ἀφαιρουμένο</supplied>υ <seg type="suppliedword"
                                >ἐκβε<supplied reason="lost">βλη</supplied></seg></seg>
                </seg>
                <seg n="57r2" type="folio">
                    <seg n="1" type="line" facs="z57r2_01">μένης καὶ ἀφαιρεθείσης ἀπ᾽ <w part="F"
                        >αὐ</w></seg>
                    <seg n="2" type="line" facs="z57r2_02"><w part="F">τῆς</w>
                        <seg type="word"><supplied reason="lost">πρὸ</supplied>ς</seg> τὴν μεταξὺ
                        τῶν <w part="I">εἰρημέ</w></seg>
                    <seg n="3" type="line" facs="z57r2_03"><w part="F">νων</w> κέντρων τοῦ βάρους
                        τοῦτον</seg>
                    <seg n="4" type="line" facs="z57r2_04">
                        <choice>
                            <abbr>ἔχσα</abbr>
                            <expan>ἔχουσα</expan>
                        </choice> τὸν λόγον, ὃν <seg type="word">ἔ<supplied reason="lost"
                            >χει</supplied></seg> τὸ βάρος</seg>
                    <seg n="5" type="line" facs="z57r2_05"><seg type="word">το<supplied
                                reason="lost">ῦ</supplied></seg>
                        <seg type="word">ἀφ<supplied reason="lost">η</supplied>ρ<supplied
                                reason="lost">η</supplied>μένου</seg> μεγέθους <seg type="word"
                                >πρὸ<supplied reason="lost">ς</supplied></seg></seg>
                    <seg n="6" type="line" facs="z57r2_06">τὸ <seg type="word">λ<supplied
                                reason="lost">οι</supplied>πὸν</seg>
                        <seg type="word"><supplied reason="lost">β</supplied>άρος</seg> τοῦ <seg
                            type="word"><supplied reason="lost"
                                >λ</supplied><unclear>ο</unclear><supplied reason="lost"
                            >ι</supplied>ποῦ</seg>
                        <seg type="word">
                            <choice>
                                <abbr>με<supplied reason="lost">γέ</supplied></abbr>
                                <expan>με<supplied reason="lost">γέθους</supplied></expan>
                            </choice>
                        </seg>.</seg>
                </seg>
                <seg n="57r2" type="folio">
                    <seg n="7" type="line" facs="z57r2_07"><seg type="word"><supplied reason="lost"
                                >ἐ</supplied>ὰ<supplied reason="lost">ν</supplied></seg>
                        <sic>
                            <seg type="word">ὁπω<supplied reason="lost">σων</supplied>οῦν</seg>
                        </sic> μεγεθέων τὸ <seg type="expandedword">
                            <choice>
                                <abbr>κέ</abbr>
                                <expan>κέν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="8" type="line" facs="z57r2_08"><seg type="wordend">τρον</seg> τοῦ <seg
                            type="word">βάρ<supplied reason="lost">ο</supplied>υς</seg> ἐπὶ τῆς
                        αὐτῆς</seg>
                    <seg n="9" type="line" facs="z57r2_09">εὐθείας <supplied reason="lost"
                        >ἦ</supplied>, καὶ <seg type="word">το<supplied reason="lost">ῦ</supplied></seg>
                        <seg type="word">ἐ<supplied reason="lost">κ</supplied></seg>
                        <seg type="word">πάν<supplied reason="lost">τ</supplied>ων</seg>
                        <seg type="suppliedword">συ<supplied reason="lost">γ</supplied></seg></seg>
                    <seg n="10" type="line" facs="z57r2_10"><seg type="wordend">κειμένου</seg>
                        <seg type="word">μεγέθο<supplied reason="lost">υ</supplied>ς</seg> τὸ
                        κέντρον <choice>
                            <abbr>ἔστ</abbr>
                            <expan>ἔσται</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z57r2_11">ἐπὶ <seg type="word"><supplied
                                reason="lost">τ</supplied>ῆς</seg> αὐτῆς <seg type="word"><supplied
                                reason="lost">εὐ</supplied>θείας</seg>.</seg>
                </seg>
                <seg n="57r2" type="folio">
                    <seg n="11" type="line" facs="z57r2_11">πάσης</seg>
                    <seg n="12" type="line" facs="z57r2_12"><seg type="word">εὐθεί<supplied
                                reason="lost">α</supplied>ς</seg> τὸ κέντρον ἐστὶ τοῦ βάρους</seg>
                    <seg n="13" type="line" facs="z57r2_13">ἡ διχοτομία τῆς εὐθείας.</seg>
                </seg>
                <seg n="57r2" type="folio">
                    <seg n="13" type="line" facs="z57r2_13">
                        <seg type="word">π<supplied reason="lost">α</supplied>ντὸς</seg>
                    </seg>
                    <seg n="14" type="line" facs="z57r2_14">τριγώνου τὸ κέντρον ἐστὶν τοῦ <seg
                            type="suppliedword"><supplied reason="lost">β</supplied>ά</seg></seg>
                    <seg n="15" type="line" facs="z57r2_15"><seg type="wordend">ρους</seg> τὸ <seg
                            type="word">ση<supplied reason="lost">μεῖ</supplied>ον</seg>, <supplied
                            reason="lost">καθ᾽</supplied>
                        <supplied reason="lost">ὃ</supplied>
                        <supplied reason="lost">αἱ</supplied>
                        <supplied reason="lost">ἐκ</supplied> τῶν</seg>
                    <seg n="16" type="line" facs="z57r2_16">γωνιῶν τοῦ <seg n="word">τ<supplied
                                reason="lost">ρ</supplied>ιγώνου</seg> ἐπὶ <choice>
                            <abbr>μέσ</abbr>
                            <expan>μέσας</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z57r2_17">
                        <seg type="word"><supplied reason="lost">τὴ</supplied>ς</seg>
                        <seg type="word">π<supplied reason="lost">λ</supplied>ευ<supplied
                                reason="lost">ρ</supplied>ὰς</seg> ἀγόμεναι εὐθεῖαι</seg>
                    <seg n="18" type="line" facs="z57r2_18">τέμνουσιν ἀλλήλας.</seg>
                </seg>
                <seg n="57r2" type="folio">
                    <seg n="18" type="line" facs="z57r2_18">παντὸς <w part="I">πα</w></seg>
                    <seg n="19" type="line" facs="z57r2_19"><w part="F">ραλληλογράμμου</w> τὸ
                        κέντρον ἐστὶν</seg>
                    <seg n="20" type="line" facs="z57r2_20">
                        <supplied reason="lost">τοῦ</supplied>
                        <supplied reason="lost">βάρους</supplied>
                        <supplied reason="lost">τὸ</supplied>
                        <supplied reason="lost">σημεῖον,</supplied>
                        <supplied reason="lost">καθ᾽</supplied>
                        <supplied reason="lost">ὃ</supplied>
                        <supplied reason="lost">αἱ</supplied>
                    </seg>
                    <seg n="21" type="line" facs="z57r2_21">
                        <supplied reason="lost">διάμετροι</supplied>
                        <supplied reason="lost">συμπίπτουσιν.</supplied>
                    </seg>
                </seg>
                <seg n="57r2" type="folio">
                    <seg n="21" type="line" facs="z57r2_21">
                        <supplied reason="lost">κύκλου</supplied>
                    </seg>
                </seg>
                <seg n="64v2" type="folio">
                    <seg n="1" type="line" facs="z64v2_01">τὸ <seg type="word"><supplied
                                reason="lost">κ</supplied>έντρον</seg> τοῦ <seg type="word"
                                ><supplied reason="lost">βά</supplied>ρους</seg> ἐστὶν ὃ
                        <expan>καὶ</expan></seg>
                    <seg n="2" type="line" facs="z64v2_02"><supplied reason="lost">τοῦ</supplied>
                        <supplied reason="lost">κύκλου</supplied> ἐστὶ κέντρον.</seg>
                </seg>
                <seg n="64v2" type="folio">
                    <seg n="2" type="line" facs="z64v2_02">παντὸς</seg>
                </seg>
            </ab>
