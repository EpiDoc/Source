                            <choice>
                                <abbr>μηχαν<supplied reason="lost">ικῶ</supplied></abbr>
                                <expan>μηχαν<supplied reason="lost">ικῶν</supplied></expan>
                            </choice>
                        </seg>,</seg>
                    <seg n="13" type="line" facs="z57r1_13"><seg type="word">ὅ<supplied
                                reason="lost">τι</supplied></seg>
                        <seg type="word"><supplied reason="lost">πᾶ</supplied>ν</seg> τμῆμα
                        ὀρθογωνίου <seg type="suppliedword"><supplied reason="lost"
                        >κ</supplied>ώ</seg></seg>
                    <seg n="14" type="line" facs="z57r1_14"><seg type="wordend">νου</seg> τομῆς
                        ἐπίτριτόν ἐστιν <w part="I">τρι</w></seg>
                    <seg n="15" type="line" facs="z57r1_15"><w part="F">γώνου</w> τοῦ βάσιν ἔχοντος
                        τὴν</seg>
                    <seg n="16" type="line" facs="z57r1_16">αὐτὴν καὶ ὕψος ἴσον, μετὰ δὲ <seg
                            type="word">το<supplied reason="lost">ῦ</supplied></seg></seg>
                    <seg n="17" type="line" facs="z57r1_17">το ἕκαστον διὰ τοῦ αὐτοῦ <seg
