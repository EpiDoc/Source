                    <seg n="10" type="line" facs="z57r1_10"><seg type="wordend">ποπεπτωκό<supplied
                                reason="lost">τ</supplied>α</seg>
                        <seg type="word">εὑρή<supplied reason="lost">σ</supplied>ειν</seg>.</seg>
                </seg>
                <seg n="57r1" type="folio">
