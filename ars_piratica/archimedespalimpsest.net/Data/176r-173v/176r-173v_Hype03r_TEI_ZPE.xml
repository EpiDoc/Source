    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="176r-173v" type="folio">
                    <seg n="01" type="line" facs="z176r-173v_01">ἑ<unclear>τέροις</unclear>
                        <unclear>ἀκολ</unclear>ουθεῖν δ<unclear>ι</unclear>π<unclear>λᾶς</unclear>
                        <gap extent="5"/>σεις <gap extent="3"/><unclear>ο</unclear><gap extent="5"/></seg>
                    <seg n="02" type="line" facs="z176r-173v_02"><gap extent="4"
                            /><unclear>ωσει</unclear><gap extent="1"/>, ἀλλὰ κ<unclear>αὶ</unclear>
                        γράφει <unclear>α</unclear>φ<gap extent="1"/><unclear>αυ</unclear><gap
                            extent="8"/> φ<unclear>ησὶ</unclear></seg>
                    <seg n="03" type="line" facs="z176r-173v_03">εἶν<supplied reason="lost"
                        >αι</supplied> δεινὸν εἰ ὑπὲρ τῆς τῶν Ἑλλήνων ἐλευθερίας <seg
                            type="unclearword">πλ<unclear>εί</unclear></seg></seg>
                    <seg n="04" type="line" facs="z176r-173v_04"><seg type="wordend">
                            <unclear>ω</unclear>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr>προθυμί</abbr>
                                <expan>προθυμίαν</expan>
                            </choice>
                        </seg> Ἀθηναῖο<unclear>ι</unclear> Θηβαίων <unclear>παρ</unclear>έσχοντο.
                            <supplied reason="lost">καὶ</supplied>
                        <supplied reason="lost">γ</supplied><unclear>ά</unclear><supplied
                            reason="lost">ρ</supplied>,</seg>
                    <seg n="05" type="line" facs="z176r-173v_05">εἴ γε τὰ περὶ
                        τὴ<unclear>ν</unclear> μάχην
                            <unclear>κ</unclear>ατ<unclear>ω</unclear><supplied reason="lost"
                            >ρ</supplied>θ<supplied reason="lost">ώ</supplied><unclear>θ</unclear>η,
                        πάντες <seg type="clearword">
                            <choice>
                                <sic>ἀνελο</sic>
                                <corr>ἂν ἐλο</corr>
                            </choice>
                        </seg></seg>
                    <seg n="06" type="line" facs="z176r-173v_06"><seg type="wordend">γίζοντο</seg>
                        ὅτι καὶ τ<unclear>ὸν</unclear> πόλ<unclear>εμον</unclear>
                        τ<unclear>ὸ</unclear>ν <unclear>π</unclear>ρὸ<unclear>ς</unclear> Φίλιππον
                            <seg type="suppliedword">
                            <unclear>π</unclear>
                            <supplied reason="lost">ο</supplied>
                        </seg></seg>
                    <seg n="07" type="line" facs="z176r-173v_07"><seg type="wordend">λλὰ</seg> ἔτη
                            <unclear>ἡ</unclear> πόλ<unclear>ις</unclear> αὐτὴ μόνη
                            <unclear>ὑ</unclear>π<unclear>ὲρ</unclear> ἁπ<unclear>άν</unclear>των
                            <seg type="unclearword">ἐπολέμη</seg></seg>
                    <seg n="08" type="line" facs="z176r-173v_08"><seg type="wordend"
                            >σ<unclear>εν</unclear></seg> καὶ <unclear>Βυ</unclear>ζάντιον
                            διέσω<unclear>σ</unclear>εν τοῖς αὐτοῖς τέλεσι·</seg>
                    <seg n="09" type="line" facs="z176r-173v_09">καὶ μ<unclear>ηδ’</unclear>
                        <unclear>ἓν</unclear> παρα<unclear>καλ</unclear>έσαντες
                        ἐ<unclear>πὶ</unclear> τῶν αγώ<unclear>νων</unclear> τὰ <seg
                            type="unclearword">π<unclear>λ</unclear>εῖ</seg></seg>
                    <seg n="10" type="line" facs="z176r-173v_10"><seg type="wordend">στα</seg>
                            συμβα<unclear>λό</unclear>με<unclear>νοι</unclear>
                        <unclear>Ἀ</unclear>θην<unclear>αῖ</unclear>οι ἦσαν,
                            ὥ<unclear>στ</unclear><supplied reason="lost">ε</supplied> αὐ<unclear>τὰ</unclear>
                        <seg type="word">
                            <choice>
                                <abbr>ταῦ</abbr>
                                <expan>
                                    <unclear>ταῦ</unclear>
                                    <supplied reason="lost">τα</supplied>
                                </expan>
                            </choice>
                        </seg></seg>
                    <seg n="11" type="line" facs="z176r-173v_11"><unclear>τὰ</unclear> ἆθλ<unclear>α</unclear>
                        <unclear>ὑ</unclear>μῖν ἐξ αὐτῶν <seg type="word">
                            <choice>
                                <abbr>μέγισ</abbr>
                                <expan><unclear>μ</unclear>έγιστα</expan>
                            </choice>
                        </seg> εἶναι. πρὸς δὲ <seg type="suppliedword"
                                ><unclear>τ</unclear>ο<unclear>ύ</unclear></seg></seg>
                    <seg n="12" type="line" facs="z176r-173v_12"><seg type="wordend"><supplied
                                reason="lost">τ</supplied><unclear>οι</unclear>ς</seg>, ὦ ἄνδρες
                            δι<unclear>κ</unclear>αστ<unclear>αί</unclear>, <supplied reason="lost"
                            >δ</supplied><unclear>εῖ</unclear>
                        σ<unclear>κέ</unclear>ψ<unclear>α</unclear>σθαι ἐ<unclear>π</unclear>ὶ
                            τ<unclear>ῆ</unclear>ς <seg type="unclearword">ἀλη</seg></seg>
                    <seg n="13" type="line" facs="z176r-173v_13"><seg type="wordend"
                            >θ<unclear>εί</unclear>ας</seg> τίν’ <unclear>ἦ</unclear><supplied
                            reason="lost">ν</supplied>
                        ἑ<unclear>κ</unclear>ατ<unclear>έ</unclear>ροις <unclear>ὑ</unclear>μῶν
                            <unclear>τὰ</unclear>
                        <seg type="word">
                            <choice>
                                <abbr>προσον</abbr>
                                <expan>π<supplied reason="lost">ρ</supplied>
                                    <unclear>ο</unclear>
                                    <supplied reason="lost">σ</supplied>ό<unclear>ν</unclear>τα
                                </expan>
                            </choice>
                        </seg> πρὸς</seg>
                    <seg n="14" type="line" facs="z176r-173v_14">Φ<unclear>ί</unclear>λιππον·
                        Θηβαίοις μὲν εἰρ<unclear>ή</unclear>νη καὶ <unclear>συμ</unclear>μάχια, <w
                            part="I">ὑ</w></seg>
                    <seg n="15" type="line" facs="z176r-173v_15"><w part="F">μῖν</w> δὲ πόλεμος
                            τ<unclear>ούτου</unclear> μετασχεῖν <unclear>ἡμεῖς</unclear>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἠ</supplied>
                        </seg></seg>
                    <seg n="16" type="line" facs="z176r-173v_16"><seg type="wordend">ξιώσαμεν</seg>,
                            <unclear>ὃν</unclear> ἀνάγκη ἦν, εἰ μὴ ἐκεῖνοι
                            με<unclear>τ</unclear>εβάλ<unclear>ο</unclear>ντο,</seg>
                    <seg n="17" type="line" facs="z176r-173v_17"><unclear>α</unclear>ὐτοὺς μόνους
                            <supplied reason="lost">πο</supplied>λεμεῖν
                            <unclear>κα</unclear><supplied reason="lost">ὶ</supplied>
                        <supplied reason="lost">πάντα</supplied>
                        <seg type="word">
                            <choice>
                                <abbr>τἀναλώμα</abbr>
                                <expan>
                                    <supplied reason="lost">τἀναλώματα</supplied>
                                </expan>
                            </choice>
                        </seg></seg>
                    <seg n="18" type="line" facs="z176r-173v_18">ἀναλίσκειν κ<unclear>αὶ</unclear>
                            δι<unclear>α</unclear>κινδυνεύειν ἐν τῆι ἡμετέρα<unclear>ι</unclear></seg>
                    <seg n="19" type="line" facs="z176r-173v_19">χώραι· τούτων δὲ τοιούτων
                        ὑπαρχόντων καὶ <w part="I">Φι</w></seg>
                    <seg n="20" type="line" facs="z176r-173v_20"><w part="F">λίππου</w>
                        ἐ<unclear>πὶ</unclear> τῶν <unclear>ὁ</unclear>ρ<unclear>ί</unclear>ων τῆς
                            <unclear>Β</unclear>οι<unclear>ω</unclear>τί<unclear>ας</unclear>
                        <seg type="word">
                            <choice>
                                <abbr>ὄντ</abbr>
                                <expan>ὄ<unclear>ν</unclear>τος</expan>
                            </choice>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr>πότερ</abbr>
                                <expan>πότερον</expan>
                            </choice>
                        </seg></seg>
                    <seg n="21" type="line" facs="z176r-173v_21">ἔδει ἡμᾶς τὴν
                        δύν<unclear>α</unclear>μιν τὴν Θη<unclear>βα</unclear>ίων
                        προσλα<unclear>β</unclear>εῖν</seg>
                    <seg n="22" type="line" facs="z176r-173v_22">καὶ τὴν χώραν καὶ
                        τ<unclear>ῶ</unclear>ν ἀναλωμάτων ἃ ἑκόντες</seg>
                    <seg n="23" type="line" facs="z176r-173v_23">συν<unclear>εβ</unclear>άλοντο,
                            <unclear>ἢ</unclear> πρὸς <seg type="word">
                            <choice>
                                <abbr>πάν</abbr>
                                <expan>πάντα</expan>
                            </choice>
                        </seg> ἐρίζ<unclear>ο</unclear>ντας καὶ <seg type="clearword">ἀγνωμο</seg></seg>
                    <seg n="24" type="line" facs="z176r-173v_24"><seg type="wordend">
                            <choice>
                                <abbr>νοῦντ</abbr>
                                <expan>νοῦντας</expan>
                            </choice>
                        </seg> αὐτοὺς μόνους προελέσθαι <seg type="word">
                            <choice>
                                <abbr>ἀγωνίζεσθ</abbr>
                                <expan>ἀγ<unclear>ων</unclear>ίζεσθαι</expan>
                            </choice>
                        </seg>;</seg>
                    <seg n="25" type="line" facs="z176r-173v_25">καὶ νῦν γε, ὦ ἄνδρες Ἀθηναῖοι,
                            πολ<unclear>λ</unclear>ο<unclear>ὶ</unclear>
                            θ<unclear>ηβ</unclear>αίω<unclear>ν</unclear></seg>
                    <seg n="26" type="line" facs="z176r-173v_26">ἀκροῶνται ἐν <unclear>τῶι</unclear>
                            δ<unclear>ι</unclear>καστηρίω<unclear>ι</unclear>
                        σ<unclear>χο</unclear>λὴν ἄγοντες</seg>
                    <seg n="27" type="line" facs="z176r-173v_27">(<choice>
                            <sic><unclear>ὁ</unclear>ς</sic>
                            <corr>ὡς</corr>
                        </choice> οὐ<unclear>κ</unclear> ἂν ἐβουλόμην) οἵ, ἐὰν μὲν
                            <unclear>ἀ</unclear>πολύσητ<unclear>ε</unclear>
                        <w part="I">τοῦ</w></seg>
                    <seg n="28" type="line" facs="z176r-173v_28"><w part="F">τον</w> τὸν
                        <unclear>ἀ</unclear>γῶνα, ὑπολήψονται τὸν μὲν κατήγορον <seg
                            type="unclearword">
                            <unclear>ἀ</unclear>
                        </seg></seg>
                    <seg n="29" type="line" facs="z176r-173v_29"><seg type="wordend"
                                >π<unclear>ό</unclear>πλ<unclear>η</unclear>κτον</seg> εἶναι καὶ
                            μαινόμενο<unclear>ν</unclear>, <unclear>ὑ</unclear>μᾶς δὲ
                        <unclear>ἐπὶ</unclear></seg>
                    <seg n="30" type="line" facs="z176r-173v_30">τῶν αὐτ<unclear>ῶ</unclear>ν
                        μένειν, ὡς π<unclear>ρ</unclear>οσῆκεν ὑμῖν· ἐὰ<unclear>ν</unclear> δὲ</seg>
                    <seg n="31" type="line" facs="z176r-173v_31">κατ<unclear>α</unclear>γνῶτε,
                            <unclear>π</unclear>ῶς οὐκ <unclear>ἂ</unclear>ν δικαίως <seg
                            type="word">
                            <choice>
                                <abbr>ἐγκαλέσαιε</abbr>
                                <expan>ἐγκα<unclear>λέσαιεν</unclear></expan>
                            </choice>
                        </seg></seg>
                    <seg n="32" type="line" facs="z176r-173v_32"><unclear>ὑ</unclear>μῖν, εἰ ἐπὶ τὰ
                            <seg type="word">
                            <choice>
                                <abbr>αὐ</abbr>
                                <expan>αὐτὰ</expan>
                            </choice>
                        </seg> πα<unclear>ρ</unclear>εκαλέσατε <unclear>αὐτούς</unclear> ἐφ’
                            <unclear>οἷς</unclear></seg>
                </seg>
