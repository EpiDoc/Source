    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="125r1" type="folio">
                    <seg n="1" type="line" facs="z125r1_01">λοιπᾶν μείζονας, καὶ τὰ <seg
                            type="clearword">τετρά</seg></seg>
                    <seg n="2" type="line" facs="z125r1_02"><seg type="wordend">γωνα</seg> δὲ τὰ ἀπὸ
                        τᾶν ΑΦ ΓΧ ΕΨ ΗΩ</seg>
                    <seg n="3" type="line" facs="z125r1_03"><supplied reason="lost">Ι</supplied>Ϡ
                        ΛϞ· δέδεικται <seg type="word">γὰ<unclear>ρ</unclear>
                        </seg> τοῦτο ἐν τοῖς</seg>
                    <seg n="4" type="line" facs="z125r1_04">ἐπάνω· <seg type="word">
                            <choice>
                                <abbr>ἐλάονα</abbr>
                                <expan>ἐλάττο<supplied reason="lost">να</supplied></expan>
                            </choice>
                        </seg> ἄρα ἐντὶ τὰ</seg>
                    <seg n="5" type="line" facs="z125r1_05"><seg type="word">ῥηθ<supplied
                                reason="lost">έν</supplied>τα</seg> χωρία τῶν <choice>
                            <abbr>τετραγώνω</abbr>
                            <expan>τετραγώνων</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z125r1_06">τῶν ἀπὸ τῶν ΑΒ ΓΔ ΕΖ ΗΘ ΙΚ ΛΜ.</seg>
                </seg>
                <seg n="125r1" type="folio">
                    <seg n="7" type="line" facs="z125r1_07">λοιπὸν δὲ δείξομεν <expan>ὅτι</expan>
                        μείζονά <seg type="clearword">
                            <choice>
                                <abbr>ἐ</abbr>
                                <expan>ἐν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="8" type="line" facs="z125r1_08"><seg type="wordend">τι</seg> τῶν
                        τετράγωνον τῶν ἀπὸ τᾶν</seg>
                    <seg n="9" type="line" facs="z125r1_09">ΓΔ ΕΖ ΗΘ ΙΚ ΛΜ ΝΞ. <supplied
                            reason="lost">πάλιν</supplied>
                        <seg type="word">
                            <supplied reason="lost">δ</supplied>ὴ</seg>
                        <supplied reason="lost">τὰ</supplied></seg>
                    <seg n="10" type="line" facs="z125r1_10">τετράγωνα τὰ ἀπὸ τᾶν ΓΔ ΕΖ ΗΘ</seg>
                    <seg n="11" type="line" facs="z125r1_11">ΙΚ ΛΜ ΝΞ ἴσα ἐντὶ τοῖς τε ἀπὸ τᾶν</seg>
                    <seg n="12" type="line" facs="z125r1_12">ΧΓΕ ΨΗ ΩΙ ϠΛϞ καὶ τῶι <seg
                            type="clearword">περιεχο</seg></seg>
                    <seg n="13" type="line" facs="z125r1_13"><seg type="wordend">μένωι</seg> ὑπό τε
                        τᾶς ΝΞ καὶ <supplied reason="lost">τᾶς</supplied>
                        <seg type="clearword">δι</seg></seg>
                    <seg n="14" type="line" facs="z125r1_14"><seg type="wordend">πλασίας</seg> πασᾶν
                        τᾶν ΧΓ ΕΨ ΩΙ</seg>
                    <seg n="15" type="line" facs="z125r1_15">ϠΛϞ. καί <expan>ἐστι</expan> κοινὰ μὲν
                        τὰ ἀπὸ τᾶν</seg>
                    <seg n="16" type="line" facs="z125r1_16">ΧΔ ΨΖ ωΔ ϠΚ Μ<unclear>Ϟ</unclear> ΝΞ,
                            <expan>μεῖζον</expan> δὲ τὸ ὑπό</seg>
                    <seg n="17" type="line" facs="z125r1_17">τε τᾶς ΝΞ καὶ τᾶς ἴσας <choice>
                            <abbr>πάσς</abbr>
                            <expan>πάσαις</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z125r1_18"><choice>
                            <abbr>τς</abbr>
                            <expan>ταῖς</expan>
                        </choice> ΟΧ <supplied reason="lost">ΠΨ</supplied> ΡΩ <supplied
                            reason="lost">Σ</supplied>Ϡ <seg type="word">
                            <unclear>Τ</unclear>
                            <supplied reason="lost">Ϟ</supplied>
                        </seg> ΥΝ <seg type="word">το<supplied reason="lost">ῦ</supplied>
                        </seg> ὑπὸ <seg type="word">τ<supplied reason="lost">ᾶς</supplied>
                        </seg></seg>
                </seg>
                <seg n="132v1" type="folio">
                    <seg n="1" type="line" facs="z132v1_01"><supplied reason="lost">ΝΞ</supplied>
                        <supplied reason="lost">καὶ</supplied> τᾶς <seg type="word">
                            <unclear>δ</unclear>ι<unclear>πλασί</unclear><supplied reason="lost"
                            >ας</supplied>
                        </seg>
                        <unclear>πασᾶν</unclear></seg>
                    <seg n="2" type="line" facs="z132v1_02">τᾶν ΓΧ ΕΨ ΗΩ ΙϠ <unclear>Λ</unclear>Ϟ,
                        ἐντὶ δὲ καὶ</seg>
                    <seg n="3" type="line" facs="z132v1_03">τετράγωνα τὰ ἀπὸ τᾶν ΧΟ ΨΠ</seg>
                    <seg n="4" type="line" facs="z132v1_04">ΩΡ ϠΣ ϞΤ ΥΝ τῶν ἀπὸ τᾶν ΓΧ
                        Ε<unclear>Υ</unclear></seg>
                    <seg n="5" type="line" facs="z132v1_05"><unclear>Γ</unclear>Η ωΙ ϠΛ Ϟ
                            <expan>μείζονα</expan> ἢ <seg type="word">
                            <supplied reason="lost">τρι</supplied>πλάσια</seg>· <seg
                            type="clearword">δέ</seg></seg>
                    <seg n="6" type="line" facs="z132v1_06"><seg type="wordend">δεικται</seg>
                        <expan>γὰρ</expan> καὶ τοῦτο· μείζονα ἄρα <seg type="clearword">ἐν</seg></seg>
                    <seg n="7" type="line" facs="z132v1_07"><seg type="wordend">τὶ</seg>
                        <sic>γὰρ η θέντα</sic>
                        <seg type="word">
                            <unclear>χω</unclear>ρία</seg> τῶν <seg type="clearword">τετραγώ</seg></seg>
                    <seg n="8" type="line" facs="z132v1_08"><seg type="wordend">νων</seg> τῶν ἀπὸ
                        τᾶν ΓΔ ΕΖ ΗΘ ΙΚ ΛΜ</seg>
                    <seg n="9" type="line" facs="z132v1_09">ΝΞ. <choice>
                            <abbr>ἑξ</abbr>
                            <expan>ἑξῆς</expan>
                        </choice> τὸ ΣΞΑΜΑ</seg>
                    <figure n="11.1">
                        <figDesc>Figure 11.1</figDesc>
                    </figure>
                </seg>
                <seg n="125r2" type="folio">
                    <seg n="1" type="line" facs="z125r2_01">Καὶ τοίνυν εἴ κα <seg type="word"
                                >ὁμο<supplied reason="lost">ῖα</supplied>
                        </seg>
                        <seg type="clearword"><supplied reason="lost">ἀν</supplied>αγραφέν</seg></seg>
                    <seg n="2" type="line" facs="z125r2_02"><seg type="wordend">τι</seg> ἀπὸ πασᾶν,
                        ἀπό τε <supplied reason="lost">τᾶν</supplied>
                        <supplied reason="lost">τῶι</supplied>
                        <seg type="word">
                            <supplied reason="lost">ἴσ</supplied>ωι</seg></seg>
                    <seg n="3" type="line" facs="z125r2_03">ἀλλαλᾶν <seg type="word">
                            ὑ<unclear>π</unclear>εχουσᾶν</seg> καὶ <seg type="word">
                            <unclear>ἀ</unclear>πὸ</seg> τᾶν</seg>
                    <seg n="4" type="line" facs="z125r2_04">ἰσᾶν τᾶ μεγίστα, <seg type="word"
                                >εἴδ<supplied reason="lost">εα</supplied>
                        </seg>, <seg type="word">πάν<supplied reason="lost">τα</supplied>
                        </seg> τὰ <seg type="word">
                            <supplied reason="lost">δ</supplied>
                            <unclear>ὲ</unclear>
                        </seg></seg>
                    <seg n="5" type="line" facs="z125r2_05">ἀπὸ τᾶν τῶι ἴσωι ἀλλαλᾶν <seg
                            type="clearword">ὑπερε</seg></seg>
                    <seg n="6" type="line" facs="z125r2_06"><seg type="wordend">χουσᾶν</seg> χωρὶς
                        τοῦ ἀπὸ τᾶς <seg type="unclearword">ἐλα<unclear>χ</unclear>ίσ</seg></seg>
                    <seg n="7" type="line" facs="z125r2_07"><seg type="wordend">τας</seg> εἴδεος
                        ἐλάσσονα λόγον <seg type="clearword">ἑξοῦν</seg></seg>
                    <seg n="8" type="line" facs="z125r2_08"><seg type="wordend">τι</seg>
                        <supplied reason="lost">ἢ</supplied> τὸ τετράγωνον τὸ ἀπὸ τᾶς <seg
                            type="clearword">με</seg></seg>
                    <seg n="9" type="line" facs="z125r2_09"><seg type="wordend">γίστας</seg> ποτὶ τὸ
                        ἴσον ἀμφοτέροις</seg>
                    <seg n="10" type="line" facs="z125r2_10">τῶι τε περιεχομένωι ὑπό τε τᾶς</seg>
                    <seg n="11" type="line" facs="z125r2_11">μεγίστας καὶ τᾶς ἐλαχίστας καὶ</seg>
                    <seg n="12" type="line" facs="z125r2_12">τῶι τρίτωι μέρει τοῦ ἀπὸ τᾶς <seg
                            type="clearword">ὑπε</seg></seg>
                    <seg n="13" type="line" facs="z125r2_13"><seg type="wordend">ροχᾶς</seg>, ἇ
                        ὑπερέχει ἁ μεγίστα τᾶς <seg type="clearword">ἐ</seg></seg>
                    <seg n="14" type="line" facs="z125r2_14"><seg type="wordend">λαχίστας</seg>,
                        ποτὶ δὲ τὰ ἀπὸ τᾶν <seg type="clearword">αὐ</seg></seg>
                    <seg n="15" type="line" facs="z125r2_15"><seg type="wordend">τᾶν</seg>
                        <seg type="word">εἴδ<unclear>ε</unclear>α</seg> χωρὶς <seg type="word"
                                >τ<supplied reason="lost">οῦ</supplied>
                        </seg> ἀπὸ τᾶς <choice>
                            <abbr>μεγίστ</abbr>
                            <expan>μεγίστας</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z125r2_16"><seg type="word">
                                μεί<unclear>ζ</unclear>ο<supplied reason="lost">ν</supplied>α</seg>
                        τοῦ αὐτοῦ λόγου· τὸν <choice>
                            <abbr>αὐτὸ</abbr>
                            <expan>αὐτὸν</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z125r2_17"><expan>γὰρ</expan> ἕξουσι <seg
                            type="word">
                            <unclear>λό</unclear>γον</seg> τὰ ὁμοῖα εἴδεα</seg>
                    <seg n="18" type="line" facs="z125r2_18">τοῖς τετραγώνοις.</seg>
                </seg>
                <seg n="125r2" type="folio">
                    <seg n="18" type="line" facs="z125r2_18">εἴ κα εὐθεῖα</seg>
                    <seg n="19" type="line" facs="z125r2_19">
                        <supplied reason="lost">ἐπιζευχθῆ</supplied>
                        <supplied reason="lost">γραμμὰ</supplied>
                        <supplied reason="lost">ἐν</supplied>
                        <supplied reason="lost">ἐπιπέδωι</supplied>
                        <supplied reason="lost">καὶ</supplied>
                        <seg type="suppliedword">μέ</seg>
                    </seg>
                </seg>
                <seg n="132v2" type="folio">
                    <seg n="1" type="line" facs="z132v2_01">
                        <seg type="wordend">νοντο<supplied reason="lost">ς</supplied></seg> τοῦ <seg
                            type="word">
                            <supplied reason="lost">ἑτ</supplied>έρ<supplied reason="lost"
                            >ου</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">π</supplied>έρατ<supplied reason="lost"
                            >ος</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">αὐτᾶ</supplied>ς</seg></seg>
                    <seg n="2" type="line" facs="z132v2_02">ἰσοταχέως περιενεχθεῖσα <seg
                            type="clearword">ὁσα</seg></seg>
                    <seg n="3" type="line" facs="z132v2_03"><seg type="wordend">κισοῦν</seg>
                        ἀποκατασταθῆι πάλιν,</seg>
                    <seg n="4" type="line" facs="z132v2_04"><seg type="word">ὅ<unclear>θ</unclear>εν</seg>
                        <seg type="word">ὥ<supplied reason="lost">ρ</supplied>μ<supplied
                                reason="lost">α</supplied>σεν</seg>, ἅμα δὲ τᾶ <seg type="clearword"
                            >γραμ</seg></seg>
                    <seg n="5" type="line" facs="z132v2_05"><seg type="wordend">μᾶι</seg>
                        περιαγομέναι φέρηταί τι <seg type="clearword">ση</seg></seg>
                    <seg n="6" type="line" facs="z132v2_06"><seg type="wordend">μεῖον</seg>
                        <seg type="word">ἰσοτ<supplied reason="lost">αχέ</supplied>ως</seg>
                        <seg type="word">αὐτ<unclear>ὸ</unclear>
                        </seg> ἑωυτῶ</seg>
                    <seg n="7" type="line" facs="z132v2_07">κατὰ τᾶς <seg type="word">
                            εὐ<unclear>θ</unclear>είας</seg> ἀρξάμενον</seg>
                    <seg n="8" type="line" facs="z132v2_08">ἀπὸ τοῦ <seg type="word">μένοντ<supplied
                                reason="lost">ο</supplied>ς</seg> πέρατος, τὸ <seg type="clearword"
                            >σα</seg></seg>
                    <seg n="9" type="line" facs="z132v2_09"><seg type="wordend">μεῖον</seg> ἕλικα
                        γράψει ἐν τῶι ἐπιπέδωι.</seg>
                </seg>
                <seg n="132v2" type="folio">
                    <seg n="10" type="line" facs="z132v2_10">καλείσθω οὖν τὸ μὲν πέρας τᾶς</seg>
                    <seg n="11" type="line" facs="z132v2_11">εὐθείας τὸ μένον περιαγομένας</seg>
                    <seg n="12" type="line" facs="z132v2_12">αὐτᾶς ἀρχὰ τᾶς <seg type="word">
                                ἕλικ<unclear>ος</unclear>
                        </seg>.</seg>
                </seg>
                <seg n="132v2" type="folio">
                    <seg n="12" type="line" facs="z132v2_12">
                        <supplied reason="lost">ἁ</supplied>
                        <seg type="word">
                            <supplied reason="lost">δ</supplied>ὲ</seg>
                        <seg type="unclearword">θ<unclear>έ</unclear></seg>
                    </seg>
                    <seg n="13" type="line" facs="z132v2_13"><seg type="wordend">σις</seg> τᾶς
                        γραμμᾶς, ἀφ’ ἇς ἄρξατο</seg>
                    <seg n="14" type="line" facs="z132v2_14">ἁ εὐθεῖα περιφέρεσθαι, ἀρχὰ τᾶς</seg>
                    <seg n="15" type="line" facs="z132v2_15">περιφορᾶς.</seg>
                </seg>
                <seg n="132v2" type="folio">
                    <seg n="15" type="line" facs="z132v2_15">εὐθεῖα, ἃν μὲν <seg type="word">
                            <unclear>αὐ</unclear>τᾶι</seg></seg>
                    <seg n="16" type="line" facs="z132v2_16">πρώται περιφορᾶ <seg type="word"
                                >δια<supplied reason="lost">π</supplied>ορευσθῆ</seg></seg>
                    <seg n="17" type="line" facs="z132v2_17">τὸ σαμεῖον <seg type="word">
                                τ<unclear>ὸ</unclear>
                        </seg> κατὰ τᾶς εὐθείας</seg>
                </seg>
            </ab>
        </body>
