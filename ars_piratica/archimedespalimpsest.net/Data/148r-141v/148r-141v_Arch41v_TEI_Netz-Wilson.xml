    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="148r1" type="folio">
                    <seg n="1" type="line" facs="z148r1_01">ποτὶ τὸν δεύτερον κύκλον <choice>
                            <abbr>τοῦτο</abbr>
                            <expan>τοῦτον</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z148r1_02">ἔχει τὸν λόγον, ὃν ἔχει τὰ <num>Ζ</num>
                        ποτὶ</seg>
                    <seg n="3" type="line" facs="z148r1_03">τὰ <num>ΙΒ</num>, ὅς ἐστιν ὁ αὐτὸς τῶι
                        ὃν ἔχει</seg>
                    <seg n="4" type="line" facs="z148r1_04">τὰ συναμφότερα τό τε <seg
                            type="clearword">περιεχόμε</seg></seg>
                    <seg n="5" type="line" facs="z148r1_05"><seg type="wordend">νον</seg> ὑπὸ τᾶς
                            <seg type="word">
                            <supplied reason="lost">ἐ</supplied>κ</seg> τοῦ κέντρου τοῦ</seg>
                    <seg n="6" type="line" facs="z148r1_06"><num>Β</num> κύκλου καὶ τᾶς ἐκ τοῦ
                        κέντρου</seg>
                    <seg n="7" type="line" facs="z148r1_07">τοῦ <num>Α</num> κύκλου καὶ τὸ τρίτον
                        μέρος</seg>
                    <seg n="8" type="line" facs="z148r1_08">τοῦ τετραγώνου τοῦ ἀπὸ τᾶς <seg
                            type="clearword">ὑπερο</seg></seg>
                    <seg n="9" type="line" facs="z148r1_09"><seg type="wordend">χᾶς</seg>, ἇ
                        ὑπερέχει ἁ ἐκ τοῦ <seg type="word">
                            <choice>
                                <abbr>κέντρ</abbr>
                                <expan>κέν<unclear>τρ</unclear><supplied reason="lost"
                                >ου</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="10" type="line" facs="z148r1_10">τοῦ <num>Β</num> κύκλου τᾶς ἐκ τοῦ <choice>
                            <abbr>κέντρ</abbr>
                            <expan>κέντρου</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z148r1_11">τοῦ <num>Α</num> κύκλου ποτὶ τὸ <seg
                            type="word">
                            <choice>
                                <abbr>τετράγωνο</abbr>
                                <expan>τετράγων<supplied reason="lost">ο</supplied>ν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="12" type="line" facs="z148r1_12">τὸ ἀπὸ τᾶς ἐκ τοῦ κέντρου τοῦ</seg>
                    <seg n="13" type="line" facs="z148r1_13"><num>Β</num> κύκλου.</seg>
                </seg>
                <seg n="148r1" type="folio">
                    <seg n="13" type="line" facs="z148r1_13">ἔστω ἕλιξ, ἐφ’ ἇς ἁ ΑΒ</seg>
                    <seg n="14" type="line" facs="z148r1_14">ΓΔΕ, ἐν τᾶι δευτέραι περιφορᾶι</seg>
                    <seg n="15" type="line" facs="z148r1_15">γεγραμμέναι, ἔστω δὲ τὸ μὲν Θ <seg
                            type="clearword">σα</seg></seg>
                    <seg n="16" type="line" facs="z148r1_16"><seg type="wordend">μεῖον</seg> ἀρχὰ
                        τᾶς ἕλικος, ἁ δὲ ΘΕ</seg>
                    <seg n="17" type="line" facs="z148r1_17">εὐθεῖα ἐν τᾶι ἀρχᾶι τᾶς <seg
                            type="clearword">περιφο</seg></seg>
                    <seg n="18" type="line" facs="z148r1_18">
                        <seg type="wordend">ρᾶς</seg> ἁ πρώτα, δὲ ΑΕ ἐν τᾶι <seg type="suppliedword"
                            >ἀρ</seg>
                    </seg>
                </seg>
                <seg n="141v1" type="folio">
                    <seg n="1" type="line" facs="z141v1_01">
                        <seg type="wordend">
                            <supplied reason="lost">χᾶι</supplied>
                        </seg>
                        <supplied reason="lost">τᾶς</supplied>
                        <supplied reason="lost">περιφορᾶς</supplied>
                        <supplied reason="lost">ἁ</supplied>
                        <supplied reason="lost">δευτέρα,</supplied>
                        <supplied reason="lost">ὁ</supplied>
                    </seg>
                    <seg n="2" type="line" facs="z141v1_02">δὲ κύκλος ὁ ΑΖ ΗΙ ὁ δεύτερος</seg>
                    <seg n="3" type="line" facs="z141v1_03"><seg type="word">
                            <unclear>ἔσ</unclear>
                            <supplied reason="lost">τ</supplied>
                            <unclear>ω</unclear>
                        </seg>, καὶ αἱ ΑΗ ΙΖ διάμετροι <expan>πρὸς</expan></seg>
                    <seg n="4" type="line" facs="z141v1_04"><seg type="word">
                            <supplied reason="lost">ὀρθ</supplied>ὰς</seg> ἀλλάλαις. δεικτέον <seg
                            type="word">
                            <supplied reason="lost">ὅ</supplied>τι</seg></seg>
                    <seg n="5" type="line" facs="z141v1_05">τὸ περιεχόμενον <seg type="word"
                                >χωρί<supplied reason="lost">ον</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">ὑ</supplied>πό</seg></seg>
                    <seg n="6" type="line" facs="z141v1_06">τε τᾶς ΑΒΓΔΕ ἕλικος καὶ τᾶς</seg>
                    <seg n="7" type="line" facs="z141v1_07">ΑΕ εὐθείας ποτὶ τὸν ΑΖΗΙ <seg
                            type="clearword">κύ</seg></seg>
                    <seg n="8" type="line" facs="z141v1_08"><seg type="wordend">κλον</seg> λόγον
                        ἔχει, ὃν τὰ <num>Ζ</num>
                        <expan>ποτὶ</expan>
                        <num>ΙΒ</num>.</seg>
                </seg>
                <seg n="141v1" type="folio">
                    <seg n="9" type="line" facs="z141v1_09">ἔστω δή τις κύκλος ὁ Ϟ, ἁ δὲ ἐκ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z141v1_10">κέντρου τοῦ Ϟ κύκλου δυνάμει <seg
                            type="clearword">ἴ</seg></seg>
                    <seg n="11" type="line" facs="z141v1_11"><seg type="wordend">σα</seg> τῶι τε ὑπὸ
                        τᾶν ΑΘ ΘΕ <seg type="clearword">περιε</seg></seg>
                    <seg n="12" type="line" facs="z141v1_12"><seg type="wordend">χομένωι</seg> καὶ
                        τῶι τρίτωι μέρει</seg>
                    <seg n="13" type="line" facs="z141v1_13">τοῦ ἀπὸ τᾶς ΔΕ τετραγώνου· <seg
                            type="clearword">ἕ</seg></seg>
                    <seg n="14" type="line" facs="z141v1_14"><seg type="wordend">ξει</seg> δὴ ὁ Ϟ
                        κύκλος <expan>πρὸς</expan> τὸν ΑΗΖΙ ὡς</seg>
                    <seg n="15" type="line" facs="z141v1_15">ἑπτὰ ποτὶ δώδεκα, <choice>
                            <abbr>δι</abbr>
                            <expan>διότι</expan>
                        </choice> καὶ ἁ ἐκ</seg>
                    <seg n="16" type="line" facs="z141v1_16">τοῦ κέντρου αὐτοῦ ποτὶ τὰν ἐκ</seg>
                    <seg n="17" type="line" facs="z141v1_17">τοῦ κέντρου τοῦ ΑΖΗΙ κύκλου
                        <expan>τοῦτ</expan></seg>
                </seg>
                <seg n="148r2" type="folio">
                    <seg n="1" type="line" facs="z148r2_01">ἔχει δυνάμει <seg type="word">
                            <unclear>τ</unclear>ὸν</seg> λόγον. <seg type="suppliedword"><supplied
                                reason="lost">δει</supplied>χ<unclear>θή</unclear></seg></seg>
                    <figure n="27.1">
                        <figDesc>Figure 27.1</figDesc>
                    </figure>
                    <seg n="2" type="line" facs="z148r2_02"><seg type="wordend">σεται</seg> οὖν ἴσος
                        ὁ Ϟ κύκλος τῶι <seg type="clearword">
                            <expan>περι</expan>
                        </seg></seg>
                    <seg n="3" type="line" facs="z148r2_03"><seg type="wordend">εχομένωι</seg>
                        χωρίωι ὑπό τε τᾶς</seg>
                    <seg n="4" type="line" facs="z148r2_04">ΑΒ ΓΔΕ ἕλικος καὶ τᾶς ΑΕ <seg
                            type="clearword">εὐ</seg></seg>
                    <seg n="5" type="line" facs="z148r2_05">
                        <seg type="wordend">θείας</seg>. </seg>
                </seg>
                <seg n="148r2" type="folio">
                    <seg n="5" type="line" facs="z148r2_05">εἰ γὰρ μή, <seg type="word">ἤτ<supplied
                                reason="lost">ο</supplied>ι</seg> μείζων <expan>ἐστὶν</expan></seg>
                    <seg n="6" type="line" facs="z148r2_06">ἢ <seg type="word">ἐλάτ<supplied
                                reason="lost">τ</supplied>ων</seg>. ἔστω δὴ πρότερον,</seg>
                    <seg n="7" type="line" facs="z148r2_07">εἰ <seg type="word">δυνα<supplied
                                reason="lost">τόν</supplied>
                        </seg>, μείζων. δυνατὸν δή</seg>
                </seg>
                <seg n="141v2" type="folio">
                    <seg n="1" type="line" facs="z141v2_01">
                        <supplied reason="lost">ἐστι</supplied>
                        <supplied reason="lost">περὶ</supplied>
                        <supplied reason="lost">τὸ</supplied>
                        <seg type="word">χ<supplied reason="lost">ωρί</supplied>ον</seg>
                        <seg type="clearword">περιγρά</seg>
                    </seg>
                    <seg n="2" type="line" facs="z141v2_02"><seg type="wordend">ψαι</seg> σχᾶμα
                        ἐπίπεδον ἐξ <seg type="clearword">ὁμοί</seg></seg>
                    <seg n="3" type="line" facs="z141v2_03"><seg type="wordend">ων</seg> τομέων
                        συγκείμενον, ὥστε</seg>
                    <seg n="4" type="line" facs="z141v2_04">τὸ <seg type="word">π<supplied
                                reason="lost">ε</supplied>ρ<supplied reason="lost"
                            >ι</supplied>γραφὲν </seg> σχᾶμα μεῖζον</seg>
                    <seg n="5" type="line" facs="z141v2_05">εἶμεν <seg type="word">τ<supplied
                                reason="lost">οῦ</supplied>
                        </seg> χωρίου ἐλάσσονι ἢ ὧι <seg type="suppliedword">ὑ</seg></seg>
                    <seg n="6" type="line" facs="z141v2_06"><seg type="wordend"><supplied
                                reason="lost">περ</supplied>έχει</seg> ὁ Ϟ κύκλος τοῦ χωρίου.</seg>
                    <seg n="7" type="line" facs="z141v2_07"><seg type="word">περι<supplied
                                reason="lost">γ</supplied>εγράφθω</seg>, καὶ ἔστω, ἐξ ὧν</seg>
                    <seg n="8" type="line" facs="z141v2_08">σύγκειται τὸ περιγεγραμμένον</seg>
                    <seg n="9" type="line" facs="z141v2_09">σχᾶμα, μέγιστος μὲν ὁ ΘΑΚ <seg
                            type="clearword">το</seg></seg>
                    <seg n="10" type="line" facs="z141v2_10"><seg type="wordend">μεύς</seg>,
                        ἐλάχιστος δὲ ὁ ΘοΔ· <choice>
                            <abbr>δῆλ</abbr>
                            <expan>δῆλον</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z141v2_11">οὖν, <expan>ὅτι</expan> τὸ περιγραφὲν
                        σχᾶμα</seg>
                    <seg n="12" type="line" facs="z141v2_12">ἔλασσόν ἐστιν τοῦ κύκλου. <seg
                            type="clearword">ἐκ</seg></seg>
                    <seg n="13" type="line" facs="z141v2_13"><seg type="wordend">βεβλήσθωσαν</seg>
                        αἱ εὐθεῖαι αἱ <seg type="clearword">ποι</seg></seg>
                    <seg n="14" type="line" facs="z141v2_14"><seg type="wordend">οῦσαι</seg> ποτὶ εὸ
                        Θ ἴσας γωνίας,</seg>
                    <seg n="15" type="line" facs="z141v2_15">ἔστ’ ἂν ποτὶ τὰν τοῦ δευτέρου</seg>
                    <seg n="16" type="line" facs="z141v2_16">κύκλου περιφέρειαν <seg
                            type="clearword">πέσων</seg></seg>
                    <seg n="17" type="line" facs="z141v2_17"><seg type="wordend">τι</seg>. <seg
                            type="word">
                            <supplied reason="lost">ἐ</supplied>ντὶ</seg> δή τινες γραμμαὶ τῶι</seg>
                    <seg n="18" type="line" facs="z141v2_18"><seg type="word">
                            <supplied reason="lost">ἴ</supplied>σωι</seg> ἀλλαλᾶν <seg type="word"
                                >ὑπερ<unclear>έ</unclear><supplied reason="lost"
                        >χ</supplied>ουσαι</seg></seg>
                </seg>
            </ab>
