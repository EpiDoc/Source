    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="14v1" type="folio">
                    <seg n="1" type="line" facs="z14v1_01">πρὸς ΔΗ δυνάμει, οὕτως ἡ ΖΒ
                        <expan>πρὸς</expan> ΒΗ </seg>
                    <seg n="2" type="line" facs="z14v1_02">μήκει, τουτέστιν ἡ ΜΝ πρὸς ΝΘ. ὡς </seg>
                    <seg n="3" type="line" facs="z14v1_03">δὲ <unclear>ἡ</unclear> ΜΝ πρὸς ΝΘ μήκει,
                            <seg type="word">οὕ<supplied reason="lost">τ</supplied>ως</seg> ἡ </seg>
                    <seg n="4" type="line" facs="z14v1_04">ΜΝ <expan>πρὸς</expan> ΝΞ δυνάμει·
                            <expan>καὶ</expan> ὡς ἄρα ἡ ΑΖ </seg>
                    <seg n="5" type="line" facs="z14v1_05">πρὸς <seg type="word">Δ<supplied
                                reason="lost">Η</supplied></seg>
                        <seg type="word"><unclear>δυν</unclear>άμει</seg>, οὕτως ἡ ΜΝ
                        <expan>πρὸς</expan>
                    </seg>
                    <seg n="6" type="line" facs="z14v1_06">ΝΞ <seg type="word">δ<supplied
                                reason="lost">υ</supplied>νάμει·</seg> ὥστε καὶ μήκει ἐν τῶι </seg>
                    <seg n="7" type="line" facs="z14v1_07">αὐτῶ λόγωι. καὶ ὡς ἄρα ὁ ἀπὸ ΑΖ </seg>
                    <seg n="8" type="line" facs="z14v1_08"><seg type="word"
                        >κ<unclear>ύ</unclear>βος</seg> πρὸς τὸν ἀπὸ ΔΗ κύβον, <seg
                            type="suppliedword">οὕ</seg></seg>
                    <seg n="9" type="line" facs="z14v1_09"><seg type="wordend"><supplied
                                reason="lost">τ</supplied>ως</seg> ἀπὸ ΜΝ κύβος πρὸς τὸν <seg
                            type="clearword">ἀ</seg></seg>
                    <seg n="10" type="line" facs="z14v1_10"><seg type="wordend">πὸ</seg> ΝΞ κύβον.
                        ἀλλ' ὡς μὲν ὁ ἀπὸ </seg>
                    <seg n="11" type="line" facs="z14v1_11">ΝΞ κύβος πρὸς τὸν ἀπὸ ΔΗ, οὕτως</seg>
                    <seg n="12" type="line" facs="z14v1_12">ΑΒΓ τμᾶμα πρὸς τὸ ΔΒΕ τμᾶμα,</seg>
                    <seg n="13" type="line" facs="z14v1_13">ὡς <seg type="word">δ<supplied
                                reason="lost">ὲ</supplied></seg>
                        <seg type="word"><unclear>ἀ</unclear>πὸ</seg> ΜΝ κύβος πρὸς τὸν ἀπὸ</seg>
                    <seg n="14" type="line" facs="z14v1_14">ΝΞ κύβον. ἀλλ' ὡς μὲν ὁ ἀπὸ ΑΖ</seg>
                    <seg n="15" type="line" facs="z14v1_15">κύβος πρὸς τὸν ἀπὸ ΔΗ κύβον, <seg
                            type="clearword">οὕ</seg></seg>
                    <seg n="16" type="line" facs="z14v1_16"><seg type="wordend">τως</seg> ΑΒ ΓΑ
                        τμᾶμα πρὸς τὸ <seg type="word">ΔΒ<unclear>Ε</unclear></seg></seg>
                    <seg n="17" type="line" facs="z14v1_17">τμᾶμα, ὡς δὲ ὁ ἀπὸ ΜΝ κύβος</seg>
                    <seg n="18" type="line" facs="z14v1_18">πρὸς τὸν ἀπὸ ΝΗΞ κύβον, οὕτως</seg>
                    <seg n="19" type="line" facs="z14v1_19">ἡ ΜΝ πρὸς ΝΤ· <seg type="word"><supplied
                                reason="lost">ὥσ</supplied>τε</seg> καὶ <seg type="clearword"
                        >διελόν</seg></seg>
                    <seg n="20" type="line" facs="z14v1_20"><seg type="wordend">τι</seg> ἐστὶν ὡς ὁ
                        ΑΔ ΕΓ τόμος πρὸς </seg>
                </seg>
                <seg n="19r1" type="folio">
                    <seg n="1" type="line" facs="z19r1_01"><seg type="word"><supplied reason="lost"
                                >τ</supplied>ὸ</seg> ΔΒ τμᾶμα, <seg type="word">οὕ<supplied
                                reason="lost">τω</supplied>ς</seg> ἡ <supplied reason="lost"
                        >ΜΤ</supplied> πρὸς </seg>
                    <seg n="2" type="line" facs="z19r1_02">ΝΤ, τουτέστι τὰ ΓΕ τῆς ΗΖ πρὸς </seg>
                    <seg n="3" type="line" facs="z19r1_03">ΤΝ. <expan>καὶ</expan> ἐπὶ τὸ στερεὸν τὸ
                        βάσιν μὲν </seg>
                    <seg n="4" type="line" facs="z19r1_04">ἔχον <seg type="word"
                            >τ<unclear>ὸ</unclear></seg> ἀπὸ ΑΖ τετράγωνον, ὕψος </seg>
                    <seg n="5" type="line" facs="z19r1_05">δὲ τὴν συγκειμένην ἔκ τε τῆς Β <choice>
                            <abbr>τ</abbr>
                            <expan>τᾶς</expan>
                        </choice>
                    </seg>
                    <seg n="6" type="line" facs="z19r1_06">ΔΗ καὶ τῆς ΑΖ, πρὸς τὸν ἀπὸ ΑΖ </seg>
                    <seg n="7" type="line" facs="z19r1_07">κύβον λόγον ἔχει, ὃν ἡ Β τῆς ΔΗ </seg>
                    <seg n="8" type="line" facs="z19r1_08">μετὰ τᾶς ΑΖ πρὸς ΖΑ, ὥστε καὶ ἡ </seg>
                    <seg n="9" type="line" facs="z19r1_09"><unclear>Σ</unclear> τῆς ΝΞ μετὰ τῆς ΝΜ
                        πρὸς ΝΜ, </seg>
                    <seg n="10" type="line" facs="z19r1_10">ἔστι δὲ καὶ ὡς ὁ ἀπὸ ΑΖ κύβος
                            <expan>πρὸς</expan>
                    </seg>
                    <seg n="11" type="line" facs="z19r1_11">τὸν ἀπὸ ΔΗ κύβον, οὕτως ἡ ΜΝ </seg>
                    <seg n="12" type="line" facs="z19r1_12">πρὸς ΝΤ, ὥστε ἀπὸ ΔΗ κύβος </seg>
                    <seg n="13" type="line" facs="z19r1_13">πρὸς τὸ στερεὸν τὸ βάσιν μὲν <seg
                            type="clearword">ἔ</seg></seg>
                    <seg n="14" type="line" facs="z19r1_14"><seg type="wordend">χον</seg> τὸ ἀπὸ ΔΗ
                        τετράγωνον, </seg>
                    <seg n="15" type="line" facs="z19r1_15">ὕψος δὲ τὴν συγκειμένην ἔκ τε </seg>
                    <seg n="16" type="line" facs="z19r1_16">τῆς Β τῆς ΑΖ μετὰ τῆς ΔΗ, <seg
                            type="clearword">οὕ</seg></seg>
                    <seg n="17" type="line" facs="z19r1_17"><seg type="wordend">τως</seg> ἡ ΔΗ πρὸς
                        τὴν <choice>
                            <abbr>συγκειμενη</abbr>
                            <expan>συγκειμένην</expan>
                        </choice>
                    </seg>
                    <seg n="18" type="line" facs="z19r1_18">ἔκ τε τῆς Β τῆς ΑΖ καὶ τῆς ΔΗ, </seg>
                </seg>
                <seg n="14v2" type="folio">
                    <seg n="1" type="line" facs="z14v2_01">ὥστε καὶ ἡ <seg type="word"
                            ><unclear>Τ</unclear>Ν</seg>
                        <expan>πρὸς</expan> τὴν <seg type="word">
                            <choice>
                                <abbr><supplied reason="lost">συγκειμ</supplied>ένη</abbr>
                                <expan><supplied reason="lost">συγκειμ</supplied>ένην</expan>
                            </choice>
                        </seg>
                    </seg>
                    <seg n="2" type="line" facs="z14v2_02">ἔκ τε τῆς Β τῆς ΟΝ καὶ <seg type="word"
                                >τῆ<unclear>ς</unclear></seg>
                        <supplied reason="lost">ΤΝ</supplied>, </seg>
                    <seg n="3" type="line" facs="z14v2_03">γέγονεν οὖν <seg type="word"
                                >τάσσαρ<supplied reason="lost">α</supplied></seg>
                        <seg type="word"><supplied reason="lost"
                                >μεγ</supplied><unclear>έ</unclear>θ<supplied reason="lost"
                            >εα</supplied>,</seg>
                    </seg>
                    <seg n="4" type="line" facs="z14v2_04">τὸ στερεὸν τὸ βάσιν <seg type="word"
                                >μ<supplied reason="lost">ὲν</supplied></seg>
                        <seg type="word">ἔ<supplied reason="lost">χο</supplied>ν</seg>
                    </seg>
                    <seg n="5" type="line" facs="z14v2_05">τὸ ἀπὸ ΑΖ τετράγωνον, ὕψος δὲ </seg>
                    <seg n="6" type="line" facs="z14v2_06">τὴν συγκειμένην ἔκ τε τῆς Β τῆς </seg>
                    <seg n="7" type="line" facs="z14v2_07">ΔΗ καὶ τῆς ΑΖ, κύβος καὶ ὁ ἀπὸ </seg>
                    <seg n="8" type="line" facs="z14v2_08">ΔΗ κύβος καὶ τὸ στερεὸν τὸ <seg
                            type="clearword">βά</seg></seg>
                    <seg n="9" type="line" facs="z14v2_09"><seg type="wordend">σιν</seg> μὲν ἔχον τὸ
                        ἀπὸ ΔΗ <seg type="suppliedword"><supplied reason="lost"
                        >τ</supplied>ετράγω</seg></seg>
                    <seg n="10" type="line" facs="z14v2_10"><seg type="wordend">νον,</seg> ὕψος δὲ
                        τὴν συγκειμένην </seg>
                    <seg n="11" type="line" facs="z14v2_11">ἔκ τε τῆς Β τῆς ΑΖ καὶ τῆς ΔΗ, </seg>
                    <seg n="12" type="line" facs="z14v2_12">τέταρσι μεγέθεσιν ἀνάλογον <seg
                            type="clearword">σὺν</seg></seg>
                    <seg n="13" type="line" facs="z14v2_13"><seg type="wordend">δυο</seg>
                        λαμβανομένοις, τῆι τε <seg type="clearword">συγ</seg></seg>
                    <seg n="14" type="line" facs="z14v2_14"><seg type="wordend">κειμένηι</seg> ἔκ τε
                        τῆς Β τῆς ΝΞ καὶ </seg>
                    <seg n="15" type="line" facs="z14v2_15">τῆς ΝΜ καὶ ἑτέρου μεγέθεος <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice>
                    </seg>
                    <seg n="16" type="line" facs="z14v2_16">ΜΝ καὶ ἄλλο ἑξῆς ἡ ΝΓ καὶ <seg
                            type="clearword">τελευ</seg></seg>
                    <seg n="17" type="line" facs="z14v2_17"><seg type="wordend">ταῖον</seg> ἡ
                        συγκειμένη ἔκ τε τῆς ΒΗ </seg>
                    <seg n="18" type="line" facs="z14v2_18">τῆς ΝΟ καὶ τῆς ΝΤ· δι' ἴσου ἄρα </seg>
                    <seg n="19" type="line" facs="z14v2_19">γενήσεται ὡς τὸ στερεὸν τὸ βάσιν </seg>
                    <seg n="20" type="line" facs="z14v2_20">μὲν ἔχον τὸ ἀπὸ ΑΖ τετράγωνος, </seg>
                    <seg n="21" type="line" facs="z14v2_21">ὕψος δὲ τὴν <seg type="word">
                            <choice>
                                <abbr><supplied reason="lost">συγκειμ</supplied>ένη</abbr>
                                <expan><supplied reason="lost">συγκειμ</supplied>ένην</expan>
                            </choice>
                        </seg> ἔκ τε τῆς </seg>
                </seg>
                <seg n="19r2" type="folio">
                    <seg n="1" type="line" facs="z19r2_01">Β τῆς ΔΗ καὶ τῆς ΑΖ, <seg type="word"
                                >π<supplied reason="lost">ρὸ</supplied>ς</seg> τὸ <seg
                            type="clearword">στε</seg></seg>
                    <seg n="2" type="line" facs="z19r2_02"><seg type="wordend">ρεὸν</seg> τὸ βάσιν
                        μὲν ἔχον τὸ ἀπὸ ΔΗ </seg>
                    <seg n="3" type="line" facs="z19r2_03">τετράγωνον, ὕψος δὲ τὴν <seg
                            type="clearword">συγκειμέ</seg></seg>
                    <seg n="4" type="line" facs="z19r2_04"><seg type="wordend">νην</seg> ἔκ τε τῆς Β
                        τῆς ΑΖ καὶ τῆς </seg>
                    <seg n="5" type="line" facs="z19r2_05">ΔΗ, οὕτως ἡ συγκειμένη ἔκ τε τῆς </seg>
                    <seg n="6" type="line" facs="z19r2_06">Β τῆς ΝΞ καὶ τῆς ΜΝ πρὸς τὴν </seg>
                    <seg n="7" type="line" facs="z19r2_07">συγκειμένην ἔκ τε τῆς Β τῆς ΝΟ </seg>
                    <seg n="8" type="line" facs="z19r2_08">καὶ τῆς ΝΤ. ἀλλ' ὡς τὸ εἰρημένον </seg>
                    <seg n="9" type="line" facs="z19r2_09">στερεὸν πρὸς τὸ εἰρημένον <seg
                            type="clearword">στερε</seg></seg>
                    <seg n="10" type="line" facs="z19r2_10"><seg type="word">όν</seg>, οὕτως ἡ ΘΙ
                        πρὸς ΙΚ· οὕτως ἡ </seg>
                    <seg n="11" type="line" facs="z19r2_11">συγκειμένη πρὸς τὴν <choice>
                            <abbr>συγκειμεν</abbr>
                            <expan>συγκειμένην</expan>
                        </choice>· </seg>
                    <seg n="12" type="line" facs="z19r2_12">ὥστε καὶ συνθέντι καὶ τῶν <seg
                            type="clearword">ἡγου</seg></seg>
                    <seg n="13" type="line" facs="z19r2_13"><seg type="wordend">μένων</seg> τὰ Ε·
                        ἔστιν ἄρα ὡς ἡ ΖΗ </seg>
                    <seg n="14" type="line" facs="z19r2_14">πρὸς ΙΚ, οὕτως ἡ Ε <seg type="clearword"
                            >συναμφοτέ</seg></seg>
                    <seg n="15" type="line" facs="z19r2_15"><seg type="wordend">ρου</seg> τῆς ΜΝΤ
                            <expan>καὶ</expan> Ι <choice>
                            <abbr>συναμφοτερ</abbr>
                            <expan>συναμφοτέρου</expan>
                        </choice>
                    </seg>
                    <seg n="16" type="line" facs="z19r2_16"><seg type="word"><supplied reason="lost"
                                >τ</supplied>ῆ<supplied reason="lost">ς</supplied></seg>
                        <seg type="word"><supplied reason="lost">Ν</supplied>Ξ</seg> ΝΟ πρὸς τὴν Β
                        τῆς ΟΝ </seg>
                    <seg n="17" type="line" facs="z19r2_17"><expan>καὶ</expan> ΝΤ. καὶ ὡς ἡ ΖΗ πρὸς
                        ΖΚ <choice>
                            <abbr>ουσα</abbr>
                            <expan>οὖσαν</expan>
                        </choice></seg>
                </seg>
            </ab>
        </body>
    </text>
