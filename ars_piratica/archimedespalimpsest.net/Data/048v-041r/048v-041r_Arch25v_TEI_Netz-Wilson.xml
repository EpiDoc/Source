    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="48v1" type="folio">
                    <seg n="1" type="line" facs="z48v1_01"><seg type="word"
                        ><unclear>τ</unclear>ῆς</seg> προκειμένης πρὸς τὸν ἄξονα</seg>
                    <seg n="2" type="line" facs="z48v1_02">αὐτοῦ τοῦ <seg type="word"
                            ><unclear>κ</unclear>ωνοειδέος</seg> καὶ τὴν <seg type="suppliedword"
                            >τετρα</seg></seg>
                    <seg n="3" type="line" facs="z48v1_03"><seg type="wordend"><supplied
                                reason="lost">πλ</supplied>ασ<supplied reason="lost"
                        >ίαν</supplied></seg> αὐτῆς τῆς <sic>προκειμένη</sic></seg>
                    <seg n="4" type="line" facs="z48v1_04"><seg type="word"><unclear>ὄ</unclear>ντων</seg>
                        <unclear>δὲ</unclear> καὶ ἄλλων πλειόνων <w part="I">ὁ</w></seg>
                    <seg n="5" type="line" facs="z48v1_05"><w part="F">μοίων</w>
                        <seg type="word">το<supplied reason="lost">ύ</supplied>το<supplied
                                reason="lost">ι</supplied><unclear>ς</unclear></seg> θεωρουμένων τὰ</seg>
                    <seg n="6" type="line" facs="z48v1_06">
                        <seg type="word">πλεί<supplied reason="lost">ω</supplied></seg> περιλήψομεν. <sic>
                            <seg type="word">ἀρκο<unclear>ῦν</unclear>τος</seg>
                        </sic></seg>
                    <seg n="7" type="line" facs="z48v1_07"><seg type="word"><supplied reason="lost"
                                >γ</supplied>ὰ<unclear>ρ</unclear></seg> ὁ τρόπος ὑποδέδεικται διὰ <choice>
                            <abbr>τ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z48v1_08"><seg type="word">προειρημέν<supplied
                                reason="lost">ω</supplied>ν</seg>.</seg>
                </seg>
                <seg n="48v1" type="folio">
                    <seg n="8" type="line" facs="z48v1_08">ἐὰν εἰς πρίσμα</seg>
                    <seg n="9" type="line" facs="z48v1_09"><seg type="word"
                        >ὀ<unclear>ρ</unclear>θὸν</seg> τετραγώνους ἔχοντι <choice>
                            <abbr>βάσ</abbr>
                            <expan>βάσεις</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z48v1_10">κύλινδρος ἐγγραφῆ τὰς μὲν <seg
                            type="suppliedword">βά</seg></seg>
                    <seg n="11" type="line" facs="z48v1_11"><seg type="wordend"
                                ><unclear>σ</unclear>ει<supplied reason="lost">ς</supplied></seg>
                        ἔχων ἐν τοῖς ἀπεναντίον</seg>
                    <seg n="12" type="line" facs="z48v1_12">τετραγώνοις, τὴν δὲ <seg type="word">
                            <choice>
                                <abbr>ἐπιφάν<supplied reason="lost">εια</supplied></abbr>
                                <expan>ἐπιφάν<supplied reason="lost">εια</supplied>ν</expan>
                            </choice>
                        </seg>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z48v1_13">λοιπῶν παραλληλογράμμων</seg>
                    <seg n="14" type="line" facs="z48v1_14">τεσσάρων ἐπιπέδων <seg type="clearword"
                            >ἐφαπτόμε</seg></seg>
                    <seg n="15" type="line" facs="z48v1_15"><seg type="wordend">νον</seg>, διὰ δὲ
                        τοῦ κέντρου τοῦ κύκλου, ὅ <expan>ἐστι</expan></seg>
                    <seg n="16" type="line" facs="z48v1_16">βάσις τοῦ κυλίνδρου, καὶ μιᾶς <seg
                            type="clearword">πλευ</seg></seg>
                    <seg n="17" type="line" facs="z48v1_17">
                        <seg type="wordend">ρᾶς</seg> τοῦ <seg type="word"><supplied reason="lost"
                                >τ</supplied>ε<supplied reason="lost">τραγώνου</supplied></seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἐπίπε</supplied>
                        </seg>
                    </seg>
                </seg>
                <seg n="41r1" type="folio">
                    <seg n="1" type="line" facs="z41r1_01">
                        <seg type="wordend">δον</seg>
                        <seg type="word">ἀχ<supplied reason="lost">θ</supplied><unclear>ῆ</unclear></seg>
                        <seg type="word">τ<supplied reason="lost">ὸ</supplied></seg>
                        <seg type="word"><supplied reason="lost"
                                >ἀ</supplied>π<unclear>ο</unclear><supplied reason="lost"
                                >τ</supplied>μ<supplied reason="lost"
                            >η</supplied>θ<unclear>ὲ</unclear>ν</seg>
                        <seg type="unclearword"><unclear>σ</unclear>χῆ</seg>
                    </seg>
                    <seg n="2" type="line" facs="z41r1_02"><seg type="wordend">
                            <unclear>μα</unclear>
                        </seg> ὑπὸ τοῦ <seg type="word"><supplied reason="lost"
                                >ἀ</supplied>χ<unclear>θέ</unclear>ντος</seg>
                        <choice>
                            <abbr>ἐπιπέδ</abbr>
                            <expan>ἐπιπέδου</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z41r1_03"><supplied reason="lost">
                            <num>ϛ</num>
                        </supplied> ἐστὶ μέρος τοῦ <seg type="word">ὅλο<unclear>υ</unclear></seg>
                        <seg type="word">πρίσματ<unclear>ο</unclear>ς</seg>,</seg>
                    <seg n="4" type="line" facs="z41r1_04"><expan>διὰ</expan> τοῦ τρόπου <seg
                            type="word">τούτ<unclear>ο</unclear>υ</seg>
                        <seg type="word">θεωρεῖτ<unclear>α</unclear>ι</seg>.</seg>
                    <seg n="5" type="line" facs="z41r1_05">δείξαντες δὴ <seg type="word"
                                >ἀναχωρή<supplied reason="lost"
                                >σ</supplied><unclear>ο</unclear>μ<supplied reason="lost"
                            >εν</supplied></seg></seg>
                    <seg n="6" type="line" facs="z41r1_06">ἐπὶ τὴν διὰ τῶν <w part="I"
                        >γεωμετρουμέ</w></seg>
                    <seg n="7" type="line" facs="z41r1_07"><w part="F">νων</w> ἀπόδειξιν <seg
                            type="word">αὐτο<supplied reason="lost">ῦ</supplied></seg>.</seg>
                </seg>
                <seg n="41r1" type="folio">
                    <seg n="7" type="line" facs="z41r1_07">
                        <seg type="word">νοεί<unclear>σθω</unclear></seg>
                    </seg>
                    <seg n="8" type="line" facs="z41r1_08">πρίσμα ὀρθὸν <seg type="word"
                                >τετρ<supplied reason="lost">αγ</supplied><unclear>ώ</unclear>νους</seg>
                        <seg type="word">
                            <choice>
                                <abbr>ἔχ</abbr>
                                <expan>ἔχ<supplied reason="lost">ον</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="9" type="line" facs="z41r1_09">βάσεις ἐν τῶι πρίσματι <w part="I"
                        >κύλιν</w></seg>
                    <seg n="10" type="line" facs="z41r1_10"><w part="F">δρος</w>
                        <seg type="word">ἐγγεγρ<supplied reason="lost">α</supplied>μμένος</seg> ὡς
                            <w part="I">εἴρη</w></seg>
                    <seg n="11" type="line" facs="z41r1_11"><w part="F">ται</w>, <seg type="word"
                                >τμηθέντο<supplied reason="lost">ς</supplied></seg> δὲ τοῦ <seg
                            type="suppliedword">πρί<supplied reason="lost">σ</supplied>μ<supplied
                                reason="lost">α</supplied></seg>
                    </seg>
                    <seg n="12" type="line" facs="z41r1_12"><seg type="wordend">τος</seg>
                        <expan>διὰ</expan> τοῦ ἄξονος ἐπιπέδωι <seg type="suppliedword">ὀρ</seg></seg>
                    <seg n="13" type="line" facs="z41r1_13"><seg type="wordend"><supplied
                                reason="lost">θ</supplied>ῶι</seg>
                        <expan>πρὸς</expan> τὸ <seg type="word">ἐπίπεδο<supplied reason="lost"
                            >ν</supplied></seg> τὸ <w part="I">ἀποτετμη</w></seg>
                    <seg n="14" type="line" facs="z41r1_14"><w part="F">κὸς</w> τὸ <seg type="word"
                                ><supplied reason="lost">τ</supplied>μ<supplied reason="lost"
                            >ῆ</supplied>μα</seg> τοῦ κυλίνδρου <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z41r1_15">
                        <seg type="word">μὲ<supplied reason="lost">ν</supplied></seg>
                        <seg type="word"><unclear>π</unclear><supplied reason="lost"
                                >ρί</supplied><unclear>σ</unclear>ματ<unclear>ος</unclear></seg>
                        <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">οῦ</supplied></expan>
                            </choice>
                        </seg>
                        <seg type="word">κύλ<supplied reason="lost"
                            >ί</supplied>νδρο<unclear>υ</unclear></seg>
                    </seg>
                    <seg n="16" type="line" facs="z41r1_16"><seg><supplied reason="lost"
                                >κ</supplied>ο<unclear>ιν</unclear><supplied reason="lost"
                            >ὴ</supplied></seg>
                        <seg type="word"><supplied reason="lost">το</supplied>μὴ</seg> ἔστω ΑΒ <seg
                            type="unclearword">παραλληλ<unclear>ό</unclear></seg></seg>
                </seg>
                <seg n="48v2" type="folio">
                    <seg n="1" type="line" facs="z48v2_01"><seg type="wordend">γραμμον</seg>, τοῦ δὲ
                        ἐπιπέδου τοῦ <w part="I">ἀ</w></seg>
                    <seg n="2" type="line" facs="z48v2_02"><w part="F">ποτετμηκότος</w> τὸ <seg
                            type="word">τμῆμ<unclear>α</unclear></seg>
                        <seg type="word">ἀ<supplied reason="lost">π</supplied>ὸ</seg></seg>
                    <seg n="3" type="line" facs="z48v2_03">τοῦ κυλίνδρου καὶ τοῦ <expan>διὰ</expan>
                        τοῦ <seg type="suppliedword">ἄ<supplied reason="lost">ξο</supplied></seg></seg>
                    <seg n="4" type="line" facs="z48v2_04"><seg type="wordend">νος</seg> ἠγμένου
                        ἐπιπέδου ὀρθοῦ <expan>πρὸς</expan></seg>
                    <seg n="5" type="line" facs="z48v2_05">τὸ ἐπίπεδον τὸ ἀποτετμηκὸς τὸ</seg>
                    <seg n="6" type="line" facs="z48v2_06">ἀπὸ τοῦ κυλίνδρου τμῆμα <w part="I"
                        >κοι</w></seg>
                    <seg n="7" type="line" facs="z48v2_07"><w part="F">νὴ</w> τομὴ ἔστω ἡ ΒΓ εὐθεῖα, <choice>
                            <abbr>ἄξω</abbr>
                            <expan>ἄξων</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z48v2_08">δὲ ἔστω τοῦ πρίσματος καὶ <seg
                            type="word">τ<supplied reason="lost">ο</supplied>ῦ</seg></seg>
                    <seg n="9" type="line" facs="z48v2_09">κυλίνδρου ἡ ΓΔ εὐθεῖα, καὶ <w part="I"
                            >τεμνέ</w></seg>
                    <seg n="10" type="line" facs="z48v2_10"><w part="F">τω</w> αὐτὴν ἡ ΕΖ <seg
                            type="word">δί<unclear>χ</unclear>α</seg> καὶ <expan>πρὸς</expan>
                        <choice>
                            <abbr>ὀρθ</abbr>
                            <expan>ὀρθάς</expan>
                        </choice>,</seg>
                    <seg n="11" type="line" facs="z48v2_11"><expan>καὶ</expan>
                        <expan>διὰ</expan> τῆς ΕΖ ἐπίπεδον ἀνεστάτω</seg>
                    <seg n="12" type="line" facs="z48v2_12">ὀρθὸν <expan>πρὸς</expan> τὴν ΓΔ·
                        ποιήσει δὴ <w part="I">τοῦ</w></seg>
                    <seg n="13" type="line" facs="z48v2_13"><w part="F">το</w> ἐν μὲν τῶ πρίσματι
                        τομὴν</seg>
                    <seg n="14" type="line" facs="z48v2_14">τετράγωνον, ἐν δὲ τῶι κυλίνδρωι</seg>
                    <seg n="15" type="line" facs="z48v2_15">τομὴν κύκλον. ἔστω οὖν τοῦ μὲν</seg>
                    <seg n="16" type="line" facs="z48v2_16">πρίσματος τομὴ τὸ ΜΝ <w part="I"
                        >τετρά</w></seg>
                    <seg n="17" type="line" facs="z48v2_17"><w part="F">γωνον</w>, τοῦ δὲ κυλίνδρου
                        ὁ ΞΟ ΠΟ</seg>
                    <seg n="18" type="line" facs="z48v2_18">
                        <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice>
                        <seg type="word">ἔ<unclear>στ</unclear><supplied reason="lost"
                        >ω</supplied></seg> δὴ <seg type="word">
                            <unclear>τ</unclear>
                            <supplied reason="lost">ὰ</supplied>
                        </seg> Π<gap extent="1"/>
                        <seg type="word"><unclear>σ</unclear>η<unclear>μεῖ</unclear><supplied
                                reason="lost">α</supplied></seg>
                        <gap extent="3"/>
                    </seg>
                    <seg n="19" type="line" facs="z48v2_19">
                        <gap extent="3"/>
                        <supplied reason="lost">ὥστε</supplied>
                        <supplied reason="lost">ἐφαπτέσθω</supplied>
                        <supplied reason="lost">ὁ</supplied>
                        <supplied reason="lost">
                            <expan>κύκλος</expan>
                        </supplied>
                    </seg>
                </seg>
                <seg n="41r2" type="folio">
                    <seg n="1" type="line" facs="z41r2_01">τῶν τοῦ <seg type="word">τετρα<supplied
                                reason="lost">γ</supplied><unclear>ώ</unclear>ν<supplied
                                reason="lost">ου</supplied></seg>
                        <seg type="word"><supplied reason="lost"
                            >π</supplied>λ<unclear>ευ</unclear>ρῶν</seg></seg>
                    <seg n="2" type="line" facs="z41r2_02">κατὰ τὰ ΞΟΠΡ σημεῖα, <seg type="word"
                                >το<supplied reason="lost">ῦ</supplied></seg> δὲ</seg>
                    <seg n="3" type="line" facs="z41r2_03">ἐπιπέδου τοῦ ἀποτετμηκότος</seg>
                    <seg n="4" type="line" facs="z41r2_04">τὸ τμῆμα ἀπὸ τοῦ <seg type="word"
                                ><supplied reason="lost">κ</supplied>υλίνδρου</seg></seg>
                    <seg n="5" type="line" facs="z41r2_05">καὶ τοῦ διὰ τῆς ΕΖ ἀχθέντος</seg>
                    <seg n="6" type="line" facs="z41r2_06">ἐπιπέδου ὀρθοῦ <expan>πρὸς</expan> τὸν
                        ἄξονα</seg>
                    <seg n="7" type="line" facs="z41r2_07">τοῦ κυλίνδρου κοινὴ τομὴ <seg type="word"
                                ><unclear>ἔ</unclear>στω</seg></seg>
                    <seg n="8" type="line" facs="z41r2_08">ἡ ΚΛ εὐθεῖα· τέμνει δὲ αὐτὴν δίχα</seg>
                    <seg n="9" type="line" facs="z41r2_09">ἡ ΠΘΞ. ἤχθω δέ τις εὐθεῖα ἐν τῶ</seg>
                    <seg n="10" type="line" facs="z41r2_10">ΟΠΡ ἡμικυκλίωι ἡ ΣΤ <expan>πρὸς</expan>
                        <choice>
                            <abbr>ὀρθ</abbr>
                            <expan>ὀρθὰς</expan>
                        </choice>
                        <w part="I">οὖ</w></seg>
                    <seg n="11" type="line" facs="z41r2_11"><w part="F">σα</w> τῆι ΠΞ, καὶ ἀπὸ τῆς
                        ΣΤ <w part="I">ἐπί</w></seg>
                    <seg n="12" type="line" facs="z41r2_12"><w part="F">πεδον</w> ἀνασταθὲν ὀρθὸν
                            <expan>πρὸς</expan>
                        <seg type="word">
                            <choice>
                                <abbr>
                                    <unclear>τ</unclear>
                                </abbr>
                                <expan>
                                    <unclear>τ</unclear>
                                    <supplied reason="lost">ὴν</supplied>
                                </expan>
                            </choice>
                        </seg></seg>
                    <seg n="13" type="line" facs="z41r2_13">ΞΠ ἐκβεβλήσθω ἐφ᾽ ἑκάτερα</seg>
                    <seg n="14" type="line" facs="z41r2_14">τὸ ἐπίπεδον, <seg type="word"
                                >ἐ<unclear>ν</unclear></seg> ὧι <expan>ἐστιν</expan> ὁ ΞΟΠΡ <seg
                            type="suppliedword">κύ</seg></seg>
                    <seg n="15" type="line" facs="z41r2_15"><seg type="wordend">κλ<supplied
                                reason="lost">ος·</supplied></seg>
                        <seg type="word"><supplied reason="lost"
                            >π</supplied><unclear>οι</unclear>ήσει</seg> δὴ τοῦτο ἐν τῶι <w part="I"
                            >ἡμικυ</w></seg>
                    <seg n="16" type="line" facs="z41r2_16"><w part="F">λίνδρωι</w>, οὗ
                        <expan>ἐστι</expan> βάσις τὸ ΟΠΡ <w part="I">ἡμικύ</w></seg>
                    <seg n="17" type="line" facs="z41r2_17"><w part="F">κλιος</w>, <seg type="word"
                                ><supplied reason="lost">ὕ</supplied><unclear>ψ</unclear>ος</seg> δὲ
                        ὁ ἄξων τοῦ <w part="I">πρίσ</w></seg>
                </seg>
            </ab>
        </body>
