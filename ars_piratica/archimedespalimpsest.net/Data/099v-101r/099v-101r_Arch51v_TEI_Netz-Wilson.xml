    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="99v1" type="folio">
                    <seg n="1" type="line" facs="z99v1_01">τῆς ἐπιφανείας τοῦ κυλίνδρου. <w part="I"
                            >ἐ</w></seg>
                    <seg n="2" type="line" facs="z99v1_02"><w part="F">δείχθη</w> δὲ
                        <expan>ὅτι</expan> οὐδὲ ἐλάσσων· ἴσον <expan>ἄρα</expan>
                        <expan>ἐστίν</expan>.</seg>
                </seg>
                <seg n="99v1" type="folio">
                    <seg n="3" type="line" facs="z99v1_03">παντὸς κώνου ἰσοσκελοῦς <choice>
                            <abbr>χωρ</abbr>
                            <expan>χωρὶς</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z99v1_04">τῆς βάσεως ἡ ἐπιφάνεια ἴση</seg>
                    <seg n="5" type="line" facs="z99v1_05"><expan>ἐστὶ</expan> κύκλωι οὗ ἡ ἐκ τοῦ
                        κέντρου <w part="I">μέ</w></seg>
                    <seg n="6" type="line" facs="z99v1_06"><w part="F">σον</w> λόγον ἔχει τῆς
                        πλευρᾶς</seg>
                    <seg n="7" type="line" facs="z99v1_07">τοῦ κώνου καὶ τῆς ἐκ τοῦ <w part="I"
                        >κέν</w></seg>
                    <seg n="8" type="line" facs="z99v1_08"><w part="F">τρου</w> τοῦ κύκλου ὅς ἐστιν
                        βάσις <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                </seg>
                <seg n="101r1" type="folio">
                    <seg n="1" type="line" facs="z101r1_01"><seg type="word"><supplied reason="lost"
                                >κώ</supplied><unclear>ν</unclear>ου</seg>. ἔστω κῶνος <seg
                            type="word">ἰ<supplied reason="lost">σοσ</supplied>κελής</seg></seg>
                    <seg n="2" type="line" facs="z101r1_02">οὗ βάσις ὁ Α <expan>κύκλος</expan> ἡ δὲ
                        ἐκ τοῦ <w part="I">κέν</w></seg>
                    <seg n="3" type="line" facs="z101r1_03"><w part="F">τρου</w> ἔστω ἡ Γ τῆι δὲ
                        πλευρά <seg type="word">
                            <supplied reason="lost">τοῦ</supplied>
                        </seg></seg>
                    <seg n="4" type="line" facs="z101r1_04">κώνου ἔστω ἴση ἡ Δ τῶν δὲ Γ Δ</seg>
                    <seg n="5" type="line" facs="z101r1_05">μέση ἀνάλογον ἡ Ε ὁ δὲ Β <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z101r1_06">ἐχέτω τὴν ἐκ τοῦ κέντρου τῆ Ε</seg>
                    <seg n="7" type="line" facs="z101r1_07">ἴσην· λέγω <expan>ὅτι</expan> ὁ κύκλος
                            <expan>ἐστὶν</expan> ἴσος τῆι <w part="I">ἐ</w></seg>
                    <seg n="8" type="line" facs="z101r1_08"><w part="F">πιφανείαι</w> τοῦ κώνου
                        χωρὶς τῆς</seg>
                    <seg n="9" type="line" facs="z101r1_09">βάσεως.</seg>
                </seg>
                <seg n="101r1" type="folio">
                    <seg n="9" type="line" facs="z101r1_09">εἰ γὰρ μή ἐστιν ἴσος ἤτοι</seg>
                    <seg n="10" type="line" facs="z101r1_10">μείζων <expan>ἐστὶν</expan> ἢ ἐλάσσων.
                        ἔστω <w part="I">πρό</w></seg>
                    <seg n="11" type="line" facs="z101r1_11"><w part="F">τερον</w> ἐλάσσων. ἔστι δὴ
                            <seg type="word"><unclear>δ</unclear>ύο</seg>
                        <w part="I">με</w></seg>
                    <seg n="12" type="line" facs="z101r1_12"><w part="F">γέθη</w> ἄνισα ἥ τε <seg
                            type="word">ἐπι<supplied reason="lost">φάνεια</supplied></seg></seg>
                    <seg n="13" type="line" facs="z101r1_13">τοῦ κώνου καὶ ὁ Β <seg type="word"
                                ><unclear>κύκλ</unclear>ος</seg> καὶ</seg>
                    <seg n="14" type="line" facs="z101r1_14">μείζων ἡ ἐπιφάνεια τοῦ κώνου·</seg>
                    <seg n="15" type="line" facs="z101r1_15">δυνατὸν ἄρα εἰς τὸν Β κύκλον <w
                            part="I">πολύ</w></seg>
                    <seg n="16" type="line" facs="z101r1_16"><w part="F">γωνον</w> ἰσόπλευρον <seg
                            type="word">ἐγ<supplied reason="lost"
                                >γ</supplied><unclear>ρ</unclear><supplied reason="lost"
                            >ά</supplied>ψαι</seg>
                        <expan>καὶ</expan></seg>
                    <seg n="17" type="line" facs="z101r1_17">ἄλλο περιγράψαι ὅμοιον τῶι <seg
                            type="suppliedword">ἐγ<supplied reason="lost">γ</supplied>ε</seg></seg>
                </seg>
                <seg n="99v2" type="folio">
                    <seg n="1" type="line" facs="z99v2_01"><seg type="wordend"><supplied
                                reason="lost">γραμμ</supplied>ένωι</seg> ὥστε τὸ <seg
                            type="unclearword">περιγε<unclear>γραμ</unclear></seg></seg>
                    <seg n="2" type="line" facs="z99v2_02"><seg type="wordend">μένον</seg> πρὸς τὸ <choice>
                            <abbr>ἐγγεγραμμένο</abbr>
                            <expan>ἐγγεγραμμένον</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z99v2_03">ἐλάσσονα λόγον ἔχειν τοῦ ὃν <w part="I"
                        >ἔ</w></seg>
                    <seg n="4" type="line" facs="z99v2_04"><w part="F">χει</w> ἡ ἐπιφάνεια τοῦ κώνου
                            <expan>πρὸς</expan>
                        <choice>
                            <abbr>τὸ</abbr>
                            <expan>τὸν</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z99v2_05">Β κύκλον. νοείσθω δὴ καὶ περὶ <choice>
                            <abbr><supplied reason="lost">τ</supplied>ὸ</abbr>
                            <expan>τὸν</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z99v2_06">Α κύκλον πολύγωνον <seg
                            type="expandedword">
                            <choice>
                                <abbr>περιγεγρα</abbr>
                                <expan>περιγεγραμμένον</expan>
                            </choice>
                        </seg></seg>
                    <seg n="7" type="line" facs="z99v2_07"><seg type="wordend">μένον</seg> ὅμοιον
                        τῶι περὶ τὸν Β <w part="I">κύ</w></seg>
                    <seg n="8" type="line" facs="z99v2_08"><w part="F">κλον</w> περιγεγραμμένωι καὶ
                            <w part="I">ἀ</w></seg>
                    <seg n="9" type="line" facs="z99v2_09"><w part="F">πὸ</w> τοῦ περὶ τὸν Α κύκλον
                        περὶ</seg>
                    <seg n="10" type="line" facs="z99v2_10">περιγεγραμμένου <choice>
                            <abbr>πολύγωνο</abbr>
                            <expan>πολύγωνο<unclear>ν</unclear></expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z99v2_11">πυραμὶς ἀνεστάτω <w part="I">ἀναγε</w></seg>
                    <seg n="12" type="line" facs="z99v2_12"><w part="F">γραμμένη</w> τὴν αὐτὴν <choice>
                            <abbr>κορυφ</abbr>
                            <expan>κορυφὴν</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z99v2_13">ἔχουσα τῶι κώνω. ἐπεὶ οὖν <w part="I"
                        >ὅ</w></seg>
                    <seg n="14" type="line" facs="z99v2_14"><w part="F">μοιά</w> ἐστιν τὰ πολύγωνα
                        τὰ <expan>περὶ</expan></seg>
                    <seg n="15" type="line" facs="z99v2_15">τοὺς Α Β κύκλους <w part="I"
                        >περιγεγραμ</w></seg>
                    <seg n="16" type="line" facs="z99v2_16"><w part="F">μένα</w> τὸν αὐτὸν ἔχει
                        λόγον <expan>πρὸς</expan></seg>
                    <seg n="17" type="line" facs="z99v2_17">ἄλληλα ὃν αἱ ἐκ τοῦ κέντρου</seg>
                    <seg n="18" type="line" facs="z99v2_18"><seg type="word">δ<supplied
                                reason="lost">υν</supplied><unclear>ά</unclear>μει</seg> πρὸς <seg
                            type="word">ἀλλήλα<unclear>ς</unclear></seg>
                        <choice>
                            <abbr>τουτ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice></seg>
                </seg>
                <seg n="101r2" type="folio">
                    <seg n="1" type="line" facs="z101r2_01">ὃν ἔχει ἡ Γ <expan>πρὸς</expan> Ε
                        δυνάμει <choice>
                            <abbr>τουτ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice></seg>
