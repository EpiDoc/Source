    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="149r1" type="folio">
                    <seg n="1" type="line" facs="z149r1_01">
                        <expan>ἔστι</expan> δὲ ἡ ἐπιφάνεια τοῦ <w part="I">περιγε</w></seg>
                    <seg n="2" type="line" facs="z149r1_02"><w part="F">γραμμένου</w> σχήματος περὶ <choice>
                            <abbr>τη</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z149r1_03">σφαῖραν <seg type="word"
                            >μεί<unclear>ζ</unclear>ων</seg> ἢ <seg type="unclearword"
                            >τε<unclear>τ</unclear>ραπλα</seg></seg>
                    <seg n="4" type="line" facs="z149r1_04"><seg type="wordend">σία</seg> τοῦ
                        μεγίστου <seg type="word"><unclear>κύ</unclear>κλου</seg> τῶν ἐν</seg>
                    <seg n="5" type="line" facs="z149r1_05">τῆι σφαίραι, μεῖζον <expan>ἄρα</expan> ἢ
                            <w part="I">τετρα</w></seg>
                    <seg n="6" type="line" facs="z149r1_06"><w part="F">πλάσιον</w> ἔσται τὸ σχῆμα
                        τὸ <w part="I">περι</w></seg>
                    <seg n="7" type="line" facs="z149r1_07"><w part="F">γεγραμμένον</w> περὶ <seg
                            type="word"><unclear>τ</unclear>ὴν</seg>
                        <choice>
                            <abbr>σφαιρα</abbr>
                            <expan>σφαῖραν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z149r1_08">τοῦ κώνου τοῦ βάσιν μὲν ἔχοντος</seg>
                    <seg n="9" type="line" facs="z149r1_09">τὸν μέγιστον κύκλον, ὕψος δὲ τὴν</seg>
                    <seg n="10" type="line" facs="z149r1_10">ἐκ τοῦ κέντρου τῆς σφαίρας,</seg>
                    <seg n="11" type="line" facs="z149r1_11">ἐπειδὴ καὶ ὁ κῶνος ὁ ἴσος αὐτῶι</seg>
                    <seg n="12" type="line" facs="z149r1_12">μείζων ἢ τετραπλάσιος <w part="I"
                        >γίνε</w></seg>
                    <seg n="13" type="line" facs="z149r1_13"><w part="F">ται</w> τοῦ εἰρημένου κώνου
                            <w part="I">βά</w></seg>
                    <seg n="14" type="line" facs="z149r1_14"><w part="F">σιν</w> τε γὰρ μείζονα ἢ <w
                            part="I">τετρα</w></seg>
                    <seg n="15" type="line" facs="z149r1_15"><w part="F">πλασίαν</w> ἔχει καὶ ὕψος
                        ἴσον.</seg>
                </seg>
                <seg n="149r1" type="folio">
                    <seg n="16" type="line" facs="z149r1_16">ἐὰν ἐν σφαίραι <choice>
                            <abbr>σχ</abbr>
                            <expan>σχῆμα</expan>
                        </choice>
                        <w part="I">ἐγγεγραμμέ</w></seg>
                    <seg n="17" type="line" facs="z149r1_17"><w part="F">νον</w> καὶ ἄλλο <w
                            part="I">περιγεγραμμέ</w></seg>
                    <seg n="18" type="line" facs="z149r1_18"><w part="F">νον</w> ὑπὸ ὁμοίων
                        πολυγώνων</seg>
                    <seg n="19" type="line" facs="z149r1_19">τὸν <seg type="word">αὐτὸ<supplied
                                reason="lost">ν</supplied></seg>
                        <seg type="word"><supplied reason="lost">τ</supplied>ρόπον</seg> τοῖς <choice>
                            <abbr>πρότερ</abbr>
                            <expan>πρότερον</expan>
                        </choice></seg>
                </seg>
                <seg n="154v1" type="folio">
                    <seg n="1" type="line" facs="z154v1_01">κατεσκευασμένοις, ἡ <w part="I"
                        >ἐπιφά</w></seg>
                    <seg n="2" type="line" facs="z154v1_02"><w part="F">νεια</w> τοῦ περιγεγραμμένου
                            <w part="I">σχή</w></seg>
                    <seg n="3" type="line" facs="z154v1_03"><w part="F">ματος</w>
                        <expan>πρὸς</expan> τὴν τοῦ <w part="I">ἐγγεγραμμέ</w></seg>
                    <seg n="4" type="line" facs="z154v1_04"><w part="F">νου</w> ἐπιφάνειαν
                        διπλασίονα</seg>
                    <seg n="5" type="line" facs="z154v1_05">λόγον ἔχει ἤπερ ἡ <seg type="word"
                                >πλευ<unclear>ρ</unclear>ὰ</seg> τοῦ</seg>
                    <seg n="6" type="line" facs="z154v1_06">περιγεγραμμένου <choice>
                            <abbr>πολυγών</abbr>
                            <expan>πολυγώνου</expan>
                        </choice>
                    </seg>
                    <seg n="7" type="line" facs="z154v1_07">περὶ τὸν <seg type="word"
                                >μέγιστο<unclear>ν</unclear></seg>
                        <seg type="word"><supplied reason="lost"
                                >κύ</supplied><unclear>κ</unclear>λ<unclear>ο</unclear>ν</seg> πρὸς <choice>
                            <abbr>τὴ</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z154v1_08">πλευρὰν τοῦ ἐγγεγραμμένου</seg>
                    <seg n="9" type="line" facs="z154v1_09">πολυγώνου ἐν τῶι <seg type="word"
                                >αὐτῶ<unclear>ι</unclear></seg><choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλ<unclear>ω</unclear></expan>
                        </choice>,</seg>
                    <seg n="10" type="line" facs="z154v1_10">αὐτὸ δὲ τὸ σχῆμα τὸ <w part="I"
                            >περιγεγραμ</w></seg>
                    <seg n="11" type="line" facs="z154v1_11"><w part="F">μένον</w>
                        <expan>πρὸς</expan> τὸ σχῆμα <w part="I">τριπλασί</w></seg>
                    <seg n="12" type="line" facs="z154v1_12"><w part="F">ονα</w> λόγον ἔχει τοῦ
                        αὐτοῦ <choice>
                            <abbr>λόγ</abbr>
                            <expan>λόγου</expan>
                        </choice>. </seg>
                </seg>
                <seg n="154v1" type="folio">
                    <seg n="13" type="line" facs="z154v1_13">ἔστω ἐν σφαίραι <seg type="word"
                                >κ<unclear>ύ</unclear>κλος</seg> ὁ ΑΒ ΓΔ,</seg>
                    <seg n="14" type="line" facs="z154v1_14">καὶ <seg type="word"
                            >ἐγγ<unclear>εγρά</unclear>φθω</seg> εἰς αὐτὸν <w part="I">πο</w></seg>
                    <seg n="15" type="line" facs="z154v1_15"><w part="F">λύγωνον</w>
                        <seg type="word">ἰ<unclear>σ</unclear><supplied reason="lost"
                            >όπ</supplied>λευρον</seg>, τὸ δὲ <w part="I">πλῆ</w></seg>
                    <seg n="16" type="line" facs="z154v1_16"><w part="F">θος</w> τῶν <seg
                            type="word">πλευρ<unclear>ῶ</unclear>ν</seg> αὐτοῦ μετρείσθω</seg>
                </seg>
                <seg n="149r2" type="folio">
                    <seg n="1" type="line" facs="z149r2_01">ὑπὸ <seg type="word">τετράδο<supplied
                                reason="lost">ς</supplied></seg>, <seg type="word"><supplied
                                reason="lost">κ</supplied>αὶ</seg>
                        <seg type="word">
                            <unclear>ἄλ</unclear>
                            <supplied reason="lost">λο</supplied>
                        </seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">περιγε</supplied>
                        </seg></seg>
                    <seg n="2" type="line" facs="z149r2_02"><seg type="wordend">γράφθω</seg> περὶ
                        τὸν κύκλον <choice>
                            <abbr><supplied reason="lost">ὅ</supplied>μοιο</abbr>
                            <expan><supplied reason="lost">ὅ</supplied>μοιον</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z149r2_03">τῶι ἐγγεγραμμένωι, ἐπὶ δὲ τοῦ <seg
                            type="suppliedword">περι</seg></seg>
                    <seg n="4" type="line" facs="z149r2_04">
                        <seg type="wordend">γεγραμμένο<supplied reason="lost">υ</supplied></seg>
                        <seg type="word">πολυγώνο<unclear>υ</unclear></seg>
                        <seg type="suppliedword">πλευ</seg>
                    </seg>
                    <seg n="5" type="line" facs="z149r2_05"><seg type="wordend"><supplied
                                reason="lost">ρ</supplied>α<unclear>ὶ</unclear></seg> ἐπιψαυέτωσαν
                        τοῦ κύκλου <seg type="unclearword">
                            <choice>
                                <abbr>κ</abbr>
                                <expan>κ<unclear>α</unclear>τὰ</expan>
                            </choice>
                        </seg></seg>
                    <seg n="6" type="line" facs="z149r2_06"><seg type="wordend">τὰ</seg> μέσα τῶν
                        περιφερειῶν τῶν</seg>
                    <seg n="7" type="line" facs="z149r2_07">ἀποτεμνομένων ὑπὸ τῶν <seg type="word"
                                >το<supplied reason="lost">ῦ</supplied></seg></seg>
                    <seg n="8" type="line" facs="z149r2_08">ἐγγεγραμμένου πολυγώνου <w part="I"
                        >πλευ</w></seg>
                    <seg n="9" type="line" facs="z149r2_09"><w part="F">ρῶν</w>, αἱ δὲ ΕΗ ΖΘ <choice>
                            <abbr>μετροι</abbr>
                            <expan>διάμετροι</expan>
                        </choice>
                        <expan>πρὸς</expan>
                        <choice>
                            <abbr>ὀρθ</abbr>
                            <expan>ὀρθὰς</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z149r2_10">ἔστωσαν ἀλλήλαις τοῦ κύκλου <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z149r2_11">
                        <seg type="word">περιλαμβάνο<supplied reason="lost">ν</supplied>τος</seg> τὸ
                            <w part="I">περιγε</w></seg>
                    <seg n="12" type="line" facs="z149r2_12"><w part="F">γραμμένον</w> πολύγωνον καὶ
                            <w part="I">ὁμοί</w></seg>
                    <seg n="13" type="line" facs="z149r2_13"><w part="F">ως</w> κείμεναι ταῖς ΑΓ ΒΔ
                            <w part="I">διαμέ</w></seg>
                    <seg n="14" type="line" facs="z149r2_14"><w part="F">τροις</w>, καὶ νοείσθωσαν
                            <w part="I">ἐπεζευγ</w></seg>
                    <seg n="15" type="line" facs="z149r2_15"><w part="F">μεναι</w> ἐπὶ τὰς
                        ἀπεναντίον <w part="I">γω</w></seg>
                    <seg n="16" type="line" facs="z149r2_16"><w part="F">νίας</w> τοῦ πολυγώνου, αἳ <choice>
                            <abbr>γίγνοντ</abbr>
                            <expan>γίγνονται</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z149r2_17">ἀλλήλαις τε καὶ τῆι ΒΖ ΘΔ <w part="I"
                            >παράλ</w></seg>
                    <seg n="18" type="line" facs="z149r2_18"><w part="F">ληλοι</w>. μενούσης δὴ τῆς
                        ΕΗ <seg type="suppliedword">δια</seg></seg>
                    <seg n="19" type="line" facs="z149r2_19">
                        <seg type="wordend">μέτρ<supplied reason="lost">ου</supplied></seg>
                        <expan>καὶ</expan>
                        <supplied reason="lost">περιενεχθεισῶν</supplied>
                        <supplied reason="lost">τῶν</supplied>
                    </seg>
                </seg>
                <seg n="154v2" type="folio">
                    <seg n="1" type="line" facs="z154v2_01">περιμέτρων τῶν πολυγώνων</seg>
                    <seg n="2" type="line" facs="z154v2_02">περὶ τὴν τοῦ κύκλου <choice>
                            <abbr>περιφέρει</abbr>
                            <expan>περιφέρειαν</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z154v2_03">τὸ μὲν περιγεγραμμένον <w part="I"
                        >σχῆ</w></seg>
                    <seg n="4" type="line" facs="z154v2_04"><w part="F">μα</w> ἔσται ἐν τῆι σφαίραι,
                        τὸ δὲ <w part="I">ἐγ</w></seg>
                    <seg n="5" type="line" facs="z154v2_05"><w part="F">γεγραμμένον</w>· δεικτέον
                        οὖν <expan>ὅτι</expan> ἡ <choice>
                            <abbr>μ</abbr>
                            <expan>μὲν</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z154v2_06">ἐπιφάνεια τοῦ <w part="I"
                        >περιγεγραμμέ</w></seg>
                    <seg n="7" type="line" facs="z154v2_07"><w part="F">νου</w> σχήματος
                        <expan>πρὸς</expan> τὴν <choice>
                            <abbr>ἐπιφάνει</abbr>
                            <expan>ἐπιφάνειαν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z154v2_08">τοῦ ἐγγεγραμμένου διπλασίονα</seg>
                    <seg n="9" type="line" facs="z154v2_09">λόγον ἔχει <choice>
                            <abbr>εἴ</abbr>
                            <expan>εἴπερ</expan>
                        </choice> ἡ ΕΛ <expan>πρὸς</expan> ΑΚ τὸ δὲ</seg>
                    <seg n="10" type="line" facs="z154v2_10">σχῆμα τὸ περιγεγραμμένον
                        <expan>πρὸς</expan></seg>
                    <seg n="11" type="line" facs="z154v2_11">τὸ ἐγγεγραμμένον <choice>
                            <abbr>τριπλασίον</abbr>
                            <expan>τριπλασίονα</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z154v2_12">
                        <seg type="word">λό<unclear>γ</unclear>ον</seg> ἔχει τοῦ <seg type="word"
                                >α<unclear>ὐ</unclear>τοῦ</seg> λόγου.</seg>
                </seg>
                <seg n="154v2" type="folio">
                    <seg n="12" type="line" facs="z154v2_12">ἔστω <expan>γὰρ</expan></seg>
                    <seg n="13" type="line" facs="z154v2_13">ὁ μὲν Μ <seg type="word"
                                ><unclear>κ</unclear>ύ<unclear>κ</unclear>λος</seg> ἴσος τῆι <w
                            part="I">ἐπιφα</w></seg>
                    <seg n="14" type="line" facs="z154v2_14"><w part="F">νείαι</w>
                        <seg type="word">το<unclear>ῦ</unclear></seg> περιγεγραμμένου <w part="I"
                        >πε</w></seg>
                    <seg n="15" type="line" facs="z154v2_15"><w part="F">ρὶ</w> τὴν σφαῖραν, ὁ δὲ Ν
                        ἴσος τῆι <seg type="unclearword">ἐπ<unclear>ι</unclear></seg></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
