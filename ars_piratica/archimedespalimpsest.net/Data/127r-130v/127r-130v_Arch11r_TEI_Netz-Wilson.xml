    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="127r1" type="folio">
                    <seg n="1" type="line" facs="z127r1_01">διπλασία τῆς Χ. <seg type="word"
                                >δῆλο<supplied reason="lost">ν,</supplied></seg> ὅτι ἡ <w part="I"
                        >ὑ</w></seg>
                    <seg n="2" type="line" facs="z127r1_02"><w part="F">περοχή,</w> ἧ ὑπερέχει τὸ <w
                            part="I">τετράγω</w></seg>
                    <seg n="3" type="line" facs="z127r1_03"><w part="F">νον</w> τὸ ἀπὸ τῆς ΒΔ τοῦ
                        ἀπὸ τῆς</seg>
                    <seg n="4" type="line" facs="z127r1_04">ΒΤ, πρὸς τὸ τετράγωνον τὸ ἀπὸ τῆς</seg>
                    <seg n="5" type="line" facs="z127r1_05">ΒΔ ἐστὶν ἤ ἐστιν ὁ ἄξων τοῦ <w part="I"
                            >τμή</w></seg>
                    <seg n="6" type="line" facs="z127r1_06"><w part="F">ματος</w> ἢ <seg type="word"
                                >ἡμιό<supplied reason="lost">λ</supplied>ιος</seg> τῆς μέχρι <seg
                            type="word">
                            <unclear>
                                <choice>
                                    <abbr>τ</abbr>
                                    <expan>τοῦ</expan>
                                </choice>
                            </unclear>
                        </seg>
                    </seg>
                    <seg n="7" type="line" facs="z127r1_07">ἄξονος. μείζονι ἄρα ὑπεροχῆ <supplied
                            reason="lost">τὸ</supplied></seg>
                    <seg n="8" type="line" facs="z127r1_08">τετράγωνον τὸ ἀπὸ τῆς ΒΔ τοῦ <w part="I"
                            >ἀ</w></seg>
                    <seg n="9" type="line" facs="z127r1_09"><w part="F">πὸ</w> τῆς ΦΧ ἢ τὸ
                        τετράγωνον τὸ <seg type="unclearword">
                            <unclear>ἀ</unclear>
                        </seg></seg>
                    <seg n="10" type="line" facs="z127r1_10"><seg type="wordend">πὸ</seg> τῆς ΒΔ τοῦ
                            <seg type="word">τ<unclear>ετ</unclear>ραγώνου</seg> τούτου</seg>
                    <seg n="11" type="line" facs="z127r1_11">ἀπὸ τῆς ΒΤ· ὥστε <seg type="word"
                                >ἔλασ<unclear>σό</unclear>ν</seg> ἐστιν <unclear>ἡ</unclear></seg>
                    <seg n="12" type="line" facs="z127r1_12">ΦΧ τῆς ΒΤ· καὶ ἡ Φ ἄρα τῆς ΒΡ.</seg>
                </seg>
                <seg n="127r1" type="folio">
                    <seg n="13" type="line" facs="z127r1_13">ἔστω οὖν <seg type="word"
                            >τῆ<unclear>ι</unclear></seg> Φ ἴση ἡ ΡΨ, καὶ ἡ Ψ<supplied reason="lost"
                            >Ε</supplied></seg>
                    <seg n="14" type="line" facs="z127r1_14">ὀρθὴ ἤχθω τῆι ΒΔ δυναμένη</seg>
                    <seg n="15" type="line" facs="z127r1_15"><seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ὸ</unclear>
                        </seg> ἥμισυ τοῦ περιεχομένου <w part="I">ὑ</w></seg>
                    <seg n="16" type="line" facs="z127r1_16"><w part="F">πὲρ</w> τῆς ΚΡ ΨΒ. φαμὶ δὴ
                        τὸ <w part="I">τμῆ</w></seg>
                    <seg n="17" type="line" facs="z127r1_17"><seg type="wordend">μα</seg> ἀφεθὲν ἐς
                        τὸ ὑγρόν, ὥστε <choice>
                            <abbr>τη</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z127r1_18">βάσιν αὐτοῦ ὅλην εἶναι ἐν τῶι</seg>
                    <seg n="19" type="line" facs="z127r1_19">ὑγρῶι, καταστήσεσθαι οὕτως,</seg>
                    <seg n="20" type="line" facs="z127r1_20"><seg type="word"><supplied
                                reason="lost">ὥσ</supplied><unclear>τ</unclear>ε</seg> τὸν ἄξονα <choice>
                            <abbr>αὐτ</abbr>
                            <expan>αὐτ<unclear>οῦ</unclear></expan>
                        </choice> πρὸς τὴν</seg>
                </seg>
                <seg n="130v1" type="folio">
                    <seg n="1" type="line" facs="z130v1_01">ἐπιφάνειαν τοῦ ὑγροῦ γωνίαν</seg>
                    <seg n="2" type="line" facs="z130v1_02">ποιεῖν ἴσην τῆι Β.</seg>
                </seg>
                <seg n="130v1" type="folio">
                    <seg n="2" type="line" facs="z130v1_02">ἀφείσθω μὲν</seg>
                    <seg n="3" type="line" facs="z130v1_03">γὰρ τὸ τμῆμα, ὡς εἴρηται, ἐς τὸ</seg>
                    <seg n="4" type="line" facs="z130v1_04">ὑγρόν, καὶ μὴ <seg type="word"
                                >ποιείτ<unclear>ω</unclear></seg> ὁ ἄξων <expan>πρὸς</expan></seg>
                    <seg n="5" type="line" facs="z130v1_05">τὴν <seg type="word"
                            >ἐπι<unclear>φ</unclear>άνειαν</seg> τοῦ ὑγροῦ γωνίαν Ϙ<add
                            rend="superscript">Η</add>
                        <w part="I">τῆ</w></seg>
                    <seg n="6" type="line" facs="z130v1_06"><w part="F">ι</w> Β, ἀλλὰ μείζονα
                        πρότερον.</seg>
                </seg>
                <seg n="130v1" type="folio">
                    <seg n="6" type="line" facs="z130v1_06">
                        <w part="I">τμη</w>
                    </seg>
                    <seg n="7" type="line" facs="z130v1_07"><w part="F">θέντος</w> δὲ αὐτοῦ ἐπιπέδωι
                            <seg type="word"><unclear>ὀ</unclear>ρθῶι</seg></seg>
                    <seg n="8" type="line" facs="z130v1_08">πρὸς τὴν <seg type="word">ἐπιφ<supplied
                                reason="lost">άν</supplied>ειαν</seg> τοῦ ὑγροῦ</seg>
                    <seg n="9" type="line" facs="z130v1_09">ἔστω τοῦ τμήματος τομὴ ἡ ΑΠ</seg>
                    <seg n="10" type="line" facs="z130v1_10">ΟΛ ὀρθογωνίου κώνου τομή,</seg>
                    <seg n="11" type="line" facs="z130v1_11">τῆς δὲ τοῦ ὑγροῦ ἐπιφανείας</seg>
                    <seg n="12" type="line" facs="z130v1_12"><sic>ΤΙ α</sic>, ἄξων δ’ ἔστω τοῦ <choice>
                            <abbr>τμηματ</abbr>
                            <expan>τμήματος</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z130v1_13">καὶ διάμετρος ἡ ΝΟ, καὶ <w part="I"
                            >τετμήσ</w></seg>
                    <seg n="14" type="line" facs="z130v1_14"><w part="F">θω</w> κατὰ τὰ ΩΘ, ὡς καὶ
                            <w part="I">πρό</w></seg>
                    <seg n="15" type="line" facs="z130v1_15"><w part="F">τερον,</w> ἤχθω δὲ καὶ ἡ
                        μὲν ΥΠ</seg>
                    <seg n="16" type="line" facs="z130v1_16">παρὰ τὴν ΤΙ ἐφαπτομένην</seg>
                    <seg n="17" type="line" facs="z130v1_17">τῆς τομῆς κατὰ τὸ Π, ἡ δὲ
                        Π<unclear>Η</unclear></seg>
                </seg>
                <seg n="127r2" type="folio">
                    <seg n="1" type="line" facs="z127r2_01"><seg type="word"><supplied reason="lost"
                                >παρ</supplied>ὰ</seg>
                        <seg type="word">τὴ<supplied reason="lost">ν</supplied></seg> Ν<supplied
                            reason="lost">Ο</supplied>, <supplied>ἡ</supplied> δὲ <supplied
                            reason="lost">ΠΣ</supplied>
                        <supplied reason="lost">κάθετος</supplied></seg>
                    <seg n="2" type="line" facs="z127r2_02"><seg type="word"><supplied reason="lost"
                                >ἐ</supplied>πὶ</seg> τὸν <seg type="word">ἄξον<unclear>α.</unclear></seg>
                        <supplied reason="lost">ἐπεὶ</supplied>
                        <supplied reason="lost">γὰρ</supplied>
                        <supplied reason="lost">ὁ</supplied>
                        <supplied reason="lost">ἄξων</supplied></seg>
                    <seg n="3" type="line" facs="z127r2_03">τοῦ τμήματος πρὸς τὴν <seg
                            type="suppliedword">ἐπι<supplied reason="lost">φάνει</supplied></seg></seg>
                    <seg n="4" type="line" facs="z127r2_04"><seg type="wordend">αν</seg> τοῦ ὑγροῦ
                        ποιεῖ <seg type="word">γω<supplied reason="lost">νίαν</supplied></seg>
                        <seg type="word">μ<unclear>εί</unclear><supplied reason="lost"
                            >ζονα</supplied></seg></seg>
                    <seg n="5" type="line" facs="z127r2_05">τῆς ΒΕ, εἴη <seg type="word"
                            ><unclear>ἂ</unclear>ν</seg> ἡ ΣΥΠ <seg type="word">
                            <unclear>μεῖζο</unclear>
                            <supplied reason="lost">ν</supplied>
                        </seg>
                        <unclear>τῆς</unclear></seg>
                    <seg n="6" type="line" facs="z127r2_06">Β· τὸ ἄρα <seg type="word"
                                >τετ<unclear>ρά</unclear>γωνον</seg> τὸ ἀπὸ <seg type="word"
                                >τ<unclear>ῆ</unclear>ς</seg></seg>
                    <seg n="7" type="line" facs="z127r2_07"><unclear>ΠΣ</unclear>
                        <seg type="word">πρὸ<unclear>ς</unclear></seg>
                        <seg type="word"><unclear>τ</unclear>ὸ</seg> τετράγωνον τὸ ἀπὸ <seg
                            type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">ῆς</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="8" type="line" facs="z127r2_08">ΣΥ <seg type="word"
                            >μ<unclear>εί</unclear>ζονα</seg> λόγον ἔχει ἢ <seg type="word"
                                >τ<supplied reason="lost">ὸ</supplied></seg>
                        <seg type="suppliedword">τετρά</seg></seg>
                    <seg n="9" type="line" facs="z127r2_09"><seg type="wordend">γω<supplied
                                reason="lost">νον</supplied></seg>
                        <seg type="word">
                            <supplied reason="lost">τὸ</supplied>
                        </seg>
                        <seg type="word"><unclear>ἀ</unclear>πὸ</seg> τῆς ΨΕ πρὸς τὸ <w part="I"
                            >τετρά</w></seg>
                    <seg n="10" type="line" facs="z127r2_10"><w part="F">γωνον</w> τὸ ἀπὸ τῆς
                            <unclear>ΨΒ</unclear>. <expan>καὶ</expan> Κ<supplied reason="lost"
                        >Ρ</supplied> ἄρα</seg>
                    <seg n="11" type="line" facs="z127r2_11"><supplied reason="lost">
                            <expan>πρὸς</expan>
                        </supplied> ΣΥ μείζονα λόγον ἔχει <w part="I">ἡμι</w></seg>
                    <seg n="12" type="line" facs="z127r2_12"><w part="F">σείας</w> τῆς ΚΡ πρὸς <seg
                            type="word"><supplied reason="lost">τ</supplied>ὴν</seg> ΨΒ· <choice>
                            <abbr>ελασσω</abbr>
                            <expan>ἐλάσσων</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z127r2_13">ἄρα ἡ ΣΥ ἢ διπλάσια τῆς ΨΒ.</seg>
                    <seg n="14" type="line" facs="z127r2_14">καὶ ΠΣΟ τῆς ΨΒ ἐλάσσων· ἡ
                        Σ<unclear>Ω</unclear></seg>
                    <seg n="15" type="line" facs="z127r2_15">ἄρα <seg type="word"
                            >μεί<unclear>ζ</unclear>ων</seg> τῆς ΡΨ καὶ ἡ ΠΗ</seg>
                    <seg n="16" type="line" facs="z127r2_16"><seg type="word"
                                ><unclear>τ</unclear>ῆ<supplied reason="lost">ς</supplied></seg>
                        <unclear>Φ.</unclear> καὶ ἐπεὶ τὸ τμῆμα τῶι <w part="I">βά</w></seg>
                    <seg n="17" type="line" facs="z127r2_17"><w part="F">ρει</w> λόγον ἔχει πρὸς τὸ
                        ὑγρόν, ὃν ἡ</seg>
                    <seg n="18" type="line" facs="z127r2_18">ὑπεροχή, ἧ μεῖζόν ἐστιν τὸ <w part="I"
                            >τετρά</w></seg>
                    <seg n="19" type="line" facs="z127r2_19"><w part="F">γωνον</w> τὸ ἀπὸ τῆς ΒΔ τοῦ
                            <w part="I">τετρα</w></seg>
                    <seg n="20" type="line" facs="z127r2_20"><w part="F">γώνου</w> τοῦ ἀπὸ τῆς ΦΧ,
                        πρὸς τὸ</seg>
                    <seg n="21" type="line" facs="z127r2_21"><seg type="word">τε<supplied
                                reason="lost">τράγ</supplied>ω<supplied reason="lost"
                        >νο</supplied>ν</seg> τὸ ἀπὸ τῆς ΒΔ, ὃν δὲ</seg>
                </seg>
                <seg n="130v2" type="folio">
                    <seg n="1" type="line" facs="z130v2_01">λόγον ἔχει τὸ τμῆμα τῶι βάρει <supplied
                            reason="lost">
                            <expan>πρὸς</expan>
                        </supplied></seg>
                    <seg n="2" type="line" facs="z130v2_02">τὸ ὑγρόν, τοῦτον ἔχει τὸν λόγον</seg>
                    <seg n="3" type="line" facs="z130v2_03">τὸ δεδυκὸς αὐτοῦ τμῆμα πρὸς τὸ <choice>
                            <abbr>ολο</abbr>
                            <expan>ὅλον</expan>
                        </choice>,</seg>
                    <seg n="4" type="line" facs="z130v2_04"><seg type="word"
                        ><unclear>ὅ</unclear>τι</seg> τὸν αὐτὸν ἕξει λόγον τὸ δεδυκὸς</seg>
                    <seg n="5" type="line" facs="z130v2_05">αὐτοῦ <seg type="word"
                            >τμῆ<unclear>μ</unclear>α</seg> πρὸς τὸ ὅλον τμῆμα,</seg>
                    <seg n="6" type="line" facs="z130v2_06">ὃν ἡ ὑπεροχή, <supplied reason="lost"
                        >ἧ</supplied> ὑπερέχει τὸ <w part="I">τε</w></seg>
                    <seg n="7" type="line" facs="z130v2_07"><w part="F">τράγωνον</w> τὸ ἀπὸ τῆς ΒΔ
                        τοῦ <w part="I">τε</w></seg>
                    <seg n="8" type="line" facs="z130v2_08"><w part="F">τραγώνου</w> τοῦ ἀπὸ τῆς ΦΧ,
                        πρὸς</seg>
                    <seg n="9" type="line" facs="z130v2_09">τὸ τετράγωνον τὸ ἀπὸ ΒΔ· ἕξει οὖν</seg>
                    <seg n="10" type="line" facs="z130v2_10"><expan>καὶ</expan> τὸ ὅλον τμῆμα πρὸς
                        τὸ ἐκτὸς</seg>
                    <seg n="11" type="line" facs="z130v2_11">τοῦ ὑγροῦ λόγον, ὃν τὸ ἀπὸ τῆς ΒΔ</seg>
                    <seg n="12" type="line" facs="z130v2_12">πρὸς τὸ ἀπὸ τῆς ΦΧ. ὃν δὲ λόγον</seg>
                    <seg n="13" type="line" facs="z130v2_13">ἔχει τὸ ὅλον τμῆμα πρὸς τὸ ἐκτὸς</seg>
                    <seg n="14" type="line" facs="z130v2_14">τοῦ ὑγροῦ, τοῦτον ἔχει τὸ ἀπὸ τῆς</seg>
                    <seg n="15" type="line" facs="z130v2_15">ΝΟ πρὸς τὸ ἀπὸ τῆς
                        ΜΠ<unclear>Ι·</unclear> ἴση <expan>ἄρα</expan></seg>
                    <seg n="16" type="line" facs="z130v2_16">ἡ Μ<supplied reason="lost">Π</supplied>
                        <seg type="word"><supplied reason="lost">τ</supplied>ῆι</seg>
                        <unclear>Φ</unclear>Χ. <supplied reason="lost">ἡ</supplied> δὲ ΠΗ <seg
                            type="word"><supplied reason="lost">ἐ</supplied>δείχ<supplied
                                reason="lost">θη</supplied></seg>
                        <choice>
                            <abbr>μειζ</abbr>
                            <expan>μεῖζ<unclear>ον</unclear></expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z130v2_17"><unclear>τ</unclear>ῆς Φ· ἡ Μ<supplied
                            reason="lost">Η</supplied>
                        <supplied>
                            <expan>ἄρα</expan>
                        </supplied>
                        <seg type="word"><supplied reason="lost">ἐ</supplied>λάσσων</seg>
                        <seg type="word">ἐστὶ<supplied reason="lost">ν</supplied></seg></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
