    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="94r1" type="folio">
                    <num>Β</num>
                </seg>
                <seg n="94r1" type="folio">
                    <seg n="1" type="line" facs="z94r1_01"><seg type="word"
                            >Ἀ<unclear>ρ</unclear>χιμήδης</seg> Δοσιθέωι <choice>
                            <abbr>χαιρ</abbr>
                            <expan>χαίρειν</expan>
                        </choice>.</seg>
                </seg>
                <seg n="94r1" type="folio">
                    <seg n="2" type="line" facs="z94r1_02">πρότερον μὲν ἐπέστειλάς μοι</seg>
                    <seg n="3" type="line" facs="z94r1_03">γράψαι τῶν προβλημάτων τὰς</seg>
                    <seg n="4" type="line" facs="z94r1_04"><choice>
                            <abbr>αποδειξ</abbr>
                            <expan>ἀποδείξεις</expan>
                        </choice>, ὧν αὐτὸς τὰς <choice>
                            <abbr>προτασ</abbr>
                            <expan>προτάσεις</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z94r1_05">ἀπέστειλα Κόνωνι· <choice>
                            <abbr>συμβνει</abbr>
                            <expan>συμβαίνει</expan>
                        </choice> δὲ</seg>
                    <seg n="6" type="line" facs="z94r1_06">αὐτῶν τὰ πλεῖστα γράφεσθαι
                        <expan>διὰ</expan></seg>
                    <seg n="7" type="line" facs="z94r1_07">τῶν θεωρημάτων, ὧν <choice>
                            <abbr>προτερ</abbr>
                            <expan>πρότερον</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z94r1_08">ἀπέστειλά σοι τὰς ἀποδείξεις,</seg>
                    <seg n="9" type="line" facs="z94r1_09"><expan>ὅτι</expan> τε πάσης σφαίρας ἡ <w
                            part="I">ἐπιφά</w></seg>
                    <seg n="10" type="line" facs="z94r1_10"><w part="F">νεια</w> τετραπλασία
                            <expan>ἐστὶ</expan> τοῦ <w part="I">μεγίσ</w></seg>
                    <seg n="11" type="line" facs="z94r1_11"><w part="F">του</w> κύκλου τῶν ἐν τῆι
                        σφαίραι,</seg>
                    <seg n="12" type="line" facs="z94r1_12"><expan>καὶ</expan>
                        <choice>
                            <abbr>δι</abbr>
                            <expan>διὅτι</expan>
                        </choice> παντὸς τμήματος <w part="I">σφαί</w></seg>
                    <seg n="13" type="line" facs="z94r1_13"><w part="F">ρας</w> τῆι ἐπιφανείαι ἴσος
                        ἐστὶ <w part="I">κύ</w></seg>
                    <seg n="14" type="line" facs="z94r1_14"><w part="F">κλος</w>, οὗ ἡ ἐκ τοῦ
                        κέντρου ἴση <expan>ἐστὶ</expan></seg>
                    <seg n="15" type="line" facs="z94r1_15">τῆι εὐθείαι τῆι ἀπὸ τῆς <w part="I"
                        >κορυ</w></seg>
                    <seg n="16" type="line" facs="z94r1_16"><w part="F">φῆς</w> τοῦ τμήματος ἐπὶ τὴν
                            <seg type="unclearword">πε</seg></seg>
                    <seg n="17" type="line" facs="z94r1_17"><seg type="wordend"
                            >ριφέρ<unclear>ει</unclear>αν</seg> τῆς βάσεως <w part="I">ἀγομέ</w></seg>
                    <seg n="18" type="line" facs="z94r1_18"><w part="F">νηι</w>, καὶ <choice>
                            <abbr>δι</abbr>
                            <expan>διότι</expan>
                        </choice> πάσης <seg type="word"><unclear>σφ</unclear><supplied
                                reason="lost">αί</supplied>ρα<supplied reason="lost"
                        >ς</supplied></seg> ὁ</seg>
                    <seg n="19" type="line" facs="z94r1_19">κύλινδρος ὁ βάσιν μὲν ἔχων</seg>
                </seg>
                <seg n="91v1" type="folio">
                    <seg n="1" type="line" facs="z91v1_01">τὸν μέγιστον κύκλον τῶν ἐν τῆι</seg>
                    <seg n="2" type="line" facs="z91v1_02">σφαίφαι, ὕψος δὲ ἴσον τῆι <seg
                            type="expandedword">
                            <choice>
                                <abbr>με</abbr>
                                <expan>διαμέτρωι</expan>
                            </choice>
                        </seg></seg>
                    <seg n="3" type="line" facs="z91v1_03"><seg type="wordend">τρωι</seg> τῆς
                        σφαίρας, αὐτός τε</seg>
                    <seg n="4" type="line" facs="z91v1_04">ἡμιόλιός <expan>ἐστι</expan> τῶι μεγέθει <choice>
                            <expan>τῆς</expan>
                            <abbr>τ</abbr>
                        </choice>
                        <w part="I">σφαί</w></seg>
                    <seg n="5" type="line" facs="z91v1_05"><w part="F">ρας</w> καὶ ἡ ἐπιφάνεια αὐτοῦ
                            <w part="I">ἡ</w></seg>
                    <seg n="6" type="line" facs="z91v1_06"><w part="F">μιολία</w> τῆς ἐπιφανείας <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z91v1_07">σφαίρας, <expan>καὶ</expan>
                        <choice>
                            <abbr>δι</abbr>
                            <expan>διότι</expan>
                        </choice> πᾶς τομεὺς</seg>
                    <seg n="8" type="line" facs="z91v1_08">στερεὸς ἴσος <expan>ἐστὶ</expan> κώνωι
                        τῶι <w part="I">βά</w></seg>
                    <seg n="9" type="line" facs="z91v1_09"><w part="F">σιν</w> μὲν ἔχοντι τὸν κύκλον
                        τὸν</seg>
                    <seg n="10" type="line" facs="z91v1_10">ἴσον τῆι ἐπιφανείαι τοῦ <w part="I"
                        >τμή</w></seg>
                    <seg n="11" type="line" facs="z91v1_11"><w part="F">ματος</w> τῆς σφαίρας τοῦ ἐν <choice>
                            <abbr>τ</abbr>
                            <expan>τῶι</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z91v1_12">τομεῖ, ὕψος δὲ ἴσον τῆι ἐκ τοῦ <seg
                            type="expandedword">
                            <choice>
                                <abbr>κε</abbr>
                                <expan>κέντρου</expan>
                            </choice>
                        </seg></seg>
                    <seg n="13" type="line" facs="z91v1_13"><seg type="wordend">τρου</seg> τῆς
                        σφαίρας. ὅσα μὲν <expan>οὖν</expan></seg>
                    <seg n="14" type="line" facs="z91v1_14">τῶν θεωρημάτων καὶ <w part="I"
                        >προβλη</w></seg>
                    <seg n="15" type="line" facs="z91v1_15"><w part="F">μάτων</w>
                        <expan>γράφεται</expan>
                        <expan>διὰ</expan> τούτων τῶν <w part="I">θεωρη</w></seg>
                    <seg n="16" type="line" facs="z91v1_16"><w part="F">μάτων</w>, ἐν τῶιδε τῶι
                        βιβλίωι <w part="I">γρά</w></seg>
                    <seg n="17" type="line" facs="z91v1_17"><w part="F">ψας</w> ἀπέσταλκά σοι, ὅσα
                        δὲ δι’ <choice>
                            <abbr>αλλ</abbr>
                            <expan>ἄλλης</expan>
                        </choice></seg>
                </seg>
                <seg n="94r2" type="folio">
                    <seg n="1" type="line" facs="z94r2_01">εὑρίσκονται θεωρίας, τῶ τε περὶ</seg>
                    <seg n="2" type="line" facs="z94r2_02">ἑλίκων καὶ τῶ περὶ τῶν <w part="I"
                        >κωνο</w></seg>
                    <seg n="3" type="line" facs="z94r2_03"><w part="F">ειδῶν</w>, πειράσομαι διὰ
                        τάχους</seg>
                    <seg n="4" type="line" facs="z94r2_04">ἀποστεῖλαι. τὸ δὲ πρῶτον ἦν τῶν</seg>
                    <seg n="5" type="line" facs="z94r2_05">προβλημάτων τόδε· σφαίρας</seg>
                    <seg n="6" type="line" facs="z94r2_06">δοθείσης ἐπίπεδον χωρίον <w part="I"
                        >εὑ</w></seg>
                    <seg n="7" type="line" facs="z94r2_07"><w part="F">ρεῖν</w> ἴσον τῆι ἐπιφανείαι
                        τῆς <seg type="expandedword">
                            <choice>
                                <abbr>σφ</abbr>
                                <expan>σφαίρας</expan>
                            </choice>
                        </seg></seg>
                    <seg n="8" type="line" facs="z94r2_08"><seg type="wordend">ρας</seg>. ἔ̓́στιν δὲ
                        τοῦτο φανερὸν <w part="I">δεδειγ</w></seg>
                    <seg n="9" type="line" facs="z94r2_09"><w part="F">μένον</w> ἐκ τῶν προειρημένων
                            <w part="I">θε</w></seg>
                    <seg n="10" type="line" facs="z94r2_10"><w part="F">ωρημάτων</w>· τὸ γὰρ <choice>
                            <abbr>τετραπλασι</abbr>
                            <expan>τετραπλάσιον</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z94r2_11"><choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> μεγίστου κύκλου τῶν ἐν τῆι <seg type="expandedword">
                            <choice>
                                <abbr>σφ</abbr>
                                <expan>σφαίραι</expan>
                            </choice>
                        </seg></seg>
                    <seg n="12" type="line" facs="z94r2_12"><seg type="wordend">ραι</seg> ἐπίπεδόν
                        τε χωρίον <expan>ἐστὶ</expan> καὶ <w part="I">ἴ</w></seg>
                    <seg n="13" type="line" facs="z94r2_13"><w part="F">σον</w> τῆι ἐπιφανείαι τῆς <choice>
                            <abbr>σφαιρ</abbr>
                            <expan>σφαίρας</expan>
                        </choice>.</seg>
                </seg>
                <seg n="94r2" type="folio">
                    <num>Α</num>
                </seg>
                <seg n="94r2" type="folio">
                    <seg n="14" type="line" facs="z94r2_14">τὸ δεύτερον ἦν· Κώνου <choice>
                            <abbr>δοθεντ</abbr>
                            <expan>δοθέντος</expan>
                        </choice>
                    </seg>
                    <seg n="15" type="line" facs="z94r2_15">ἢ κυλίνδρου σφαῖραν εὑρεῖν τῶι</seg>
                    <seg n="16" type="line" facs="z94r2_16">κώνωι ἢ τῶι κυλίνδρωι ἴσην.</seg>
                </seg>
                <seg n="94r2" type="folio">
                    <seg n="16" type="line" facs="z94r2_16">
                        <choice>
                            <abbr>εστ</abbr>
                            <expan>ἔστω</expan>
                        </choice>
                    </seg>
                    <seg n="17" type="line" facs="z94r2_17">διδόμενος κῶνος ἢ κύλινδρος</seg>
                    <seg n="18" type="line" facs="z94r2_18">ὁ Α καὶ τῶι Α ἴση ἡ Β σφαῖρα, καὶ </seg>
                    <seg n="19" type="line" facs="z94r2_19">κείσθω τοῦ Α κώνου ἢ κυλίνδρου</seg>
                </seg>
                <seg n="91v2" type="folio">
                    <seg n="1" type="line" facs="z91v2_01">ἡμιόλιος κύλινδρος ὁ ΓΖΔ, τῆς δὲ</seg>
                    <seg n="2" type="line" facs="z91v2_02">Β σφαίρας ἡμιόλιος κύλινδρος, οὗ</seg>
                    <seg n="3" type="line" facs="z91v2_03">βάσις ὁ περὶ διάμετρον τὴν ΗΘ <w part="I"
                            >κύ</w></seg>
                    <seg n="4" type="line" facs="z91v2_04"><w part="F">κλος,</w> ἄξων δὲ ὁ ΚΛ ἴσος
                        τῆι <w part="I">δια</w></seg>
                    <seg n="5" type="line" facs="z91v2_05"><w part="F">μέτρωι</w> τῆς Β σφαίρας·
                        ἴσος <expan>ἄρα</expan>
                        <expan>ἐστὶν</expan> ὁ Ε</seg>
                    <seg n="6" type="line" facs="z91v2_06">κύλινρδος τῶι Κ κυλίνδρωι τῶν δὲ</seg>
                    <seg n="7" type="line" facs="z91v2_07">ἴσων κυλίνδρων <choice>
                            <abbr>αντιπεπονθασι</abbr>
                            <expan>ἀντιπεπόνθασιν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z91v2_08">αἱ βάσεις τοῖς ὕψεσιν· ὡς
                        <expan>ἄρα</expan> ὁ Ε <choice>
                            <abbr>κυκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice>
                    </seg>
                    <seg n="9" type="line" facs="z91v2_09"><expan>πρὸς</expan> τὸν Κ <choice>
                            <abbr>κυκλο</abbr>
                            <expan>κύκλον,</expan>
                        </choice>
                        <choice>
                            <abbr>τουτ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice>
                        <expan>ὡς</expan> τὸ ἀπὸ τῆς ΓΔ</seg>
                    <seg n="10" type="line" facs="z91v2_10"><expan>πρὸς</expan> τὸ ἀπὸ τῆς ΗΘ, οὕτως
                        ἡ ΚΛ <expan>πρὸς</expan> ΕΖ. </seg>
                    <seg n="11" type="line" facs="z91v2_11">ἴση δὲ ἡ ΚΛ τῆι ΗΘ ὁ γὰρ ἡμιόλιος </seg>
                    <seg n="12" type="line" facs="z91v2_12">κύλινδρος τῆς σφαίρας ἴσον ἔχει </seg>
                    <seg n="13" type="line" facs="z91v2_13">τὸν ἄξονα τῆι <choice>
                            <abbr>μετρωι</abbr>
                            <expan>διαμέτρωι</expan>
                        </choice> τῆς <w part="I">σφαί</w></seg>
                    <seg n="14" type="line" facs="z91v2_14"><w part="F">ρας,</w>
                        <expan>καὶ</expan> ὁ Κ κύκλος <choice>
                            <abbr>μεγιστ</abbr>
                            <expan>μέγιστός</expan>
                        </choice> ἐστι τῶν ἐν</seg>
                    <seg n="15" type="line" facs="z91v2_15">τῆι σφαίραι· <expan>ὡς</expan>
                        <expan>ἄρα</expan> τὸ ἀπὸ ΓΔ <expan>πρὸς</expan> τὸ ἀπὸ </seg>
                    <seg n="16" type="line" facs="z91v2_16">ΗΘ, <expan>οὕτως</expan> ἡ ΗΘ
                            <expan>πρὸς</expan> τὴν ΕΖ. ἔστω τῶ ἀπὸ ΗΘ</seg>
                    <seg n="17" type="line" facs="z91v2_17">ἴσον τὸ ὑπὸ ΓΔ, ΜΝ· <seg type="word"
                                ><unclear>ὡ</unclear>ς</seg>
                        <expan>ἄρα</expan> ἡ ΓΔ <expan>πρὸς</expan> ΜΝ,</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
