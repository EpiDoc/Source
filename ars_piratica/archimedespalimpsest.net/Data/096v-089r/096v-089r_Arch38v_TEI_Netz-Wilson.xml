    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="96v1" type="folio">
                    <seg n="1" type="line" facs="z96v1_01"><seg type="wordend">ψαύη</seg> εὐθεῖα μὴ
                        κατὰ τὸ πέρας τᾶς</seg>
                    <seg n="2" type="line" facs="z96v1_02">ἕλικος, τὰ δὲ ἄλλα τᾶι αὐτᾶι <seg
                            type="clearword">κατα</seg></seg>
                    <seg n="3" type="line" facs="z96v1_03"><seg type="wordend">σκευασθέντι</seg>,
                            <expan>ὅτι</expan> ἁ μεταξὺ εὐθεῖα</seg>
                    <seg n="4" type="line" facs="z96v1_04">τᾶς <seg type="word">πο<supplied
                                reason="lost">τὶ</supplied>
                        </seg> τὰν ἐπιψαύουσαν <seg type="clearword">συμ</seg></seg>
                    <seg n="5" type="line" facs="z96v1_05"><seg type="wordend">πίπτουσα</seg> καὶ
                        τᾶς <seg type="word">ἀρχᾶ<unclear>ς</unclear>
                        </seg>
                        <seg type="word">
                            <unclear>τ</unclear>ᾶς</seg></seg>
                    <seg n="6" type="line" facs="z96v1_06">ἕλικος ἴσα ἐστὶν ὅλα τᾶ τοῦ <seg
                            type="suppliedword">
                            <choice>
                                <abbr>γραφέ</abbr>
                                <expan>γραφ<supplied reason="lost">έ</supplied>ν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="7" type="line" facs="z96v1_07"><seg type="wordend">τος</seg> κύκλου
                        περιφερεία καὶ ἔτι τᾶ</seg>
                    <seg n="8" type="line" facs="z96v1_08">μεταξὺ τῶν <seg type="word">
                                εἰρημέ<unclear>ν</unclear>ων</seg> σαμείων,</seg>
                    <seg n="9" type="line" facs="z96v1_09">ὡσαύτως τᾶς <seg type="word"
                                >περιφερ<supplied reason="lost">εία</supplied>ς</seg>
                        <seg type="clearword">λαμ</seg></seg>
                    <seg n="10" type="line" facs="z96v1_10"><seg type="wordend">βανομένας·</seg> καὶ
                        εἴ κα τᾶς ἐν <seg type="suppliedword">ὁποι</seg></seg>
                    <seg n="11" type="line" facs="z96v1_11"><seg type="wordend">
                            <supplied reason="lost">αοῦν</supplied>
                        </seg> γεγραμμένας <seg type="word">περιφορᾶ<supplied reason="lost"
                            >ς</supplied>
                        </seg></seg>
                    <seg n="12" type="line" facs="z96v1_12">ἕλικος ἐπιψαύει <seg type="word"
                                >τ<supplied reason="lost">ις</supplied>
                        </seg> εὐθεῖα μὴ <expan>κατὰ</expan></seg>
                    <seg n="13" type="line" facs="z96v1_13"><supplied reason="lost">τὸ</supplied>
                        πέρας <supplied reason="lost">τᾶς</supplied> ἕλικος, τὰ δὲ ἄλλα</seg>
                    <seg n="14" type="line" facs="z96v1_14"><supplied reason="lost">τὰ</supplied>
                        <supplied reason="lost">αὐτὰ</supplied>
                        <sic>
                            <seg type="word">
                                <supplied reason="lost">κ</supplied>ατασκευασθαιωντι</seg>
                        </sic>, ὅτι</seg>
                    <seg n="15" type="line" facs="z96v1_15">ἁ <seg type="word">
                            <supplied reason="lost">με</supplied>ταξὺ</seg>
                        <seg type="word">εὐ<supplied reason="lost">θεῖα</supplied>
                        </seg> τῶν εἰρημένων</seg>
                    <seg n="16" type="line" facs="z96v1_16">σαμείων πολλαπλασία <seg type="word">
                                τ<unclear>έ</unclear>
                        </seg> ἐστιν</seg>
                    <seg n="17" type="line" facs="z96v1_17">τᾶς τοῦ γραφέντος κύκλου <seg
                            type="suppliedword">περιφε</seg></seg>
                    <seg n="18" type="line" facs="z96v1_18"><seg type="wordend">ρεί<supplied
                                reason="lost">α</supplied>ς</seg> κατὰ τὸν ἑνὶ ἐλάσσονα <seg
                            type="suppliedword">
                            <supplied reason="lost">ἀ</supplied>
                        </seg></seg>
                </seg>
                <seg n="89r1" type="folio">
                    <seg n="1" type="line" facs="z89r1_01">
                        <seg type="wordend">
                            <supplied reason="lost">ριθμὸν</supplied>
                        </seg>
                        <supplied reason="lost">τοῦ,</supplied>
                        <supplied reason="lost">καθ’</supplied>
                        <supplied reason="lost">ὃν</supplied>
                        <supplied reason="lost">αἱ</supplied>
                        <supplied reason="lost">περιφοραὶ</supplied>
                    </seg>
                    <seg n="2" type="line" facs="z89r1_02">λέγονται, καὶ ἔτι ἴσα τᾶ μεταξὺ <choice>
                            <abbr>τ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z89r1_03">εἰρημένων σαμείων ὁμοίως</seg>
                    <seg n="4" type="line" facs="z89r1_04">λαμβανομένας.</seg>
                    <figure n="21.1">
                        <figDesc>Figure 21.1</figDesc>
                    </figure>
                </seg>
                <seg n="89r1" type="folio">
                    <seg n="5" type="line" facs="z89r1_05">λαμβάνοντα τὸ χωρίον <supplied
                            reason="lost">τὸ</supplied>
                        <seg type="suppliedword"><supplied reason="lost">π</supplied>ερ<supplied
                                reason="lost">ι</supplied></seg></seg>
                    <seg n="6" type="line" facs="z89r1_06"><seg type="wordend">εχόμενον</seg> ὑπό
                            <seg type="word">
                            <unclear>τ</unclear>ε</seg> τᾶς <seg type="word">ἕλι<supplied
                                reason="lost">κος</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ᾶ<supplied reason="lost"
                            >ς</supplied>
                        </seg></seg>
                </seg>
                <seg n="96v2" type="folio">
                    <seg n="1" type="line" facs="z96v2_01">ἐν τᾶι πρώται περιφορᾶι <seg
                            type="clearword">γε</seg>
                    </seg>
                    <seg n="2" type="line" facs="z96v2_02">
                        <seg type="wordend">γραμμένας</seg> καὶ τᾶς εὐθείας</seg>
                    <seg n="3" type="line" facs="z96v2_03">τᾶς πρώτας ἐν τᾶι ἀρχᾶι</seg>
                    <seg n="4" type="line" facs="z96v2_04">τᾶς <seg type="word">
                            <supplied reason="lost">πε</supplied>ριφορᾶς</seg> δυνατόν ἐστι</seg>
                    <seg n="5" type="line" facs="z96v2_05">περὶ αὐτὸ σχῆμα <seg type="word">
                                ἐπίπ<unclear>ε</unclear>δον</seg></seg>
                    <seg n="6" type="line" facs="z96v2_06">περιγράψαι <seg type="word">
                            <unclear>κ</unclear>αὶ</seg> ἄλλο <seg type="word"> ἐγγρά<supplied
                                reason="lost">ψαι</supplied>
                        </seg></seg>
                    <seg n="7" type="line" facs="z96v2_07">ἐξ ὁμοίων τομέων <seg type="word">
                            <choice>
                                <abbr>συγκείμεν</abbr>
                                <expan>συγκεί<supplied reason="lost"
                                    >με</supplied><unclear>ν</unclear>ον</expan>
                            </choice>
                        </seg>,</seg>
                    <seg n="8" type="line" facs="z96v2_08">ὥστε τὸ περιγεγραμμένον <supplied
                            reason="lost">τοῦ</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἐγ</supplied>
                        </seg>
                    </seg>
                    <seg n="9" type="line" facs="z96v2_09">
                        <seg type="wordend">γεγραμμένου</seg> μείζων εἶμεν <seg type="clearword"
                        >ἐ</seg>
                    </seg>
                    <seg n="10" type="line" facs="z96v2_10">
                        <seg type="wordend">λάσσονι</seg> παντὸς τοῦ προτεθέν</seg>
                    <seg n="11" type="line" facs="z96v2_11">τος χωρίου.</seg>
                </seg>
                <seg n="96v2" type="folio">
                    <seg n="11" type="line" facs="z96v2_11">ἔστω ἕλιξ, ἐφ’ ἇς</seg>
                    <seg n="12" type="line" facs="z96v2_12">ἁ Α<supplied reason="lost">Β</supplied>
                        <seg type="word">
                            <unclear>Γ</unclear>
                            <supplied reason="lost">Δ</supplied>
                        </seg>, ἐν τᾶι πρώται <seg type="clearword">περιφο</seg>
                    </seg>
                    <seg n="13" type="line" facs="z96v2_13">
                        <seg type="wordend">ρᾶι</seg> γεγραμμένα, ἔστω δὲ <seg type="clearword"
                        >ἀρ</seg>
                    </seg>
                    <seg n="14" type="line" facs="z96v2_14">
                        <seg type="wordend">χὰ</seg> μὲν τᾶς ἕλικος τὸ Θ <seg type="clearword"
                        >σαμεῖ</seg>
                    </seg>
                    <seg n="15" type="line" facs="z96v2_15">
                        <seg type="wordend">ον</seg>, ἀρχὰ δὲ τᾶς περιφορᾶς ἁ</seg>
                    <seg n="16" type="line" facs="z96v2_16">ΘΑ, ὁ δὲ πρῶτος <seg type="word">
                            <supplied reason="lost">κ</supplied>ύκλος</seg> ὁ ΖΗ ΙΑ,</seg>
                    <seg n="17" type="line" facs="z96v2_17">αἱ δὲ ΑΗ ΖΙ διάμετροι αὐτοῦ
                        <expan>πρὸς</expan></seg>
                    <seg n="18" type="line" facs="z96v2_18">ὀρθὰς ἀλλάλαις. <unclear>ἀεὶ</unclear>
                        δὴ τᾶς <seg type="suppliedword">ὀρ</seg>
                    </seg>
                </seg>
                <seg n="89r2" type="folio">
                    <seg n="1" type="line" facs="z89r2_01">
                        <seg type="wordend">
                            <supplied reason="lost">θᾶς</supplied>
                        </seg>
                        <supplied reason="lost">γωνίας</supplied>
                        <supplied reason="lost">δίχα</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">τεμνομέ</supplied>
                        </seg>
                    </seg>
                    <seg n="2" type="line" facs="z89r2_02">
                        <seg type="wordend">νας</seg> καὶ τοῦ τομέως τοῦ τὴν</seg>
                    <seg n="3" type="line" facs="z89r2_03">ὀρθὴν γωνίαν περιέχοντος</seg>
                    <seg n="4" type="line" facs="z89r2_04">ἐσσεῖται τὸ καταλειπόμενον</seg>
                    <seg n="5" type="line" facs="z89r2_05">τοῦ τομέως ἔλασσον τοῦ <seg
                            type="clearword">προτε</seg>
                    </seg>
                    <seg n="6" type="line" facs="z89r2_06">
                        <seg type="wordend">θέντος·</seg> καὶ ἔστω <choice>
                            <abbr>γεγενημέν</abbr>
                            <expan>γεγενημένος</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z89r2_07"><unclear>ὁ</unclear> τομεὺς ὁ ΑΘΚ ἐλάσσων
                        τοῦ</seg>
                    <seg n="8" type="line" facs="z89r2_08">προτεθέντος χωρίου. <seg type="clearword"
                            >διαιρείσθω</seg>
                    </seg>
                    <seg n="9" type="line" facs="z89r2_09">
                        <seg type="wordend">σαν</seg> δὴ αἱ γωνίαι αἱ <seg type="clearword"
                        >τέσσα</seg>
                    </seg>
                    <seg n="10" type="line" facs="z89r2_10">
                        <seg type="wordend">ρες</seg> ὀρθαὶ εἰς τὰς ἴσας <choice>
                            <abbr>γωνί</abbr>
                            <expan>γωνίας</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z89r2_11">τὰς περιεχομένας ὑπὸ τᾶν</seg>
                    <seg n="12" type="line" facs="z89r2_12">ΑΘ ΘΚ, καὶ αἱ ποιοῦσαι τὰς <seg
                            type="clearword">γω</seg>
                    </seg>
                    <seg n="13" type="line" facs="z89r2_13">
                        <seg type="wordend">νίας</seg> εὐθεῖαι ἐς τὰν κατὰ τὰν</seg>
                    <seg n="14" type="line" facs="z89r2_14">ἕλικα ἄχθωσιν. καθ’ ἃ δὴ <seg
                            type="clearword">τέ</seg>
                    </seg>
                    <seg n="15" type="line" facs="z89r2_15">
                        <seg type="wordend">μνει</seg> σημεῖον ἁ ΘΚ τὰν ἕλικα,</seg>
                    <seg n="16" type="line" facs="z89r2_16">ἔστω τὸ Λ, καὶ κέντρωι τὸ Θ, <seg
                            type="clearword">δια</seg>
                    </seg>
                    <seg n="17" type="line" facs="z89r2_17">
                        <seg type="wordend">στάματι</seg> δὲ τῶι ΘΛ <seg type="word">
                            <unclear>κύ</unclear>κλος</seg>
                        <seg type="clearword">γε</seg>
                    </seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
