    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="176v-173r" type="folio">
                    <seg n="01" type="line" facs="z176v-173r_01"><unclear>ὑ</unclear><supplied
                            reason="lost">μῶν</supplied>
                        <supplied reason="lost">αὐ</supplied>τῶν ὡς
                            ἀδικ<unclear>ού</unclear>ν<unclear>τ</unclear>ων κατέγνωτε; Διώνδας</seg>
                    <seg n="02" type="line" facs="z176v-173r_02">δὲ, ἐπειδ<unclear>ὴ</unclear> ἡ
                        τύχη τ<unclear>ὸν</unclear> ἀγῶνα οὐκ ἴσως ἐβρά<unclear>β</unclear>ευσεν,
                            τὰ<unclear>ς</unclear></seg>
                    <seg n="03" type="line" facs="z176v-173r_03"
                            >ἐ<unclear>π</unclear>ιβασ<unclear>ί</unclear><supplied reason="lost"
                        >ας</supplied> λαβὼν τῶι λόγωι τὰ ὑπὸ πάντων <seg type="unclearword"
                            >ὁμολογού</seg></seg>
                    <seg n="04" type="line" facs="z176v-173r_04"><seg type="wordend"
                            >μεν<unclear>α</unclear></seg> μ<unclear>ὴ</unclear>
                        καλῶ<unclear>ς</unclear> βεβουλεῦσθαι λοιδορεῖ, καὶ οὐκ αἰσθάνεται,</seg>
                    <seg n="05" type="line" facs="z176v-173r_05">ὃ <seg type="word">
                            <choice>
                                <abbr>κ</abbr>
                                <expan><unclear>κ</unclear>αὶ</expan>
                            </choice>
                        </seg>
                        <unclear>β</unclear>λάπτει τὴν πόλιν. <unclear>κ</unclear>ράτιστον μέν γε
                        πάντων, οἶ<unclear>μ</unclear>αι,</seg>
                    <seg n="06" type="line" facs="z176v-173r_06">ἐστὶν τὸ νικᾶν,
                        <unclear>εἰ</unclear> δ’ ἄρα συμβ<unclear>αί</unclear>νοι ἀποτυχεῖν
                            το<unclear>ὺς</unclear> περ<supplied reason="lost">ὶ</supplied></seg>
                    <seg n="07" type="line" facs="z176v-173r_07">τοιούτων
                            ἀγωνιζ<unclear>ο</unclear>μένο<unclear>υ</unclear>ς
                        οἵ<unclear>ω</unclear>νπερ ἡμεῖς – π<unclear>ο</unclear>λλὰ δ’ <w part="I"
                        >ἔ</w></seg>
                    <seg n="08" type="line" facs="z176v-173r_08"><w part="F">χων</w>
                        <seg type="word">
                            <choice>
                                <abbr>παραδείγμα</abbr>
                                <expan>παραδείγματα</expan>
                            </choice>
                        </seg> περὶ αὐτῶν λέγειν, τὰ μὲν τῆς <w part="I">πό</w></seg>
                    <seg n="09" type="line" facs="z176v-173r_09"><w part="F">λεως</w> τῆς ἡμετέρας
                        παραλείψω, ἐνίοτε γὰρ οἱ <w part="I">ἔπαι</w></seg>
                    <seg n="10" type="line" facs="z176v-173r_10"><w part="F">νοι</w> ἐξεταζόμενοι ἐν
                        αὐτοῖς τού<unclear>τοις</unclear> καθ’ ὧν ἂν ὦσιν</seg>
                    <seg n="11" type="line" facs="z176v-173r_11">προσέ<unclear>στησαν</unclear> τοῖς
                        ἀκούουσιν. ἀλλὰ σκέψασθε <seg type="unclearword">Λακε</seg></seg>
                    <seg n="12" type="line" facs="z176v-173r_12"><seg type="wordend"
                                >δαιμον<unclear>ίου</unclear>ς</seg> ὅτι ἐν μὲν Κορίνθωι μαχόμενοι
                            <seg type="word">
                            <choice>
                                <abbr>ἐνίκω</abbr>
                                <expan>ἐνίκων</expan>
                            </choice>
                        </seg>,</seg>
                    <seg n="13" type="line" facs="z176v-173r_13">ἐν Θερμοπύλαις δὲ πάντες ἀπώλοντο
                            ἀ<unclear>λλ’</unclear> ὅ<unclear>μ</unclear>ως</seg>
                    <seg n="14" type="line" facs="z176v-173r_14">τῆς μὲν νίκης αὐτῶν ουδεὶς <seg
                            type="word">
                            <choice>
                                <abbr>λόγ</abbr>
                                <expan>λόγος</expan>
                            </choice>
                        </seg>, τὴν δ’ ἧτταν <w part="I">πάν</w></seg>
                    <seg n="15" type="line" facs="z176v-173r_15"><w part="F">τες</w> ἐγκωμιάζουσιν.
                        διὰ τί; ὅτι τὴν μὲν ὑπὲρ τῆς</seg>
                    <seg n="16" type="line" facs="z176v-173r_16">αὐτῶν πλεον<unclear>ε</unclear>ξίας
                        πρός τινας διελεγχθέντες</seg>
                    <seg n="17" type="line" facs="z176v-173r_17"><supplied reason="lost">ἐνίκησαν,</supplied>
                        <supplied reason="lost">ἐν</supplied>
                        <supplied reason="lost">τῆι</supplied>
                        <supplied reason="lost">δὲ</supplied> περὶ τῆς ἐλευθερίας τῶν <seg
                            type="suppliedword">Ἑλ</seg></seg>
                    <seg n="18" type="line" facs="z176v-173r_18"><seg type="wordend"><supplied
                                reason="lost">λή</supplied><unclear>ν</unclear>ων</seg> ἀγωνιζόμενοι
                        κατεκόπησαν ἅπαντ<unclear>ες</unclear>.</seg>
                    <seg n="19" type="line" facs="z176v-173r_19"><unclear>νυ</unclear>νὶ δὲ ὁ
                            κ<unclear>ατ</unclear>ήγορ<unclear>ος</unclear> περὶ τῶν ψηφισμάτων <w
                            part="I">λό</w></seg>
                    <seg n="20" type="line" facs="z176v-173r_20"><w part="F">γους</w> ποιεῖται τῶν
                            πρ<unclear>ὸ</unclear> τῆς μάχης γενομένων καὶ</seg>
                    <seg n="21" type="line" facs="z176v-173r_21"><unclear>διαβ</unclear>άλλει καί
                        φησι <unclear>δ</unclear>εῖν αὐτῶν τὴν ταχίστην <w part="I">ἐ</w></seg>
                    <seg n="22" type="line" facs="z176v-173r_22"><w part="F">πιλαθέσθαι</w>. ἐγὼ δὲ
                        τοῖς θεοῖς εὔχομαι ὧν νυνὶ</seg>
                    <seg n="23" type="line" facs="z176v-173r_23">ψηφιζόμεθα ἀπαλλαγῆναι ἡμᾶς ὡς
                        τάχιστα</seg>
                    <seg n="24" type="line" facs="z176v-173r_24"><seg type="word">
                            <choice>
                                <abbr>κ</abbr>
                                <expan><unclear>κ</unclear>αὶ</expan>
                            </choice>
                        </seg> ἐπιλαθέσθαι. τὰ μὲν <seg type="word">
                            <choice>
                                <abbr>γ</abbr>
                                <expan>γὰρ</expan>
                            </choice>
                        </seg> τοῖς τῶν προγόνων <seg type="unclearword">ψη</seg></seg>
                    <seg n="25" type="line" facs="z176v-173r_25"><seg type="wordend"
                            >φ<unclear>ίσ</unclear>μασιν</seg> ἀκόλουθά ἐστιν, τ<unclear>ὰ</unclear>
                        <unclear>δ</unclear>ὲ το<unclear>ῖ</unclear>ς Μεγαρέων</seg>
                    <seg n="26" type="line" facs="z176v-173r_26">καὶ
                            <unclear>Κ</unclear>ο<unclear>ρ</unclear>ιν<unclear>θ</unclear>ίων
                        δογμασιν· εἰς <unclear>τοῦτο</unclear> δὲ <choice>
                            <sic>εικει</sic>
                            <corr>ἥκει</corr>
                        </choice> κακίας</seg>
                    <seg n="27" type="line" facs="z176v-173r_27">καὶ ἀπονοίας, ὥστε καὶ
                        π<unclear>ε</unclear>ρὶ τῆς πολιτείας <w part="I">ἡ</w></seg>
                    <seg n="28" type="line" facs="z176v-173r_28"><w part="F">μῖν</w> κατηγορίας
                        ποιεῖται. καίτοι, ὦ Διώνδ<unclear>α</unclear>,</seg>
                    <seg n="29" type="line" facs="z176v-173r_29">εἰ βούλει
                        <unclear>εἰ</unclear>δέναι σαφῶς, ποτέρους ἡμῶν οὗτοι</seg>
                    <seg n="30" type="line" facs="z176v-173r_30">ὑπολαμβάνουσιν βέλτιον
                            πολιτεύεσ<unclear>θαι</unclear>, <w part="I">ἐρώτη</w></seg>
                    <seg n="31" type="line" facs="z176v-173r_31"><w part="F">σον</w> αὐτῶν ἕκαστον,
                            <seg type="word">
                            <choice>
                                <abbr>τίν</abbr>
                                <expan>τίνα</expan>
                            </choice>
                        </seg> μάλιστα ἐν Μεγάροις <seg type="unclearword"
                        >μισο<unclear>ῦ</unclear></seg></seg>
                    <seg n="32" type="line" facs="z176v-173r_32"><seg type="wordend">σιν</seg>·
                        ἀποκρίνονται σοι, ὅτι <choice>
                            <sic>πυοδωρον</sic>
                            <corr>Πτοιόδωρον</corr>
                        </choice>, ἐν Ἐρετρίαι δὲ</seg>
                </seg>
            </ab>
