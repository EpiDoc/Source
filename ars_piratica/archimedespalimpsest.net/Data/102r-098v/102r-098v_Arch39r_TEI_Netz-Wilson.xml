    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="102r1" type="folio">
                    <seg n="1" type="line" facs="z102r1_01"><seg type="wordend">γράφθω·</seg>
                        πεσεῖται δὲ <seg type="word">αὐτο<supplied reason="lost">ῦ</supplied>
                        </seg>
                        <supplied reason="lost">ἁ</supplied></seg>
                    <seg n="2" type="line" facs="z102r1_02">
                        <seg type="word">
                            <unclear>μ</unclear>ὲν</seg>
                        <seg type="word">
                            <unclear>ε</unclear>
                            <supplied reason="lost">ἰς</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ὰ</seg>
                        <seg type="word">προαγ<supplied reason="lost">εύμεν</supplied>α</seg>
                        <seg type="suppliedword"><supplied reason="lost">περ</supplied>ι</seg>
                    </seg>
                    <seg n="3" type="line" facs="z102r1_03"><seg type="wordend">φέρειαι</seg> ἐντὸς
                        τᾶς ἕλικος, ἁ</seg>
                    <seg n="4" type="line" facs="z102r1_04"><seg type="word">δ<supplied
                                reason="lost">ὲ</supplied>
                        </seg> εἰς τὰ ἑπόμενα ἐκτός. <seg type="suppliedword">γ<supplied
                                reason="lost">ε</supplied></seg></seg>
                    <seg n="5" type="line" facs="z102r1_05"><seg type="wordend">γράφθω</seg> δὴ ἁ
                            <seg type="word">περιφέρει<supplied reason="lost">α</supplied>
                        </seg>, <choice>
                            <abbr>ἔστ</abbr>
                            <expan>ἔσται</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z102r1_06">κἂν συμπέσηι τᾶ ΘΑ κατὰ</seg>
                    <seg n="7" type="line" facs="z102r1_07"><seg type="word">τ<unclear>ὸ</unclear>
                        </seg> Ο ἁ ΟΜ καὶ τᾶι μετὰ τὰν</seg>
                    <seg n="8" type="line" facs="z102r1_08"><supplied reason="lost">Θ</supplied>Κ
                        εὐθεῖαν <seg type="word">π<supplied reason="lost">ο</supplied>τὶ</seg> τὰν
                            <seg type="word">
                            <unclear>ἕ</unclear>λικα</seg></seg>
                    <seg n="9" type="line" facs="z102r1_09"><seg type="word">ποτιπ<supplied
                                reason="lost">ιπτούσ</supplied>α</seg>. πάλιν δὴ <expan>καὶ</expan>,</seg>
                    <seg n="10" type="line" facs="z102r1_10">καθ’ ὃ <seg type="word">
                            <supplied reason="lost">τ</supplied>έ<supplied reason="lost"
                            >μνει</supplied>
                        </seg>
                        <seg type="word">σαμεῖο<unclear>ν</unclear>
                        </seg> ἁ ΘΜ,</seg>
                    <seg n="11" type="line" facs="z102r1_11"><supplied reason="lost">ἔστω</supplied>
                        <supplied reason="lost">τὸ</supplied>
                        <supplied reason="lost">Ν,</supplied>
                        <supplied reason="lost">καὶ</supplied>
                        <seg type="word">
                            <supplied reason="lost">κ</supplied>έντρωι</seg> τῶι Θ, <seg
                            type="suppliedword">δι</seg></seg>
                    <seg n="12" type="line" facs="z102r1_12"><seg type="wordend">α<supplied
                                reason="lost">στ</supplied>άματι</seg> δὲ τᾶ ΘΝ κύκλος</seg>
                    <seg n="13" type="line" facs="z102r1_13">γεγράφθω, ἔσται καὶ <seg
                            type="clearword">συμπέ</seg></seg>
                    <seg n="14" type="line" facs="z102r1_14"><seg type="wordend">σηι</seg> ἁ
                        περιφέρεια τοῦ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλου</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z102r1_15">καὶ τᾶ μετὰ τὰν ΘΜ <seg
                            type="suppliedword">ποτιπ<supplied reason="lost">ί</supplied></seg></seg>
                    <seg n="16" type="line" facs="z102r1_16"><seg type="wordend">πτου<supplied
                                reason="lost">σ</supplied>αν</seg> ποτὶ τὰν ἕλικα, <seg
                            type="clearword">ὁ</seg></seg>
                    <seg n="17" type="line" facs="z102r1_17"><seg type="wordend">μοίως</seg> δὲ καὶ
                        διὰ τῶν <choice>
                            <abbr>ἄλλω</abbr>
                            <expan>ἄλλων</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z102r1_18">πάντων, καθ’ ἃν τέμνοντι <choice>
                            <abbr>τὰ</abbr>
                            <expan>τὰν</expan>
                        </choice></seg>
                    <seg n="19" type="line" facs="z102r1_19">ἕλικα αἱ τὰς ἴσας γωνίας</seg>
                </seg>
                <seg n="98v1" type="folio">
                    <seg n="1" type="line" facs="z98v1_01">ποιοῦσαι, κύκλοι <choice>
                            <abbr>γεγράφθωσ</abbr>
                            <expan>γεγράφθωσαν</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z98v1_02">κέντρωι τῶι Θ, ἔστ’ ἂν συμπέσηι</seg>
                    <seg n="3" type="line" facs="z98v1_03">ἑκάστας ἁ περιφέρεια τᾶ τε</seg>
                    <seg n="4" type="line" facs="z98v1_04">προαγευμέναι εὐθείαι καὶ τᾶ</seg>
                    <seg n="5" type="line" facs="z98v1_05">ἑπομέναι· ἔσται δή τι περὶ τὸ</seg>
                    <seg n="6" type="line" facs="z98v1_06">λαφθὲν χωρίον <seg type="clearword"
                            >περιγεγραμ</seg></seg>
                    <seg n="7" type="line" facs="z98v1_07"><seg type="wordend">μένον</seg> ἐξ ὁμοίων
                            <seg type="word">
                            <supplied reason="lost">τομ</supplied>έων</seg>
                        <seg type="clearword">συγ</seg></seg>
                    <seg n="8" type="line" facs="z98v1_08"><seg type="wordend">κείμενον</seg> καὶ
                        ἄλλο <seg type="clearword">ἐγγεγραμ</seg></seg>
                    <seg n="9" type="line" facs="z98v1_09"><seg type="wordend">μένον</seg>. ὅτι δὲ
                        τὸ <seg type="clearword">περιγεγραμ</seg></seg>
                    <seg n="10" type="line" facs="z98v1_10"><seg type="wordend">μένον</seg> σχᾶμα
                        τοῦ <seg type="clearword">ἐγγεγραμμέ</seg></seg>
                    <seg n="11" type="line" facs="z98v1_11"><seg type="wordend">νου</seg> μείζων
                        ἐστιν ἐλάσσονι <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">οῦ</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="12" type="line" facs="z98v1_12">προτεθὲν χωρίου δειχθήσεται.</seg>
                    <seg n="13" type="line" facs="z98v1_13">ἔστι γὰρ ὁ μὲν ΘΛο τομεὺς <choice>
                            <abbr>ἴσ</abbr>
                            <expan>ἴσος</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z98v1_14">τῶ ΘΜΛ, ὁ δὲ ΘΝΠ τῶι ΘΝΡ, ὁ</seg>
                    <seg n="15" type="line" facs="z98v1_15">δὲ ΘΧΣ τῶι ΘΧΤ, ἔστι δὲ καὶ <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z98v1_16">ἄλλων τομέων ἕκαστος τῶν</seg>
                </seg>
                <seg n="102r2" type="folio">
                    <seg n="1" type="line" facs="z102r2_01">ἐν τῶι ἐγγεγραμμένωι <seg type="word">
                                σχά<unclear>μ</unclear><supplied reason="lost">ατι</supplied>
                        </seg></seg>
                    <seg n="2" type="line" facs="z102r2_02"><seg type="word">ἴσ<supplied
                                reason="lost">ο</supplied>ς</seg> τῶι κοινὰν ἔχοντι <seg type="word"
                            > πλ<supplied reason="lost">ευρὰν</supplied>
                        </seg></seg>
                    <seg n="3" type="line" facs="z102r2_03"><seg type="word">τομ<supplied
                                reason="lost">εῖ</supplied>
                        </seg> τῶν ἐν <seg type="word">τ<supplied reason="lost">ῶι</supplied>
                        </seg>
                        <seg type="suppliedword">περιγεγρ<supplied reason="lost">αμ</supplied></seg></seg>
                    <seg n="4" type="line" facs="z102r2_04"><seg type="wordend">μένωι</seg> σχάματι
                        τομέων. <seg type="word">δῆλ<supplied reason="lost">ο</supplied>ν </seg></seg>
                    <seg n="5" type="line" facs="z102r2_05">οὖν, <expan>ὅτι</expan> καὶ <seg
                            type="word"> πά<supplied reason="lost">ντ</supplied>ες</seg> οἱ τομεῖς
                            <seg type="unclearword"> πά<unclear>ν</unclear>
                        </seg></seg>
                    <seg n="6" type="line" facs="z102r2_06"><seg type="wordend">τεσσιν</seg> ἴσοι
                            <seg type="word">ἐσο<supplied reason="lost">ῦ</supplied>νται</seg>· <seg
                            type="word">
                            <supplied reason="lost">ἴσο</supplied>ν</seg>
                        <seg type="word">ἄρ<unclear>α</unclear>
                        </seg></seg>
                    <seg n="7" type="line" facs="z102r2_07"><seg type="word">
                            <supplied reason="lost">ἐστὶ</supplied>ν</seg> τὸ <seg type="word"
                                >ἐγγεγραμμ<supplied reason="lost">ένο</supplied>ν</seg>
                        <seg type="word">
                            <supplied reason="lost">σχᾶ</supplied>μα</seg></seg>
                    <seg n="8" type="line" facs="z102r2_08">
                        <supplied reason="lost">ἐν</supplied>
                        <supplied reason="lost">τῶι</supplied>
                        <supplied reason="lost">χωρίωι</supplied>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ῶ<supplied reason="lost"
                            >ι</supplied>
                        </seg>
                        <seg type="suppliedword"><supplied reason="lost"
                        >περιγε</supplied>γραμμέ</seg>
                    </seg>
                    <seg n="9" type="line" facs="z102r2_09"><seg type="wordend">νωι</seg> περὶ <seg
                            type="word">
                            <unclear>τ</unclear>ὸ</seg> χωρίον σχήματι <seg type="clearword"
                        >χω</seg></seg>
                    <seg n="10" type="line" facs="z102r2_10"><seg type="wordend">ρὶς</seg> τοῦ
                            ΘΑ<supplied reason="lost">Κ</supplied>
                        <seg type="word">τ<supplied reason="lost">ο</supplied>μέως</seg>· μόνος
                            <expan>γὰρ</expan></seg>
                    <seg n="11" type="line" facs="z102r2_11">ἑωυτὴν λέλαπται τῶν ἐν τῶι</seg>
                    <seg n="12" type="line" facs="z102r2_12"><seg type="word">περιγεγραμμέν<supplied
                                reason="lost">ωι</supplied>
                        </seg>
                        <supplied reason="lost">σχάματι</supplied>. <seg type="clearword">δῆ</seg></seg>
                    <seg n="13" type="line" facs="z102r2_13"><seg type="wordend">λον</seg> οὖν,
                            <expan>ὅτι</expan> τὸ <choice>
                            <abbr>περιγεγραμμένο</abbr>
                            <expan>περιγεγραμμένον</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z102r2_14">σχᾶμα τοῦ ἐγγεγραμμένου <seg
                            type="clearword">μεῖ</seg></seg>
                    <seg n="15" type="line" facs="z102r2_15"><seg type="wordend">ζόν</seg> ἐστι τῶ
                        ΑΘΚΘ τομεῖ, ὃς <seg type="clearword">ἐλάσ</seg></seg>
                    <seg n="16" type="line" facs="z102r2_16"><seg type="wordend">σων</seg> ἐστὶν τοῦ
                            <seg type="word">πρ<supplied reason="lost">οτεθέντ</supplied>ος </seg>.</seg>
                    <figure n="22.1">
                        <figDesc>Figure 22.1</figDesc>
                    </figure>
                </seg>
                <seg n="98v2" type="folio">
                    <seg n="1" type="line" facs="z98v2_01">ἐκ τούτου δὲ φανερὸν <expan>ὅτι</expan>
                        <choice>
                            <abbr>δυνατ</abbr>
                            <expan>δυνατόν</expan>
                        </choice>
                        <expan>ἐστι</expan></seg>
                    <seg n="2" type="line" facs="z98v2_02">περὶ τὸ εἰρημένον <seg type="word"
                                >χωρί<supplied reason="lost">ο</supplied>ν</seg>
                        <seg type="clearword">σχᾶ</seg></seg>
                    <seg n="3" type="line" facs="z98v2_03"><seg type="wordend">μα</seg>, οἷον
                        εἴρηται, γράφειν, ὥστε</seg>
                    <seg n="4" type="line" facs="z98v2_04">τὸ περιγεγραμμένον <seg type="word">
                                σχ<unclear>ᾶ</unclear>μα</seg></seg>
                    <seg n="5" type="line" facs="z98v2_05">μεῖζον εἶναι τοῦ <seg type="word"
                                >χωρί<supplied reason="lost">ου</supplied>
                        </seg>
                        <choice>
                            <abbr>ἔλασσό</abbr>
                            <expan>ἔλασσόν</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z98v2_06"><expan>εἶναι</expan> παντὸς τοῦ
                        προτεθέντος <seg type="clearword">χωρί</seg></seg>
                    <seg n="7" type="line" facs="z98v2_07"><seg type="wordend">ου</seg>, καὶ πάλιν
                        ἐγγράφειν, <seg type="word">ὥσ<supplied reason="lost">τ</supplied>ε </seg>
                        τὸ</seg>
                    <seg n="8" type="line" facs="z98v2_08">χωρίον ὁμοίως μεῖζον εἶμεν <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z98v2_09">ἐγγραφέντος σχάματος <seg
                            type="clearword">ἐλάσ</seg></seg>
                    <seg n="10" type="line" facs="z98v2_10"><seg type="wordend">σονι</seg> παντὸς
                        τοῦ προτεθέντος <choice>
                            <abbr>χωρί</abbr>
                            <expan>χωρίου</expan>
                        </choice>.</seg>
                </seg>
            </ab>
        </body>
    </text>
