                        <supplied reason="lost">τῆι</supplied></seg>
                    <seg n="4" type="line" facs="z167v1_04"><seg type="word">μ<supplied
                                reason="lost">ὲ</supplied><unclear>ν</unclear></seg>
                        <seg type="word"><supplied reason="lost"
                            >π</supplied>εριφερεία<unclear>ι</unclear></seg> τοῦ τμήματος</seg>
