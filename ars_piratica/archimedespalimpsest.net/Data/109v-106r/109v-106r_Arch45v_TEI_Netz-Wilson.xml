    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="109v1" type="folio">
                    <seg n="1" type="line" facs="z109v1_01">πρὸς δὲ <seg type="word"
                                >τ<unclear>ο</unclear><supplied reason="lost"
                        >ύ</supplied>τοις</seg>, ὅτι πάσης <choice>
                            <abbr>σφαιρ</abbr>
                            <expan>σφαίρας</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z109v1_02">ὁ <seg type="word">κύλινδ<supplied
                                reason="lost">ρος</supplied></seg> ὁ βάσιν μὲν ἔχων</seg>
                    <seg n="3" type="line" facs="z109v1_03"><seg type="word"
                        ><unclear>ἴ</unclear>σην</seg> τῶι μεγίστῶι κύκλῶι τῶν ἐν </seg>
                    <seg n="4" type="line" facs="z109v1_04">τῆι σφαίραι, ὕψος δὲ ἴσον τῆι <w
                            part="I">δια</w></seg>
                    <seg n="5" type="line" facs="z109v1_05"><w part="F">μέτρωι</w> τῆς σφαίρας αὐτός
                        τε <w part="I">ἡ</w></seg>
                    <seg n="6" type="line" facs="z109v1_06"><w part="F">μιόλιός</w> ἐστιν τῆς
                        σφαίρας, καὶ ἡ</seg>
                    <seg n="7" type="line" facs="z109v1_07">ἐπιφάνεια αὐτοῦ τῆς <choice>
                            <abbr>επιφανει</abbr>
                            <expan>ἐπιφανείας</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z109v1_08">τῆς σφαίρας. ταῦτα δὲ τὰ <seg
                            type="unclearword">συμ</seg></seg>
                    <seg n="9" type="line" facs="z109v1_09"><seg type="wordend"
                            ><unclear>πτώ</unclear>ματα</seg> τῆι φύσει <choice>
                            <expan>προυπῆρχεν</expan>
                            <abbr>προυπηρχ</abbr>
                        </choice></seg>
                    <seg n="10" type="line" facs="z109v1_10">περὶ τὰ εἰρημένα σχήματα, <w part="I"
                        >ἠ</w></seg>
                    <seg n="11" type="line" facs="z109v1_11"><w part="F">γνοεῖτο</w> δὲ ὑπὸ τῶν πρὸ
                        ἡμῶν <choice>
                            <expan>περὶ</expan>
                            <abbr>π</abbr>
                        </choice></seg>
                    <seg n="12" type="line" facs="z109v1_12">γεωμετρίαν ἀνεστραμμένων</seg>
                    <seg n="13" type="line" facs="z109v1_13">οὐδενὸς αὐτῶν <seg type="word"
                                >ἐπινενοη<supplied reason="lost">κότος</supplied></seg></seg>
                    <seg n="14" type="line" facs="z109v1_14">ὅτι τούτων τῶν σχαμάτων ἐστὶν</seg>
                    <seg n="15" type="line" facs="z109v1_15">συμμετρία· διόπερ οὐκ ἂν <w part="I"
                            >ὀκνή</w></seg>
                    <seg n="16" type="line" facs="z109v1_16"><w part="F">σαιμι</w> ἀντιπαραβαλεῖν
                        αὐτὰ</seg>
                    <seg n="17" type="line" facs="z109v1_17"><expan>πρός</expan> τε τὰ τοῖς ἄλλοις
                        γεωμέτραις τε</seg>
                    <seg n="18" type="line" facs="z109v1_18">θεωρημένα καὶ πρὸς τὰ <seg
                            type="expandedword">
                            <choice>
                                <abbr>δοξα</abbr>
                                <expan>δόξαντα</expan>
                            </choice>
                        </seg></seg>
                    <seg n="19" type="line" facs="z109v1_19"><seg type="wordend">τα</seg> πολὺ
                        ὑπερέχειν τῶν ὑπὸ <w part="I">Εὐ</w></seg>
                </seg>
                <seg n="106r1" type="folio">
                    <seg n="1" type="line" facs="z106r1_01"><w part="F">δόξου</w> περὶ τὰ στερεὰ
                            <seg type="expandedword">
                            <choice>
                                <expan>θεωρηθέντων</expan>
                                <abbr>θεωρηθε</abbr>
                            </choice>
                        </seg></seg>
                    <seg n="2" type="line" facs="z106r1_02"><seg type="wordend">των</seg>, ὅτι πᾶσα
                        πυραμὶς <w part="I">τρί</w></seg>
                    <seg n="3" type="line" facs="z106r1_03"><w part="F">τον</w> ἐστὶ μέρος πρίσματος <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z106r1_04">βάσιν ἔχοντος τὴν αὐτὴν τῆι <w part="I"
                            >πυ</w></seg>
                    <seg n="5" type="line" facs="z106r1_05"><w part="F">ραμίδι</w> καὶ ὕψος ἴσον,
                        καὶ <expan>ὅτι</expan> πᾶς</seg>
                    <seg n="6" type="line" facs="z106r1_06">κῶνος τρίτον μέρος ἐστὶν τοῦ <w part="I"
                            >κυ</w></seg>
                    <seg n="7" type="line" facs="z106r1_07"><w part="F">λίνδρου</w> τοῦ βάσιν
                        ἔχοντος <choice>
                            <abbr>τη</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z106r1_08">αὐτὴν τῶι κώνωι καὶ ὕψος ἴσον·</seg>
                    <seg n="9" type="line" facs="z106r1_09">καὶ γὰρ τούτων <choice>
                            <abbr>προυπαρχοντ</abbr>
                            <expan>προυπαρχόντων</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z106r1_10">φυσικῶς περὶ ταῦτα τὰ <w part="I"
                        >σχάμα</w></seg>
                    <seg n="11" type="line" facs="z106r1_11"><w part="F">τα</w>, πολλῶν πρὸ Εὐδόξου
                            <w part="I">γεγενη</w></seg>
                    <seg n="12" type="line" facs="z106r1_12"><w part="F">μένων</w> ἀξίων λόγου
                        γεωμετρῶν</seg>
                    <seg n="13" type="line" facs="z106r1_13">συνέβαινεν ὑπὸ πάντων <w part="I"
                        >ἀγνο</w></seg>
                    <seg n="14" type="line" facs="z106r1_14"><w part="F">εῖσθαι</w> μηδ’ ὑφ’ ἑνὸς <w
                            part="I">κατανοηθῆ</w></seg>
                    <seg n="15" type="line" facs="z106r1_15"><w part="F">ναι</w>. ἐ̓ξέσται δὲ περὶ
                        τούτων <w part="I">ἐπισκέ</w></seg>
                    <seg n="16" type="line" facs="z106r1_16"><w part="F">ψασθαι</w> τοῖς
                        δυνησομένοις. <w part="I">ὤ</w></seg>
                </seg>
                <seg n="109v2" type="folio">
                    <seg n="1" type="line" facs="z109v2_01"><w part="F">φειλε</w> μὲν οὖν Κόνωνος
                        ἔτι <w part="I">ζῶν</w></seg>
                    <seg n="2" type="line" facs="z109v2_02"><w part="F">τος</w> ἐκδίδοσθαι ταῦτα·
                        τῆνον</seg>
                    <seg n="3" type="line" facs="z109v2_03">γὰρ ὑπολαμβάνομέν που <w part="I">μά</w></seg>
                    <seg n="4" type="line" facs="z109v2_04"><w part="F">λιστα</w> ἂν δύνασθαι
                        κατανοῆσαι</seg>
                    <seg n="5" type="line" facs="z109v2_05">ταῦτα καὶ τὴν ἁρμόζουσαν <w part="I"
                        >ὑ</w></seg>
                    <seg n="6" type="line" facs="z109v2_06"><w part="F">πὲρ</w> αὐτῶν ἀπόφασιν <seg
                            type="expandedword">
                            <choice>
                                <abbr>ποιησ</abbr>
                                <expan>ποιήσασθαι</expan>
                            </choice>
                        </seg></seg>
                    <seg n="7" type="line" facs="z109v2_07"><seg type="wordend">θαι</seg>·
                        δοκιμάζοντες δὲ καλῶς</seg>
                    <seg n="8" type="line" facs="z109v2_08">ἔχειν μεταδιδόναι τοῖς οἰκείοις</seg>
                    <seg n="9" type="line" facs="z109v2_09">τῶν μαθημάτων <w part="I">ἀποστέλλο</w></seg>
                    <seg n="10" type="line" facs="z109v2_10"><w part="F">μέν</w> σοι τὰς ἀποδείξεις
                            <w part="I">ἀναγρά</w></seg>
                    <seg n="11" type="line" facs="z109v2_11"><w part="F">ψαντες</w>, ὑπὲρ ὧν ἐξέσται
                        τοῖς</seg>
                    <seg n="12" type="line" facs="z109v2_12">περὶ τὰ μαθήματα <w part="I"
                        >ἀναστρε</w></seg>
                    <seg n="13" type="line" facs="z109v2_13"><w part="F">φομένοις</w> ἐπισκέψασθαι.
                            <w part="I">ἐρω</w></seg>
                    <seg n="14" type="line" facs="z109v2_14"><w part="F">μένως</w>.</seg>
                </seg>
                <seg n="109v2" type="folio">
                    <seg n="14" type="line" facs="z109v2_14">γράφονται πρῶτον</seg>
                    <seg n="15" type="line" facs="z109v2_15">τά τε ἀξιώματα καὶ τὰ <w part="I"
                        >λαμ</w></seg>
                    <seg n="16" type="line" facs="z109v2_16"><w part="F">βανόμενα</w> εἰς τὰς
                        ἀποδείξεις</seg>
                    <seg n="17" type="line" facs="z109v2_17">αὐτῶν.</seg>
                </seg>
                <seg n="109v2" type="folio">
                    <seg n="17" type="line" facs="z109v2_17"><expan>εἰσί</expan> τινες ἐν ἐπιπέδωι</seg>
                    <seg n="18" type="line" facs="z109v2_18">καμπύλαι γραμμαὶ <w part="I"
                        >πεπερασ</w></seg>
                </seg>
                <seg n="106r2" type="folio">
                    <seg n="1" type="line" facs="z106r2_01">μέναι, αἳ τῶν τὰ πέρατα <w part="I"
                            >ἐπιζευ</w></seg>
                    <seg n="2" type="line" facs="z106r2_02"><w part="F">γνυουσῶν</w> αὐτῶν <seg
                            type="word">εὐ<supplied reason="lost">θειῶν</supplied></seg> ἤτοι</seg>
                    <seg n="3" type="line" facs="z106r2_03">ὅλαι <seg type="word">ἐπ<supplied
                                reason="lost">ὶ</supplied></seg>
                        <seg type="word"><supplied reason="lost">τ</supplied>ὰ</seg>
                        <supplied reason="lost">αὐτά <expan>εἰσιν</expan> ἢ οὐδὲν</supplied>
                        <seg type="suppliedword">
                            <unclear>ἔχ</unclear>
                            <supplied reason="lost">ου</supplied>
                        </seg></seg>
                    <seg n="4" type="line" facs="z106r2_04"><seg type="wordend">σιν</seg> ἐπὶ τὰ
                        ἕτερα.</seg>
                </seg>
                <seg n="106r2" type="folio">
                    <seg n="4" type="line" facs="z106r2_04"><supplied reason="lost">ἐπὶ
                        τὰ</supplied> αὐτὰ</seg>
                    <seg n="5" type="line" facs="z106r2_05">δὴ κοίλην καλῶ τὴν τοιαύτην <w part="I"
                            >γραμ</w></seg>
                    <seg n="6" type="line" facs="z106r2_06"><w part="F">μήν</w>, ἐν ᾗ ἐὰν δύο
                        σημείων <w part="I">λαμ</w></seg>
                    <seg n="7" type="line" facs="z106r2_07"><w part="F">βανομένων</w> ὁποιωνοῦν αἱ
                            <w part="I">μετα</w></seg>
                    <seg n="8" type="line" facs="z106r2_08"><w part="F">ξὺ</w> τῶν σημείων εὐθεῖαι
                        ἤτοι</seg>
                    <seg n="9" type="line" facs="z106r2_09">πᾶσαι ἐπὶ τὰ αὐτὰ πίπτουσιν <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z106r2_10">γραμμῆς, ἢ τινὲς μὲν ἐπὶ τὰ <w part="I"
                            >αὐ</w></seg>
                    <seg n="11" type="line" facs="z106r2_11"><w part="F">τά</w>, τινὲς δὲ κατ’
                        αὐτῆς, ἐπὶ τὰ</seg>
                    <seg n="12" type="line" facs="z106r2_12">ἕτερα δὲ μηδεμία.</seg>
                </seg>
                <seg n="106r2" type="folio">
                    <seg n="12" type="line" facs="z106r2_12">ὁμοίως δὴ <choice>
                            <abbr>κ</abbr>
                            <expan>καὶ</expan>
                        </choice>
                    </seg>
                    <seg n="13" type="line" facs="z106r2_13">ἐπιφάνειαί τινές <expan>εἰσιν</expan>
                        <w part="I">πεπερασ</w></seg>
                    <seg n="14" type="line" facs="z106r2_14"><w part="F">μέναι</w>, αὐταὶ μὲν οὐκ ἐν
                            <w part="I">ἐπιπέ</w></seg>
                    <seg n="15" type="line" facs="z106r2_15"><w part="F">δωι</w>, τὰ δὲ πέρατα
                        ἔχουσαι ἐν <w part="I">ἐ</w></seg>
                    <seg n="16" type="line" facs="z106r2_16"><w part="F">πιπέδωι</w>, αἳ τοῦ
                        ἐπιπέδου, ἐν ὧι</seg>
                    <seg n="17" type="line" facs="z106r2_17">τὰ πέρατα ἔχουσιν, ἤτοι ὅλαι</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
