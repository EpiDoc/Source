    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="60r1" type="folio">
                    <seg n="1" type="line" facs="z60r1_01">τὰν <unclear>Η</unclear>ΚΘ περιφέρειαν
                            <seg type="clearword">με</seg></seg>
                    <seg n="2" type="line" facs="z60r1_02"><seg type="wordend">θ’</seg> ὅλας τᾶς τοῦ
                        ΘσΗΚ κύκλου</seg>
                    <seg n="3" type="line" facs="z60r1_03">περιφερείας, ὃν δὲ λόγον <seg
                            type="clearword">
                            <choice>
                                <abbr>ἔχ</abbr>
                                <expan>ἔχου</expan>
                            </choice>
                        </seg></seg>
                    <seg n="4" type="line" facs="z60r1_04"><seg type="wordend">σιν</seg> αἱ ὕστερον
                        εἰρημέναι <seg type="clearword">πε</seg></seg>
                    <seg n="5" type="line" facs="z60r1_05"><seg type="wordend">ριφέρειαι</seg>,
                        τοῦτον ἔχει τὸν <choice>
                            <abbr>λόγ</abbr>
                            <expan>λόγον</expan>
                        </choice></seg>
                    <seg n="6" type="line" facs="z60r1_06">ἁ ΧΑ εὐθεῖα ποτὶ τὰν ΑΔ <choice>
                            <abbr>εὐθεῖ</abbr>
                            <expan>εὐθεῖαν</expan>
                        </choice>·</seg>
                    <seg n="7" type="line" facs="z60r1_07">δέδεικται γὰρ τοῦτο· ἐλάσσονα</seg>
                    <seg n="8" type="line" facs="z60r1_08">ἄρα λόγον ἔχει ἁ ΙΑ ποτὶ τὰν</seg>
                    <seg n="9" type="line" facs="z60r1_09">ΑΡ ἢ αΧ ποτὶ τὰν ΑΔ· ὅπερ <seg
                            type="clearword">ἀδύ</seg></seg>
                    <seg n="10" type="line" facs="z60r1_10"><seg type="wordend">νατον</seg> ἴσα μὲν
                        γὰρ ἁ ΡΑ τᾶι ΑΔ,</seg>
                    <seg n="11" type="line" facs="z60r1_11">μείζων δὲ ἁ ΙΑ τᾶς ΑΧ. δῆλον</seg>
                    <seg n="12" type="line" facs="z60r1_12">οὖν, <expan>ὅτι</expan> ἀμβλεῖά
                            <expan>ἐστιν</expan> ἁ <seg type="clearword">περιεχομέ</seg></seg>
                    <seg n="13" type="line" facs="z60r1_13"><seg type="wordend">να</seg> ὑπὸ τᾶν
                        ΑΔΖ· ὥστε ἁ λοιπὰ</seg>
                    <seg n="14" type="line" facs="z60r1_14">ὀξεῖά ἐστιν.</seg>
                </seg>
                <seg n="60r1" type="folio">
                    <seg n="14" type="line" facs="z60r1_14">τὰ δ’ αὐτὰ <seg type="clearword"
                        >συμβή</seg></seg>
                    <seg n="15" type="line" facs="z60r1_15"><seg type="wordend">σεται</seg>, εἴ κα ἁ
                        ἐπιψαύουσα</seg>
                    <seg n="16" type="line" facs="z60r1_16">κατὰ τὸ πέρας τᾶς <seg type="word"
                                >ἕλ<supplied reason="lost">ι</supplied>κο<supplied reason="lost"
                            >ς</supplied>
                        </seg>
                        <seg type="suppliedword"><supplied reason="lost">ἐπ</supplied>ι</seg></seg>
                    <seg n="17" type="line" facs="z60r1_17">
                        <seg type="wordend">ψαύοι</seg>. </seg>
                </seg>
                <seg n="60r1" type="folio">
                    <seg n="17" type="line" facs="z60r1_17">ὁμοίως δὲ <seg type="word">δει<supplied
                                reason="lost">χ</supplied><unclear>θ</unclear><supplied
                                reason="lost">ήσ</supplied><unclear>ετ</unclear><supplied
                                reason="lost">α</supplied>ι</seg>,</seg>
                </seg>
                <seg n="61v1" type="folio">
                    <seg n="1" type="line" facs="z61v1_01"><seg type="word">κα<supplied
                                reason="lost">ὶ</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">ε</supplied>ἴ</seg> κα τᾶς ἐν <seg type="word"
                                >ὁποια<supplied reason="lost">οῦ</supplied>ν</seg>
                        <seg type="clearword">γεγραμ</seg></seg>
                    <seg n="2" type="line" facs="z61v1_02"><seg type="wordend">μένας</seg> ἕλικος
                        ἐπιψαύει τις</seg>
                    <seg n="3" type="line" facs="z61v1_03">εὐθεῖα, καὶ εἰ κατὰ τὸ πέρας</seg>
                    <seg n="4" type="line" facs="z61v1_04">αὐτᾶς, <expan>ὅτι</expan>
                        <seg type="word">ἀνί<unclear>σο</unclear>υς</seg> ποιήσει <choice>
                            <abbr>τ</abbr>
                            <expan>τὰς</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z61v1_05"><seg type="word">
                            <supplied reason="lost">γ</supplied>ωνίας</seg> ποτὶ τὰν ἀπὸ τᾶς <seg
                            type="clearword">ἁ</seg></seg>
                    <seg n="6" type="line" facs="z61v1_06"><seg type="wordend">φᾶς</seg>
                        ἐπιζευχθεῖσαν ἐπὶ τὰν</seg>
                    <seg n="7" type="line" facs="z61v1_07"><seg type="word">
                            <supplied reason="lost">ἀρ</supplied>χὰν</seg> τᾶς ἕλικος καὶ τὰν
                            <expan>μὲν</expan></seg>
                    <seg n="8" type="line" facs="z61v1_08"><seg type="word">
                            <supplied reason="lost">ἐ</supplied>
                            <unclear>ν</unclear>
                        </seg>
                        <seg type="word">
                            <unclear>τ</unclear><supplied reason="lost">οῖ</supplied>ς </seg>
                        προαγευμένοις <seg type="clearword">ἀμ</seg></seg>
                    <seg n="9" type="line" facs="z61v1_09"><seg type="wordend">βλεῖαν</seg>, τὰν δὲ
                        ἐν τοῖς <seg type="clearword">ἑπομέ</seg></seg>
                    <seg n="10" type="line" facs="z61v1_10"><seg type="wordend">νοις</seg> ὀξεῖαν. <choice>
                            <abbr>ἑξ</abbr>
                            <expan>ἑξῆς</expan>
                        </choice> τὸ <choice>
                            <abbr>ΣΧΑΜ</abbr>
                            <expan>ΣΧΑΜΑ</expan>
                        </choice></seg>
                    <figure n="17.1">
                        <figDesc>Figure 17.1</figDesc>
                    </figure>
                </seg>
                <seg n="60r2" type="folio">
                    <seg n="1" type="line" facs="z60r2_01">εἴ κα τᾶς ἕλικος τᾶς ἐν τᾶι <seg
                            type="clearword">πρώ</seg></seg>
                    <seg n="2" type="line" facs="z60r2_02"><seg type="wordend">ται</seg> περιφορᾶι
                        γεγραμμένας</seg>
                    <seg n="3" type="line" facs="z60r2_03">εὐθεῖα γραμμὰ ἐπιψαύηι <seg
                            type="clearword">κα</seg></seg>
                    <seg n="4" type="line" facs="z60r2_04"><seg type="wordend">τὰ</seg> τὸ πέρας τᾶς
                        ἕλικος, ἀπὸ</seg>
                    <seg n="5" type="line" facs="z60r2_05">δὲ τοῦ σαμείου, ὅ ἐστιν <seg type="word">
                                ἀρ<supplied reason="lost">χ</supplied>ὰ</seg> τᾶς <seg
                            type="clearword">ἕ</seg></seg>
                    <seg n="6" type="line" facs="z60r2_06"><seg type="wordend">λικος</seg>, ποτ’
                        ὀρθὰς ἀχθῆι τις</seg>
                    <seg n="7" type="line" facs="z60r2_07">τᾶι ἀρχᾶι τᾶς περιφορᾶς, ἁ</seg>
                    <seg n="8" type="line" facs="z60r2_08">ἀχθεῖσα συμπεσεῖται τᾶι <seg
                            type="clearword">ἐπιψαυ</seg></seg>
                    <seg n="9" type="line" facs="z60r2_09"><seg type="wordend">ούσα</seg>, καὶ ἁ
                        μεταξὺ εὐθεῖα τᾶς <seg type="clearword">ἐ</seg></seg>
                    <seg n="10" type="line" facs="z60r2_10"><seg type="wordend">πιψαυούσας</seg> καὶ
                        τᾶς ἀρχᾶς <seg type="word">
                            <unclear>τ</unclear>ᾶ<unclear>ς</unclear>
                        </seg></seg>
                    <seg n="11" type="line" facs="z60r2_11">ἕλικος τᾶς ἕλικος ἴσα ἐσσεῖται</seg>
                    <seg n="12" type="line" facs="z60r2_12">τᾶι τοῦ πρώτου κύκλου <seg
                            type="clearword">περιφε</seg></seg>
                    <seg n="13" type="line" facs="z60r2_13">
                        <seg type="wordend">ρείαι</seg>. </seg>
                </seg>
                <seg n="60r2" type="folio">
                    <seg n="13" type="line" facs="z60r2_13">ἔστω ἕλιξ ἁ ΑΒ ΓΔΘ,</seg>
                    <seg n="14" type="line" facs="z60r2_14">ἔστω δὲ τὸ Α σημεῖον ἀρχὰ τᾶς <choice>
                            <abbr>ἕλικ</abbr>
                            <expan>ἕλικος</expan>
                        </choice>,</seg>
                    <seg n="15" type="line" facs="z60r2_15">ἁ δὲ ΘΑ γραμμὰ ἀρχὰ τᾶς <seg
                            type="clearword">πε<unclear>ρι</unclear></seg></seg>
                    <seg n="16" type="line" facs="z60r2_16"><seg type="wordend">φορᾶς</seg>, ὁ δὲ
                        ΘΗΚ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice> ὁ πρῶτος,</seg>
                    <seg n="17" type="line" facs="z60r2_17">ἐπιψαυέτω δέ τις τᾶς ἕλικος <choice>
                            <abbr>κα</abbr>
                            <expan>κατὰ</expan>
                        </choice></seg>
                </seg>
                <seg n="61v2" type="folio">
                    <seg n="1" type="line" facs="z61v2_01">τὸ Θ ἁ ΘΖ, καὶ ἀπὸ τοῦ Α <seg type="word"
                            > ἄχ<supplied reason="lost">θ</supplied>ω</seg></seg>
                    <seg n="2" type="line" facs="z61v2_02"><expan>πρὸς</expan> ὀρθὰς τᾶ ΘΑ ἁ ΑΖ· <choice>
                            <abbr>συμπεσεῖτ</abbr>
                            <expan>συμπεσεῖται</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z61v2_03">δὴ αὕτα ποτὶ τὰν ΘΖ, ἐπεὶ αἱ ΖΘ,</seg>
                    <seg n="4" type="line" facs="z61v2_04">ΘΑ ὀξεῖαν γωνίαν <choice>
                            <abbr>περιέχουσι</abbr>
                            <expan>περιέχουσιν</expan>
                        </choice>.</seg>
                    <seg n="5" type="line" facs="z61v2_05">συμπιπτέτω κατὰ τὸ Ζ. δεικτέον,
                            <expan>ὅτι</expan></seg>
                    <seg n="6" type="line" facs="z61v2_06">ἁ ΖΑ ἴσα <expan>ἐστὶ</expan> τᾶι τοῦ ΘΚΗ
                        κύκλου</seg>
                    <seg n="7" type="line" facs="z61v2_07">περιφερείαι.</seg>
                </seg>
                <seg n="61v2" type="folio">
                    <seg n="7" type="line" facs="z61v2_07">εἰ γὰρ μή, ἤτοι <seg type="clearword"
                        >μεί</seg></seg>
                    <seg n="8" type="line" facs="z61v2_08"><seg type="wordend">ζων</seg> ἐστὶν ἢ
                        ἐλάσσων. ἔστω <seg type="clearword">πρό</seg></seg>
                    <seg n="9" type="line" facs="z61v2_09"><seg type="wordend">τερον</seg>, εἰ
                        δυνατόν, μείζων. <seg type="clearword">ἔλα</seg></seg>
                    <seg n="10" type="line" facs="z61v2_10"><seg type="wordend">βον</seg> δή τινα
                        εὐθεῖαν τὰν ΛΑ</seg>
                    <seg n="11" type="line" facs="z61v2_11">τᾶς μὲν ΖΑ εὐθείας <seg type="word">
                                ἐλάσσον<supplied reason="lost">α,</supplied>
                        </seg></seg>
                    <seg n="12" type="line" facs="z61v2_12">τᾶς δὲ τοῦ ΘΗΚ κύκλου <seg
                            type="clearword">περιφε</seg></seg>
                    <seg n="13" type="line" facs="z61v2_13"><seg type="wordend">ρείας</seg> μείζονα.
                        ἔστι δὴ <seg type="word">κ<unclear>ύ</unclear>κλος</seg></seg>
                    <seg n="14" type="line" facs="z61v2_14">τις ὁ ΘΗΚ καὶ ἐν τῶι <seg type="word">
                                κ<unclear>ύ</unclear>κλωι</seg></seg>
                    <seg n="15" type="line" facs="z61v2_15">γραμμὰ ἐλάσσων τᾶς <seg
                            type="suppliedword">διαμέ</seg></seg>
                    <seg n="16" type="line" facs="z61v2_16"><seg type="wordend">τρ<supplied
                                reason="lost">ου</supplied></seg> ἁ ΘΗ <expan>καὶ</expan>
                        <expan>λόγος</expan>, ὃν ἔχει <supplied reason="lost">ἁ</supplied>
                        <seg type="word">
                            <supplied reason="lost">Θ</supplied>
                            <unclear>Α</unclear>
                        </seg>
                        <supplied reason="lost">
                            <expan>πρὸς</expan>
                        </supplied> ΑΛ,</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
