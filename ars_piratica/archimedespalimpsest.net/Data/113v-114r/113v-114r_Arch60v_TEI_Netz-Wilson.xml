    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="113v1" type="folio">
                    <seg n="1" type="line" facs="z113v1_01">ὕψος δὲ ἴσον τῆι ἐκ τοῦ κέντρου <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice>
                    </seg>
                    <seg n="2" type="line" facs="z113v1_02">σφαίρας, μείζων <expan>ἐστὶ</expan> τοῦ
                            <w part="I">ἐγγεγραμμέ</w></seg>
                    <seg n="3" type="line" facs="z113v1_03"><w part="F">νου</w> σχήματος σὺν τῶι
                        κώνωι· ὁ <expan>γὰρ</expan></seg>
                    <seg n="4" type="line" facs="z113v1_04">προειρημένος κῶνος μείζων
                        <expan>ἐστὶ</expan>
                    </seg>
                    <seg n="5" type="line" facs="z113v1_05">τοῦ κώνου τοῦ ἴσου τῶι σχήματι </seg>
                    <seg n="6" type="line" facs="z113v1_06">σὺν τῶι κώνωι <choice>
                            <abbr>το</abbr>
                            <expan>τοῦ</expan>
                        </choice> βάσιν μὲν ἔχον</seg>
                    <seg n="7" type="line" facs="z113v1_07">τὸς τὴν βάσιν τοῦ τμήματος, </seg>
                    <seg n="8" type="line" facs="z113v1_08">τὴν δὲ κορυφὴν <expan>πρὸς</expan> τῶι
                            <seg type="word">κέντρω<supplied reason="lost">ι</supplied></seg>, <choice>
                            <abbr>ττ</abbr>
                            <expan>τουτέστι</expan>
                        </choice>
                    </seg>
                    <seg n="9" type="line" facs="z113v1_09">τὴν βάσιν μὲν ἔχοντος ἴσην τῆι <seg
                            type="unclearword">ἐ</seg></seg>
                    <seg n="10" type="line" facs="z113v1_10"><seg type="wordend"
                            >π<unclear>ι</unclear>φανείαι</seg> τοῦ σχήματος, τὸ δὲ <w part="I"
                        >ὔ</w></seg>
                    <seg n="11" type="line" facs="z113v1_11"><w part="F">ψος</w> τῆι ἀπὸ τοῦ κέντρου
                        ἐπὶ <choice>
                            <abbr>μία</abbr>
                            <expan>μίαν</expan>
                        </choice>
                    </seg>
                    <seg n="12" type="line" facs="z113v1_12">πλευρὰν τοῦ πολυγώνου καθέτωι </seg>
                    <seg n="13" type="line" facs="z113v1_13">ἠγμένηι· ἥ τε γὰρ βάσις τῆς <w part="I"
                            >βάσε</w></seg>
                    <seg n="14" type="line" facs="z113v1_14"><w part="F">ως</w> μείζων
                        <expan>ἐστὶ</expan> δέδεικται γὰρ τοῦτο </seg>
                    <seg n="15" type="line" facs="z113v1_15"><expan>καὶ</expan> τὸ ὕψος τοῦ ὕψους.</seg>
                    <figure n="1.38-1.1">
                        <figDesc>Figure 1 of corollary 1.38-1</figDesc>
                    </figure>
                </seg>
                <seg n="114r1" type="folio">
                    <seg n="1" type="line" facs="z114r1_01">
                        <seg type="word">ἔστ<supplied reason="lost">ω</supplied></seg>
                        <seg type="word"><supplied reason="lost">σφ</supplied>αῖρα</seg> καὶ ἐν
                        αὐτῆι μέγιστος</seg>
                    <seg n="2" type="line" facs="z114r1_02"><seg type="word"
                            >κ<unclear>ύ</unclear>κλος</seg> ὁ ΑΒΓ, καὶ τετμήσθω <choice>
                            <abbr>ελασσ</abbr>
                            <expan>ἔλασσον</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z114r1_03">ἡμικυκλίου, ὃ ἀποτέμνει ἡ ΑΒ,
                        <expan>καὶ</expan>
                    </seg>
                    <seg n="4" type="line" facs="z114r1_04">κέντρον τὸ Δ, καὶ ἀπὸ τοῦ κέντρου</seg>
                    <seg n="5" type="line" facs="z114r1_05">τοῦ Δ ἐπὶ τὰ ΑΒ ἐπεζεύχθωσαν αἱ</seg>
                    <seg n="6" type="line" facs="z114r1_06">ΑΔ ΔΒ, καὶ περὶ τὸν γεννηθέντα</seg>
                    <seg n="7" type="line" facs="z114r1_07">τομέα περιγεγράφθω <choice>
                            <abbr>πολυγων</abbr>
                            <expan>πολύγωνον</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z114r1_08"><expan>καὶ</expan> περὶ αὐτὸ κύκλος·
                        ἕξει δὴ τὸ αὐτὸ </seg>
                    <seg n="9" type="line" facs="z114r1_09">κέντρον <sic>το</sic>
                        <unclear>Α</unclear>ΒΓ <seg type="word"><supplied reason="lost"
                            >κύ</supplied>κλωι</seg>. ἐὰν δὴ </seg>
                </seg>
                <seg n="113v2" type="folio">
                    <seg n="1" type="line" facs="z113v2_01">μενούσης <seg type="word">τ<supplied
                                reason="lost">ῆ</supplied>ς</seg> ΕΚ <choice>
                            <abbr>περιενεχθε</abbr>
                            <expan>περιενεχθὲν</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z113v2_02">τὸ πολύγωνον εἰς τὸ <seg type="word"
                                >α<unclear>ὐ</unclear>τὸ</seg> πάλιν <w part="I">ἀ</w></seg>
                    <seg n="3" type="line" facs="z113v2_03"><w part="F">ποκατασταθῆ</w>, ὁ <w
                            part="I">περιγεγραμ</w></seg>
                    <seg n="4" type="line" facs="z113v2_04"><w part="F">μένος</w> κύκλος κατὰ
                        ἐπιφανείας</seg>
                    <seg n="5" type="line" facs="z113v2_05">οἰσθήσεται σφαίρα, καὶ αἱ γωνίαι</seg>
                    <seg n="6" type="line" facs="z113v2_06"><seg type="word">
                            <unclear>το</unclear>
                            <supplied reason="lost">ῦ</supplied>
                        </seg>
                        <seg type="word"><unclear>πολυ</unclear>γώνου</seg> κύκλου γράψουσιν,</seg>
                    <seg n="7" type="line" facs="z113v2_07">ὧν αἱ <choice>
                            <abbr>μετροι</abbr>
                            <expan>διάμετροι</expan>
                        </choice>
                        <seg type="word">ἐπιζευγνύο<unclear>υ</unclear>σι</seg> τὰς</seg>
                    <seg n="8" type="line" facs="z113v2_08">γωνίας τοῦ πολυγώνου <seg type="word"
                                >ο<unclear>ὖσ</unclear>αι</seg>
                        <w part="I">πα</w></seg>
                    <seg n="9" type="line" facs="z113v2_09"><w part="F">ράλληλοι</w> τῆι ΑΒ, τὰ
                            <unclear>δὲ</unclear>
                        <seg type="word"><supplied reason="lost"
                            >σ</supplied>η<unclear>μ</unclear>εῖα</seg>, <w part="I">κα</w></seg>
                    <seg n="10" type="line" facs="z113v2_10"><w part="F">θ’</w> ἃ <choice>
                            <abbr>απτοντ</abbr>
                            <expan>ἅ<unclear>π</unclear><supplied reason="lost"
                            >τ</supplied>ονται</expan>
                        </choice> τοῦ <seg type="word">ἐ<unclear>λ</unclear>άσσ<supplied
                                reason="lost">ο</supplied>νος</seg>
                        <seg type="word"><unclear>κύκ</unclear>λου</seg></seg>
                    <seg n="11" type="line" facs="z113v2_11"><seg type="word"
                        >α<unclear>ἱ</unclear></seg> τοῦ πολυγώνου <seg type="word"
                                >π<unclear>λ</unclear><supplied reason="lost"
                        >ευρ</supplied>αί</seg>, <seg type="word">κύκλο<unclear>υ</unclear>ς</seg></seg>
                    <seg n="12" type="line" facs="z113v2_12"><seg type="word"
                            >γράφο<unclear>υ</unclear>σιν</seg> ἐν τῆι ἐλάσσονι <choice>
                            <abbr>σφαιρ</abbr>
                            <expan>σφαίραι</expan>
                        </choice>,</seg>
                    <seg n="13" type="line" facs="z113v2_13">ὧν διάμετροι <choice>
                            <abbr>εσον</abbr>
                            <expan>ἔσονται</expan>
                        </choice> αἱ <seg type="suppliedword">ἐπιζευγνύ</seg></seg>
                    <seg n="14" type="line" facs="z113v2_14"><seg type="wordend">ο<supplied
                                reason="lost">υ</supplied>σαι</seg> τὰς ἁφὰς παράλληλοι <choice>
                            <abbr>ουσ</abbr>
                            <expan>οὖσαι</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z113v2_15">τῆι ΑΒ, αἱ δὲ πλευραὶ κατὰ <choice>
                            <abbr>κωνικ</abbr>
                            <expan>κωνικῶν</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z113v2_16">ἐπιφανειῶν οἰσθήσονται,
                        <expan>καὶ</expan> ἔσται</seg>
                    <seg n="17" type="line" facs="z113v2_17">τὸ περιγραφὲν σχῆμα ὑπὸ <w part="I"
                            >κωνι</w></seg>
                    <seg n="18" type="line" facs="z113v2_18"><w part="F">κῶν</w> ἐπιφανειῶν
                        περιεχόμενον,</seg>
                </seg>
                <seg n="114r2" type="folio">
                    <seg n="1" type="line" facs="z114r2_01">βάσις ὁ περὶ τὴν ΖΗ <seg type="word"
                                ><unclear>κ</unclear>ύ<unclear>κ</unclear>λος</seg>· ἡ δὲ</seg>
                    <seg n="2" type="line" facs="z114r2_02">τοῦ εἰρημένου σχήματος <w part="I"
                        >ἐπιφά</w></seg>
                    <seg n="3" type="line" facs="z114r2_03"><w part="F">νεια</w> μείζων
                        <expan>ἐστὶ</expan> τῆς τοῦ ἐλάσσονος</seg>
                    <seg n="4" type="line" facs="z114r2_04">τμήματος ἐπιφανείας, οὗ βάσις</seg>
                    <seg n="5" type="line" facs="z114r2_05">οἱ περὶ τὴν ΑΒ κύκλοι.</seg>
                </seg>
                <seg n="114r2" type="folio">
                    <seg n="5" type="line" facs="z114r2_05">ἤχθωσαν <expan>γὰρ</expan></seg>
                    <seg n="6" type="line" facs="z114r2_06">ἐφαπτόμεναι αἱ ΑΜ ΒΝ· κατὰ <w part="I"
                            >κω</w></seg>
                    <seg n="7" type="line" facs="z114r2_07"><w part="F">νικῆς</w>
                        <expan>ἄρα</expan> ἐπιφανείας οἰσθήσονται,</seg>
                    <seg n="8" type="line" facs="z114r2_08">καὶ τὸ σχῆμα τὸ γενηθὲν ὑπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z114r2_09">πολυγώνου τοῦ ΑΜ ΘΕ ΝΒ μείζονα</seg>
                    <seg n="10" type="line" facs="z114r2_10">ἕξει τὴν <seg type="word"
                                >ἐπιφάνεια<supplied reason="lost">ν</supplied></seg> τοῦ <choice>
                            <abbr>τμηματ</abbr>
                            <expan>τμήματος</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z114r2_11">τῆς σφαίρας, <seg type="word"
                                >ο<unclear>ὗ</unclear></seg> βάσις ὁ περὶ</seg>
                    <seg n="12" type="line" facs="z114r2_12"><seg type="word">διάμετρο<supplied
                                reason="lost">ν</supplied></seg> τὴν ΑΒ κύκλος· πέρας</seg>
                    <seg n="13" type="line" facs="z114r2_13">γὰρ ἐν <seg type="word">ἑν<supplied
                                reason="lost">ὶ</supplied></seg> ἐπιπέδωι τὸ αὐτὸ <seg
                            type="expandedword">
                            <choice>
                                <abbr>εχ</abbr>
                                <expan>ἔχουσι</expan>
                            </choice>
                        </seg></seg>
                    <seg n="14" type="line" facs="z114r2_14"><seg type="wordend">
                            <abbr>σι</abbr>
                        </seg> τὸν περὶ διάμετρον τὴν ΑΒ <seg type="suppliedword"><supplied
                                reason="lost">κ</supplied><unclear>υ</unclear>́</seg></seg>
                    <seg n="15" type="line" facs="z114r2_15"><seg type="wordend">κλον</seg>,
                            <expan>καὶ</expan> περιλαμβάνεται τὸ <w part="I">τμῆ</w></seg>
                    <seg n="16" type="line" facs="z114r2_16"><w part="F">μα</w> ὑπὸ τοῦ σχήματος.
                        ἀλλ’ ἡ <sic>γεγενη</sic></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
