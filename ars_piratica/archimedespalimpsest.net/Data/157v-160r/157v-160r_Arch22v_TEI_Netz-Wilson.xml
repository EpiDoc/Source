    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="157v1" type="folio">
                    <seg n="1" type="line" facs="z157v1_01">Κ<supplied reason="lost">Θ</supplied>
                        <seg type="word"
                        >πα<unclear>ρα</unclear>λλ<unclear>η</unclear>λογράμμωι</seg> ἡ ΜΝ</seg>
                    <seg n="2" type="line" facs="z157v1_02"><seg type="word">
                            <supplied reason="lost">τῆ</supplied>
                            <unclear>ι</unclear>
                        </seg> ΚΛ παράλληλος <seg type="word">τε<supplied reason="lost"
                            >μνέ</supplied>τω</seg> δὲ</seg>
                    <seg n="3" type="line" facs="z157v1_03"><supplied reason="lost">αὐτὴ</supplied>
                        τὴν μὲν <supplied reason="lost">σφαῖραν</supplied>
                        <supplied reason="lost">κατὰ</supplied>
                        <supplied reason="lost">τὰ</supplied>
                        <supplied reason="lost">ΞΟ</supplied>
                        <supplied reason="lost">τὸν</supplied></seg>
                    <seg n="4" type="line" facs="z157v1_04"><supplied reason="lost">δὲ</supplied>
                        <supplied reason="lost">κῶνον</supplied> κατὰ τὰ ΠΡ <gap/></seg>
                    <seg n="5" type="line" facs="z157v1_05">πλευρὰς <gap/>
                        <supplied reason="lost">καὶ</supplied>
                        <supplied reason="lost">ἀπὸ</supplied>
                        <supplied reason="lost">τῆς</supplied>
                    </seg>
                    <seg n="6" type="line" facs="z157v1_06">ΜΝ ἐπίπεδον <supplied reason="lost"
                            >ἀνεστάτω</supplied>
                        <supplied reason="lost">ὀρθὸν</supplied>
                        <supplied reason="lost">
                            <expan>πρὸς</expan>
                        </supplied>
                    </seg>
                    <seg n="7" type="line" facs="z157v1_07">τὴν ΑΓ· ποιήσει <supplied reason="lost"
                            >δὴ</supplied>
                        <supplied reason="lost">τοῦτο</supplied>
                        <supplied reason="lost">ἐν</supplied>
                        <supplied reason="lost">μὲν</supplied></seg>
                    <seg n="8" type="line" facs="z157v1_08">τῶι κυλίνδρωι <seg type="word"
                                >το<supplied reason="lost">μὴν</supplied></seg>
                        <seg type="word"><supplied reason="lost">κύκλ</supplied>ον</seg>, οὗ
                            <expan>ἐστι</expan></seg>
                    <seg n="9" type="line" facs="z157v1_09">διάμετρος ἡ ΜΝ, ἐν <seg type="word"
                                >δ<supplied reason="lost">ὲ</supplied></seg> τῶι <seg
                            type="clearword">τμήμα</seg></seg>
                    <seg n="10" type="line" facs="z157v1_10"><seg type="wordend">τι</seg> τῆς
                        σφαίρας <seg type="word">το<supplied reason="lost">μὴν</supplied></seg>
                        κύκλον, οὗ</seg>
                    <seg n="11" type="line" facs="z157v1_11">διάμετρος ἡ ΞΟ, ἐν δὲ τῶι κώνωι,</seg>
                    <seg n="12" type="line" facs="z157v1_12">οὗ βάσις ὁ περὶ διάμετρον τὴν ΕΖ</seg>
                    <seg n="13" type="line" facs="z157v1_13">κύκλον, κορυφὴ <supplied reason="lost"
                            >δὲ</supplied>
                        <supplied reason="lost">τὸ</supplied>
                        <supplied reason="lost">Α</supplied>
                        <seg type="word"><supplied reason="lost">ση</supplied>μεῖον</seg>, <w
                            part="I">κύ</w></seg>
                    <seg n="14" type="line" facs="z157v1_14"><w part="F">κλον</w>, <seg type="word"
                                >ο<supplied reason="lost">ὗ</supplied></seg> διάμετρος ἔσται ἡ ΠΡ.
                            <w part="I">ὁ</w></seg>
                    <seg n="15" type="line" facs="z157v1_15"><w part="F">μοίως</w> δὴ τοῖς πρότερον
                            <seg type="suppliedword">δειχθ<supplied reason="lost"
                        >ήσε</supplied></seg></seg>
                    <seg n="16" type="line" facs="z157v1_16">ται ἰσόρροπον περὶ τὸ Α <seg
                            type="word">
                            <choice>
                                <abbr>σημεῖ<unclear>ο</unclear></abbr>
                                <expan>σημεῖ<unclear>ο</unclear>ν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="17" type="line" facs="z157v1_17">ὁ κύκλος, οὗ <choice>
                            <abbr>μετρος</abbr>
                            <expan>διάμετρος</expan>
                        </choice> ἡ ΜΝ, <w part="I">αὐ</w></seg>
                    <seg n="18" type="line" facs="z157v1_18"><w part="F">τοῦ</w> μένων ἀμφοτέροις <choice>
                            <abbr>τ</abbr>
                            <expan>τοῖς</expan>
                        </choice>
                        <expan>κύκλοις</expan>,</seg>
                    <seg n="19" type="line" facs="z157v1_19">ὧν <choice>
                            <abbr>μετρος</abbr>
                            <expan>διάμετρος</expan>
                        </choice> ἡ ΞΟ Π<unclear>Ρ</unclear>, <seg type="expandedword"
                                ><unclear>μ</unclear>ε<supplied reason="lost">τ</supplied>ε<supplied
                                reason="lost">νε</supplied>χ<supplied reason="lost"
                        >θεῖ</supplied></seg>
                    </seg>
                </seg>
                <seg n="160r1" type="folio">
                    <seg n="1" type="line" facs="z160r1_01"><seg type="wordend">σι</seg>
                        <expan>καὶ</expan>
                        <choice>
                            <abbr>τεθεῖσι</abbr>
                            <expan>τεθεῖσιν</expan>
                        </choice> τοῦ ζυγοῦ κατὰ τὸ Θ,</seg>
                    <seg n="2" type="line" facs="z160r1_02">ὥστε ἑκατέρου αὐτῶν κέντρον</seg>
                    <seg n="3" type="line" facs="z160r1_03"><expan>εἶναι</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> βάρους τὸ Θ· τοῦτο <seg type="word">γο<unclear>ῦν</unclear></seg>
                        <seg type="suppliedword">δέ</seg></seg>
                    <seg n="4" type="line" facs="z160r1_04"><seg type="wordend"><supplied
                                reason="lost">δεικτ</supplied>αι</seg>. συμπληρωθέντων
                        <expan>οὖν</expan>
                    </seg>
                    <seg n="5" type="line" facs="z160r1_05"><seg type="word">
                            <supplied reason="lost">ὑ</supplied>
                            <unclear>πὸ</unclear>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">ῶν</supplied></expan>
                            </choice>
                        </seg>
                        <expan>κύκλων</expan> τοῦ τε κυλινδροῦ καὶ</seg>
                    <seg n="6" type="line" facs="z160r1_06">τοῦ <seg type="word"><supplied
                                reason="lost">κώ</supplied>νου</seg>
                        <expan>καὶ</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> τμήματος τῆς</seg>
                    <seg n="7" type="line" facs="z160r1_07">σφαίρας τοῦ ἔχοντος ἄξονα</seg>
                    <seg n="8" type="line" facs="z160r1_08">τὴν ΑΗ εὐθεῖαν <seg type="word"
                                >ἰσόρροπο<supplied reason="lost">ι</supplied></seg> περὶ</seg>
                    <seg n="9" type="line" facs="z160r1_09"><seg type="word"><supplied reason="lost"
                                >τ</supplied>ὸ</seg> Α σημεῖον. πάντες οἱ <expan>κύκλοι</expan> ἐν
                            <seg type="word">τ<supplied reason="lost">ῶι</supplied></seg></seg>
                    <seg n="10" type="line" facs="z160r1_10"><seg type="word"><supplied
                                reason="lost">κυ</supplied><unclear>λ</unclear><supplied
                                reason="lost">ί</supplied>νδρ<supplied reason="lost">ωι</supplied></seg>
                        <seg type="word"><unclear>αὐ</unclear>τοῦ</seg> μένοντες <seg
                            type="unclearword">πᾶ</seg></seg>
                    <seg n="11" type="line" facs="z160r1_11"><seg type="wordend"
                            >σ<unclear>ι</unclear></seg> τοῖς <seg type="word">ἐ<supplied
                                reason="lost">ν</supplied></seg>
                        <seg type="word"><supplied reason="lost">τ</supplied>ῶ<supplied
                                reason="lost">ι</supplied></seg>
                        <seg type="word">κώ<supplied reason="lost">ν</supplied>ωι</seg> κύκλοις</seg>
                    <seg n="12" type="line" facs="z160r1_12"><supplied reason="lost">
                            <expan>καὶ</expan>
                        </supplied>
                        <supplied reason="lost">πᾶσι</supplied>
                        <seg type="word">ἐ<supplied reason="lost">ν</supplied></seg>
                        <supplied reason="lost">τῶι</supplied>
                        <seg type="word"><supplied reason="lost"
                                >τ</supplied><unclear>μ</unclear><supplied reason="lost"
                                >ή</supplied><unclear>μ</unclear>ατι</seg> τῆς <seg
                            type="expandedword">
                            <choice>
                                <abbr>σφ</abbr>
                                <expan>σφαί</expan>
                            </choice>
                        </seg></seg>
                    <seg n="13" type="line" facs="z160r1_13">
                        <seg type="wordend">ρας</seg>
                        <supplied reason="lost">μετενεχθεῖσι</supplied>
                        <unclear>τοῦ</unclear>
                        <seg type="word">ζυγ<unclear>ο</unclear>ῦ</seg>
                    </seg>
                    <seg n="14" type="line" facs="z160r1_14">
                        <supplied reason="lost">καὶ</supplied>
                        <supplied reason="lost">τεθεῖσι</supplied>
                        <seg type="word"><supplied reason="lost">οὕ</supplied>τως</seg>, <seg
                            type="word">ὥ<unclear>στ</unclear>ε</seg>
                        <seg type="word">
                            <choice>
                                <abbr><supplied reason="lost">κ</supplied>έντρ</abbr>
                                <expan><supplied reason="lost">κ</supplied>έντρον</expan>
                            </choice>
                        </seg>
                    </seg>
                    <seg n="15" type="line" facs="z160r1_15"><supplied reason="lost">τοῦ</supplied>
                        <supplied reason="lost">βάρους</supplied>
                        <seg type="word"><supplied reason="lost">ἐστὶ</supplied>ν</seg> τὸ Θ <seg
                            type="word">ἰσόρροπο<unclear>ς</unclear></seg>
                    </seg>
                    <seg n="16" type="line" facs="z160r1_16">ἄρα <gap extent="15"/>
                        <unclear>ειαν</unclear> καὶ</seg>
                </seg>
                <seg n="157v2" type="folio">
                    <seg n="1" type="line" facs="z157v2_01">ὁ κύλινδρος αὐτοῦ μένων <w part="I"
                        >συ</w></seg>
                    <seg n="2" type="line" facs="z157v2_02"><w part="F">ναμφοτέροις</w> τῶι
                            <unclear>τε</unclear> κώνωι</seg>
                    <seg n="3" type="line" facs="z157v2_03">καὶ τῶι τμήματι τῆς <seg type="word">
                            <choice>
                                <abbr>σφαίρ</abbr>
                                <expan>σφαίρ<supplied reason="lost">ας</supplied></expan>
                            </choice>
                        </seg>
                    </seg>
                    <seg n="4" type="line" facs="z157v2_04">μετενηνεγμένοις καὶ <seg type="word">
                            <choice>
                                <abbr>κ<supplied reason="lost">ει</supplied><unclear>μέν</unclear></abbr>
                                <expan>κ<supplied reason="lost"
                                        >ει</supplied><unclear>μέν</unclear><supplied reason="lost"
                                        >οις</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="5" type="line" facs="z157v2_05">τοῦ <seg type="word"
                            >ζ<unclear>υγ</unclear>οῦ</seg> κατὰ τὸ Θ. τεμνέσθω</seg>
                    <seg n="6" type="line" facs="z157v2_06"><seg type="word"
                        ><unclear>δ</unclear>ὲ</seg> ἡ ΑΓ κατὰ τὰ ΦΧ σημεῖα <expan>οὕτως</expan>,</seg>
                    <seg n="7" type="line" facs="z157v2_07">ὥστε τὴν μὲν ΑΧ <supplied reason="lost">
                            <expan>εἶναι</expan>
                        </supplied> ἴσην τῆι ΧΗ,</seg>
                    <seg n="8" type="line" facs="z157v2_08"><seg type="word"
                        ><unclear>τ</unclear>ὴν</seg> δὲ ΑΦ τρίτον <seg type="word"
                            >μ<unclear>έ</unclear>ρος</seg> τῆς</seg>
                    <seg n="9" type="line" facs="z157v2_09">ΑΗ· ἔσται δὴ <seg type="word"
                                >το<supplied reason="lost">ῦ</supplied></seg> μὲν κυλίνδρου</seg>
                    <seg n="10" type="line" facs="z157v2_10">κέντρον τοῦ βάρους τὸ Χ
                        <expan>διὰ</expan> τὸ <w part="I">διχο</w></seg>
                    <seg n="11" type="line" facs="z157v2_11"><w part="F">τομίαν</w> εἶναι τὸ Χ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> ἄξονος.</seg>
                    <seg n="12" type="line" facs="z157v2_12">ἐπεὶ οὖν ἰσορροπεῖ περὶ τὸ <supplied
                            reason="lost">Α</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">ση</supplied>
                        </seg></seg>
                    <seg n="13" type="line" facs="z157v2_13"><seg type="wordend"
                            ><unclear>μ</unclear>εῖον</seg> τὰ εἰρημένα <seg type="word"
                                >μεγέθ<unclear>η</unclear></seg>, <choice>
                            <abbr>ἔστ</abbr>
                            <expan>ἔσται</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z157v2_14">ὡς ὁ κύλινδρος πρὸς <seg type="word"
                                >ἀμ<unclear>φότ</unclear>ερα</seg></seg>
                    <seg n="15" type="line" facs="z157v2_15">τόν <seg type="word"
                            >τ<unclear>ε</unclear></seg> κῶνον, τὸ ΕΖ, καὶ τὸ τμῆμα</seg>
                    <seg n="16" type="line" facs="z157v2_16">τῆς <seg type="word"
                            ><unclear>σ</unclear>φαίρας</seg> τὸ Β<unclear>Α</unclear>Δ,
                            <expan>οὕτως</expan> ἡ ΘΑ</seg>
                    <seg n="17" type="line" facs="z157v2_17"><expan>πρὸς</expan> ΑΧ. καὶ ἐπεὶ
                        τριπλασία ἐστὶν</seg>
                    <seg n="18" type="line" facs="z157v2_18"><supplied reason="lost">ἡ</supplied> ΗΑ
                        τῆς ΑΦ, τρίτον μέρος <expan>ἐστὶν</expan></seg>
                    <seg n="19" type="line" facs="z157v2_19">τὸ ὑπὸ <sic>
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τῆς</expan>
                            </choice>
                        </sic> ΓΑ ΑΦ τὸ ὑπὸ ΓΑ ΑΗ, <seg type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>του</expan>
                            </choice>
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τέστιν</expan>
                            </choice>
                        </seg>
                    </seg>
                </seg>
                <seg n="160r2" type="folio">
                    <seg n="1" type="line" facs="z160r2_01">τῶν ὑπὸ ΑΗ ΗΒ. ἔστω δὴ καὶ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z160r2_02">ἀπὸ τῆς ΒΗ τρίτον μέρος τὸ</seg>
                    <seg n="3" type="line" facs="z160r2_03">ὑπὸ ΓΑ ΑΥ. λοιπὸν ἄρα τὸ ἀπὸ</seg>
                    <seg n="4" type="line" facs="z160r2_04">ΑΗ τριπλάσιόν <unclear>
                            <expan>ἐστιν</expan>
                        </unclear> τοῦ ὑπὸ ΑΓ ΨΦ,</seg>
                    <seg n="5" type="line" facs="z160r2_05">τῶ δὲ ἀπὸ ΑΗ ἴσον τὸ ἀπὸ τῆς</seg>
                    <seg n="6" type="line" facs="z160r2_06">ΗΕ. τὸ <expan>ἄρα</expan> ὑπὸ ΑΓ ΥΦ
                        τρίτον <choice>
                            <abbr>μέρ</abbr>
                            <expan>μέρος</expan>
                        </choice>
                        <expan>ἐστὶν</expan></seg>
                    <seg n="7" type="line" facs="z160r2_07">τὸ ἀπὸ ΕΗ, ἴση δὲ ἡ ΓΑ τῆι ΑΘ,</seg>
                    <seg n="8" type="line" facs="z160r2_08">τὸ <expan>ἄρα</expan> ὑπο ΘΑ ΥΦ τρίτον
                        μέρος <expan>ἐστὶ</expan></seg>
                    <seg n="9" type="line" facs="z160r2_09">τοῦ ἀπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice> ΗΕ. καὶ ἐπεί <expan>ἐστιν</expan> ὡς τὸ <seg type="clearword"
                        >ἀ</seg></seg>
                    <seg n="10" type="line" facs="z160r2_10"><seg type="wordend">πὸ</seg> τῆς ΚΗ
                            <expan>πρὸς</expan> τὸ ἀπὸ Η<unclear>Ε</unclear>, οὕτως</seg>
                    <seg n="11" type="line" facs="z160r2_11">ὁ κύκλος ὁ <seg type="word"
                                >π<unclear>ερὶ</unclear></seg>
                        <seg type="word">δ<unclear>ιά</unclear>μετρον</seg> τὴν</seg>
                    <seg n="12" type="line" facs="z160r2_12">ΚΑ <expan>πρὸς</expan> τὸν κύκλον τὸν
                        περὶ <seg type="expandedword">
                            <unclear>
                                <expan>διά</expan>
                            </unclear>
                        </seg></seg>
                    <seg n="13" type="line" facs="z160r2_13"><seg type="wordend">μετρον</seg> τὴν
                        ΖΕ, <seg type="word">ὡ<unclear>ς</unclear></seg> δὲ ὁ <seg type="word"
                                >κύ<unclear>κ</unclear>λ<supplied reason="lost">ος</supplied></seg>
                    </seg>
                    <seg n="14" type="line" facs="z160r2_14"><expan>πρὸς</expan>
                        <seg type="word">τὸ<supplied reason="lost">ν</supplied></seg>
                        <seg type="word">
                            <choice>
                                <abbr>κύκλ</abbr>
                                <expan>κύκλ<supplied reason="lost">ον</supplied></expan>
                            </choice>
                        </seg>, οὕτως ὁ <seg type="unclearword">κύλ<unclear>ι</unclear>ν</seg></seg>
                    <seg n="15" type="line" facs="z160r2_15"><seg type="wordend">δρος</seg>,
                            <supplied reason="lost">οὗ</supplied>
                        <supplied reason="lost">
                            <expan>ἐστιν</expan>
                        </supplied>
                        <seg type="word"><supplied reason="lost">β</supplied>ά<supplied
                                reason="lost">σ</supplied>ις</seg> ὁ <seg type="word"
                            >περ<unclear>ὶ</unclear></seg>
                        <choice>
                            <abbr>μετρ</abbr>
                            <expan>διάμετρον</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z160r2_16">τὴν ΚΛ <seg type="word">
                            <choice>
                                <abbr>κύ<unclear>κ</unclear>λ</abbr>
                                <expan>κύ<unclear>κ</unclear>λος</expan>
                            </choice>
                        </seg>, <expan>πρὸς</expan> τὸν <choice>
                            <abbr>κύλινδρο</abbr>
                            <expan>κύλινδρον</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z160r2_17"><seg type="word">
                            <supplied reason="lost">ο</supplied>
                            <unclear>ὗ</unclear>
                        </seg> βάσις ὁ περὶ <choice>
                            <abbr>μετρό</abbr>
                            <expan>διάμετρόν</expan>
                        </choice>
                        <seg type="word">
                            <choice>
                                <abbr>ἐστ<unclear>ι</unclear></abbr>
                                <expan>ἐστ<unclear>ι</unclear>ν</expan>
                            </choice>
                        </seg>.</seg>
                </seg>
