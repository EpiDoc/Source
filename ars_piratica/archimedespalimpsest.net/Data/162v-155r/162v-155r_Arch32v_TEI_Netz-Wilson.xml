    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="162v1" type="folio">
                    <seg n="1" type="line" facs="z162v1_01"><supplied reason="lost">ΑΒ</supplied>
                        τετραγώνου μείζονα λόγον <seg type="word">ἔ<unclear>χ</unclear>ει</seg></seg>
                    <seg n="2" type="line" facs="z162v1_02"><supplied reason="lost">ἢ</supplied>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>οῦ</unclear>
                        </seg> αὐτοῦ <seg type="word">
                            <unclear>λ</unclear>
                            <supplied reason="lost">ό</supplied>
                            <unclear>γ</unclear>
                            <supplied reason="lost">ου</supplied>
                        </seg>.</seg>
                </seg>
                <seg n="162v1" type="folio">
                    <seg n="2" type="line" facs="z162v1_02">
                        <seg type="word">ἀπολε<unclear>λ</unclear><supplied reason="lost"
                            >άφθω</supplied>
                        </seg>
                    </seg>
                    <seg n="3" type="line" facs="z162v1_03"><supplied reason="lost">ἀφ’</supplied>
                        <seg type="word">
                            <unclear>ἑ</unclear>κάστας</seg> τᾶν τῶι <seg type="word">
                                ἴσ<unclear>ω</unclear><supplied reason="lost">ι</supplied>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr>ἀλλαλᾶ</abbr>
                                <expan><supplied reason="lost"
                                >ἀλλ</supplied><unclear>α</unclear>λᾶν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="4" type="line" facs="z162v1_04"><seg type="word">
                            <supplied reason="lost">ὑπερεχ</supplied><unclear>ο</unclear>υσᾶν</seg>
                        ἴσαι τᾶι ὑπεροχᾶι·</seg>
                    <seg n="5" type="line" facs="z162v1_05"><seg type="word">ὃ<unclear>ν</unclear>
                        </seg>
                        <supplied reason="lost">δὴ</supplied>
                        <seg type="word">
                            <unclear>λ</unclear>όγον</seg> ἔχει τὸ ἀπὸ τᾶς <unclear>Α</unclear>Β
                            <seg type="suppliedword">
                            <unclear>πο</unclear>
                        </seg></seg>
                    <seg n="6" type="line" facs="z162v1_06"><seg type="wordend">
                            <supplied reason="lost">τὶ</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">συνα</supplied>μφότερα</seg> τό τε ὑπὸ <seg
                            type="word">τ<unclear>ᾶν</unclear>
                        </seg> ΑΒ</seg>
                    <seg n="7" type="line" facs="z162v1_07"><supplied reason="lost">ΦΒ</supplied>
                        <seg type="word">
                            <supplied reason="lost">περιεχόμ</supplied><unclear>εν</unclear>ον</seg>
                        καὶ τὸ τρίτον <seg type="clearword">μέ</seg></seg>
                    <seg n="8" type="line" facs="z162v1_08"><seg type="wordend">ρος</seg>
                        <supplied reason="lost">τοῦ</supplied> ἀπὸ τᾶς ΑΦ <seg type="word">
                                τετραγώνο<supplied reason="lost">υ,</supplied>
                        </seg></seg>
                    <seg n="9" type="line" facs="z162v1_09">τοῦτον ἔχει τὸν λόγον τό τε ἀπὸ <seg
                            type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">ᾶς</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="10" type="line" facs="z162v1_10">ΟΔ τετράγωνον ποτί τε τὸ <seg
                            type="suppliedword">περιε</seg></seg>
                    <seg n="11" type="line" facs="z162v1_11"><seg type="wordend">χ<supplied
                                reason="lost">ό</supplied>μενον</seg> ὑπὸ τᾶν ΟΔ
                        Δ<unclear>Χ</unclear> καὶ τὸ</seg>
                    <seg n="12" type="line" facs="z162v1_12">τρίτον μέρος τοῦ ἀπὸ <supplied
                            reason="lost">τᾶς</supplied> ΧΟ <seg type="clearword">τε</seg></seg>
                    <seg n="13" type="line" facs="z162v1_13"><seg type="wordend">τραγώνου</seg> καὶ
                        τὸ <seg type="word">τετράγω<unclear>ν</unclear><supplied reason="lost"
                            >ον</supplied>
                        </seg>
                        <seg type="word">
                            <unclear>τ</unclear>ὸ</seg>
                        <seg type="clearword">ἀ</seg></seg>
                    <seg n="14" type="line" facs="z162v1_14"><seg type="wordend">πὸ</seg> τᾶς
                            <unclear>Π</unclear>Ζ ποτὶ τὸ <seg type="word">
                                περ<unclear>ιεχό</unclear>μεν<supplied reason="lost"
                                >ο</supplied><unclear>ν</unclear>
                        </seg></seg>
                    <seg n="15" type="line" facs="z162v1_15">ὑπὸ τᾶν ΠΖ ΨΖ καὶ τὸ τρίτον <seg
                            type="clearword">μέ</seg></seg>
                    <seg n="16" type="line" facs="z162v1_16"><seg type="wordend">ρος</seg>
                        <seg type="word">
                            <supplied reason="lost">το</supplied>ῦ</seg> ἀπὸ τᾶς
                        <unclear>Ψ</unclear>Π <seg type="word">
                            <choice>
                                <abbr>τετραγών</abbr>
                                <expan>τετραγών<supplied reason="lost">ου</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="17" type="line" facs="z162v1_17">καὶ τὰ ἀπὸ τᾶν ἀλλᾶν <choice>
                            <abbr>τετράγων</abbr>
                            <expan>τετράγωνα</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z162v1_18">ποτὶ τὰ ὁμοίως λαμβανόμενα</seg>
                </seg>
                <seg n="155r1" type="folio">
                    <seg n="1" type="line" facs="z155r1_01">
                        <supplied reason="lost">χωρία·</supplied>
                        <supplied reason="lost">καὶ</supplied>
                        <supplied reason="lost">τὰ</supplied>
                        <supplied reason="lost">πάντα</supplied>
                        <supplied reason="lost">δὴ</supplied>
                        <supplied reason="lost">τὰ</supplied>
                        <supplied reason="lost">ἀπὸ</supplied>
                    </seg>
                    <seg n="2" type="line" facs="z155r1_02">πασᾶν τᾶν ΟΔ ΠΖ ΡΟΣ ΚΤ ΜΥΞ</seg>
                    <seg n="3" type="line" facs="z155r1_03">ποτί τε πάντα περιεχομέναν <seg
                            type="clearword">ὑ</seg></seg>
                    <seg n="4" type="line" facs="z155r1_04"><seg type="wordend">πό</seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ε</seg> τᾶς ΝΞ καὶ τᾶς <seg
                            type="word">
                            <unclear>π</unclear>ά<unclear>σ</unclear>αις</seg> ταῖς</seg>
                    <seg n="5" type="line" facs="z155r1_05">εἰρημέναις <seg type="word">γρ<supplied
                                reason="lost">α</supplied>μμαῖς</seg> καὶ τὰ</seg>
                    <seg n="6" type="line" facs="z155r1_06"><seg type="word">
                            <unclear>τ</unclear>ρ<unclear>ιτ</unclear>αμό<supplied reason="lost"
                            >ρια</supplied>
                        </seg> τῶν τετραγώνων <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z155r1_07"><seg type="word">
                            <supplied reason="lost">ἀ</supplied>πὸ</seg> τᾶν ΟΧ ΠΨ ΡΩ ΣϠ ΤϞ ΥΝ τὸν</seg>
                    <seg n="8" type="line" facs="z155r1_08">αὐτὸν ἑξοῦντι λόγον, ὃν τὰ ἀπὸ <seg
                            type="word">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">ᾶς</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="9" type="line" facs="z155r1_09"><unclear>ΑΒ</unclear>
                        <seg type="word">
                            <supplied reason="lost">τετ</supplied>ρά<supplied reason="lost"
                                >γ</supplied>ω<supplied reason="lost">νον</supplied>
                        </seg> ποτὶ τὰ <seg type="unclearword">συναμ</seg></seg>
                    <seg n="10" type="line" facs="z155r1_10"><seg type="wordend"
                            >φό<unclear>τ</unclear>ερα</seg> τό <unclear>τε</unclear> ὑπὸ τᾶν ΑΒ ΦΒ
                            <seg type="clearword">πε</seg></seg>
                    <seg n="11" type="line" facs="z155r1_11"><seg type="wordend">ριεχόμενον</seg>
                        <seg type="word">
                            <unclear>κ</unclear>αὶ</seg> τὸ <seg type="word">
                            <supplied reason="lost">τρί</supplied>τον</seg> μέρος</seg>
                    <seg n="12" type="line" facs="z155r1_12">τοῦ ἀπὸ Φ<supplied reason="lost"
                        >Α</supplied> τετραγώνου. εἰ οὖν κα</seg>
                    <seg n="13" type="line" facs="z155r1_13">δειχθῆι τό τε <seg type="word"
                                >περι<supplied reason="lost">ε</supplied>χόμενον</seg> ὑπό</seg>
                    <seg n="14" type="line" facs="z155r1_14">τε τᾶς ΝΞ <expan>καὶ</expan> τᾶς ἴσας
                        πάσαις <choice>
                            <abbr>τς</abbr>
                            <expan>ταῖς</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z155r1_15">ΟΔ ΠΖ ΡΘ <unclear>Σ</unclear>Κ
