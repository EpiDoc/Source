    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="57v1" type="folio">
                    <seg n="1" type="line" facs="z57v1_01">μεγέθη ἐν τόποις ὁποιοισοῦν, ἢ τὰ</seg>
                    <seg n="2" type="line" facs="z57v1_02">πάντα ἤ τινα αὐτῶν, <supplied
                            reason="lost">καὶ</supplied>
                        <seg type="word"><supplied reason="lost">τ</supplied>ὰ</seg>
                        <w part="I">ὕστε</w></seg>
                    <seg n="3" type="line" facs="z57v1_03"><w part="F">ρα</w> μεγέθη πρὸς τὰ <seg
                            type="word">ὁμό<supplied reason="lost">λ</supplied>ογα</seg> ἐν</seg>
                    <seg n="4" type="line" facs="z57v1_04">τοῖς αὐτοῖς λόγοις ἦ, πάντα τὰ</seg>
                    <seg n="5" type="line" facs="z57v1_05">πρῶτα μεγέθη πρὸς πάντα τὰ</seg>
                    <seg n="6" type="line" facs="z57v1_06">λεγόμενα τὸν αὐτὸν ἕξει λόγον,</seg>
                    <seg n="7" type="line" facs="z57v1_07">ὃν ἔχει πάντα τὰ ὕστερον πρὸς</seg>
                    <seg n="8" type="line" facs="z57v1_08">πάντα τὰ λεγόμενα.</seg>
                </seg>
                <seg n="57v1" type="folio">
                    <seg n="8" type="line" facs="z57v1_08">ἔστω</seg>
                    <seg n="9" type="line" facs="z57v1_09">τμῆμα τὸ ΑΒΓ περιεχόμενον</seg>
                    <seg n="10" type="line" facs="z57v1_10">ὑπὸ εὐθείας τῆς ΑΓ καὶ <w part="I"
                        >ὀρθο</w></seg>
                    <seg n="11" type="line" facs="z57v1_11"><w part="F">γωνίου</w> κώνου τομῆς τῆς
                        ΑΒΓ,</seg>
                    <seg n="12" type="line" facs="z57v1_12">καὶ τετμήσθω δίχα ἡ ΑΓ
                        <expan>κατὰ</expan> τὸ Δ,</seg>
                    <seg n="13" type="line" facs="z57v1_13"><expan>καὶ</expan> παρὰ τὴν διάμετρον
                        ἤχθω ἡ</seg>
                    <seg n="14" type="line" facs="z57v1_14">ΔΒΕ, καὶ ἐπεζεύχθωσαν αἱ ΑΒ</seg>
                    <seg n="15" type="line" facs="z57v1_15">ΒΓ.</seg>
                </seg>
                <seg n="57v1" type="folio">
                    <seg n="15" type="line" facs="z57v1_15">λέγω <expan>ὅτι</expan> ἐπίτριτόν ἐστιν
                        τὸ ΑΒΓ</seg>
                    <seg n="16" type="line" facs="z57v1_16">τμῆμα τοῦ ΑΒΓ τριγώνου.</seg>
                </seg>
                <seg n="57v1" type="folio">
                    <seg n="16" type="line" facs="z57v1_16">
                        <w part="I">ἤχθω</w>
                    </seg>
                    <seg n="17" type="line" facs="z57v1_17"><w part="F">σαν</w> ἀπὸ τῶν ΑΓ σημείων ἡ
                        μὲν</seg>
                    <seg n="18" type="line" facs="z57v1_18">ΑΖ παρὰ τὴν ΔΒΕ, ἡ δὲ ΓΖ <w part="I"
                            >ἐπιψαύ</w></seg>
                    <seg n="19" type="line" facs="z57v1_19"><w part="F">ουσα</w> τῆς τομῆς, καὶ <seg
                            n="ἐκβεβλήσθω" type="suppliedword">ἐκβεβλήσ</seg></seg>
                    <seg n="20" type="line" facs="z57v1_20">
                        <seg type="wordend">
                            <supplied reason="lost">
                                <w part="F">θω</w>
                            </supplied>
                        </seg>
                        <seg type="word">
                            <unclear>ἡ</unclear>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">ΓΒ</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">καί</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τετμήσθω</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τὴν</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">ΑΖ</supplied>
                        </seg>
                    </seg>
                </seg>
                <seg n="64r1" type="folio">
                    <seg n="1" type="line" facs="z64r1_01"><seg type="word"><supplied reason="lost"
                                >κα</supplied>τὰ</seg> τὸ Κ, <seg type="word">
                            <unclear>καὶ</unclear>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">κ</supplied>
                            <unclear>εί</unclear>
                            <supplied reason="lost">σθω</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τῆι</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">ΓΚ</supplied>
                        </seg> ἴση</seg>
                    <seg n="2" type="line" facs="z64r1_02">ἡ ΚΘ, καὶ νοείσθω ζυγὸς ὁ ΓΘ <choice>
                            <expan>καὶ</expan>
                            <abbr>κ</abbr>
                        </choice></seg>
                    <seg n="3" type="line" facs="z64r1_03">μέσον αὐτοῦ τὸ Κ καὶ τὸ ΕΔ <w part="I"
                        >πα</w></seg>
                    <seg n="4" type="line" facs="z64r1_04"><w part="F">ράλληλος</w> τυχοῦσα ἡ
                    ΜΞ.</seg>
                </seg>
                <seg n="64r1" type="folio">
                    <seg n="4" type="line" facs="z64r1_04">ἐπεὶ οὖν</seg>
                    <seg n="5" type="line" facs="z64r1_05">παραβολή ἐστιν ἡ ΓΒΑ, καὶ <w part="I"
                        >ἐφά</w></seg>
                    <seg n="6" type="line" facs="z64r1_06"><w part="F">πτεσθαι</w> ἡ ΓΕ, καὶ
                        τεταγμένως ἡ</seg>
                    <seg n="7" type="line" facs="z64r1_07">ΓΔ, ἴση <expan>ἐστὶν</expan> ἡ ΕΒ τῆι ΒΔ·
                        τοῦτο γὰρ ἐν</seg>
                    <seg n="8" type="line" facs="z64r1_08">τοῖς στοιχείοις δείκνυται· διὰ δὴ</seg>
                    <seg n="9" type="line" facs="z64r1_09">τοῦτο, <seg type="word"><supplied
                                reason="lost">κ</supplied>αὶ</seg>
                        <choice>
                            <abbr>δι</abbr>
                            <expan>διότι</expan>
                        </choice> παράλληλοί <choice>
                            <abbr>εἰσι</abbr>
                            <expan>εἰσιν</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z64r1_10">αἱ ΖΑ ΜΞ τῆι ΕΔ, ἴση ἐστὶν καὶ ἡ</seg>
                    <seg n="11" type="line" facs="z64r1_11">μὲν ΜΝ τῆι ΝΞ, ἡ δὲ ΖΚ τῆι ΚΑ.</seg>
                    <seg n="12" type="line" facs="z64r1_12">καὶ ἐπεί ἐστιν ὡς ἡ ΓΔ <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice> ΑΞ, <seg type="unclearword">οὕ</seg></seg>
                    <seg n="13" type="line" facs="z64r1_13"><seg type="wordend"
                            >τω<unclear>ς</unclear></seg> ἡ ΜΞ πρὸς ΞΘ, τοῦτο <expan>γὰρ</expan> τὸ</seg>
                    <seg n="14" type="line" facs="z64r1_14">λῆμμα <seg type="word"
                            ><unclear>δ</unclear>είκνυται</seg>, ὡς δὲ ἡ ΓΑ <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z64r1_15">ΑΞ, οὕτως ἡ ΓΚ πρὸς ΚΝ, καὶ ἴση</seg>
                    <seg n="16" type="line" facs="z64r1_16">ἐστὶν ἡ ΓΚ τῆι ΚΘ, ὡς ἄρα ἡ ΘΚ</seg>
                    <seg n="17" type="line" facs="z64r1_17">πρὸς <seg type="word"
                            >Κ<unclear>Ν</unclear></seg>, οὕτως ἡ ΜΞ πρὸς ΞΟ.</seg>
                </seg>
                <seg n="57v2" type="folio">
                    <seg n="1" type="line" facs="z57v2_01">καὶ ἔστι τὸ Ν σημεῖον κέντρον</seg>
                    <seg n="2" type="line" facs="z57v2_02">τοῦ βάρους τῆς ΜΞ εὐθείας, <w part="I"
                        >ἐ</w></seg>
                    <seg n="3" type="line" facs="z57v2_03"><w part="F">πείπερ</w> ἴση ἐστὶν ἡ <seg
                            type="word">Μ<unclear>Ν</unclear></seg> τῆι ΝΞ,</seg>
                    <seg n="4" type="line" facs="z57v2_04">ἐὰν ἄρα τῆι ΞΟ ἴσην θῶμεν τὸ</seg>
                    <seg n="5" type="line" facs="z57v2_05">ΝΤ κέντρον τοῦ βάρους αὐτῆς τὸ</seg>
                    <seg n="6" type="line" facs="z57v2_06">Θ, ὅπως ἴση ἡ ΤΘ τῆι ΘΗ, <w part="I"
                        >ἰσορ</w></seg>
                    <seg n="7" type="line" facs="z57v2_07"><w part="F">ροπήσει</w> ἡ ΤΗ τῆι ΜΞ αὐτοῦ
                            <w part="I">με</w></seg>
                    <seg n="8" type="line" facs="z57v2_08"><w part="F">νούσηι</w> διὰ τὸ
                        ἀντιπεπονθότως</seg>
                    <seg n="9" type="line" facs="z57v2_09">τετμῆσθαι τὴν ΘΝ τοῖς ΤΗ ΜΞ</seg>
                    <seg n="10" type="line" facs="z57v2_10">βάρεσιν, καὶ ὡς τὴν ΘΚ πρὸς ΚΝ,</seg>
                    <seg n="11" type="line" facs="z57v2_11">οὕτως τὴν ΜΞ πρὸς τὴν ΗΤ· <w part="I"
                        >ὥσ</w></seg>
                    <seg n="12" type="line" facs="z57v2_12"><w part="F">τε</w> τοῦ ἐξ ἀμφοτέρων
                        βάρους <seg type="expandedword">
                            <choice>
                                <abbr>κέ</abbr>
                                <expan>κέν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="13" type="line" facs="z57v2_13"><seg type="wordend">τρον</seg> ἐστὶν τοῦ
                        βάρους τὸ Κ. <w part="I">ὁμοί</w></seg>
                    <seg n="14" type="line" facs="z57v2_14"><w part="F">ως</w> δὲ καὶ ὅσαι <seg
                            type="word"><unclear>ἐ</unclear>ὰν</seg> ἀχθῶσιν</seg>
                    <seg n="15" type="line" facs="z57v2_15">ἐν τῶι ΖΑΓ τριγώνωι <w part="I"
                        >παράλλη</w></seg>
                    <seg n="16" type="line" facs="z57v2_16"><w part="F">λοι</w> τῆι ΗΔ,
                        ἰσορροπήσουσιν <w part="I">αὐ</w></seg>
                    <seg n="17" type="line" facs="z57v2_17"><w part="F">τοῦ</w> μενούσαις ταῖς <w
                            part="I">ἀπολαμβα</w></seg>
                    <seg n="18" type="line" facs="z57v2_18"><w part="F">νομέναις</w> ἀπ᾽ αὐτῶν ὑπὸ
                            <seg type="word">τῆ<supplied reason="lost">ς</supplied></seg></seg>
                    <seg n="19" type="line" facs="z57v2_19">τομῆς μετενεχθείσαις περὶ</seg>
                    <seg n="20" type="line" facs="z57v2_20"><seg type="word">
                            <supplied reason="lost">κ</supplied>
                            <unclear>έ</unclear>
                            <supplied reason="lost">ν</supplied>
                            <unclear>τρον</unclear>
                        </seg>
                        <seg type="word"><unclear>τ</unclear>οῦ</seg>
                        <seg type="word">βά<supplied reason="lost">ρους</supplied></seg>
                        <seg type="word">
                            <supplied reason="lost">τὸ</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">Θ</supplied>
                        </seg>. καὶ</seg>
                    <seg n="21" type="line" facs="z57v2_21">
                        <seg type="word">
                            <supplied reason="lost">ἔσται</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τοῦ</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">ἐκ</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">πάντων</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τῶ</supplied>
                            <unclear>ν</unclear>
                        </seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">β</supplied>
                            <unclear>α</unclear>
                        </seg>
                    </seg>
                </seg>
                <seg n="64r2" type="folio">
                    <seg n="1" type="line" facs="z64r2_01"><seg type="wordend">ρῶν</seg> κέντρων τοῦ
                        βάρους τὸ Κ.</seg>
                    <seg n="2" type="line" facs="z64r2_02">καὶ ἐπεὶ ἐκ μὲν <seg type="word"
                                >τῶ<supplied reason="lost">ν</supplied></seg> ἐν τῶι <seg
                            type="word">ΖΑ<supplied reason="lost">Γ</supplied></seg></seg>
                    <seg n="3" type="line" facs="z64r2_03">τριγώνωι <seg type="word"
                                >συνέστηκε<unclear>ν</unclear></seg>, <seg type="word"
                            >ἐ<unclear>κ</unclear></seg> δὲ τῶν</seg>
                    <seg n="4" type="line" facs="z64r2_04">ἐν τῆι τομῆι ὁμοίως τῆι ΟΞ <w part="I"
                            >λαμ</w></seg>
                    <seg n="5" type="line" facs="z64r2_05"><w part="F">βανομένων</w> συνέστηκε τὸ
                        ΑΒΓ</seg>
                    <seg n="6" type="line" facs="z64r2_06">τμῆμα, <seg type="word"
                                >ἰσορροπήσ<unclear>ει</unclear></seg> ἄρα τὸ</seg>
                    <seg n="7" type="line" facs="z64r2_07">ΖΑΓ τρίγωνον αὐτοῦ <choice>
                            <abbr>μενόντω</abbr>
                            <expan>μενόντων</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z64r2_08">τμήματι τῆς τομῆς <w part="I">τεθέν</w></seg>
                    <seg n="9" type="line" facs="z64r2_09"><w part="F">τι</w> περὶ κέντρον τοῦ
                        βάρους <pb/> τὸ Θ</seg>
                    <seg n="10" type="line" facs="z64r2_10">κατὰ τὸ Κ σημεῖον, ὥστε τοὺς <w part="I"
                            >ἐ</w></seg>
                    <seg n="11" type="line" facs="z64r2_11"><w part="F">ξ</w> ἀμφοτέρων κέντρον
                        εἶναι</seg>
                    <seg n="12" type="line" facs="z64r2_12">τοῦ βάρους τὸ Κ. τετμήσθω δὲ</seg>
                    <seg n="13" type="line" facs="z64r2_13">ἡ ΓΚ τῶι Χ, ὡς τετραπλασίαν</seg>
                    <seg n="14" type="line" facs="z64r2_14">εἶναι τὴν ΓΚ τῆς ΚΧ· ἔσται ἄρα</seg>
                    <seg n="15" type="line" facs="z64r2_15">τὸ Χ σημεῖον κέντρον τοῦ <choice>
                            <abbr>βάρς</abbr>
                            <expan>βάρους</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z64r2_16">τὸ ΑΖΓ τρίγωνον· τοῦτο <expan>γὰρ</expan>
                        <seg type="word">
                            <choice>
                                <abbr>δείκν<supplied reason="lost">υ</supplied>τ</abbr>
                                <expan>δείκν<supplied reason="lost">υ</supplied>ται</expan>
                            </choice>
                        </seg></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
