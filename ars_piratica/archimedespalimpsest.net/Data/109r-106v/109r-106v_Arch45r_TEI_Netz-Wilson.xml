    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="109r1" type="folio">
                    <seg n="1" type="line" facs="z109r1_01">καὶ τὸ τρίτον μέρος τοῦ ἀπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τᾶς</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z109r1_02">ΗΑ, τὸ δὲ ΝΠ χωρίον ποτὶ τὸ Π <seg
                            type="suppliedword">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τοῦ</expan>
                            </choice>
                        </seg></seg>
                    <seg n="3" type="line" facs="z109r1_03"><seg type="wordend">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ον</unclear>
                        </seg> ἔχει τὸν λόγον, ὃν τὰ <seg type="suppliedword">συ<supplied
                                reason="lost">ν</supplied>αμ</seg></seg>
                    <seg n="4" type="line" facs="z109r1_04"><seg type="wordend">φ<supplied
                                reason="lost">ότ</supplied>ερα</seg> τό τε <seg type="word"
                                >ὑπ<supplied reason="lost">ὸ</supplied>
                        </seg> τᾶν ΗΘΑ <expan>καὶ</expan></seg>
                    <seg n="5" type="line" facs="z109r1_05"><unclear>τὸ</unclear>
                        <seg type="word">
                            <unclear>τ</unclear>ρίτον</seg> μέρος τοῦ ἀπὸ τᾶς ΗΑ</seg>
                    <seg n="6" type="line" facs="z109r1_06"><seg type="word">
                                τ<unclear>ε</unclear>τ<unclear>ρ</unclear>αγώνου</seg> ποτὶ <choice>
                            <abbr>συναμφότερ</abbr>
                            <expan>συναμφότερον</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z109r1_07"><seg type="word">τ<supplied
                                reason="lost">ό</supplied>
                        </seg> τε ὑπὸ τᾶν Η<unclear>Α</unclear>Θ καὶ <seg type="word"> τ<supplied
                                reason="lost">ὸ</supplied>
                        </seg>
                        <seg type="word">
                            <choice>
                                <abbr>τρίτο</abbr>
                                <expan>τρίτ<unclear>ο</unclear>ν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="8" type="line" facs="z109r1_08"><seg type="word">
                            <supplied reason="lost">μ</supplied>έρος</seg> τοῦ ἀπὸ τᾶς ΗΑ <seg
                            type="suppliedword">τετρα</seg></seg>
                    <seg n="9" type="line" facs="z109r1_09"><seg type="wordend"><supplied
                                reason="lost">γ</supplied>ώνου</seg>, ἕξει καὶ τὸ Ξ ποτὶ τὸ Π</seg>
                    <seg n="10" type="line" facs="z109r1_10"><seg type="word">τ<supplied
                                reason="lost">ο</supplied>ῦτον</seg> τὸν λόγον, ὃν ἔχει <seg
                            type="suppliedword">συναμ</seg></seg>
                    <seg n="11" type="line" facs="z109r1_11"><seg type="wordend">φ<supplied
                                reason="lost">ό</supplied>τερα</seg> τό τε ὑπὸ τᾶν ΘΗ ΗΑ</seg>
                    <seg n="12" type="line" facs="z109r1_12">καὶ δύο τριταμόρια τοῦ ἀπὸ</seg>
                    <seg n="13" type="line" facs="z109r1_13"><seg type="word">τ<supplied
                                reason="lost">ᾶ</supplied>ς</seg> ΗΑ ποτὶ συναμφότερον</seg>
                    <seg n="14" type="line" facs="z109r1_14">τό <seg type="word">
                            <supplied reason="lost">τ</supplied>ε</seg> ὑπὸ τᾶν ΘΗ ΗΑ καὶ δύο <seg
                            type="suppliedword">τρι</seg></seg>
                    <seg n="15" type="line" facs="z109r1_15"><seg type="wordend">τ<supplied
                                reason="lost">α</supplied>μόρια</seg> τοῦ ἀπὸ τᾶς ΗΑ ποτὶ</seg>
                    <seg n="16" type="line" facs="z109r1_16"><seg type="word">σ<supplied
                                reason="lost">υν</supplied>αμφότε<supplied reason="lost"
                            >ρ</supplied>ον </seg> τό τε ὑπὸ τᾶν</seg>
                    <seg n="17" type="line" facs="z109r1_17">ΘΗ ΗΑ καὶ τὸ <seg type="word">
                            <supplied reason="lost">τρί</supplied>τον</seg> μέρος τοῦ</seg>
                    <seg n="18" type="line" facs="z109r1_18">ἀπὸ τᾶς ΗΑ. τὰ δὲ <seg
                            type="suppliedword">συναμφ<supplied reason="lost"
                    >ό</supplied></seg></seg>
                </seg>
                <seg n="106v1" type="folio">
                    <seg n="1" type="line" facs="z106v1_01"><seg type="wordend">τερα</seg> τό τε ὑπὸ
                        τᾶν ΘΗ ΗΑ καὶ</seg>
                    <seg n="2" type="line" facs="z106v1_02">δύο τριταμόρια τοῦ ἀπὸ τᾶς</seg>
                    <seg n="3" type="line" facs="z106v1_03">ΗΑ ποτὶ συναμφότερα τό τε</seg>
                    <seg n="4" type="line" facs="z106v1_04">ὑπὸ τᾶν ΘΗΑ καὶ τὸ τρίτον <seg
                            type="clearword">μέ</seg></seg>
                    <seg n="5" type="line" facs="z106v1_05"><seg type="wordend">ρος</seg> τοῦ ἀπὸ
                        τᾶς ΗΑ <seg type="clearword">τετραγώ</seg></seg>
                    <seg n="6" type="line" facs="z106v1_06"><seg type="wordend">νου</seg> τοῦτον
                        ἔχει τὸν λόγον, ὃν</seg>
                    <seg n="7" type="line" facs="z106v1_07">ἔχει συναμφότερα ἅ τε ΘΗ
                        <expan>καὶ</expan></seg>
                    <seg n="8" type="line" facs="z106v1_08">δύο τριταμόρια τᾶς ΗΑ ποτὶ</seg>
                    <seg n="9" type="line" facs="z106v1_09">συναμφοτέραν τὰν ΘΗ καὶ</seg>
                    <seg n="10" type="line" facs="z106v1_10">τὸ τρίτον μέρος τᾶς ΗΑ· <choice>
                            <abbr>δῆλ</abbr>
                            <expan>δῆλον</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z106v1_11">οὖν, <expan>ὅτι</expan> καὶ τὸ Ξ χωρίον
                        ποτὶ τὸ</seg>
                    <seg n="12" type="line" facs="z106v1_12">Ν χωρίον τοῦτον ἔχει τὸν <choice>
                            <abbr>λόγο</abbr>
                            <expan>λόγον</expan>
                        </choice>,</seg>
                    <seg n="13" type="line" facs="z106v1_13">ὃν συναμφότερα ἅ τε ΘΗ
                        <expan>καὶ</expan></seg>
                    <seg n="14" type="line" facs="z106v1_14">δύο <seg type="word">τριταμόρι<supplied
                                reason="lost">α</supplied>
                        </seg> τᾶς ΗΑ ποτὶ</seg>
                    <seg n="15" type="line" facs="z106v1_15">συναμφότερον τὰν ΘΗ καὶ τὸ</seg>
                    <seg n="16" type="line" facs="z106v1_16">τρίτον μέρος τᾶς ΗΑ.</seg>
                    <seg n="17" type="line" facs="z106v1_17"><choice>
                            <abbr>ἈΡΧΙΜΉΔ</abbr>
                            <expan>ἈΡΧΙΜΉΔους</expan>
                        </choice>
                        <expan>ΠΕΡῚ</expan> ἙΛΙΚΩΝ</seg>
                </seg>
                <seg n="109r2" type="folio">
                    <seg n="1" type="line" facs="z109r2_01">ΑΡΧΙΜΗΔΟΥΣ <expan>ΠΕΡΙ</expan>
                        <choice>
                            <abbr>Τ</abbr>
                            <expan>ΤΗΣ</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z109r2_02">ΣΦΑΙΡΑΣ <expan>ΚΑΙ</expan>
                        <seg type="expandedword">
                            <choice>
                                <abbr>ΚΥΛΙ</abbr>
                                <expan>ΚΥΛΙΝΔΡΟΥ</expan>
                            </choice>
                        </seg>
                    </seg>
                    <seg n="3" type="line" facs="z109r2_03">
                        <seg type="wordend">ΡΟΥ</seg>
                    </seg>
                </seg>
                <seg n="109r2" type="folio">
                    <seg n="4" type="line" facs="z109r2_04">Ἀρχιμήδης Δοσιθέωι χαίρειν</seg>
                </seg>
                <seg n="109r2" type="folio">
                    <seg n="5" type="line" facs="z109r2_05">πρότερον μὲν ἀπέσταλκά σοι</seg>
                    <seg n="6" type="line" facs="z109r2_06">τῶν ὑφ’ ἡμῶν <w part="I">τεθεωρημέ</w></seg>
                    <seg n="7" type="line" facs="z109r2_07"><w part="F">νων</w> γράψας μετὰ <choice>
                            <abbr>αποδειξε</abbr>
                            <expan>ἀποδείξεως</expan>
                        </choice>,</seg>
                    <seg n="8" type="line" facs="z109r2_08"><expan>ὅτι</expan> πᾶν τμῆμα τὸ <w
                            part="I">περιεχόμε</w></seg>
                    <seg n="9" type="line" facs="z109r2_09"><w part="F">νον</w> ὑπό τε εὐθείας καὶ
                            <seg type="suppliedword"><supplied reason="lost">ὀρ</supplied>θογω</seg></seg>
                    <seg n="10" type="line" facs="z109r2_10"><seg type="wordend">νίου</seg> κώνου
                        τομῆς ἐπίτριτόν <expan>ἐστι</expan></seg>
                </seg>
                <seg n="106v2" type="folio">
                    <seg n="1" type="line" facs="z106v2_01">τριγώνου τοῦ βάσιν</seg>
                    <seg n="2" type="line" facs="z106v2_02">τὴν αὐτὴν ἔχοντος τῶι τμήματι καὶ <choice>
                            <abbr>υψ</abbr>
                            <expan>ὕψος</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z106v2_03">ἴσον· ὕστερον δὲ ἡμῖν <w part="I"
                        >ὑποπε</w></seg>
                    <seg n="4" type="line" facs="z106v2_04"><w part="F">σόντων</w> θεωρημάτων <seg
                            type="word">ἀξίω<supplied reason="lost">ν</supplied></seg></seg>
                    <seg n="5" type="line" facs="z106v2_05">λόγου πεπραγματεύμεθα περὶ</seg>
                    <seg n="6" type="line" facs="z106v2_06">τὰς ἀποδείξεις αὐτῶν. ἔστι δὲ</seg>
                    <seg n="7" type="line" facs="z106v2_07">τάδε· πρῶτον μέν, <expan>ὅτι</expan>
                        πάσης</seg>
                    <seg n="8" type="line" facs="z106v2_08">σφαίρας ἡ ἐπιφάνεια <w part="I"
                        >τετραπλα</w></seg>
                    <seg n="9" type="line" facs="z106v2_09"><w part="F">σία</w> ἐστὶν τοῦ μεγίστου
                        κύκλου τῶν</seg>
                    <seg n="10" type="line" facs="z106v2_10">ἐν αὐτῆι· ἔπειτα δέ, <expan>ὅτι</expan>
                        παντὸς</seg>
                    <seg n="11" type="line" facs="z106v2_11">τμήματος σφαίρας τῆι <w part="I"
                        >ἐπιφα</w></seg>
                    <seg n="12" type="line" facs="z106v2_12"><w part="F">νείαι</w> ἴσος
                        <expan>ἐστὶ</expan> κύκλος, οὗ ἡ ἐκ τοῦ </seg>
                    <seg n="13" type="line" facs="z106v2_13">κέντρου ἴση <expan>ἐστὶ</expan> τῆι
                        εὐθείαι τῆι <w part="I">ἀ</w>
                    </seg>
                    <seg n="14" type="line" facs="z106v2_14"><w part="F">πὸ</w> τῆς κορυφῆς τοῦ
                        τμήματος</seg>
                    <seg n="15" type="line" facs="z106v2_15">ἀγομένηι ἐπὶ τὴν περιφέρειαν <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z106v2_16">κύκλου, ὅς <expan>ἐστι</expan> βάσις
                        τοῦ τμήματος·</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
