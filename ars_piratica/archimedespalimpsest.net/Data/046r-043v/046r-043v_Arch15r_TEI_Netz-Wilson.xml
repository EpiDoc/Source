    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="46r1" type="folio">
                    <seg n="1" type="line" facs="z46r1_01">τὸ <unclear>ΙΣ</unclear> ΑΩ γωνίαι ἴσαι
                        εἰσίν, καὶ ΒΕ</seg>
                    <seg n="2" type="line" facs="z46r1_02">εὐθεῖα τῆς ΒΤ ἴση καὶ ἡ ΣΡ τῆι </seg>
                    <seg n="3" type="line" facs="z46r1_03">ΠΡΤ <expan>καὶ</expan> ἡ ΗϠ τῆι ΦΗ καὶ ἡ
                        ϠΟ τῆι</seg>
                    <seg n="4" type="line" facs="z46r1_04">Η<supplied reason="lost">Ι.</supplied>
                        ἐπεὶ δὲ διπλῆ ἐστιν ἡ ΦΥ τῆς</seg>
                    <seg n="5" type="line" facs="z46r1_05">ΥΙ, φανερὸν, <expan>ὅτι</expan> ἡ ΗϠ
                        μείζων <expan>ἐστὶν</expan></seg>
                    <seg n="6" type="line" facs="z46r1_06">ἢ διπλῆ τῆς ϠΘ. ἔστω οὖν ἡ ΗΛ</seg>
                    <seg n="7" type="line" facs="z46r1_07"><sic>Λ</sic> τῆς ΛΘ <sic>Λ</sic>
                        διπλασίων· πάλιν</seg>
                    <seg n="8" type="line" facs="z46r1_08">δ’ ἐκ τούτων δῆλον, ὡς οὐ μενεῖ</seg>
                    <seg n="9" type="line" facs="z46r1_09">τὸ τμῆμα, ἀλλ’ ἐπικλιθήσεται</seg>
                    <seg n="10" type="line" facs="z46r1_10">ἐπὶ τὰ αὐτὰ τῶι Α. ἐπεὶ δὴ καθ’ <choice>
                            <abbr>ε</abbr>
                            <expan>ἓν</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z46r1_11">σημεῖον ὑποτέθη τὸ τμῆμα <w part="I"
                        >ἅ</w></seg>
                    <seg n="12" type="line" facs="z46r1_12"><w part="F">πτεσθαι</w> τοῦ ὑγροῦ,
                        δῆλον, ὅτι <w part="I">κα</w></seg>
                    <seg n="13" type="line" facs="z46r1_13"><w part="F">τὰ</w> πλείονα τόπον ἡ βάσις
                        ὑπὸ</seg>
                    <seg n="14" type="line" facs="z46r1_14">τοῦ ὑγροῦ καταληφθήσεται.</seg>
                </seg>
                <seg n="46r1" type="folio">
                    <seg n="15" type="line" facs="z46r1_15">ἈΡΧΙΜΉΔΟΥΣ</seg>
                    <seg n="16" type="line" facs="z46r1_16">ὈΧΟΥΜΈΝΩΝ</seg>
                    <seg n="17" type="line">Β</seg>
                </seg>
                <seg n="46r1" type="folio">
                    <num>Β</num>
                </seg>
                <seg n="46r1" type="folio">
                    <figure n="2.10.5">
                        <figDesc xml:lang="eng">Figure 2.10.5</figDesc>
                    </figure>
                </seg>
                <seg n="46r2" type="folio">
                    <seg n="1" type="line" facs="z46r2_01">ΑΡΧΙΜΗΔΟΥΣ ΠΕΡΙ ΤΩΝ <w part="I">ΜΗ</w></seg>
                    <seg n="2" type="line" facs="z46r2_02"><w part="F">ΧΑΝΙΚΩΝ</w> ΘΕΩΡΗΜΑΤΩΝ
                            <expan>ΠΡΟΣ</expan></seg>
                    <seg n="3" type="line" facs="z46r2_03">ΕΡΑΤΟΣΘΕΝΗΝ. ΕΦΟΔΟΣ</seg>
                </seg>
                <seg n="46r2" type="folio">
                    <seg n="4" type="line" facs="z46r2_04">Ἀρχιμήδης Ἐρατοσθένει εὖ <seg
                            type="expandedword">
                            <abbr>πρά</abbr>
                        </seg></seg>
                    <seg n="5" type="line" facs="z46r2_05"><seg type="wordend">
                            <choice>
                                <abbr>ειν</abbr>
                                <expan>ττειν</expan>
                            </choice>
                        </seg>.</seg>
                </seg>
                <seg n="46r2" type="folio">
                    <seg n="5" type="line" facs="z46r2_05">ἀπέστειλά σοι πρότερον</seg>
                    <seg n="6" type="line" facs="z46r2_06">τῶν <seg type="word">εὑρημένω<supplied
                                reason="lost">ν</supplied></seg> θεωρημάτων</seg>
                    <seg n="7" type="line" facs="z46r2_07">ἀναγράψας <seg type="word"
                            >αὐτ<unclear>ὰ</unclear>ς</seg> τὰς <w part="I">προτά</w></seg>
                    <seg n="8" type="line" facs="z46r2_08"><w part="F">σεις</w> φάμενος εὑρίσκειν
                        ταύτας</seg>
                    <seg n="9" type="line" facs="z46r2_09">τὰς ἀποδείξεις, ἃς οὐκ εἶπον</seg>
                    <seg n="10" type="line" facs="z46r2_10">ἐπὶ τοῦ παρόντος· ἦσαν δὲ τῶν <w
                            part="I">ἀ</w></seg>
                    <seg n="11" type="line" facs="z46r2_11"><w part="F">πεσταλμένων</w> θεωρημάτων</seg>
                    <seg n="12" type="line" facs="z46r2_12">αἱ προτάσεις αἵδε· τοῦ μὲν</seg>
                    <seg n="13" type="line" facs="z46r2_13">
                        <num value="1">Α</num>
                    </seg>
                    <seg n="13" type="line" facs="z46r2_13">πρώτου· ἐὰν εἰς πρίσμα ὀρθὸν <w part="I"
                            >πα</w></seg>
                    <seg n="14" type="line" facs="z46r2_14"><w part="F">ραλληλόγραμμον</w> ἔχον
                        βάσιν</seg>
                    <seg n="15" type="line" facs="z46r2_15">κύλινδρος ἐγγραφῆι τὰς μὲν</seg>
                    <seg n="16" type="line" facs="z46r2_16">βάσεις ἔχων ἐν τοῖς <w part="I"
                        >ἀπεναν</w></seg>
                    <seg n="17" type="line" facs="z46r2_17"><w part="F">τίον</w> παραλληλογράμμοις,
                        τὰς</seg>
                    <seg n="18" type="line" facs="z46r2_18">δὲ πλευρὰς ἐπὶ τῶν λοιπῶν <seg
                            type="unclearword">τ<unclear>εσ</unclear></seg></seg>
                    <seg n="19" type="line" facs="z46r2_19"><seg type="wordend">σάρων</seg> ἐπιπέδων
                            <w part="I">ἐφαπτομέ</w>
                    </seg>
                </seg>
                <seg n="43v2" type="folio">
                    <seg n="1" type="line" facs="z43v2_01"><w part="F">νας</w>, διὰ δὲ τοῦ κέντρου
                        τοῦ κύκλου,</seg>
                    <seg n="2" type="line" facs="z43v2_02">ὅ ἐστι βάσις τοῦ κυλίνδρου, καὶ <w
                            part="I">μι</w></seg>
                    <seg n="3" type="line" facs="z43v2_03"><w part="F">ᾶς</w> πλευρᾶς τοῦ τετραγώνου
                        τοῦ</seg>
                    <seg n="4" type="line" facs="z43v2_04">ἐν τῶι κατεναντίον <seg type="word"
                                >ἐπιπέδω<supplied reason="lost">ι</supplied></seg></seg>
                    <seg n="5" type="line" facs="z43v2_05">ἀχθῆι ἐπίπεδον, τὸ ἀχθὲν <w part="I"
                        >ἐπί</w></seg>
                    <seg n="6" type="line" facs="z43v2_06"><w part="F">πεδον</w> ἀποτέμη τμῆμα ἀπὸ</seg>
                    <seg n="7" type="line" facs="z43v2_07">τοῦ κυλίνδρου, ὅ ἐστι <w part="I"
                            >περιεχόμε</w></seg>
                    <seg n="8" type="line" facs="z43v2_08"><w part="F">νον</w> ὑπὸ δύο ἐπιπέδων καὶ
                            <w part="I">ἐπι</w></seg>
                    <seg n="9" type="line" facs="z43v2_09"><w part="F">φανείας</w> κυλίνδρου, ἑνὸς
                        μὲν</seg>
                    <seg n="10" type="line" facs="z43v2_10">τοῦ ἀχθέντος, ἑτέρου δὲ ἐν ὧι ἡ</seg>
                    <seg n="11" type="line" facs="z43v2_11"><seg type="word"
                            ><unclear>β</unclear>άσις</seg> ἐστὶν τοῦ κυλίνδρου τῆς <w part="I"
                        >με</w></seg>
                    <seg n="12" type="line" facs="z43v2_12"><w part="F">ταξὺ</w> τῶν εἰρημένων <w
                            part="I">ἐπιπέ</w></seg>
                    <seg n="13" type="line" facs="z43v2_13"><w part="F">δων</w> τὸ ἀποτμηθὲν ἀπὸ τοῦ</seg>
                    <seg n="14" type="line" facs="z43v2_14">κυλίνδρου τμῆμα ἕκτον μέρος</seg>
                    <seg n="15" type="line" facs="z43v2_15"><seg type="word">ἐστ<supplied
                                reason="lost">ὶ</supplied></seg> τοῦ ὅλου πρίσματος.</seg>
                    <seg n="16" type="line" facs="z43v2_16">
                        <num>Β</num>
                    </seg>
                    <seg n="16" type="line" facs="z43v2_16">τοῦ δὲ ἑτέρου <seg type="word"
                                >θε<unclear>ω</unclear>ρήματος</seg> ἡ <choice>
                            <abbr>πρό</abbr>
                            <expan>πρότασις</expan>
                        </choice></seg>
                    <seg n="17" type="line" facs="z43v2_17">ἥδε <expan>ὅτι</expan> ἐὰν εἰς κύβον
                            <seg type="word">κύλινδρο<unclear>ς</unclear></seg></seg>
                </seg>
            </ab>
