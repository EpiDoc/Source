    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="108v1" type="folio">
                    <seg n="1" type="line" facs="z108v1_01">προσκείσθωσαν αἱ ΗΑ, ΖΓ· ὅλαι
                        <expan>ἄρα</expan></seg>
                    <seg n="2" type="line" facs="z108v1_02">αἱ ΑΔ, ΔΓ μείζους <expan>εἰσὶν</expan>
                        τῶν ΑΗ, ΗΖ, ΖΓ.</seg>
                    <seg n="3" type="line" facs="z108v1_03">καὶ ἐπεὶ αἱ <seg type="word"
                            >Α<unclear>Ε</unclear></seg>, ΕΒ, ΕΓ πλευραί εἰσιν</seg>
                    <seg n="4" type="line" facs="z108v1_04">τοῦ κώνου, ἴσαι εἰσὶν διὰ τὸ <w part="I"
                            >ἰσο</w></seg>
                    <seg n="5" type="line" facs="z108v1_05"><w part="F">σκελῆ</w> εἶναι τὸν κῶνον·
                        ὁμοίως</seg>
                    <seg n="6" type="line" facs="z108v1_06">δὲ καὶ κάθετοί <expan>εἰσιν</expan> ὡς
                        ἐδείχθη ἐν τῶ</seg>
                    <seg n="7" type="line" facs="z108v1_07"><choice>
                            <abbr>λήματι</abbr>
                            <expan>λήμματι</expan>
                        </choice>, τὰ δὲ ὑπὸ τῶν καθέτων</seg>
                    <seg n="8" type="line" facs="z108v1_08">καὶ τῶν βάσεων διπλάσια ἐστιν</seg>
                    <seg n="9" type="line" facs="z108v1_09">τῶν τριγώνων· μείζονα ἐστὶν τὰ</seg>
                    <seg n="10" type="line" facs="z108v1_10">ΑΕΔ, ΔΕΓ τρίγωνα τῶν ΑΗΕ, ΗΕΖ,</seg>
                    <seg n="11" type="line" facs="z108v1_11">ΖΕΓ τριγώνων <expan>εἰσὶν</expan>
                        <expan>γὰρ</expan> αἱ μὲν ΑΗ, ΗΖ,</seg>
                    <seg n="12" type="line" facs="z108v1_12">ΖΓ ἐλάσσους τῶν ΓΑ, ΔΑ, τὰ δὲ ὕψη</seg>
                    <seg n="13" type="line" facs="z108v1_13">αὐτῶν ἴσα φανερὸν γὰρ
                        <expan>ὅτι</expan> ἡ ἀπὸ</seg>
                    <seg n="14" type="line" facs="z108v1_14">τῆς κορυφῆς τοῦ ὀρθοῦ κώνου <w part="I"
                            >ἐ</w></seg>
                    <seg n="15" type="line" facs="z108v1_15"><w part="F">πὶ</w> τὴν ἐπαφὴν τῆς
                        βάσεως <w part="I">ἐ</w></seg>
                    <seg n="16" type="line" facs="z108v1_16"><w part="F">ζευγνυμένη</w> κάθετός
                        ἐστιν ἐπὶ τὴν <w part="I">ἐ</w></seg>
                    <seg n="17" type="line" facs="z108v1_17"><w part="F">φαπτομένην</w> ὧι δὴ
                        μείζονά <choice>
                            <abbr>ἐστι</abbr>
                            <expan>ἐστιν</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z108v1_18"><seg type="word"
                        >τ<unclear>ὰ</unclear></seg> ΑΕΔ, ΔΕΓ τρίγωνα τῶν ΑΗΕ, ΗΕΖ,</seg>
                </seg>
                <seg n="107r1" type="folio">
                    <seg n="1" type="line" facs="z107r1_01">ΖΕΓ τριγώνων τῶν.</seg>
                    <seg n="2" type="line" facs="z107r1_02">ἤτοι <choice>
                            <abbr>ἔλατόν</abbr>
                            <expan>ἔλαττόν</expan>
                        </choice>
                        <expan>ἐστιν</expan>
                        <gap reason="archetype"/>
                        <w part="I">τμημά</w></seg>
                    <seg n="3" type="line" facs="z107r1_03"><w part="F">των</w> ἢ οὐ. <gap
                            reason="archetype"/></seg>
                </seg>
                <seg n="107r1" type="folio">
                    <seg n="3" type="line" facs="z107r1_03">
                        <expan>εἰσιν</expan>
                        <w part="I">ἐπιφά</w>
                    </seg>
                    <seg n="4" type="line" facs="z107r1_04"><w part="F">νειαι</w> σύνθετοι, ἥ τε τῆς
                        πυραμίδος</seg>
                    <seg n="5" type="line" facs="z107r1_05">τῆς ἐπὶ βάσεως τοῦ ΗΑΓΖ <w part="I"
                        >τρα</w></seg>
                    <seg n="6" type="line" facs="z107r1_06"><w part="F">πεζείου</w> κορυφὴν ἔχουσα
                        τὸ Ε καὶ</seg>
                    <seg n="7" type="line" facs="z107r1_07">ἡ κωνικὴ ἐπιφάνεια ἡ μεταξὺ <choice>
                            <abbr>τ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z107r1_08">ΑΕΓ μετὰ τοῦ ΑΒΓ τμήματος, καὶ</seg>
                    <seg n="9" type="line" facs="z107r1_09">πέρας ἔχουσι τὴν αὐτὴν <w part="I"
                            >περίμε</w></seg>
                    <seg n="10" type="line" facs="z107r1_10"><w part="F">τρον</w> τοῦ ΑΕΓ τριγώνου,
                        δῆλον ὡς</seg>
                    <seg n="11" type="line" facs="z107r1_11">ἡ ἐπιφάνεια τῆς πυραμίδος <w part="I"
                            >χω</w></seg>
                    <seg n="12" type="line" facs="z107r1_12"><w part="F">ρὶς</w> τοῦ ΑΕΓ τριγώνου
                        μείζων <expan>
                            <unclear>ἐστὶν</unclear>
                        </expan></seg>
                    <seg n="13" type="line" facs="z107r1_13">τῆς κωνικῆς ἐπιφανείας μετὰ</seg>
                    <seg n="14" type="line" facs="z107r1_14">τοῦ τμήματος τοῦ ΑΒΓ. κοινὸν <w
                            part="I">ἀφηι</w></seg>
                    <seg n="15" type="line" facs="z107r1_15"><w part="F">ρήσθω</w> τὸ ΑΒΓ τμῆμα·
                        λοιπὰ ἄρα</seg>
                    <seg n="16" type="line" facs="z107r1_16">τὰ τρίγωνα τὰ ΑΗΕ, ΗΕΖ, ΖΕΓ <seg
                            type="word">με<unclear>τ</unclear>ὰ</seg></seg>
                </seg>
                <seg n="108v2" type="folio">
                    <seg n="1" type="line" facs="z108v2_01">τῶν ΑΗΒΚ, ΒΖΓΑ <choice>
                            <abbr>περιλειμμάτω</abbr>
                            <expan>περιλειμμάτων</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z108v2_02">μείζονά ἐστιν τῆς κωνικῆς <w part="I"
                            >ἐπι</w></seg>
                    <seg n="3" type="line" facs="z108v2_03"><w part="F">φανείας</w> τῆς μεταξὺ τῶν
                        ΑΕ, ΕΓ.</seg>
                    <seg n="4" type="line" facs="z108v2_04">τῶν δὲ ΑΗΒΚ, ΒΖΓΛ <w part="I"
                        >περιλειμμά</w></seg>
                    <seg n="5" type="line" facs="z108v2_05"><w part="F">των</w> οὐκ ἔλασσόν ἐστιν τὸ
                        Θ <choice>
                            <abbr>χωρί</abbr>
                            <expan>χωρίον</expan>
                        </choice>·</seg>
                    <seg n="6" type="line" facs="z108v2_06">πολλῶ ἄρα τὰ ΑΗΕ, ΗΕΖ, ΖΕΓ <w part="I"
                            >τρί</w></seg>
                    <seg n="7" type="line" facs="z108v2_07"><w part="F">γωνα</w> μετὰ τοῦ Θ μείζονα
                        ἔσται <choice>
                            <abbr>τ</abbr>
                            <expan>τῆς</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z108v2_08">κωνικῆς ἐπιφανείας τῆς <w part="I"
                        >μετα</w></seg>
                    <seg n="9" type="line" facs="z108v2_09"><w part="F">ξὺ</w> τῶν ΑΕ ΕΓ. ἀλλὰ τὰ
                        ΑΗΕ, ΗΕΖ <w part="I">τρί</w></seg>
                    <seg n="10" type="line" facs="z108v2_10"><w part="F">γωνα</w> μετὰ τοῦ Θ ἐστὶν
                        τὰ ΑΕΔ, ΔΕΓ</seg>
                    <seg n="11" type="line" facs="z108v2_11">τρίγωνα· τὰ <expan>ἄρα</expan> ΑΕΔ, ΔΕΓ
                            <w part="I">τρίγω</w></seg>
                    <seg n="12" type="line" facs="z108v2_12"><w part="F">να</w> μείζονα ἔσται τῆς <choice>
                            <abbr>εἰρημέν</abbr>
                            <expan>εἰρημένης</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z108v2_13">κωνικῆς ἐπιφανείας. </seg>
                </seg>
                <seg n="108v2" type="folio">
                    <seg n="13" type="line" facs="z108v2_13">ἔστω δὴ</seg>
                    <seg n="14" type="line" facs="z108v2_14">τὸ Θ ἔλασσον τῶν <choice>
                            <abbr>περιλειμμάτω</abbr>
                            <expan>περιλειμμάτων</expan>
                        </choice>.</seg>
                    <seg n="15" type="line" facs="z108v2_15">ἀεὶ δὴ περιγράφοντες πολύγωνα</seg>
                    <seg n="16" type="line" facs="z108v2_16">περὶ τὰ τμήματα ὁμοίως δίχα</seg>
                    <seg n="17" type="line" facs="z108v2_17">τεμνομένων τῶν <w part="I"
                        >περιλειπομέ</w></seg>
                    <seg n="18" type="line" facs="z108v2_18"><w part="F">νων</w> περιφερειῶν καὶ <choice>
                            <abbr>ἀγομένω</abbr>
                            <expan>ἀγομένων</expan>
                        </choice></seg>
                </seg>
                <seg n="107r2" type="folio">
                    <seg n="1" type="line" facs="z107r2_01"><seg type="word"><supplied reason="lost"
                                >ἐ</supplied>φαπτομένων</seg> λείψομέν τινα <w part="I">ἀ</w></seg>
                    <seg n="2" type="line" facs="z107r2_02"><w part="F">πολείμματα</w>, ἃ <choice>
                            <abbr>ἔστ</abbr>
                            <expan>ἔσται</expan>
                        </choice> ἐλάσσονα τοῦ</seg>
                    <seg n="3" type="line" facs="z107r2_03">Θ χωρίου. λελείφθω καὶ ἔστω τὰ</seg>
                    <seg n="4" type="line" facs="z107r2_04">ΑΜΚ, <seg type="word"
                            >ΚΝ<unclear>Β</unclear></seg>, <seg type="word"
                        ><unclear>Β</unclear>ΞΛ</seg>, ΛΟΓ ἐλάσσονα</seg>
                    <seg n="5" type="line" facs="z107r2_05">ὄντα τοῦ Θ χωρίου, καὶ ἐπεζεύχθω</seg>
                    <seg n="6" type="line" facs="z107r2_06">ἐπὶ τὸ Ε. Πάλιν δὴ φανερὸν
                        <expan>ὅτι</expan></seg>
                    <seg n="7" type="line" facs="z107r2_07">τὰ ΑΗΕ, ΗΕΖ, ΖΕΓ τρίγωνα τῶν</seg>
                    <seg n="8" type="line" facs="z107r2_08">ΑΕΜ, ΜΕΝ, ΝΕΞ, ΞΕΟ, ΟΕΓ <w part="I"
                            >τριγώ</w></seg>
                    <seg n="9" type="line" facs="z107r2_09"><w part="F">νων</w> ἔσται μείζονα αἵ τε
                        γὰρ</seg>
                    <seg n="10" type="line" facs="z107r2_10">βάσεις τῶν βάσεών <expan>εἰσι</expan>
                        μείζους</seg>
                    <seg n="11" type="line" facs="z107r2_11">καὶ τὸ ὕψος ἴσον. ἔτι δὲ πάλιν <w
                            part="I">ὁμοί</w></seg>
                    <seg n="12" type="line" facs="z107r2_12"><w part="F">ως</w> μείζονα ἔχει
                        ἐπιφάνειαν ἡ <w part="I">πυ</w></seg>
                    <seg n="13" type="line" facs="z107r2_13"><w part="F">ραμὶς</w> ἡ βάσιν μὲν
                        ἔχουσα τὸ ΛΜΝ</seg>
                    <seg n="14" type="line" facs="z107r2_14">ΞΟΓ πολύγωνον, κορυφὴν δὲ τὸ Ε, <choice>
                            <abbr>χ</abbr>
                            <expan>χωρὶς</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z107r2_15">τοῦ ΑΕΓ τριγώνου, τῆς κωνικῆς</seg>
                    <seg n="16" type="line" facs="z107r2_16">ἐπιφανείας τῆς μεταξὺ τῶν</seg>
                    <seg n="17" type="line" facs="z107r2_17">ΑΕΓ μετὰ τοῦ ΑΒΓ τμήματος.</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
