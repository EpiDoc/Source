    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="14r1" type="folio">
                    <seg n="1" type="line" facs="z14r1_01"><seg type="wordend"
                                >φο<unclear>τέ</unclear>ρ<supplied reason="lost">ου</supplied></seg>
                        τᾶς ΑΒΔ <expan>καὶ</expan> Δ τᾶς ΓΒ </seg>
                    <seg n="2" type="line" facs="z14r1_02">λόγον ἔχει, ὃν τρία πρὸς δύο, <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice>
                    </seg>
                    <seg n="3" type="line" facs="z14r1_03">δὲ τὰ τρία πέμπτα τᾶς αὐτᾶς </seg>
                    <seg n="4" type="line" facs="z14r1_04">λόγον ἔχει, ὃν πέντε πρὸς δύο· <seg
                            type="clearword">ἐ</seg></seg>
                    <seg n="5" type="line" facs="z14r1_05"><seg type="wordend">δείχθη</seg> δὲ καὶ ὁ
                        Α <expan>πρὸς</expan> ΗΘ λόγον </seg>
                    <seg n="6" type="line" facs="z14r1_06">ἔχοντα, ὃν πέντε πρὸς <seg type="word"
                                >δύ<supplied reason="lost">ο</supplied>·</seg> καὶ </seg>
                    <seg n="7" type="line" facs="z14r1_07">ὅλα ἄρα ἁ ΒΘ πρὸς ὅλαν τὰν </seg>
                    <seg n="8" type="line" facs="z14r1_08">ΖΘ λόγον ἔχει, ὃν πέντε πρὸς τὰ </seg>
                    <seg n="9" type="line" facs="z14r1_09">δύο. εἰ δὲ τοῦτο, πεμπταμόρια <seg
                            type="clearword">ἐ</seg></seg>
                    <seg n="10" type="line" facs="z14r1_10"><seg type="wordend">ναντία</seg> ΖΘ τᾶς
                        ΔΒ· ΟΙ.</seg>
                    <figure n="1">
                        <figDesc xml:lang="eng">Figure 1</figDesc>
                    </figure>
                </seg>
                <seg n="14r1" type="folio">
                    <seg n="11" type="line" facs="z14r1_11">Παντὸς <seg type="word"
                            >τόμο<unclear>υ</unclear></seg> ἀπὸ <choice>
                            <abbr>ορθογωνι</abbr>
                            <expan>ὀρθογωνίου</expan>
                        </choice>
                    </seg>
                    <seg n="12" type="line" facs="z14r1_12">κώνου τομᾶς <choice>
                            <abbr>αφαιρουμεν</abbr>
                            <expan>ἀφαιρουμένου</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z14r1_13">τὸ κέντρον τοῦ βάρεος ἐπὶ τᾶς</seg>
                    <seg n="14" type="line" facs="z14r1_14">εὐθείας ἐστίν, διάμετρός ἐστιν </seg>
                    <seg n="15" type="line" facs="z14r1_15">τοῦ τόμου, τὸν δὲ τρόπον <choice>
                            <abbr>κειμεν</abbr>
                            <expan>κείμενον</expan>
                        </choice>·</seg>
                    <seg n="16" type="line" facs="z14r1_16">διαιρεθείσας τᾶς εὐθείας εἰς </seg>
                    <seg n="17" type="line" facs="z14r1_17">ἴσα πέντε ἐπὶ μέσου πεμπτα</seg>
                </seg>
                <seg n="19v1" type="folio">
                    <seg n="1" type="line" facs="z19v1_01"><seg type="word">μορίο<supplied
                                reason="lost">υ</supplied>,</seg> ὥστε τὸ τμᾶμα <seg type="word"
                                >αὐτο<supplied reason="lost">ῦ</supplied></seg> τὸ </seg>
                    <seg n="2" type="line" facs="z19v1_02">ἐγγύτερον τᾶς ἐλάσσονος <choice>
                            <abbr>βασι</abbr>
                            <expan>βάσιος</expan>
                        </choice>
                    </seg>
                    <seg n="3" type="line" facs="z19v1_03">τοῦ τόμου ποτὶ τὸ λοιπὸν <seg
                            type="clearword">τμᾶ</seg></seg>
                    <seg n="4" type="line" facs="z19v1_04"><seg type="wordend">μα</seg> τὸν αὐτὸν
                        ἔχει λόγον, ὃν <seg type="clearword">ἔ</seg></seg>
                    <seg n="5" type="line" facs="z19v1_05"><seg type="wordend">χει</seg> τὸ στερεὸν
                        τὸ βάσιν μὲν <choice>
                            <abbr>εχ</abbr>
                            <expan>ἔχον</expan>
                        </choice>
                    </seg>
                    <seg n="6" type="line" facs="z19v1_06">τὸ τετράγωνον τὸ ἀπὸ τᾶς <seg
                            type="clearword">μεί</seg></seg>
                    <seg n="7" type="line" facs="z19v1_07"><seg type="wordend">ζονος</seg> τᾶν
                        βάσεων τοῦ <choice>
                            <abbr>τομ</abbr>
                            <expan>τόμου</expan>
                        </choice>, </seg>
                    <seg n="8" type="line" facs="z19v1_08">ὕψος δὲ τὰν ἴσαν συναμφοτέρα </seg>
                    <seg n="9" type="line" facs="z19v1_09">τᾶ τε διπλασία τᾶς <choice>
                            <abbr>ελασσον</abbr>
                            <expan>ἐλάσσονος</expan>
                        </choice>
                    </seg>
                    <seg n="10" type="line" facs="z19v1_10">τῶν βάσιων καὶ τᾶ μείζονι, <seg
                            type="clearword">πο</seg></seg>
                    <seg n="11" type="line" facs="z19v1_11"><seg type="wordend">τὶ</seg> τὸ στερεὸν
                        τὸ βάσιν μὲν <choice>
                            <abbr>εχο</abbr>
                            <expan>ἔχον</expan>
                        </choice>
                    </seg>
                    <seg n="12" type="line" facs="z19v1_12">τὸ τετράγωνον τὸ ἀπὸ τᾶς <seg
                            type="clearword">ἐλάσ</seg></seg>
                    <seg n="13" type="line" facs="z19v1_13"><seg type="wordend">σονος</seg> τᾶν
                        βάσεων τοῦ <choice>
                            <abbr>τομ</abbr>
                            <expan>τόμου</expan>
                        </choice>, </seg>
                    <seg n="14" type="line" facs="z19v1_14">ὕψος δὲ τὰν ἴσαν ἀμφοτέρας </seg>
                    <seg n="15" type="line" facs="z19v1_15">τᾶ τε διπλασία τᾶς μείζονος </seg>
                    <seg n="16" type="line" facs="z19v1_16"><expan>καὶ</expan> τᾶ ἐλάσσονι
                    αὐτᾶν. </seg>
                </seg>
                <seg n="19v1" type="folio">
                    <seg n="16" type="line" facs="z19v1_16">
                        <choice>
                            <abbr>εστωσα</abbr>
                            <expan>ἔστωσαν</expan>
                        </choice>
                    </seg>
                </seg>
                <seg n="14r2" type="folio">
                    <seg n="1" type="line" facs="z14r2_01">ὀρθογωνίου τομᾶ δύο <seg type="word"
                                >ε<unclear>ὐ</unclear>θεῖαι</seg>
                    </seg>
                    <seg n="2" type="line" facs="z14r2_02">αἱ ΑΓ ΔΕ διάμετρος δὲ ἔστω τοῦ Α</seg>
                    <seg n="3" type="line" facs="z14r2_03">ΒΓ τμάματος ἁ ΒΖ· φανερὸν </seg>
                    <seg n="4" type="line" facs="z14r2_04">δὲ ὅτι καὶ τοῦ <seg type="word"
                                >ΑΓΔ<unclear>Ε</unclear></seg> παράλληλοί </seg>
                    <seg n="5" type="line" facs="z14r2_05">ἐντι τᾶ κατὰ <seg type="word">
                            <supplied reason="lost">τ</supplied>
                            <unclear>ὸ</unclear>
                        </seg> Β ἐφαπτομένα </seg>
                    <seg n="6" type="line" facs="z14r2_06">τᾶς τομᾶς· <seg type="word"
                            >κα<unclear>ὶ</unclear></seg>
                        <seg type="word">τ<unclear>ᾶ</unclear><supplied reason="lost"
                        >ς</supplied></seg> ΗΖ εὐθείας </seg>
                    <seg n="7" type="line" facs="z14r2_07">διαιρεθείσας εἰς πέντε ἴσα <choice>
                            <abbr>μεσο</abbr>
                            <expan>μέσον</expan>
                        </choice>
                    </seg>
                    <seg n="8" type="line" facs="z14r2_08">ἔστω πεμπταμόριον ἁ ΘΚ, ἁ δὲ </seg>
                    <seg n="9" type="line" facs="z14r2_09">ΘΙ <seg type="word"
                            >π<unclear>ρ</unclear>ὸς</seg> τὰν ΙΚ τὸν αὐτὸν ἐχέτω </seg>
                    <seg n="10" type="line" facs="z14r2_10">λόγον, ὃν ἔχει τὸ στερεὸν τὸ <choice>
                            <abbr>βασι</abbr>
                            <expan>βάσιν</expan>
                        </choice>
                    </seg>
                    <seg n="11" type="line" facs="z14r2_11">μὲν ἔχον τὸ ἀπὸ τῆς ΛΖ <seg
                            type="clearword">τετρά</seg></seg>
                    <seg n="12" type="line" facs="z14r2_12"><seg type="wordend">γωνον,</seg> ὕψος δὲ
                        τὰν ἴσαν <seg type="clearword">ἀμφο</seg></seg>
                    <seg n="13" type="line" facs="z14r2_13"><seg type="wordend">τέραις</seg> τᾶ τε Β
                        τᾶς ΖΗ καὶ τᾶ </seg>
                    <seg n="14" type="line" facs="z14r2_14">ΑΖ, ποτὶ τὸ στερεὸν τὸ βάσιν <choice>
                            <abbr>εχο</abbr>
                            <expan>ἔχον</expan>
                        </choice>
                    </seg>
                    <seg n="15" type="line" facs="z14r2_15">τὸ ἀπὸ τᾶς ΔΗ τετράγωνον, <seg
                            type="suppliedword">
                            <supplied reason="lost">ὕ</supplied>
                        </seg></seg>
                    <seg n="16" type="line" facs="z14r2_16"><seg type="wordend">ψος</seg> δὲ
                        <sic>τὰ</sic> ἴσαν ἀμφοτέραις τε </seg>
                    <seg n="17" type="line" facs="z14r2_17">Β τᾶς ΑΖ καὶ τᾶ ΔΗ. δεικτέον
                        <expan>ὅτι</expan>
                    </seg>
                    <seg n="18" type="line" facs="z14r2_18">τοῦ ΑΔΕΓ τόμου κέντρον ἐστὶ τοῦ </seg>
                    <seg n="19" type="line" facs="z14r2_19">βάρεος τὸ Ι σαμεῖον.  </seg>
                </seg>
                <seg n="14r2" type="folio">
                    <seg n="19" type="line" facs="z14r2_19">ἔστω δὴ </seg>
                    <seg n="20" type="line" facs="z14r2_20">τᾶ μὲν ΖΒ ἴσα ἁ ΜΝ, τᾶ δὲ ΗΒ</seg>
                </seg>
                <seg n="19v2" type="folio">
                    <seg n="1" type="line" facs="z19v2_01">ἴσα ἁ ΝΘ, καὶ εἰλήφθω τᾶν </seg>
                    <seg n="2" type="line" facs="z19v2_02">μὲν ΜΝΘ μέσα ἀνάλογον ἁ </seg>
                    <seg n="3" type="line" facs="z19v2_03">ΝΞ, τετάρτη δὲ ἀνάλογον ἁ ΤΝ, </seg>
                    <seg n="4" type="line" facs="z19v2_04">καὶ ὡς ἡ ΤΜ <expan>πρὸς</expan> ΤΝ, οὕτως
                        ἁ ΖΘ </seg>
                    <seg n="5" type="line" facs="z19v2_05">πρός τινα ἀπὸ τοῦ Ι, ὅπου ἐὰν </seg>
                    <seg n="6" type="line" facs="z19v2_06">ἔρχηται τὸ ἕτερον σαμεῖον· <seg
                            type="clearword">οὐ</seg></seg>
                    <seg n="7" type="line" facs="z19v2_07"><seg type="wordend">δὲν</seg>
                        <expan>γὰρ</expan>
                        <choice>
                            <abbr>φέρει</abbr>
                            <expan>διαφέρει</expan>
                        </choice> εἴτε καὶ μεταξὺ τῶν</seg>
                    <seg n="8" type="line" facs="z19v2_08">ΗΒ τὴν ΙΡ. καὶ ἐπεὶ ἐν <seg
                            type="clearword">ὀρθογωνί</seg></seg>
                    <seg n="9" type="line" facs="z19v2_09"><seg type="wordend">ου</seg> κωνίου τομᾶ
                        διάμετρός ἐστι </seg>
                    <seg n="10" type="line" facs="z19v2_10">τοῦ τμάματος ΑΖ ΒΗ ΒΖ ἤτοι </seg>
                    <seg n="11" type="line" facs="z19v2_11">ἀρχική ἐστιν τῆς τομῆς ἢ παρὰ </seg>
                    <seg n="12" type="line" facs="z19v2_12">τὴν διάμετρον ἦκται, αἱ δὲ ΑΖ ΔΗ </seg>
                    <seg n="13" type="line" facs="z19v2_13">εἰς αὐτὴν τεταγμένως εἰσίν <seg
                            type="clearword">κα</seg></seg>
                    <seg n="14" type="line" facs="z19v2_14"><seg type="wordend">τηγμένη,</seg>
                        ἐπειδὴ παράλληλοί <seg type="clearword">εἰ</seg></seg>
                    <seg n="15" type="line" facs="z19v2_15"><seg type="wordend">σι</seg> τῆι ἀπὸ τοῦ
                        Β τῆς τομῆς <seg type="clearword">ἐφα</seg></seg>
                    <seg n="16" type="line" facs="z19v2_16">
                        <seg type="wordend">πτομένη.</seg>
                    </seg>
                </seg>
                <seg n="19v2" type="folio">
                    <seg n="16" type="line" facs="z19v2_16">εἰ δὲ τοῦτο, ἔστιν ὡς ἡ ΑΖ</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
