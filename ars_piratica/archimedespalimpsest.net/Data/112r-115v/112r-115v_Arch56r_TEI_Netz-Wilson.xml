    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="112r1" type="folio">
                    <seg n="1" type="line" facs="z112r1_01">τὸ περιεχόμενον ὑπό τε τῆς ΑΕ</seg>
                    <seg n="2" type="line" facs="z112r1_02">καὶ τῆς <seg type="word">ἡμι<supplied
                                reason="lost">σ</supplied>ε<unclear>ί</unclear>ας</seg> τῶν ΚΛ, ΜΝ,
                        ἡ δὲ</seg>
                    <seg n="3" type="line" facs="z112r1_03"><seg type="word"
                        ><unclear>ἐ</unclear>κ</seg> τοῦ κέντρου τοῦ Υ δυνάσθω τὸ <seg
                            type="expandedword"/></seg>
                    <seg n="4" type="line" facs="z112r1_04"><seg type="wordend">
                            <abbr>εχό<supplied reason="lost">μενον</supplied></abbr>
                            <expan>περιεχόμενον</expan>
                        </seg>
                        <supplied reason="lost">ὑπό</supplied>
                        <seg type="word">τ<unclear>ε</unclear></seg> τῆς <seg type="word">Α<supplied
                                reason="lost">Ε</supplied></seg>
                        <expan>καὶ</expan> τῆς <seg type="suppliedword">ἡμι</seg></seg>
                    <seg n="5" type="line" facs="z112r1_05"><seg type="wordend"><supplied
                                reason="lost">σ</supplied>είας</seg> τῆς <seg type="word">Μ<supplied
                                reason="lost">Ν</supplied></seg>. <seg type="word"
                            >δι<unclear>ὰ</unclear></seg> δὴ <seg type="word"><supplied
                                reason="lost">τα</supplied><unclear>ῦ</unclear>τα</seg> ὁ μὲν</seg>
                    <seg n="6" type="line" facs="z112r1_06"><sic>
                            <expan>κύκλος</expan>
                        </sic>
                        <seg type="word"><unclear>κ</unclear>ύ<unclear>κ</unclear>λος</seg>
                        <seg type="word">
                            <supplied reason="lost">ἴ</supplied>
                            <unclear>σος</unclear>
                        </seg>
                        <seg type="word">ἐστὶ<unclear>ν</unclear></seg> τῆι <seg type="word"
                                ><unclear>ἐπ</unclear><supplied reason="lost"
                        >ι</supplied>φανείαι</seg></seg>
                    <seg n="7" type="line" facs="z112r1_07"><seg type="word"
                        ><unclear>τ</unclear>οῦ</seg> κώνου, τῆι <seg type="word"
                            >μεταξ<unclear>ὺ</unclear></seg> τοῦ <seg type="word"
                            ><unclear>κώ</unclear>νου</seg>
                        <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z112r1_08">ΕΖ, ΗΘ, ὁ δὲ Ρ τῆι μεταξὺ τῶν <seg
                            type="word">Η<unclear>Θ</unclear></seg>, ΓΔ,</seg>
                    <seg n="9" type="line" facs="z112r1_09">ὁ δὲ <unclear>Θ</unclear> τῆι μεταξὺ
                            <seg type="word">
                            <unclear>τῶ</unclear>
                            <supplied reason="lost">ν</supplied>
                        </seg> ΔΓ, ΚΛ, καὶ ἔτι <unclear>ὁ</unclear></seg>
                    <seg n="10" type="line" facs="z112r1_10">μὲν Τ <seg type="word"
                            >ἴσ<unclear>ο</unclear>ς</seg>
                        <expan>ἐστὶ</expan>
                        <seg type="word">τῆ<supplied reason="lost">ι</supplied></seg> ἐπιφανείαι τοῦ
                            <seg type="unclearword"><unclear>κ</unclear>ώ</seg></seg>
                    <seg n="11" type="line" facs="z112r1_11"><seg type="wordend">νου</seg>
                        <seg type="word">τῆ<unclear>ι</unclear></seg> μεταξὺ τῶν ΚΛ, ΜΝ, <supplied
                            reason="lost">ὁ</supplied>
                        <seg type="word"><unclear>δ</unclear>ὲ</seg> Υ</seg>
                    <seg n="12" type="line" facs="z112r1_12">τῆι τοῦ ΜΒΝ <seg type="word"><supplied
                                reason="lost">κ</supplied>ώ<supplied reason="lost"
                                >ν</supplied><unclear>ο</unclear>υ</seg> ἐπιφανεία <choice>
                            <abbr>ἴσ</abbr>
                            <expan>ἴσος</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z112r1_13"><expan>ἐστίν</expan>· <seg type="word"
                                >ο<supplied reason="lost">ἱ</supplied></seg>
                        <seg type="word"><unclear>πά</unclear>ν<unclear>τ</unclear>ες</seg>
                        <seg type="word">ἄρ<unclear>α</unclear></seg> κύκλοι ἴσοι
                        <expan>εἰσὶν</expan> τῆι</seg>
                    <seg n="14" type="line" facs="z112r1_14">τοῦ ἐγγεγραμμένου σχήματος <w part="I"
                            >ἐπι</w></seg>
                    <seg n="15" type="line" facs="z112r1_15"><w part="F">φανείαι</w>.
                        <expan>καὶ</expan> φανερὸν <expan>ὅτι</expan> αἱ ἐκ <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z112r1_16">κέντρων <seg type="word"
                            ><unclear>τ</unclear>ῶν</seg> Ο, Π, Ρ, Σ, Τ, Υ κύκλων <w part="I">δύ</w></seg>
                    <seg n="17" type="line" facs="z112r1_17"><w part="F">ναται</w> τὸ περιεχόμενον
                        ὑπό τε <choice>
                            <abbr>τῆ</abbr>
                            <expan>τῆς</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z112r1_18"><supplied reason="lost">ΑΕ καὶ δὶς τῶν</supplied>
                        <seg type="word"><supplied reason="lost">ἡμί</supplied>σεων</seg> τῆς <seg
                            type="word">Ε<unclear>Ζ</unclear></seg>, ΗΘ,</seg>
                </seg>
                <seg n="115v1" type="folio">
                    <seg n="1" type="line" facs="z115v1_01">ΓΔ, <seg type="word"
                            >Κ<unclear>Λ</unclear></seg>, ΜΝ, αἳ <seg type="word"
                            >ὅλο<unclear>ι</unclear></seg>
                        <expan>εἰσὶν</expan> αἱ ΕΖ, ΗΘ, ΚΛ, <seg type="word"
                        >Μ<unclear>Ν</unclear></seg></seg>
                    <seg n="2" type="line" facs="z115v1_02">αἱ <expan>ἄρα</expan> ἐκ τῶν κέντρων τῶν
                        Ο, Π, Ρ, Σ, Τ, Υ</seg>
                    <seg n="3" type="line" facs="z115v1_03"><seg type="word"
                                ><unclear>κ</unclear>ύ<supplied reason="lost">κλ</supplied>ων</seg>
                        δύνανται <seg type="word">τ<supplied reason="lost">ὸ</supplied></seg>
                        <seg type="suppliedword">πε<unclear>ρ</unclear><supplied reason="lost"
                            >ι</supplied>εχόμε</seg></seg>
                    <seg n="4" type="line" facs="z115v1_04"><seg type="wordend"
                            >νο<unclear>ν</unclear></seg>
                        <seg type="word">ὑπ<unclear>ό</unclear></seg> τε τῆς <seg type="word"
                                ><unclear>Α</unclear>Ε</seg> καὶ πασῶν τῶν</seg>
                    <seg n="5" type="line" facs="z115v1_05"><unclear>ΕΖ</unclear>, <seg type="word"
                                ><unclear>Η</unclear>Θ</seg>, ΓΔ, ΚΛ, ΜΝ. Ἀλλὰ καὶ ἡ ἐκ</seg>
                    <seg n="6" type="line" facs="z115v1_06">τοῦ κέντρου τοῦ Ξ <seg type="word"
                                >κύ<unclear>κ</unclear>λου</seg>
                        <seg type="word"><unclear>δ</unclear>ύν<unclear>α</unclear>ται</seg></seg>
                    <seg n="7" type="line" facs="z115v1_07">τὸ ὑπὸ τῆς <seg type="word"
                            >Α<unclear>Ε</unclear></seg> καὶ τῆς <choice>
                            <abbr>συγκειμέν</abbr>
                            <expan>συγκειμέν<supplied reason="lost">ης</supplied></expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z115v1_08">ἐκ πασῶν τῶν ΕΖ, ΗΘ, ΓΔ, ΚΛ, ΜΝ·</seg>
                    <seg n="9" type="line" facs="z115v1_09">ἡ <expan>ἄρα</expan> ἐκ τοῦ κέντρου τοῦ
                        Ξ κύκλου <seg type="unclearword"><unclear>δ</unclear>ύ</seg></seg>
                    <seg n="10" type="line" facs="z115v1_10"><seg type="wordend">ναται</seg>
                        <seg type="word"><unclear>τ</unclear>ὰς</seg> ἐκ τῶν <seg type="word"
                                >κέντρω<unclear>ν</unclear></seg>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τ<unclear>ῶν</unclear></expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z115v1_11">Ο, Π, Ρ, Σ, Τ, Υ κύκλων·
                        <expan>καὶ</expan> ὁ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλ<unclear>ος</unclear></expan>
                        </choice>
                        <expan>
                            <unclear>ἄρα</unclear>
                        </expan>
                        <unclear>ὁ</unclear> Ξ <choice>
                            <abbr>ἴσ</abbr>
                            <expan>ἴσος</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z115v1_12"><expan>ἐστὶ</expan> τοῖς Ο, Π, Ρ, Σ, Τ,
                        Υ κύκλοις. οἱ <seg type="word"><supplied reason="lost">δ</supplied>ὲ</seg>
                        Ο, Π, Ρ,</seg>
                    <seg n="13" type="line" facs="z115v1_13">Σ, Τ, Υ <seg type="word"
                            >κύ<unclear>κ</unclear>λοι</seg>
                        <seg type="word">ἀ<unclear>π</unclear>εδείχθη<supplied reason="lost"
                            >σαν</supplied></seg>
                        <supplied reason="lost">ἴσοι τῆι</supplied></seg>
                    <seg n="14" type="line" facs="z115v1_14">εἰρημένηι τοῦ <seg type="word"
                                >σχ<supplied reason="lost">ήμ</supplied>ατος</seg>
                        <w part="I">ἐπιφα</w></seg>
                    <seg n="15" type="line" facs="z115v1_15"><w part="F">νείαι</w>·
                        <expan>καὶ</expan> ὁ Ξ <expan>ἄρα</expan> κύκλος <seg type="word"
                                >ἴσο<supplied reason="lost">ς</supplied></seg> ἔσται τῆ</seg>
                    <seg n="16" type="line" facs="z115v1_16">ἐπιφανείαι τοῦ σχήματος.</seg>
                </seg>
                <seg n="112r2" type="folio">
                    <num>ΚΕ</num>
                </seg>
                <seg n="112r2" type="folio">
                    <seg n="1" type="line" facs="z112r2_01">τοῦ ἐγγεγραμμένου σχήματος εἰς <choice>
                            <abbr>τη</abbr>
                            <expan>τήν</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z112r2_02">σφαῖραν ἡ ἐπιφάνεια ἡ <seg
                            type="suppliedword">περιεχο</seg></seg>
                    <seg n="3" type="line" facs="z112r2_03"><seg type="wordend"
                                >μέ<unclear>ν</unclear><supplied reason="lost">η</supplied></seg>
                        ὑπὸ τῶν κωνικῶν <w part="I">ἐπιφα</w></seg>
                    <seg n="4" type="line" facs="z112r2_04"><w part="F">νειῶν</w> ἐλάσσων ἐστὶν ἢ <w
                            part="I">τετραπλα</w></seg>
                    <seg n="5" type="line" facs="z112r2_05"><w part="F">σία</w> τοῦ μεγίστου κύκλου
                        τῶν ἐν τῆι</seg>
                    <seg n="6" type="line" facs="z112r2_06">σφαίραι.</seg>
                </seg>
                <seg n="112r2" type="folio">
                    <seg n="6" type="line" facs="z112r2_06">ἔστω ἐν σφαίρα <choice>
                            <abbr>μ<unclear>ε</unclear><supplied reason="lost">γι</supplied>στ</abbr>
                            <expan>μέγιστος</expan>
                        </choice></seg>
                    <seg n="7" type="line" facs="z112r2_07">κύκλος ὁ ΑΒΓΔ, καὶ ἐν αὐτῶι <w part="I"
                            >ἐγγε</w></seg>
                    <seg n="8" type="line" facs="z112r2_08">
                        <w part="F">γράφθω</w>
                        <seg type="word">
                            <unclear>π</unclear>
                            <supplied reason="lost">ολύγ</supplied>
                            <unclear>ωνο</unclear>
                            <supplied reason="lost">ν</supplied>
                        </seg>
                        <choice>
                            <abbr><supplied reason="lost">α</supplied><unclear>ρ</unclear>τιογωνο</abbr>
                            <expan>ἀρτιόγωνον</expan>
                        </choice>
                    </seg>
                </seg>
                <seg n="115v2" type="folio">
                    <seg n="1" type="line" facs="z115v2_01">ἰσόπλευρον, <unclear>οὗ</unclear> αἱ
                            <seg type="word">π<unclear>λ</unclear><supplied reason="lost"
                            >ευραὶ</supplied></seg>
                        <seg type="word">
                            <supplied reason="lost">ὑπ</supplied>
                            <unclear>ὸ</unclear>
                        </seg></seg>
                    <seg n="2" type="line" facs="z115v2_02">τετράδος <seg type="word"><supplied
                                reason="lost">μ</supplied>ετροῦνται</seg>, καὶ ἐπ᾽ <choice>
                            <abbr>αυτ</abbr>
                            <expan>αὐτοῦ</expan>
                        </choice>
                    </seg>
                    <seg n="3" type="line" facs="z115v2_03">νοείσθω <seg type="word"
                            >ἐπιφ<unclear>άν</unclear>εια</seg> ἡ ὑπὸ <seg type="word"
                            >τ<unclear>ῶν</unclear></seg>
                        <w part="I">κω</w></seg>
                    <seg n="4" type="line" facs="z115v2_04"><w part="F">νικῶν</w> ἐπιφανειῶν
                        περιεχομένη·</seg>
                    <seg n="5" type="line" facs="z115v2_05"><seg type="word"><supplied reason="lost"
                                >λ</supplied>έγω</seg>
                        <expan>ὅτι</expan> ἡ ἐπιφάνεια τοῦ <w part="I">ἐγγραφέν</w></seg>
                    <seg n="6" type="line" facs="z115v2_06"><w part="F">τος</w> ἐλάσσων ἐστὶν ἢ <w
                            part="I">τετραπλα</w></seg>
                    <seg n="7" type="line" facs="z115v2_07"><w part="F">σία</w> τοῦ <seg type="word"
                                >μεγί<unclear>στ</unclear>ου</seg> κύκλου τῶν ἐν τῆι</seg>
                    <seg n="8" type="line" facs="z115v2_08">σφαίραι. </seg>
                </seg>
                <seg n="115v2" type="folio">
                    <seg n="8" type="line" facs="z115v2_08"><seg type="word"
                            >ἐπεζεύχθω<unclear>σ</unclear>αν</seg>
                        <seg type="word">γ<unclear>ὰ</unclear>ρ</seg> αἱ <w part="I">ὑ</w></seg>
                    <seg n="9" type="line" facs="z115v2_09"><w part="F">πὸ</w> δύο πλευρὰς <seg
                            type="word">ὑπο<unclear>τ</unclear>είνουσαι</seg>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z115v2_10">πολυγώνου αἱ ΕΙ, ΘΜ <seg type="word"
                                ><supplied reason="lost">κ</supplied>αὶ</seg> ταύταις</seg>
                    <seg n="11" type="line" facs="z115v2_11">παράλληλοι αἱ ΖΚ, ΔΒ, ΗΛ, <w part="I"
                            >ἐκκείσ</w></seg>
                    <seg n="12" type="line" facs="z115v2_12"><w part="F">θω</w> δέ τις <choice>
                            <abbr><unclear>κ</unclear><supplied reason="lost">υ</supplied>κλ</abbr>
                            <expan>κύκλος</expan>
                        </choice> ὁ Ρ, οὗ ἡ <seg type="word"><supplied reason="lost">ἐ</supplied>κ</seg>
                        <seg type="word"><supplied reason="lost">το</supplied>ῦ</seg>
                        <choice>
                            <abbr>κε<unclear>ν</unclear>τρ</abbr>
                            <expan>κέντρου</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z115v2_13"><seg type="word"
                            >δύν<unclear>α</unclear>ται</seg> ὑπὸ τῆς ΕΑ <seg type="word">
                            <expan>
                                <supplied reason="lost">καὶ</supplied>
                            </expan>
                        </seg>
                        <seg type="word"><unclear>τ</unclear><supplied reason="lost">ῆ</supplied>ς</seg>
                        <seg type="word">
                            <unclear>ἴ</unclear>
                            <supplied reason="lost">σης</supplied>
                        </seg></seg>
                    <seg n="14" type="line" facs="z115v2_14"><seg type="word">π<supplied
                                reason="lost">άσαι</supplied><unclear>ς</unclear></seg>
                        <choice>
                            <abbr>τς</abbr>
                            <expan>ταῖς</expan>
                        </choice> ΕΙ, <seg type="word"><unclear>Ζ</unclear>Κ</seg>, <seg type="word"
                                ><unclear>Β</unclear>Δ</seg>, <supplied reason="lost">ΗΛ, ΘΜ· διὰ
                        δὴ</supplied></seg>
                    <seg n="15" type="line" facs="z115v2_15">τὸ <seg type="word">π<supplied
                                reason="lost">ρ</supplied>οδει<unclear>χ</unclear>θὲν</seg> ἴσος
                            <seg type="word">
                            <expan>
                                <unclear>ἐστὶν</unclear>
                            </expan>
                        </seg> ὁ <seg type="word">κ<supplied reason="lost">ύκ</supplied>λος</seg>
                        <supplied reason="lost">τῆι</supplied></seg>
                    <seg n="16" type="line" facs="z115v2_16">τοῦ <seg type="word">εἰρ<supplied
                                reason="lost">η</supplied>μέν<supplied reason="lost">ο</supplied>υ</seg>
                        <seg type="word"><supplied reason="lost">σ</supplied>χήμα<supplied
                                reason="lost">το</supplied>ς</seg>
                        <seg type="word"><supplied reason="lost"
                            >ἐπι</supplied><unclear>φ</unclear>ανείαι</seg>.</seg>
                </seg>
            </ab>
        </body>
