    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="45v1" type="folio">
                    <seg n="1" type="line" facs="z45v1_01"><supplied reason="lost">τὸ</supplied>
                        <supplied reason="lost">ἀπὸ</supplied> ΒΔ <seg type="word">
                            <choice>
                                <abbr>πρ</abbr>
                                <expan>πρ<supplied reason="lost">ὸς</supplied></expan>
                            </choice>
                        </seg> τὸ ὑπὸ ΒΔ ΠΣ. ἴσον <expan>ἄρα</expan></seg>
                    <seg n="2" type="line" facs="z45v1_02">τὸ ἀπὸ ΞΣ το ὑπὸ ΒΔ ΠΣ· ἀνάλογον</seg>
                    <seg n="3" type="line" facs="z45v1_03">ἄρα εἰσὶν αἱ ΒΔ ΣΞ ΣΠ, <expan>διὰ</expan>
                        δὴ τοῦτό <expan>ἐστιν</expan></seg>
                    <seg n="4" type="line" facs="z45v1_04"><unclear>ὡς</unclear> ἡ ΒΔ
                        <expan>πρὸς</expan> ΠΣ, οὕτως τὸ ἀπὸ ΞΣ <expan>πρὸς</expan> τὸ</seg>
                    <seg n="5" type="line" facs="z45v1_05">ἀπὸ ΣΠ. ὡς δὲ ἡ ΒΔ <expan>πρὸς</expan>
                        ΠΣ, <expan>οὕτως</expan> ἡ ΔΑ</seg>
                    <seg n="6" type="line" facs="z45v1_06"><expan>πρὸς</expan> ΑΣ, <choice>
                            <abbr>τουτ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice> ἡ ΘΑ <expan>πρὸς</expan> ΑΣ· καὶ ὡς ἄρα</seg>
                    <seg n="7" type="line" facs="z45v1_07">ἡ ΘΑ <expan>πρὸς</expan> ΑΣ,
                        <expan>οὕτως</expan> τὸ ἀπὸ ΞΣ <expan>πρὸς</expan> τὸ ἀπὸ ΣΠ.</seg>
                    <seg n="8" type="line" facs="z45v1_08">ἀνεστάτω δὴ ἀπὸ τῆς ΞΟ <w part="I"
                        >ἐπίπε</w></seg>
                    <seg n="9" type="line" facs="z45v1_09"><w part="F">δον</w> ὀρθὸν
                        <expan>πρὸς</expan> τὴν <supplied reason="lost">Α</supplied>Δ· <seg
                            type="word">πο<supplied reason="lost">ι</supplied>ήσει</seg> δὲ</seg>
                    <seg n="10" type="line" facs="z45v1_10">τοῦτο ἐν μὲν τῶι τμήματι τοῦ <w part="I"
                            >ὀρ</w></seg>
                    <seg n="11" type="line" facs="z45v1_11"><w part="F">θογωνίου</w> κωνοειδέος
                        κύκλον,</seg>
                    <seg n="12" type="line" facs="z45v1_12">οὗ διάμετρος ἡ ΞΟ, ἐν δὲ τῶ <w part="I"
                            >κώ</w></seg>
                    <seg n="13" type="line" facs="z45v1_13"><w part="F">νωι</w> κύκλον, οὗ διάμετρος
                        ἡ ΠΡ.</seg>
                    <seg n="14" type="line" facs="z45v1_14"><expan>καὶ</expan> ἐπεί ἐστιν ὡς ἡ ΘΑ
                            <expan>πρὸς</expan> ΑΣ, οὕτως</seg>
                    <seg n="15" type="line" facs="z45v1_15">τὸ ἀπὸ ΞΣ <expan>πρὸς</expan> τὸ ἀπὸ ΣΠ,
                        οὕτως ὁ <w part="I">κύ</w></seg>
                    <seg n="16" type="line" facs="z45v1_16"><w part="F">κλος</w>, οὗ διάμετρος ἡ
                            Ξ<unclear>Ο</unclear>, <expan>πρὸς</expan> τὸν</seg>
                    <seg n="17" type="line" facs="z45v1_17">κύκλον, οὗ διάμετρος ἡ ΠΡ, ὡς
                        <expan>ἄρα</expan></seg>
                    <seg n="18" type="line" facs="z45v1_18">ἡ ΘΑ <expan>πρὸς</expan> ΑΣ,
                            <expan>οὕτως</expan> ὁ κύκλος, οὗ <w part="I">διάμε</w></seg>
                    <seg n="19" type="line" facs="z45v1_19"><w part="F">τρος</w> ἡ ΞΟ,
                        <expan>πρὸς</expan> τὸν κύκλον, οὗ <seg type="expandedword">
                            <choice>
                                <abbr>δμε</abbr>
                                <expan>διάμε</expan>
                            </choice>
                        </seg></seg>
                </seg>
                <seg n="44r1" type="folio">
                    <seg n="1" type="line" facs="z44r1_01"><seg type="wordend">τρος</seg> ἡ ΠΡ.
                        ἰσορροπήσει οὖν <w part="I">πε</w></seg>
                    <seg n="2" type="line" facs="z44r1_02"><w part="F">ρὶ</w> τὸ Α σημεῖον ὁ κύκλος,
                        οὗ <seg type="expandedword">
                            <choice>
                                <abbr>με</abbr>
                                <expan>διάμε</expan>
                            </choice>
                        </seg></seg>
                    <seg n="3" type="line" facs="z44r1_03"><seg type="wordend">τρος</seg> ἡ ΞΟ,
                        αὐτοῦ μένων τῶι <w part="I">κύ</w></seg>
                    <seg n="4" type="line" facs="z44r1_04"><w part="F">κλωι</w>, οὗ <choice>
                            <abbr>δμετρος</abbr>
                            <expan>διάμετρος</expan>
                        </choice> ἡ ΠΡ, <w part="I">μετενε</w></seg>
                    <seg n="5" type="line" facs="z44r1_05"><w part="F">χθέντι</w> τοῦ ζυγοῦ κατὰ τὸ
                        Θ <expan>οὕτως</expan>, <seg type="expandedword">
                            <expan>ὥσ</expan>
                        </seg></seg>
                    <seg n="6" type="line" facs="z44r1_06"><seg type="wordend">τε</seg> κέντρον <seg
                            type="word">εἶνα<unclear>ι</unclear></seg> τοῦ βάρους τὸ</seg>
                    <seg n="7" type="line" facs="z44r1_07">Θ. ἐπεὶ οὖν τοῦ μὲν κύκλου, οὗ <w
                            part="I">διά</w></seg>
                    <seg n="8" type="line" facs="z44r1_08"><w part="F">μετρος</w> ἡ ΞΟ, αὐτοῦ
                        μένοντος <w part="I">κέν</w></seg>
                    <seg n="9" type="line" facs="z44r1_09"><w part="F">τρον</w> ἐστὶν τοῦ βάρους τὸ
                        Σ, τοῦ δὲ</seg>
                    <seg n="10" type="line" facs="z44r1_10">κύκλου, οὗ διάμετρος ἡ ΠΡ, <w part="I"
                            >μετε</w></seg>
                    <seg n="11" type="line" facs="z44r1_11"><w part="F">νεχθέντος</w> ὡς ἐρρέθη <seg
                            type="word">
                            <choice>
                                <abbr>κέντρο</abbr>
                                <expan>κέντρο<unclear>ν</unclear></expan>
                            </choice>
                        </seg></seg>
                    <seg n="12" type="line" facs="z44r1_12">τοῦ βάρους τὸ Θ, καὶ <w part="I"
                            >ἀντιπεπον</w></seg>
                    <seg n="13" type="line" facs="z44r1_13"><w part="F">θότως</w> τὸν αὐτὸν ἔχει
                        λόγον ἡ</seg>
                    <seg n="14" type="line" facs="z44r1_14">ΘΑ <expan>πρὸς</expan> ΑΣ, ὃν ὁ κύκλος,
                        οὗ <w part="I">διάμε</w></seg>
                    <seg n="15" type="line" facs="z44r1_15"><w part="F">τρος</w> ἡ ΞΟ,
                        <expan>πρὸς</expan> τὸν κύκλον, οὗ <seg type="expandedword">
                            <expan>διά</expan>
                        </seg></seg>
                    <seg n="16" type="line" facs="z44r1_16"><seg type="wordend">μετρος</seg> ἡ ΠΡ,
                        ἰσορροπήσουσιν</seg>
                    <seg n="17" type="line" facs="z44r1_17">ἄρα <expan>πρὸς</expan>
                        <seg type="word">τ<supplied reason="lost">ῶι</supplied></seg>
                        <supplied reason="lost">Α</supplied> σημείω. ὁμοίως</seg>
                    <seg n="18" type="line" facs="z44r1_18">δὲ δειχθήσεται, <seg type="word"
                                ><unclear>κ</unclear>αὶ</seg>
                        <seg type="word"><supplied reason="lost">ἐ</supplied>ὰν</seg> ἄλλη</seg>
                </seg>
                <seg n="45v2" type="folio">
                    <seg n="1" type="line" facs="z45v2_01">τις ἀχθῆι ἐν τῆι τοῦ ὀρθογωνίου</seg>
                    <seg n="2" type="line" facs="z45v2_02">κώνου τομῆι παράλληλος τῆι</seg>
                    <seg n="3" type="line" facs="z45v2_03">ΒΓ, καὶ ἀπὸ τῆς ἀχθείσης <w part="I"
                        >ἐπί</w></seg>
                    <seg n="4" type="line" facs="z45v2_04"><w part="F">πεδον</w> ἀνασταθῆι ὀρθὸν
                            <expan>πρὸς</expan>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τὴν</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z45v2_05">ΑΔ, <expan>ὅτι</expan> ὁ γενόμενος κύκλος
                        ἐν τῶι</seg>
                    <seg n="6" type="line" facs="z45v2_06">τμήματι τοῦ ὀρθογωνίου <w part="I"
                        >κωνο</w></seg>
                    <seg n="7" type="line" facs="z45v2_07"><w part="F">ειδέος</w> αὐτοῦ μένων <w
                            part="I">ἰσορροπή</w></seg>
                    <seg n="8" type="line" facs="z45v2_08"><w part="F">σει</w> περὶ τὸ Α σημεῖον τῶι
                            <seg type="suppliedword"><supplied reason="lost">γ</supplied>ενομέ</seg></seg>
                    <seg n="9" type="line" facs="z45v2_09"><seg type="wordend">νωι</seg> κύκλωι ἐν
                        τῶι <seg type="word">κ<supplied reason="lost">ών</supplied>ω<supplied
                                reason="lost">ι</supplied></seg>
                        <seg type="suppliedword"><supplied reason="lost">μ</supplied>ετενε</seg></seg>
                    <seg n="10" type="line" facs="z45v2_10"><seg type="wordend">χθέντι</seg> καὶ
                        τεθέντι τοῦ ζυγοῦ <expan>κατὰ</expan></seg>
                    <seg n="11" type="line" facs="z45v2_11">τὸ Θ, ὥστε κέντρον εἶναι αὐτοῦ</seg>
                    <seg n="12" type="line" facs="z45v2_12">τοῦ βάρους τὸ Θ. <w part="I"
                        >συμπληρωθέν</w></seg>
                    <seg n="13" type="line" facs="z45v2_13"><w part="F">των</w> οὖν ὑπὸ τῶν κύκλων
                        τοῦ</seg>
                    <seg n="14" type="line" facs="z45v2_14">τε τμήματος <expan>καὶ</expan> τοῦ κώνου
                            <w part="I">ἰσορ</w></seg>
                    <seg n="15" type="line" facs="z45v2_15"><w part="F">ροπήσουσι</w> περὶ τὸ Α
                        σημεῖον</seg>
                    <seg n="16" type="line" facs="z45v2_16">τεθέντες οἱ κύκλοι οἱ ἐν τῶι <w part="I"
                            >τμή</w></seg>
                    <seg n="17" type="line" facs="z45v2_17"><w part="F">ματι</w> αὐτοῦ μένοντες πᾶσι <choice>
                            <abbr>τ</abbr>
                            <expan>τοῖς</expan>
                        </choice></seg>
                    <seg n="18" type="line" facs="z45v2_18">κύκλοις τοῖς ἐν τῶι κώνωι <w part="I"
                        >με</w></seg>
                    <seg n="19" type="line" facs="z45v2_19"><w part="F">τενεχθεῖσι</w> καὶ τεθεῖσι
                        τοῦ <choice>
                            <abbr>ζυγ</abbr>
                            <expan>ζυγοῦ</expan>
                        </choice></seg>
                </seg>
                <seg n="44r2" type="folio">
                    <seg n="1" type="line" facs="z44r2_01">κατὰ τὸ Θ οὕτως, <seg type="word"
                                >ὥστ<unclear>ε</unclear></seg>
                        <seg type="word">
                            <choice>
                                <abbr>ἑκάστ</abbr>
                                <expan>ἑκάστ<unclear>ου</unclear></expan>
                            </choice>
                        </seg>
                    </seg>
                    <seg n="2" type="line" facs="z44r2_02">αὐτῶν κέντρον εἶναι τοῦ <w part="I"
                        >βά</w></seg>
                    <seg n="3" type="line" facs="z44r2_03"><w part="F">ρους</w> τὸ Θ· ἰσόρροπον οὖν
                            <expan>καὶ</expan> τὸ</seg>
                    <seg n="4" type="line" facs="z44r2_04">τμῆμα τοῦ ὀρθογωνίου <w part="I">κω</w></seg>
                    <seg n="5" type="line" facs="z44r2_05"><w part="F">νοειδέος</w> περὶ τὸ Α
                        σημεῖον <w part="I">αὐ</w></seg>
                    <seg n="6" type="line" facs="z44r2_06"><w part="F">τοῦ</w> μένον τῶι κώνωι <w
                            part="I">μετενε</w></seg>
                    <seg n="7" type="line" facs="z44r2_07"><w part="F">χθέντι</w> καὶ τεθέντι τοῦ
                        ζυγοῦ</seg>
                    <seg n="8" type="line" facs="z44r2_08">κατὰ τὸ Θ <expan>οὕτως</expan>, ὥστε
                        κέντρον <expan>εἶναι</expan></seg>
                    <seg n="9" type="line" facs="z44r2_09">τοῦ βάρους αὐτοῦ τὸ Θ. ἐπεὶ οὖν</seg>
                    <seg n="10" type="line" facs="z44r2_10">συναμφοτέρου τῶν <w part="I">μεγεθε</w></seg>
                    <seg n="11" type="line" facs="z44r2_11"><w part="F">ῶν</w> ὡς ἑνὸς λέγομεν
                        κέντρον</seg>
                    <seg n="12" type="line" facs="z44r2_12"><expan>ἐστὶν</expan> τοῦ βάρους τὸ Α,
                        αὐτοῦ δὲ τοῦ <w part="I">κώ</w></seg>
                    <seg n="13" type="line" facs="z44r2_13"><w part="F">νου</w> τοῦ <seg type="word"
                                >μετ<unclear>ε</unclear>νηνεγμένου</seg>
                        <choice>
                            <abbr>κέντρ</abbr>
                            <expan>κέντρον</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z44r2_14">τοῦ βάρους τὸ Θ, τοῦ λοιποῦ ἄρα</seg>
                    <seg n="15" type="line" facs="z44r2_15">μεγέθους τὸ κέντρον <expan>ἐστὶ</expan>
                        τοῦ <seg type="suppliedword">βά</seg></seg>
                    <seg n="16" type="line" facs="z44r2_16"><seg type="wordend">ρ<supplied
                                reason="lost">ο</supplied>υς</seg> ἐπὶ τῆς ΑΘ εὐθείας <seg
                            type="unclearword">ἐκ<unclear>β</unclear>ε</seg></seg>
                    <seg n="17" type="line" facs="z44r2_17"><seg type="wordend">βλημένης</seg> ἐπὶ
                        τὸ Α καὶ <w part="I">ἀπολη</w></seg>
                    <seg n="18" type="line" facs="z44r2_18"><w part="F">φθεῖσα</w> αὐτῆς τῆς ΑΚ <choice>
                            <abbr>τηλικαύτ</abbr>
                            <expan>τηλικαύτης</expan>
                        </choice>,</seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
