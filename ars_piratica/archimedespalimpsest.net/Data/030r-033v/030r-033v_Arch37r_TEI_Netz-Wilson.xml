    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="30r1" type="folio">
                    <seg n="1" type="line" facs="z30r1_01"><sic>
                            <seg type="wordend">ουσα<unclear>ν</unclear></seg>
                        </sic> τοῦτον ἔχειν τὸν λόγον,</seg>
                    <seg n="2" type="line" facs="z30r1_02">ὃν ἔχει ἁ ΘΑ ποτὶ τὰν ΑΛ· τεμεῖ</seg>
                    <seg n="3" type="line" facs="z30r1_03">δὴ ἁ ΑΠ τὸν μὲν <seg type="word"
                                >κύ<supplied reason="lost">κ</supplied>λον</seg>
                        <seg type="word">
                            <supplied reason="lost">κ</supplied><unclear>α</unclear>τὰ </seg> τὸ</seg>
                    <seg n="4" type="line" facs="z30r1_04">Ρ, τὰν δὲ ἕλικα κατὰ τὸ Χ· καὶ ἕξει</seg>
                    <seg n="5" type="line" facs="z30r1_05">καὶ ἐναλλὰξ <seg type="word">τὸ<supplied
                                reason="lost">ν</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">α</supplied>ὐτὸν</seg> λόγον ἁ</seg>
                    <seg n="6" type="line" facs="z30r1_06">ΝΡ <expan>πρὸς</expan> ΡΑ, ὃν ἁ ΘΠ
                            <expan>πρὸς</expan> ΑΛ. ἁ δὲ ΘΠ <seg type="suppliedword">
                            <supplied reason="lost">πο</supplied>
                        </seg></seg>
                    <seg n="7" type="line" facs="z30r1_07"><seg type="wordend">τὶ</seg> τὰν <seg
                            type="word">
                            <supplied reason="lost">Α</supplied>
                            <unclear>Λ</unclear>
                        </seg> μείζονα λόγον ἔχει <unclear>ἢ</unclear>
                        <supplied reason="lost">ἁ</supplied></seg>
                    <seg n="8" type="line" facs="z30r1_08"><seg type="word">
                            <supplied reason="lost">Θ</supplied>
                            <unclear>Ρ</unclear>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">π</supplied>εριφέρεια</seg> ποτὶ τὰν τοῦ ΘΗΚ</seg>
                    <seg n="9" type="line" facs="z30r1_09">
                        <seg type="word">
                            <supplied reason="lost">κύκ</supplied>λου</seg>
                        <seg type="word">περιφέρ<supplied reason="lost">ει</supplied>α<supplied
                                reason="lost">ν·</supplied>
                        </seg>
                        <supplied reason="lost">ἁ</supplied>
                        <supplied reason="lost">μὲν</supplied>
                        <seg type="word">γὰ<supplied reason="lost">ρ</supplied>
                        </seg>
                        <unclear>ΘΠ</unclear>
                    </seg>
                    <seg n="10" type="line" facs="z30r1_10">εὐθεῖα μείζων <seg type="word"
                                >ἐστὶ<supplied reason="lost">ν</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ᾶς</seg> Θ<supplied reason="lost">Ρ</supplied>
                        <seg type="clearword">πε</seg></seg>
                    <seg n="11" type="line" facs="z30r1_11"><seg type="wordend">ριφερείας</seg>, ἁ
                        δὲ Α<supplied reason="lost">Λ</supplied>
                        <seg type="word">ἐλάσσω<supplied reason="lost">ν</supplied>
                        </seg> τᾶς</seg>
                    <seg n="12" type="line" facs="z30r1_12">τοῦ ΘΗΚ κύκλου περιφερείας· <seg
                            type="clearword">μεί</seg></seg>
                    <seg n="13" type="line" facs="z30r1_13"><seg type="wordend">ζονα</seg> ἄρα <seg
                            type="word">
                            <supplied reason="lost">λόγ</supplied>ον</seg>
                        <seg type="word">
                            <unclear>ἔχ</unclear>
                            <supplied reason="lost">ει</supplied>
                        </seg> ἁ <supplied reason="lost">Π</supplied>Ρ ποτὶ τὰν</seg>
                    <seg n="14" type="line" facs="z30r1_14">ΑΡ ἡ ΘΡ περιφέρεια ποτὶ <seg type="word"
                            > τ<supplied reason="lost">ὰν</supplied>
                        </seg>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> ΘΗΚ</seg>
                    <seg n="15" type="line" facs="z30r1_15">κύκλου περιφέρειαν· ὥστε καὶ ἁ</seg>
                    <seg n="16" type="line" facs="z30r1_16">ΡΑ ποτὶ τὰν ΑΝ μείζονα <seg type="word">
                                λό<unclear>γ</unclear>ον</seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἔ</supplied>
                        </seg></seg>
                    <seg n="17" type="line" facs="z30r1_17"><seg type="wordend">χει</seg> ἢ ἁ τοῦ
                        ΘΗΚ κύκλου <seg type="clearword">περιφέ</seg></seg>
                    <seg n="18" type="line" facs="z30r1_18"><seg type="wordend">ρεια</seg> ποτὶ τὰν
                        ΘΚΡ <choice>
                            <abbr>περιφέρεια</abbr>
                            <expan>περιφέρειαν</expan>
                        </choice>.</seg>
                    <seg n="19" type="line" facs="z30r1_19">ὃν <supplied reason="lost">δὲ</supplied>
                        <expan>λόγον</expan>
                        <seg type="word">ἔ<supplied reason="lost">χει</supplied>
                        </seg> ἁ τοῦ ΘΗΚ κύκλου <seg type="clearword">πε</seg></seg>
                </seg>
                <seg n="33v1" type="folio">
                    <seg n="1" type="line" facs="z33v1_01">
                        <seg type="wordend">ριφέρεια</seg> ποτὶ τὰν ΘΚΡ <seg type="suppliedword"
                                >π<unclear>ε</unclear><supplied reason="lost">ριφ</supplied>έ</seg>
                    </seg>
                    <seg n="2" type="line" facs="z33v1_02"><seg type="wordend">ρειαν</seg>, ταὐτὸν
                        ἔχει ἁ ΘΑ εὐθεῖα <seg type="clearword">πο</seg></seg>
                    <seg n="3" type="line" facs="z33v1_03"><seg type="wordend">τὶ</seg> τὰν ΑΧ· <seg
                            type="word">
                            <supplied reason="lost">δ</supplied>έ<unclear>δ</unclear>εικται </seg>
                        γὰρ τοῦτο· <seg type="clearword">μεί</seg></seg>
                    <seg n="4" type="line" facs="z33v1_04"><seg type="wordend">ζονα</seg> ἄρα λόγον
                        ἔχει ἁ ΡΑ ποτὶ</seg>
                    <seg n="5" type="line" facs="z33v1_05">τὰν ΑΗ ἢ ἁ ΘΑ ποτὶ τὰν ΑΧ· <seg
                            type="clearword">ὅ</seg></seg>
                    <seg n="6" type="line" facs="z33v1_06"><seg type="wordend">περ</seg> ἀδύνατον.
                        οὐκ ἄρα μείζων</seg>
                    <seg n="7" type="line" facs="z33v1_07">ἐστὶν οὐδὲ ἐλάσσων ἁ ΖΑ τᾶς τοῦ</seg>
                    <seg n="8" type="line" facs="z33v1_08">ΘΗΚ κύκλου περιφερείας· ἴση
                        <expan>ἄρα</expan>.</seg>
                    <figure n="19.1">
                        <figDesc>Figure 19.1</figDesc>
                    </figure>
                </seg>
                <seg n="30r2" type="folio">
                    <seg n="1" type="line" facs="z30r2_01">εἰ δὲ κατὰ <seg type="word">τᾶ<supplied
                                reason="lost">ς</supplied>
                        </seg> ἐν τᾶι <seg type="word">
                            <supplied reason="lost">δευτέ</supplied>ραι</seg>
                        <seg type="suppliedword">π<supplied reason="lost">ε</supplied></seg></seg>
                    <seg n="2" type="line" facs="z30r2_02">
                        <seg type="wordend">ριφορᾶι</seg>
                        <seg type="word">γεγ<supplied reason="lost">ραμμένας</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">ἕλι</supplied>κ<supplied reason="lost"
                            >ο</supplied>ς</seg>
                        <seg type="clearword">κα</seg>
                    </seg>
                    <seg n="3" type="line" facs="z30r2_03">
                        <seg type="wordend">τὰ</seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ὸ</seg>
                        <supplied reason="lost">πέρας</supplied>
                        <seg type="word">ἐπ<supplied reason="lost">ιψαύηι</supplied>
                        </seg>
                        <supplied reason="lost">εὐθεῖα,</supplied>
                        <supplied reason="lost">καὶ</supplied>
                        <supplied reason="lost">ἀπὸ</supplied>
                    </seg>
                    <seg n="4" type="line" facs="z30r2_04">
                        <supplied reason="lost">τᾶς</supplied>
                        <seg type="word">
                            <supplied reason="lost">ἀρχᾶ</supplied>ς</seg>
                        <supplied reason="lost">τᾶς</supplied>
                        <seg type="word">ἕ<supplied reason="lost">λ</supplied>ι<supplied
                                reason="lost">κος</supplied>
                        </seg>
                        <seg type="word">ἀχ<supplied reason="lost">θῆ</supplied>
                        </seg>
                    </seg>
                    <seg n="5" type="line" facs="z30r2_05"><supplied reason="lost">τις</supplied>
                        ποτ’ <seg type="word">
                            <supplied reason="lost">ὀ</supplied>ρ<supplied reason="lost"
                                >θ</supplied>ὰ<supplied reason="lost">ς</supplied>
                        </seg>
                        <supplied reason="lost">τᾶι</supplied>
                        <supplied reason="lost">ἀρχᾶι</supplied>
                        <supplied reason="lost">τᾶς</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">περι</supplied>
                        </seg></seg>
                    <seg n="6" type="line" facs="z30r2_06">
                        <seg type="wordend">
                            <supplied reason="lost">φορᾶς,</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">σ</supplied>υμ<supplied reason="lost"
                                >πε</supplied>σ<supplied reason="lost">εῖται</supplied>
                        </seg>
                        <supplied reason="lost">αὕτα</supplied>
                        <supplied reason="lost">ποτὶ</supplied>
                    </seg>
                    <seg n="7" type="line" facs="z30r2_07">
                        <supplied reason="lost">τὰν</supplied>
                        <supplied reason="lost">ἐπιψαύουσαν,</supplied>
                        <supplied reason="lost">καὶ</supplied>
                        <supplied reason="lost">ἐσσεῖται</supplied>
                        <supplied reason="lost">ἁ</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">εὐθεῖ</supplied>
                        </seg>
                    </seg>
                    <seg n="8" type="line" facs="z30r2_08">
                        <seg type="wordend">
                            <supplied reason="lost">α</supplied>
                        </seg>
                        <supplied reason="lost">ἁ</supplied>
                        <seg type="word">
                            <supplied reason="lost">μετα</supplied>ξ<supplied reason="lost"
                            >ὺ</supplied>
                        </seg>
                        <seg type="word">τᾶ<supplied reason="lost">ς</supplied>
                        </seg>
                        <supplied reason="lost">ἐπιψαυούσας</supplied>
                        <supplied reason="lost">καὶ</supplied>
                    </seg>
                    <seg n="9" type="line" facs="z30r2_09">τᾶς <seg type="word">
                            <supplied reason="lost">ἀ</supplied>ρχᾶ<supplied reason="lost"
                            >ς</supplied>
                        </seg>
                        <seg type="word">τᾶ<supplied reason="lost">ς</supplied>
                        </seg>
                        <supplied reason="lost">ἕλικος</supplied>
                        <supplied reason="lost">διπλασία</supplied></seg>
                    <seg n="10" type="line" facs="z30r2_10">τᾶς τοῦ <seg type="word">
                                δευτέ<unclear>ρ</unclear>ου</seg>
                        <seg type="word">κύκλο<supplied reason="lost">υ</supplied>
                        </seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">περιφερεί</supplied>
                        </seg></seg>
                    <seg n="11" type="line" facs="z30r2_11">
                        <seg type="wordend">ας</seg>. </seg>
                </seg>
                <seg n="30r2" type="folio">
                    <seg n="11" type="line" facs="z30r2_11">ἔστω γὰρ ἁ μὲν <supplied reason="lost"
                            >ΑΒΓΘ</supplied>
                        <supplied reason="lost">ἕλιξ</supplied>
                        <supplied reason="lost">ἐν</supplied>
                        <supplied reason="lost">τᾶι</supplied></seg>
                    <seg n="12" type="line" facs="z30r2_12">πρώται <seg type="word">
                                π<unclear>ε</unclear>ρ<unclear>ιφορ</unclear><supplied reason="lost"
                                >ᾶι</supplied>
                        </seg>
                        <seg type="word">
                            <unclear>γε</unclear><supplied reason="lost">γρα</supplied>μμ<supplied
                                reason="lost">έ</supplied>να </seg>,</seg>
                    <seg n="13" type="line" facs="z30r2_13">ἁ δὲ ΘΕΓ ἐν τᾶ <seg type="word">
                                δ<unclear>ευ</unclear>τέ<supplied reason="lost">ραι,</supplied>
                        </seg>
                        <supplied reason="lost">καὶ</supplied>
                        <supplied reason="lost">ὁ</supplied>
                        <seg type="word">
                            <supplied reason="lost">μ</supplied>ὲν</seg></seg>
                    <seg n="14" type="line" facs="z30r2_14">ΘΚΗ <seg type="word">κύκλ<supplied
                                reason="lost">ος</supplied>
                        </seg>
                        <supplied reason="lost">ὁ</supplied>
                        <seg type="word">πρ<supplied reason="lost">ῶτος,</supplied>
                        </seg>
                        <supplied reason="lost">ὁ</supplied>
                        <seg type="word">
                            <supplied reason="lost">δ</supplied>ὲ</seg>
                        <supplied reason="lost">Τ</supplied>ΜΝ</seg>
                    <seg n="15" type="line" facs="z30r2_15">ὁ <seg type="word">δεύτερο<supplied
                                reason="lost">ς,</supplied>
                        </seg>
                        <supplied reason="lost">ἔστω</supplied>
                        <seg type="word">δ<unclear>έ</unclear>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τι</supplied>ς</seg>
                        <seg type="word">
                            <supplied reason="lost">γρα</supplied>μμὰ</seg>
                        <seg type="suppliedword">ἐ</seg></seg>
                    <seg n="16" type="line" facs="z30r2_16"><seg type="wordend">πιψαύ<supplied
                                reason="lost">ουσα</supplied></seg>
                        <seg type="word">
                            <supplied reason="lost">τᾶ</supplied>ς</seg> ἕλικος κατὰ</seg>
                    <seg n="17" type="line" facs="z30r2_17">τὸ Θ ἁ ΤΖ, ἁ <supplied reason="lost"
                        >δὲ</supplied> Ζ<supplied reason="lost">Α</supplied> ποτ’ <seg type="word"
                                >ὀ<supplied reason="lost">ρθὰς</supplied>
                        </seg> ἄχθωι</seg>
                    <seg n="18" type="line" facs="z30r2_18">τᾶ ΤΑ· <seg type="word">συμ<supplied
                                reason="lost">πεσεῖται</supplied>
                        </seg>
                        <supplied reason="lost">δὲ</supplied> τᾶ <supplied reason="lost"
                        >αὐτᾶ</supplied> τᾶ</seg>
                    <seg n="19" type="line" facs="z30r2_19">ΤΖ διὰ τὸ δεδεῖχθαι τὰν γωνίαν <seg
                            type="clearword">ὀ</seg></seg>
                </seg>
                <seg n="33v2" type="folio">
                    <seg n="1" type="line" facs="z33v2_01"><seg type="wordend">ξεῖαν</seg> οὖσαν τὰν
                        ὑπὸ τῶν ΑΤΖ. <seg type="clearword">δει</seg></seg>
                    <seg n="2" type="line" facs="z33v2_02"><seg type="wordend">κτέον</seg> ὅτι ἁ ΖΑ
                        εὐθεῖα διπλασία</seg>
                    <seg n="3" type="line" facs="z33v2_03">ἐντὶ τᾶς τοῦ ΤΜΝ κύκλου <seg
                            type="clearword">περιφε</seg></seg>
                    <seg n="4" type="line" facs="z33v2_04">
                        <seg type="wordend">ρείας</seg>. </seg>
                </seg>
                <seg n="33v2" type="folio">
                    <seg n="4" type="line" facs="z33v2_04">εἰ γὰρ μή ἐστιν διπλασία, <seg
                            type="clearword">ἤ</seg></seg>
                    <seg n="5" type="line" facs="z33v2_05"><seg type="wordend">τοι</seg> μείζων
                        ἐστὶν ἢ διπλασία ἢ <seg type="clearword">ἐλάσ</seg></seg>
                    <seg n="6" type="line" facs="z33v2_06"><seg type="wordend">σων</seg> ἢ διπλασία.
                        ἔστω <choice>
                            <abbr>πρότερο</abbr>
                            <expan>πρότερον</expan>
                        </choice>,</seg>
                    <seg n="7" type="line" facs="z33v2_07">εἰ δυνατόν, μείζων ἢ διπλασία,</seg>
                    <seg n="8" type="line" facs="z33v2_08">καὶ λελάφθω τις εὐθεῖα ἁ ΛΑ <choice>
                            <abbr>τ</abbr>
                            <expan>τᾶς</expan>
                        </choice></seg>
                    <seg n="9" type="line" facs="z33v2_09">μὲν Ζ<supplied reason="lost">Α</supplied>
                        εὐθείας ἐλάσσων, τᾶς δὲ</seg>
                    <seg n="10" type="line" facs="z33v2_10">τοῦ <supplied reason="lost"
                        >Τ</supplied>ΜΝ κύκλου περιφερείας <seg type="clearword">μεί</seg></seg>
                    <seg n="11" type="line" facs="z33v2_11"><seg type="wordend">ζων</seg> ἢ
                        διπλασία. ἔστιν δή τις <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice></seg>
                    <seg n="12" type="line" facs="z33v2_12">ὁ ΤΜΝ καὶ ἐν τῶι κύκλωι <seg
                            type="clearword">γεγραμ</seg></seg>
                    <seg n="13" type="line" facs="z33v2_13"><seg type="wordend">μένα</seg> ἐλάσσων
                        τᾶς διαμέτρου ἁ</seg>
                    <seg n="14" type="line" facs="z33v2_14">ΤΝ, καὶ ὃν ἔχει ἁ ΤΑ ποτὶ τὰν ΑΛ,
                            <sic>μει</sic></seg>
                    <seg n="15" type="line" facs="z33v2_15">μείζων τοῦ, ὃν ἔχει ἁ ἡμίσεια τᾶς</seg>
                    <seg n="16" type="line" facs="z33v2_16">ΤΝ ποτὶ τὰν ἀπὸ τοῦ Α κάθετον</seg>
                </seg>
            </ab>
