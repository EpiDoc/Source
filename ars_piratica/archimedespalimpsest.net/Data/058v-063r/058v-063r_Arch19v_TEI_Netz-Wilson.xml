    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="58v1" type="folio">
                    <seg n="1" type="line" facs="z58v1_01">ὀρθῶι πρὸ<unclear>ς</unclear> τὸν
                            ἄξ<supplied reason="lost">ον</supplied>α <choice>
                            <abbr>ἡμιόλιό</abbr>
                            <expan>ἡμιόλιόν</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z58v1_02"><expan>ἐστι</expan> τοῦ
                        κών<unclear>ου</unclear> τοῦ βάσιν ἔχοντος</seg>
                    <seg n="3" type="line" facs="z58v1_03">τὴν αὐτὴν τῶι τμήματι καὶ τὸν <unclear>
                            <w part="I">ἄ</w>
                        </unclear></seg>
                    <seg n="4" type="line" facs="z58v1_04"><w part="F">ξονα</w> τὸν αὐτόν, ὡς διὰ
                        τοῦ <choice>
                            <abbr>τρόπ</abbr>
                            <expan>τρόπου</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z58v1_05">τούτου θεωρεῖται.</seg>
                </seg>
                <seg n="58v1" type="folio">
                    <seg n="5" type="line" facs="z58v1_05">ἔστω γὰρ <w part="I">ὀρθογώ</w></seg>
                    <seg n="6" type="line" facs="z58v1_06"><w part="F">νιον</w> κωνοειδὲς καὶ
                        τετμήσθω <w part="I">ἐ</w></seg>
                    <seg n="7" type="line" facs="z58v1_07"><w part="F">πιπέδω</w> διὰ τοῦ ἄξονος,
                        καὶ <w part="I">ποι</w></seg>
                    <seg n="8" type="line" facs="z58v1_08"><w part="F">είτω</w> τομὴν ἐν τῆι
                        ἐπιφανείαι <w part="I">ὀρ</w></seg>
                    <seg n="9" type="line" facs="z58v1_09"><w part="F">θογωνίου</w> κώνου τομὴν τὴν
                        ΑΒ,</seg>
                    <seg n="10" type="line" facs="z58v1_10">τετμήσθω δὲ καὶ ἑτέρωι ἐπιπέδωι</seg>
                    <seg n="11" type="line" facs="z58v1_11">ὀρθῶι πρὸς τὸν ἄξονα, καὶ ἔστω</seg>
                    <seg n="12" type="line" facs="z58v1_12">αὐτῶν κοινὴ τομὴ ἡ ΒΓ, ἄξων δὲ</seg>
                    <seg n="13" type="line" facs="z58v1_13">ἔστω τοῦ τμήματος ὁ ΔΑ, καὶ <w part="I"
                            >ἐκ</w></seg>
                    <seg n="14" type="line" facs="z58v1_14"><w part="F">βεβλήσθω</w> ἡ Δ<supplied
                            reason="lost">Α</supplied> ἐπὶ τὸ Θ, καὶ κείσθω</seg>
                    <seg n="15" type="line" facs="z58v1_15">αὐτῆ ἴση ἡ ΑΘ, καὶ νοείσθω <choice>
                            <abbr>ζυγ</abbr>
                            <expan>ζυγὸς</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z58v1_16">ὁ ΑΘ, μέσον δὲ αὐτοῦ τὸ Α, ἔστω δὲ ἡ</seg>
                    <seg n="17" type="line" facs="z58v1_17">τοῦ τμήματος βάσις ὁ περὶ <w part="I"
                            >διά</w></seg>
                    <seg n="18" type="line" facs="z58v1_18"><w part="F">μετρον</w> τὴν ΒΓ <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλος</expan>
                        </choice> ὀρθὸς ὢν <expan>πρὸς</expan></seg>
                    <seg n="19" type="line" facs="z58v1_19">τὴν ΑΔ, ἔστω δὲ καὶ κῶνος <choice>
                            <abbr>βάσι</abbr>
                            <expan>βάσιν</expan>
                        </choice></seg>
                </seg>
                <seg n="63r1" type="folio">
                    <seg n="1" type="line" facs="z63r1_01">μὲν ἔχων τὸν <supplied reason="lost"
                        >κύ</supplied>κλον, οὗ <expan>ἐστι</expan>
                        <choice>
                            <abbr>μετρ</abbr>
                            <expan>διάμετρος</expan>
                        </choice></seg>
                    <seg n="2" type="line" facs="z63r1_02">ἡ ΒΓ, κορυφὴ δὲ τὸ Α σημεῖον, ἔστω</seg>
                    <seg n="3" type="line" facs="z63r1_03">δὲ καὶ κύλινδρος βάσιν μὲν <choice>
                            <abbr>ἔχω</abbr>
                            <expan>ἔχων</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z63r1_04">τὸν κύκλον, οὗ <choice>
                            <abbr>δμετρος</abbr>
                            <expan>διάμετρος</expan>
                        </choice> ἡ ΒΓ, <w part="I">ἄ</w></seg>
                    <seg n="5" type="line" facs="z63r1_05"><w part="F">ξονα</w> δὲ τὸν ΑΔ, καὶ
                            <supplied reason="lost">ἤχ</supplied>θω τις ἐν</seg>
                    <seg n="6" type="line" facs="z63r1_06">τῶι παραλληλογράμμῶι <unclear>ἡ</unclear>
                        ΜΝ</seg>
                    <seg n="7" type="line" facs="z63r1_07"
                        >παράλλ<unclear>η</unclear>λ<unclear>ο</unclear>ς οὖσα τῆι ΒΓ, καὶ</seg>
                    <seg n="8" type="line" facs="z63r1_08">ἀπὸ τῆς ΜΝ ἐπίπεδον <w part="I"
                        >ἀνεστά</w></seg>
                    <seg n="9" type="line" facs="z63r1_09"><w part="F">τω</w> ὀρθὸν
                        <expan>πρὸς</expan> τὴν ΑΔ· ποιήσει δὴ</seg>
                    <seg n="10" type="line" facs="z63r1_10">τοῦτο ἐν μὲν τῶι κυλίνδρωι <choice>
                            <abbr>τομὴ</abbr>
                            <expan>τομὴν</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z63r1_11">κύκλον, οὗ <choice>
                            <abbr>μετρος</abbr>
                            <expan>διάμετρος</expan>
                        </choice> ἡ ΜΝ, ἐν δὲ</seg>
                    <seg n="12" type="line" facs="z63r1_12"><supplied reason="lost">τῶι</supplied>
                        <supplied reason="lost">τμ</supplied>ήματι τοῦ ὀρθογωνίου</seg>
                    <seg n="13" type="line" facs="z63r1_13">κωνοειδοῦς τομὴν κύκλο<supplied
                            reason="lost">ν</supplied>, οὗ <seg type="expandedword">
                            <expan>διά</expan>
                        </seg></seg>
                    <seg n="14" type="line" facs="z63r1_14"><seg type="wordend">μετρος</seg> ἡ
                    ΞΟ.</seg>
                </seg>
                <seg n="63r1" type="folio">
                    <seg n="14" type="line" facs="z63r1_14">καὶ ἐπὶ <seg type="word">
                            <choice>
                                <abbr>ὀρθο<unclear>γ</unclear>ωνί</abbr>
                                <expan>ὀρθο<unclear>γ</unclear>ωνίου</expan>
                            </choice>
                        </seg></seg>
                    <seg n="15" type="line" facs="z63r1_15">κώνου τομῆς ἐστιν ἡ ΒΑΓ, <supplied
                            reason="lost">
                            <w part="I">δι</w>
                        </supplied><w part="M">ά</w></seg>
                    <seg n="16" type="line" facs="z63r1_16"><w part="F">μετρος</w>
                        <supplied reason="lost">δὲ</supplied> αὐτ<unclear>ῆς</unclear> ἡ ΑΔ, καὶ <w
                            part="I">τεταγμέ</w></seg>
                </seg>
                <seg n="58v2" type="folio">
                    <seg n="1" type="line" facs="z58v2_01"><w part="F">νως</w> κατηγμέναι εἰσὶν αἱ
                        ΞΣ,</seg>
                    <seg n="2" type="line" facs="z58v2_02">ΒΔ, <expan>ἔστιν</expan> ὡς ἡ ΔΑ
                            <expan>πρὸς</expan> ΑΣ, οὕτως <supplied reason="lost">τὸ</supplied>
                        <unclear>ἀπ</unclear><supplied reason="lost">ὸ</supplied></seg>
                    <seg n="3" type="line" facs="z58v2_03">ΒΔ <expan>πρὸς</expan> τὸ ἀπὸ ΞΟ. ἴση δὲ
                        ἡ ΔΑ τῆι</seg>
                    <seg n="4" type="line" facs="z58v2_04">ΑΘ· ὡς <expan>ἄρα</expan> ἡ ΘΑ <expan>
                            <unclear>πρὸς</unclear>
                        </expan> ΑΣ, <seg type="word">
                            <expan>οὕ</expan>
                            <expan>τως</expan>
                        </seg> τὸ ἀπὸ ΜΣ</seg>
                    <seg n="5" type="line" facs="z58v2_05"><expan>πρὸς</expan> τὸ ἀπὸ ΣΞ. ὡς δὲ τὸ
                        ἀπὸ ΜΣ <expan>πρὸς</expan> τὸ</seg>
                    <seg n="6" type="line" facs="z58v2_06">ἀπὸ ΣΞ, οὕτως ὁ κύκλος ὁ <supplied
                            reason="lost">ἐν</supplied> τῶι <supplied reason="lost">
                            <w part="I">κυ</w>
                        </supplied></seg>
                    <seg n="7" type="line" facs="z58v2_07"><w part="F">λίνδρωι</w>, οὗ διάμετρος ἡ
                        ΜΝ, <expan>πρὸς</expan></seg>
                    <seg n="8" type="line" facs="z58v2_08">τὸν κύκλον τὸν ἐν τῶι τμήμα<supplied
                            reason="lost">τι</supplied></seg>
                    <seg n="9" type="line" facs="z58v2_09">τοῦ ὀρθογωνίου κωνοειδοῦς, οὗ</seg>
                    <seg n="10" type="line" facs="z58v2_10">διάμετρος ἡ ΞΣ· <expan>ἔστιν</expan> ἄρα
                        ὡς ἡ ΘΑ <expan>πρὸς</expan></seg>
                    <seg n="11" type="line" facs="z58v2_11">ΑΣ, οὕτως ὁ κύκλος, οὗ διάμετρος</seg>
                    <seg n="12" type="line" facs="z58v2_12">ἡ ΜΝ, <expan>πρὸς</expan> τὸν <choice>
                            <abbr>κύκλο</abbr>
                            <expan>κύκλον</expan>
                        </choice>, οὗ <supplied reason="lost">δι</supplied>άμετρος</seg>
                    <seg n="13" type="line" facs="z58v2_13">ἡ ΞΟ. ἰσόρροπος <expan>ἄρα</expan> ὁ
                            <seg type="word">
                            <choice>
                                <abbr>κύκλ</abbr>
                                <expan>κύκλ<supplied reason="lost">ος</supplied></expan>
                            </choice>
                        </seg>, οὗ <choice>
                            <abbr>μετρ</abbr>
                            <expan>διάμετρος</expan>
                        </choice></seg>
                    <seg n="14" type="line" facs="z58v2_14">ἡ ΜΝ, ὁ ἐν τῶι κυλίνδρωι
                        <expan>πρὸς</expan> τὸ</seg>
                    <seg n="15" type="line" facs="z58v2_15">Α σημεῖον αὐτοῦ μένων τῶι <supplied
                            reason="lost">
                            <w part="I">κύ</w>
                        </supplied></seg>
                    <seg n="16" type="line" facs="z58v2_16"><w part="F">κλωι</w>, οὗ διάμετρος ἡ ΞΟ,
                            <seg type="suppliedword">μ<unclear>ε</unclear>τε<supplied reason="lost"
                                >νε</supplied></seg></seg>
                    <seg n="17" type="line" facs="z58v2_17"><seg type="wordend">χθέντι</seg> καὶ
                        τεθέντι <unclear>ἐ</unclear>πὶ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> ζυγοῦ</seg>
                    <seg n="18" type="line" facs="z58v2_18">κατὰ τὸ Θ, ὥστε κέντρον αὐτοῦ</seg>
                    <seg n="19" type="line" facs="z58v2_19">εἶναι τοῦ βάρους τὸ Θ. ἐπεὶ οὖν</seg>
                </seg>
                <seg n="63r2" type="folio">
                    <seg n="1" type="line" facs="z63r2_01">τοῦ μὲν <choice>
                            <abbr>κύκλ</abbr>
                            <expan>κύκλου</expan>
                        </choice>, οὗ <expan>ἐστιν</expan> ἡ <choice>
                            <abbr>δμετρος</abbr>
                            <expan>διάμετρος</expan>
                        </choice> ἡ</seg>
                    <seg n="2" type="line" facs="z63r2_02">ΜΝ, κέντρον τοῦ βάρους τὸ Σ, <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice> δὲ</seg>
                    <seg n="3" type="line" facs="z63r2_03">κύκλου, οὗ δι<supplied reason="lost"
                        >ά</supplied>μετρος ἡ ΞΟ, <w part="I">μετε</w></seg>
                    <seg n="4" type="line" facs="z63r2_04"><w part="F">νηνεγμένου</w>
                        <supplied reason="lost">κέ</supplied>ντρον τοῦ βάρους</seg>
                    <seg n="5" type="line" facs="z63r2_05">τὸ Θ, καὶ ἀντι<supplied reason="lost"
                        >π</supplied>επονθότως τὸν</seg>
                    <seg n="6" type="line" facs="z63r2_06">αὐτὸν ἔχει λό<supplied reason="lost"
                        >γο</supplied>ν ἡ ΘΑ <expan>πρὸς</expan> ΑΣ, ὃν</seg>
                    <seg n="7" type="line" facs="z63r2_07">ὁ κύκλος, οὗ διάμετρος ἡ ΜΝ,
                        <expan>πρὸς</expan></seg>
                    <seg n="8" type="line" facs="z63r2_08">τὸν κύκλον, οὗ διάμετρος ἡ ΞΟ. <w
                            part="I">ὁ</w></seg>
                    <seg n="9" type="line" facs="z63r2_09"><w part="F">μοίως</w> δὲ δειχθήσεται, καὶ
                        ἐν ἄλλη</seg>
                    <seg n="10" type="line" facs="z63r2_10">τις ἀχθῆ ἐν τῶι ΕΓ <w part="I"
                        >παραλληλο</w></seg>
                    <seg n="11" type="line" facs="z63r2_11"><w part="F">γράμμωι</w> παρὰ τὴν ΒΓ, καὶ
                        ἀπὸ</seg>
                    <seg n="12" type="line" facs="z63r2_12">τῆς ἀχθείσης ἐπίπεδον <w part="I"
                        >ἀνα</w></seg>
                    <seg n="13" type="line" facs="z63r2_13"><w part="F">σταθῆι</w> ὀρθὸν
                        <expan>πρὸς</expan> τὴν ΑΘ, <expan>ὅτι</expan>
                        <w part="I">ἰσορ</w></seg>
                    <seg n="14" type="line" facs="z63r2_14"><w part="F">ροπήσει</w>
                        <expan>πρὸς</expan> τῶ Α σημείωι ὁ <w part="I">γενόμε</w></seg>
                    <seg n="15" type="line" facs="z63r2_15"><w part="F">νος</w> κύκλος ἐν τῶι
                        κυλίνδρωι <w part="I">αὐ</w></seg>
                    <seg n="16" type="line" facs="z63r2_16"><w part="F">τοῦ</w> μένων τῶι γενομένωι
                        ἐν τῶ</seg>
                    <seg n="17" type="line" facs="z63r2_17">τμήματι τοῦ ὀρθογωνίου <seg
                            type="unclearword">κωνο</seg></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
