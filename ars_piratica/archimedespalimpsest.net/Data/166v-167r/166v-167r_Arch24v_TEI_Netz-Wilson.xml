    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="166v1" type="folio">
                    <seg n="1" type="line" facs="z166v1_01">τὸ Α σημεῖον, ἴσος κύλινδρος <supplied
                            reason="lost">ὁ</supplied></seg>
                    <seg n="2" type="line" facs="z166v1_02">ΜΝ, καὶ τετμήσθω ἡ Α<unclear>Η</unclear>
                        <seg type="word"><supplied reason="lost">κ</supplied><unclear>α</unclear>τὰ</seg>
                        <supplied reason="lost">τὸ</supplied>
                        <supplied reason="lost">Φ</supplied></seg>
                    <seg n="3" type="line" facs="z166v1_03">οὕτως, ὥστε <seg type="word"
                                >τρι<unclear>π</unclear>λα<supplied reason="lost"
                                >σ</supplied>ί<supplied reason="lost"
                            >α</supplied><unclear>ν</unclear></seg> εἶναι <choice>
                            <abbr>τὴ</abbr>
                            <expan>τὴν</expan>
                        </choice>
                    </seg>
                    <seg n="4" type="line" facs="z166v1_04">ΑΦ τῆς ΦΗ· τὸ Φ <supplied reason="lost">
                            <expan>ἄρα</expan>
                        </supplied>
                        <seg type="word"><unclear>σ</unclear>ημεῖο<unclear>ν</unclear></seg>
                        <seg type="word">
                            <choice>
                                <abbr><unclear>κέ</unclear>ντρ<supplied reason="lost">ο</supplied></abbr>
                                <expan><unclear>κέ</unclear>ντρ<supplied reason="lost"
                                >ον</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="5" type="line" facs="z166v1_05">ἐστὶν τοῦ <seg type="word"
                            ><unclear>β</unclear>άρους</seg>
                        <seg type="word">το<unclear>ῦ</unclear></seg> ΕΑΖ κώνου· <seg
                            type="expandedword">
                            <choice>
                                <abbr>
                                    <unclear>τ</unclear>
                                </abbr>
                                <expan>
                                    <unclear>τ</unclear>
                                    <supplied reason="lost">οῦ</supplied>
                                </expan>
                            </choice>
                        </seg></seg>
                    <seg n="6" type="line" facs="z166v1_06"><seg type="wordend">το</seg> γὰρ
                        γράφεται. καὶ τετμήσθω</seg>
                    <seg n="7" type="line" facs="z166v1_07">δὴ ὁ ΜΝ <seg type="word"
                                >κύλινδρ<unclear>ο</unclear>ς</seg> ἐπιπέδωι <choice>
                            <abbr>π</abbr>
                            <expan>παρὰ</expan>
                        </choice>
                    </seg>
                    <seg n="8" type="line" facs="z166v1_08"><seg type="word">τ<unclear>ὴν</unclear></seg>
                        <seg type="word"><unclear>β</unclear>άσιν</seg> οὕτως, <seg type="word"
                                >ὥσ<unclear>τ</unclear>ε</seg> τὸν <unclear>Μ</unclear>
                        <seg type="unclearword">
                            <choice>
                                <abbr>κ<unclear>ύ</unclear>λι</abbr>
                                <expan>κ<unclear>ύ</unclear>λιν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="9" type="line" facs="z166v1_09"><seg type="wordend">δρον</seg>
                        ἰσορροπεῖν <seg type="word"><unclear>τ</unclear>ῶ</seg> ΕΑΖ κώνωι.</seg>
                    <seg n="10" type="line" facs="z166v1_10">ἐπεὶ <seg type="word"
                            >οὖ<unclear>ν</unclear></seg> ἰσόρροπος ὁ ΕΑΖ κῶνος</seg>
                    <seg n="11" type="line" facs="z166v1_11">καὶ τὸ ΑΒΔ τμῆμα αὐτοῦ <w part="I"
                            >μένον</w></seg>
                    <seg n="12" type="line" facs="z166v1_12"><w part="F">τα</w>
                        <seg type="word">τῶ<supplied reason="lost">ι</supplied></seg> ΕΑΖ κώνωι
                        μετενεχθέντι</seg>
                    <seg n="13" type="line" facs="z166v1_13">καὶ τεθέντι τοῦ ζυγοῦ κατὰ τὸ Θ, <w
                            part="I">ὥσ</w></seg>
                    <seg n="14" type="line" facs="z166v1_14"><w part="F">τε</w> κέντρον <seg
                            type="word">ε<unclear>ἶ</unclear>ναι</seg> αὐτοῦ τοῦ <seg type="word">
                                βάρ<expan>ου</expan><add>ς</add></seg></seg>
                    <seg n="15" type="line" facs="z166v1_15">τὸ Θ, καί ἐστιν <sic>το</sic> ΕΑΖ κώνωι <choice>
                            <abbr>ἴσ</abbr>
                            <expan>ἴσος</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z166v1_16">ὁ ΜΝ κύλινδρος, καὶ κεῖται <w part="I"
                            >ἑκά</w></seg>
                    <seg n="17" type="line" facs="z166v1_17"><w part="F">τερος</w>
                        <choice>
                            <abbr>τῶ</abbr>
                            <expan>τῶι</expan>
                        </choice> ΜΝ κυλίνδρωι κατὰ</seg>
                    <seg n="18" type="line" facs="z166v1_18">τὸ Θ, καὶ ἰσόρροπος ὁ ΜΝ <seg
                            type="suppliedword">
                            <choice>
                                <abbr>κύλι</abbr>
                                <expan>κύλιν</expan>
                            </choice>
                        </seg></seg>
                    <seg n="19" type="line" facs="z166v1_19"><seg type="wordend">
                            <unclear>δ</unclear>
                            <supplied reason="lost">ρος</supplied>
                        </seg> τωι Ε<unclear>Α</unclear>Ζ <seg type="word"><supplied reason="lost"
                            >κ</supplied>ώνωι</seg>. <seg type="word"><unclear>λ</unclear><supplied
                                reason="lost">οι</supplied>πὸς</seg>
                        <expan>ἄρα</expan> ὁ</seg>
                </seg>
                <seg n="167r1" type="folio">
                    <seg n="1" type="line" facs="z167r1_01">Ν κύλινδρος ἰσορροπεῖ τῶ ΒΑΔ</seg>
                    <seg n="2" type="line" facs="z167r1_02">τμήματι τῆς <seg type="word"
                            ><unclear>σ</unclear>φαίρας</seg> κατὰ</seg>
                    <seg n="3" type="line" facs="z167r1_03">τὸ Α σημεῖον. καὶ ἐπεί <seg type="word"
                                >ἐστι<unclear>ν</unclear></seg> ὡς τὸ</seg>
                    <seg n="4" type="line" facs="z167r1_04">ΒΑΔ τμῆμα τῆς σφαίρας <expan>πρὸς</expan>
                        <choice>
                            <abbr>τὸ</abbr>
                            <expan>τὸν</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z167r1_05">κῶνον, οὗ <seg type="word"
                            ><unclear>β</unclear>άσις</seg> ὁ περὶ <w part="I">διάμε</w></seg>
                    <seg n="6" type="line" facs="z167r1_06"><w part="F">τρον</w> τὴν ΒΔ κύκλος, <seg
                            type="word">κο<unclear>ρυ</unclear>φὴ</seg> δὲ</seg>
                    <seg n="7" type="line" facs="z167r1_07">τὸ Α σημεῖον, <seg type="word"
                                ><unclear>ο</unclear>ὕτως</seg> ἡ ΞΗ <expan>πρὸς</expan> ΗΓ· <seg
                            type="expandedword">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τοῦ</expan>
                            </choice>
                        </seg></seg>
                    <seg n="8" type="line" facs="z167r1_08"><seg type="wordend">το</seg>
                        <expan>γὰρ</expan> προσγέγραπται. ὡς δὲ ὁ ΒΑΔ</seg>
                    <seg n="9" type="line" facs="z167r1_09">κῶνος <expan>πρὸς</expan> τὸν ΕΖ κῶνον,
                        οὕτως ὁ</seg>
                    <seg n="10" type="line" facs="z167r1_10">κύκλος ὁ περὶ διάμετρον τὴν ΒΔ</seg>
                    <seg n="11" type="line" facs="z167r1_11">πρὸς <seg type="word"
                            >τ<unclear>ὸ</unclear>ν</seg> κύκλον τὸν περὶ <w part="I">διάμε</w></seg>
                    <seg n="12" type="line" facs="z167r1_12"><w part="F">τρον</w> τὴν ΕΖ, ὡς δὲ ὁ
