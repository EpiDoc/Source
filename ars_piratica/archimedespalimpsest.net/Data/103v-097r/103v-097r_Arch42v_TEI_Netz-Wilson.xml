                            <choice>
                                <abbr>τ</abbr>
                                <expan>τ<supplied reason="lost">οῖς</supplied></expan>
                            </choice>
                        </seg></seg>
                    <seg n="10" type="line" facs="z97r1_10">ἀπὸ τᾶν τῶι ἴσωι ἀλλαλᾶν <seg
                            type="clearword">ὑπερ</seg></seg>
                    <seg n="11" type="line" facs="z97r1_11"><seg type="wordend">εχουσᾶν</seg> χωρὶς
                        τοῦ ὑπὸ τᾶς <seg type="clearword">μεγί</seg></seg>
                    <seg n="12" type="line" facs="z97r1_12"><seg type="wordend">στας</seg> ἴσον τὸ
                        ἐγγεγραμμένον <seg type="clearword">σχᾶ</seg></seg>
                    <seg n="13" type="line" facs="z97r1_13"><seg type="wordend">μα</seg> ἐν τῶι
                        χωρίωι, τοῖς δὲ ἑτέροις</seg>
                    <seg n="14" type="line" facs="z97r1_14">ὁ κύκλος· <seg type="word">μεί<supplied
                                reason="lost">ζ</supplied>ονα</seg> οὖν <seg type="word">
                                λόγο<supplied reason="lost">ν</supplied>
                        </seg> ἔχει</seg>
                    <seg n="15" type="line" facs="z97r1_15">ὁ ΑΗΖΙ κύκλος ποτὶ τὸ <seg
                            type="clearword">ἐγγεγραμ</seg></seg>
                    <seg n="16" type="line" facs="z97r1_16"><seg type="wordend">μένον</seg> σχᾶμα ἢ
                        τὸ τετράγωνον</seg>
                    <seg n="17" type="line" facs="z97r1_17">τὸ ἀπὸ <seg type="word"
                            >τᾶ<unclear>ς</unclear>
                        </seg> ΘΑ <expan>πρὸς</expan> τὸ ὑπὸ τῶν ΘΑ</seg>
