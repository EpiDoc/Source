    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="47r1" type="folio">
                    <seg n="1" type="line" facs="z47r1_01"><w part="F">ματος</w>, τομὴν <seg
                            type="unclearword">παραλληλόγραμ</seg></seg>
                    <seg n="2" type="line" facs="z47r1_02"><seg type="wordend"
                            ><unclear>μ</unclear>ον</seg>, οὗ ἔσται <seg type="word"
                            ><unclear>μί</unclear>α</seg> μὲν πλευρὰ <sic>
                            <seg type="clearword">ἴ</seg>
                        </sic></seg>
                    <seg n="3" type="line" facs="z47r1_03"><sic>
                            <seg type="wordend">σηι</seg>
                        </sic> τῆι ΣΤ, ἡ δὲ <sic>ἑτέραι</sic> τῆι τοῦ <w part="I">κυ</w></seg>
                    <seg n="4" type="line" facs="z47r1_04"><w part="F">λίνδρου</w> πλευρᾶι, ποιήσει
                        δὲ <choice>
                            <abbr>κ</abbr>
                            <expan>καὶ</expan>
                        </choice></seg>
                    <seg n="5" type="line" facs="z47r1_05">ἐν τῶι τμήματι τῶι <w part="I"
                        >ἀποτετμη</w></seg>
                    <seg n="6" type="line" facs="z47r1_06"><w part="F">μένωι</w> ἀπὸ τοῦ κυλίνδρου
                        τὸ ΜΗ</seg>
                    <seg n="7" type="line" facs="z47r1_07">παραλληλόγραμμον, οὗ <expan>ἐστιν</expan>
                        ἡ μὲν</seg>
                    <seg n="8" type="line" facs="z47r1_08">ἑτέρα πλευρὰ ἴση τῆι ΝΥ·</seg>
                </seg>
                <seg n="47r1" type="folio">
                    <seg n="8" type="line" facs="z47r1_08">ἔστω δ’</seg>
                    <seg n="9" type="line" facs="z47r1_09"><seg type="word"
                        >οὕτ<unclear>ω</unclear>ς</seg> ἡ ΝΥ ἠγμένη ἐν τῶι ΔΕ</seg>
                    <seg n="10" type="line" facs="z47r1_10">παραλληλογράμμωι <w part="I">παράλλη</w></seg>
                    <seg n="11" type="line" facs="z47r1_11"><w part="F">λος</w> οὖσα τῆι ΒΩ ἴσην <w
                            part="I">ἀπολαμ</w></seg>
                    <seg n="12" type="line" facs="z47r1_12"><w part="F">βάνουσα</w> τὴν
                        <unclear>Σ</unclear>Ι τῆι ΠΧ. καὶ ἐπεὶ</seg>
                    <seg n="13" type="line" facs="z47r1_13">παραλληλόγραμμόν <expan>ἐστι</expan> τὸ
                        ΕΓ, <expan>καὶ</expan></seg>
                    <seg n="14" type="line" facs="z47r1_14">παράλληλος ἡ Ν<unclear>Ι</unclear> τῆι
                        ΘΓ, καὶ</seg>
                    <seg n="15" type="line" facs="z47r1_15">δὴ ἠγμέναι <sic>
                            <expan>ἐστὶν</expan>
                        </sic> καἱ ΕΘ ΘΒ, <expan>ἔστιν</expan> ὡς</seg>
                    <seg n="16" type="line" facs="z47r1_16">ἡ ΕΘ <expan>πρὸς</expan> ΘΙ,
                            <expan>οὕτως</expan> ὡς ἡ ΩΓ <expan>πρὸς</expan> ΓΝ, <seg
                            type="expandedword">του</seg></seg>
                    <seg n="17" type="line" facs="z47r1_17"><seg type="wordend">
                            <choice>
                                <abbr>τ</abbr>
                                <expan>τέστιν</expan>
                            </choice>
                        </seg> ἡ ΒΩ <expan>πρὸς</expan> ΥΝ. ὡς δὲ ἡ ΒΩ <expan>πρὸς</expan> ΥΝ,</seg>
                    <seg n="18" type="line" facs="z47r1_18"><expan>οὕτως</expan> τὸ παράλληλον τὸ <choice>
                            <abbr>γενόμενο</abbr>
                            <expan>γενόμενον</expan>
                        </choice></seg>
                    <seg n="19" type="line" facs="z47r1_19">ἐν τῶι <seg type="word"><supplied
                                reason="lost">ἡμ</supplied>ικυλινδρίωι</seg>
                        <expan>πρὸς</expan> τὸ <seg type="unclearword"><unclear>γ</unclear>ε</seg>
                    </seg>
                </seg>
                <seg n="42v1" type="folio">
                    <seg n="1" type="line" facs="z42v1_01"><seg type="wordend">νόμενον</seg> ἐν τῶι
                            <w part="I">ἀποτμήμα</w></seg>
                    <seg n="2" type="line" facs="z42v1_02"><w part="F">τι</w> τῶι ἀποτμηθέντι ἀπὸ <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="3" type="line" facs="z42v1_03">κυλίνδρου· ἀμφότερον γὰρ <choice>
                            <abbr>τ</abbr>
                            <expan>τῶν</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z42v1_04">παραλληλογράμμων ἡ αὐτὴ</seg>
                    <seg n="5" type="line" facs="z42v1_05">πλευρά <seg type="word">ἐστ<supplied
                                reason="lost">ι</supplied>ν</seg> ἡ <unclear>Ο</unclear>Τ· καὶ ἴση
                            <expan>ἐστὶν</expan> ἡ ΕΘ</seg>
                    <seg n="6" type="line" facs="z42v1_06">τῆι ΘΠ, ἡ δὲ <unclear>Ι</unclear>Θ τῆι
                        ΧΘ· καὶ ἐπεὶ</seg>
                    <seg n="7" type="line" facs="z42v1_07">ἴση <expan>ἐστὶν</expan> ἡ ΠΘ τῆι ΘΞ, ὡς
                            <expan>ἄρα</expan> ἡ Θ<gap extent="1"/></seg>
                    <seg n="8" type="line" facs="z42v1_08"><expan>πρὸς</expan> ΘΧ, οὕτως τὸ
                        γενόμενον <sic>
                            <seg type="suppliedword">παραλ</seg>
                        </sic></seg>
                    <seg n="9" type="line" facs="z42v1_09"><sic>
                            <seg type="wordend">λ<supplied reason="lost"
                                >ηλόγρ</supplied><unclear>α</unclear>μμον</seg>
                        </sic> ἔστω <choice>
                            <abbr>ἡμικυλίνδρι</abbr>
                            <expan>ἡμικυλίνδριον</expan>
                        </choice></seg>
                    <seg n="10" type="line" facs="z42v1_10"><expan>πρὸς</expan> τὸ <seg type="word"
                                >γε<unclear>ν</unclear>ό<unclear>μ</unclear>ε<unclear>ν</unclear>ο<unclear>ν</unclear></seg>
                        <seg type="word">ἐ<unclear>ν</unclear></seg>
                        <unclear>τῶι</unclear>
                        <seg type="suppliedword">ἀποτμή</seg></seg>
                    <seg n="11" type="line" facs="z42v1_11"><seg type="wordend">μα<supplied
                                reason="lost">τ</supplied>ι</seg> ἀπὸ τοῦ κυλίνδρου.</seg>
                </seg>
                <seg n="42v1" type="folio">
                    <seg n="11" type="line" facs="z42v1_11">
                        <w part="I">νοείσ</w>
                    </seg>
                    <seg n="12" type="line" facs="z42v1_12"><w part="F">θω</w> μετακείμενον τῶι ἐν
                        τῶι</seg>
                    <seg n="13" type="line" facs="z42v1_13">
                        <seg type="word">τμήματ<supplied reason="lost">ι</supplied></seg>
                        <seg type="word">
                            <choice>
                                <abbr>παραλ<unclear>λ</unclear>ηλόγραμμ</abbr>
                                <expan>παραλ<unclear>λ</unclear>ηλόγραμμον</expan>
                            </choice>
                        </seg>
                    </seg>
                    <seg n="14" type="line" facs="z42v1_14">καὶ κείμενον κατὰ τὸ
                        <unclear>Ξ</unclear>, ὥστε</seg>
                    <seg n="15" type="line" facs="z42v1_15">κέντρον <expan>εἶναι</expan>
                        <choice>
                            <abbr>αὐτ</abbr>
                            <expan>αὐτοῦ</expan>
                        </choice> τοῦ βάρους τὸ Ξ,</seg>
                    <seg n="16" type="line" facs="z42v1_16">καί <expan>ἐστι</expan>
                        <seg type="word">νο<unclear>εί</unclear>σθω</seg>
                        <seg type="word">
                            <choice>
                                <abbr><supplied reason="lost">ζυ</supplied>γ</abbr>
                                <expan><supplied reason="lost">ζυ</supplied>γὸς</expan>
                            </choice>
                        </seg> ἡ ΠΞ, μέσον</seg>
                    <seg n="17" type="line" facs="z42v1_17">
                        <supplied reason="lost">δὲ</supplied>
                        <seg type="word">α<supplied reason="lost">ὐ</supplied>το<supplied
                                reason="lost">ῦ</supplied></seg>
                        <supplied reason="lost">τὸ</supplied> Θ· <seg type="word"
                            >ἰσορροπ<unclear>εῖ</unclear></seg> δὴ <expan>περὶ</expan></seg>
                </seg>
                <seg n="47r2" type="folio">
                    <seg n="1" type="line" facs="z47r2_01">τὸ Θ σημεῖον τὸ <w part="I"
                        >παραλληλόγραμ</w></seg>
                    <seg n="2" type="line" facs="z47r2_02"><w part="F">μον</w> τὸ ἐν τῶι <seg
                            type="word">ἡμικυλινδρίω<unclear>ι</unclear></seg> τὸ <w part="I">γε</w></seg>
                    <seg n="3" type="line" facs="z47r2_03"><w part="F">νόμενον</w> τῶι <choice>
                            <abbr>παραλληλόγραμμ</abbr>
                            <expan>παραλληλόγραμμον</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z47r2_04">τῶι γενομένωι τῶι <seg
                            type="suppliedword">ἀποτμή<supplied reason="lost">μ</supplied>α</seg></seg>
                    <seg n="5" type="line" facs="z47r2_05"><seg type="wordend">τι</seg> ἀπὸ τοῦ
                        κυλίνδρου <w part="I">μετενεχθέν</w></seg>
                    <seg n="6" type="line" facs="z47r2_06"><w part="F">τι</w> καὶ τεθέντι τοῦ ζυγοῦ
                            <seg type="word">κ<unclear>ατ</unclear>ὰ</seg>
                        <seg type="word"><unclear>τ</unclear>ὸ</seg></seg>
                    <seg n="7" type="line" facs="z47r2_07">Ξ οὕτως, <sic>ἔσται</sic> κέντρον
                            <expan>εἶναι</expan> τοῦ αὐτοῦ</seg>
                    <seg n="8" type="line" facs="z47r2_08">βάρους τὸ Ξ σημεῖον. καὶ ἐπεί ἐστι</seg>
                    <seg n="9" type="line" facs="z47r2_09">τοῦ μὲν παραλληλογράμμου τοῦ</seg>
                    <seg n="10" type="line" facs="z47r2_10">γενομένου ἐν τῶι ἡμικυλινδρίωι</seg>
                    <seg n="11" type="line" facs="z47r2_11">κέντρον τοῦ βάρους τὸ Χ, τοῦ δὲ <w
                            part="I">πα</w></seg>
                    <seg n="12" type="line" facs="z47r2_12"><w part="F">ραλληλογράμμου</w> τοῦ
                        γενομένου</seg>
                    <seg n="13" type="line" facs="z47r2_13">ἐν τῶι τμήματι τῶι <w part="I"
                        >ἀποτμηθέν</w></seg>
                    <seg n="14" type="line" facs="z47r2_14"><w part="F">τι</w> μετενηνεγμένου
                        κέντρον τοῦ</seg>
                    <seg n="15" type="line" facs="z47r2_15">βάρους τὸ Ξ, καὶ τὸν αὐτὸν ἔχει <choice>
                            <abbr>λό</abbr>
                            <expan>λόγον</expan>
                        </choice></seg>
                    <seg n="16" type="line" facs="z47r2_16">ἡ ΞΘ <expan>πρὸς</expan> ΘΧ, ὃν τὸ <w
                            part="I">παραλληλόγραμ</w></seg>
                    <seg n="17" type="line" facs="z47r2_17"><w part="F">μον</w>, οὗ
                        <sic>εἴπαμεν</sic> κέντρον εἶναι</seg>
                    <seg n="18" type="line" facs="z47r2_18">τοῦ βάρους τὸ Χ, <expan>πρὸς</expan> τὸ
                            <w part="I">παραλληλό</w></seg>
                    <seg n="19" type="line" facs="z47r2_19"><w part="F">γραμμον</w>, οὗ
                        <sic>εἴπαμεν</sic> κέντρον</seg>
                </seg>
                <seg n="42v2" type="folio">
                    <seg n="1" type="line" facs="z42v2_01">εἶναι τοῦ βάρους τὸ <unclear>Ξ</unclear>,
                            <seg type="word">ὁμο<supplied reason="lost">ίω</supplied>ς</seg> δὴ</seg>
                    <seg n="2" type="line" facs="z42v2_02">δειχθήσεται, <expan>ὅτι</expan> καὶ ἐὰν
                        ἄλλη τις</seg>
                    <seg n="3" type="line" facs="z42v2_03">ἀχθῆι ἐν τῶι ΟΠΡ ἡμικυκλίωι
                        <expan>πρὸς</expan></seg>
                    <seg n="4" type="line" facs="z42v2_04">ὀρθὰς τῆι ΠΘ, καὶ ἀπὸ τῆς <w part="I"
                        >ἀ</w></seg>
                    <seg n="5" type="line" facs="z42v2_05"><w part="F">χθείσης</w> ἐπίπεδον ἀνασταθῆ</seg>
                    <seg n="6" type="line" facs="z42v2_06"><seg type="word"><unclear>ὀ</unclear>ρθὸν</seg>
                        <expan>πρὸς</expan> τὴν ΠΘ καὶ ἐκβληθῆι <seg type="suppliedword">
                            <supplied reason="lost">ἐ</supplied>
                        </seg></seg>
                    <seg n="7" type="line" facs="z42v2_07"><seg type="wordend">
                            <supplied reason="lost">φ᾽</supplied>
                        </seg> ἑκάτερα τοῦ ἐπιπέδου τοῦ <seg type="suppliedword">
                            <seg type="clearword">ἐ</seg>
                        </seg></seg>
                    <seg n="8" type="line" facs="z42v2_08"><seg type="wordend">ν</seg> ὧι
                            <expan>ἐστιν</expan> ὁ ΞΟ ΠΡ κύκλος, <expan>ὅτι</expan> τὸ <w part="I"
                            >γινόμε</w></seg>
                    <seg n="9" type="line" facs="z42v2_09"><w part="F">νον</w> παραλληλόγραμμον ἐν
                        τῶι</seg>
                    <seg n="10" type="line" facs="z42v2_10"><seg type="word"
                                >ἡμ<unclear>ι</unclear>κυ<supplied reason="lost"
                                >λι</supplied><unclear>ν</unclear>δρίωι</seg> ἰσόρροπον περὶ</seg>
                    <seg n="11" type="line" facs="z42v2_11">τὸ Θ σημεῖον <seg type="word"
                                >αὐτ<supplied reason="lost">ο</supplied><unclear>ῦ</unclear></seg>
                        μένον τῶι <w part="I">πα</w></seg>
                    <seg n="12" type="line" facs="z42v2_12"><w part="F">ραλληλογράμμωι</w> τῶι
                        γενομένωι</seg>
                    <seg n="13" type="line" facs="z42v2_13">ἐν τῶι τμήματι τῶι <seg
                            type="unclearword">ἀποτμ<unclear>η</unclear>θέν</seg></seg>
                    <seg n="14" type="line" facs="z42v2_14"><seg type="wordend">τι</seg> ἀπὸ τοῦ
                            <seg type="word">κυλίνδρο<unclear>υ</unclear></seg>
                        <seg type="suppliedword">μετενεχθέν</seg></seg>
                    <seg n="15" type="line" facs="z42v2_15"><seg type="wordend">
                            <supplied reason="lost">τι</supplied>
                        </seg>
                        <seg type="word"><unclear>κ</unclear>αὶ</seg> τεθέντι τοῦ ζυγοῦ κατὰ τὸ</seg>
                    <seg n="16" type="line" facs="z42v2_16">Ξ οὕτως, ὥστε κέντρον εἶναι <seg
                            type="word">αὐτ<unclear>ο</unclear>ῦ</seg></seg>
                    <seg n="17" type="line" facs="z42v2_17"><seg type="word"
                        >το<unclear>ῦ</unclear></seg> βάρους τὸ Ξ <seg type="word"
                            >σημεῖ<unclear>ον</unclear></seg>. καὶ <sic>
                            <seg type="word"><supplied reason="lost">πᾶ</supplied>ν</seg>
                        </sic></seg>
                </seg>
            </ab>
        </body>
    </text>
</TEI>
