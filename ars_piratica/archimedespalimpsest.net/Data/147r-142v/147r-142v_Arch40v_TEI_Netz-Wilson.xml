    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="147r1" type="folio">
                    <seg n="1" type="line" facs="z147r1_01">περιφορᾶς, ὁ δὲ ΑΝ ΖΗΙ κύκλος</seg>
                    <seg n="2" type="line" facs="z147r1_02">πρῶτος, οὗ τρίτον μέρος ἔστω <seg
                            type="clearword">ἐ</seg></seg>
                    <seg n="3" type="line" facs="z147r1_03"><seg type="wordend">ν</seg> ὧι Ϟ κύκλος.
                        δεικτέον, ὅτι ἴσον <expan>ἐστὶ</expan></seg>
                    <seg n="4" type="line" facs="z147r1_04">τὸ προειρημένον <seg type="word"
                                >χωρίο<supplied reason="lost">ν</supplied>
                        </seg> τῶι Ϟ</seg>
                    <seg n="5" type="line" facs="z147r1_05">κύκλωι.</seg>
                </seg>
                <seg n="147r1" type="folio">
                    <seg n="5" type="line" facs="z147r1_05">εἰ γὰρ μή, ἤτοι μεῖζόν ἐστιν</seg>
                    <seg n="6" type="line" facs="z147r1_06">ἢ ἔλασσον. ἔστω πρότερον, εἰ <seg
                            type="clearword">δυ</seg></seg>
                    <seg n="7" type="line" facs="z147r1_07"><seg type="wordend">νατόν</seg>,
                        ἔλασσον. δυνατὸν δή <choice>
                            <abbr>ἐστι</abbr>
                            <expan>ἐστιν</expan>
                        </choice></seg>
                    <seg n="8" type="line" facs="z147r1_08">περὶ τὸ χωρίον τὸ <seg type="word">
                            <choice>
                                <abbr>περιεχόμενο</abbr>
                                <expan>πε<supplied reason="lost">ρι</supplied>εχόμενον</expan>
                            </choice>
                        </seg></seg>
                    <seg n="9" type="line" facs="z147r1_09">ὑπό τε τᾶς ΑΒΓΔΕΘ ἕλικος <seg
                            type="word"> κ<unclear>αὶ</unclear>
                        </seg></seg>
                    <seg n="10" type="line" facs="z147r1_10"><supplied reason="lost">τᾶς</supplied>
                        <supplied reason="lost">Α</supplied>Θ <seg type="word">
                            <supplied reason="lost">εὐ</supplied>θείας</seg>
                        <seg type="word">π<supplied reason="lost">εριγρά</supplied>ψαι </seg></seg>
                    <seg n="11" type="line" facs="z147r1_11"><supplied reason="lost">σχᾶμα</supplied>
                        <seg type="word">ἐπ<supplied reason="lost">ί</supplied>πεδον</seg> ἐξ <seg
                            type="word">ὁμοί<supplied reason="lost">ων</supplied>
                        </seg>
                        <seg type="suppliedword">τ<supplied reason="lost">ο</supplied></seg></seg>
                    <seg n="12" type="line" facs="z147r1_12"><seg type="wordend">
                            <supplied reason="lost">μέων,</supplied>
                        </seg>
                        <supplied reason="lost">ὥστε</supplied> τὸ <seg type="word"> περ<supplied
                                reason="lost">ι</supplied>γρ<supplied reason="lost">αφὲν</supplied>
                        </seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">σχᾶ</supplied>
                        </seg></seg>
                    <seg n="13" type="line" facs="z147r1_13"><seg type="wordend">μα</seg> μείζονι
                        μὲν <seg type="word">τ<supplied reason="lost">ο</supplied>ῦ</seg>
                        <seg type="word">χω<supplied reason="lost">ρίο</supplied>υ</seg>
                        <seg type="suppliedword">
                            <supplied reason="lost">ἐλάσ</supplied>
                        </seg></seg>
                    <seg n="14" type="line" facs="z147r1_14"><seg type="wordend">σων</seg> τᾶς
                        ὑπεροχᾶς, <supplied reason="lost">ἇ</supplied>
                        <seg type="suppliedword">ὑπερέ</seg></seg>
                    <seg n="15" type="line" facs="z147r1_15"><seg type="wordend"><supplied
                                reason="lost">χ</supplied>ει</seg> ὁ Ϟ κύκλος τοῦ εἰρημένου <seg
                            type="clearword">χω</seg></seg>
                    <seg n="16" type="line" facs="z147r1_16"><seg type="wordend">ρίου</seg>.
                        περιγεγράφθω δή, καὶ ἔστω</seg>
                    <seg n="17" type="line" facs="z147r1_17">τῶν τομέων, ἐξ ὧν σύγκειται τὸ</seg>
                </seg>
                <seg n="142v1" type="folio">
                    <seg n="1" type="line" facs="z142v1_01">εἰρημένον σχᾶμα, μέγιστος μὲν</seg>
                    <seg n="2" type="line" facs="z142v1_02">ὁ ΘΑΚ, ἐλάχιστος δὲ ὁ ΘΕΟ· δῆλον
                            <expan>οὖν</expan>,</seg>
                    <seg n="3" type="line" facs="z142v1_03"><supplied reason="lost">
                            <expan>ὅτι</expan>
                        </supplied> τὸ περιγεγραμμένον σχᾶμα</seg>
                    <seg n="4" type="line" facs="z142v1_04">ἔλασσόν ἐστι τοῦ Ϟ κύκλου. <seg
                            type="suppliedword">ἐκβε</seg></seg>
                    <seg n="5" type="line" facs="z142v1_05"><seg type="wordend"><supplied
                                reason="lost">βλήσ</supplied>θωσαν</seg> δὲ εὐθεῖαι
                        <expan>πρὸς</expan> τὸ Θ <seg type="suppliedword">ποι</seg></seg>
                    <seg n="6" type="line" facs="z142v1_06"><seg type="wordend"><supplied
                                reason="lost">οῦσ</supplied>αι</seg> τὰς ἴσας γωνίας, ἔστ’ ἂν <seg
                            type="suppliedword">πο</seg></seg>
                    <seg n="7" type="line" facs="z142v1_07"><seg type="wordend">
                            <supplied reason="lost">τὶ</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">τ</supplied>ὰν</seg> τοῦ κύκλου περιφέρειαν</seg>
                    <seg n="8" type="line" facs="z142v1_08">πέσωντι· ἐντὶ δή τινες γραμμαὶ</seg>
                    <seg n="9" type="line" facs="z142v1_09">ἀπὸ <seg type="word">
                            <supplied reason="lost">τ</supplied>οῦ</seg> Θ ποτὶ τὰν ἕλικα <seg
                            type="clearword">ποτι</seg></seg>
                    <seg n="10" type="line" facs="z142v1_10"><seg type="wordend">πίπτουσαι</seg> τῶι
                        ἴσωι ἀλλαλᾶν</seg>
                    <seg n="11" type="line" facs="z142v1_11"><seg type="word">
                            <unclear>ὑπε</unclear>ρέχουσα<supplied reason="lost">ι</supplied>
                        </seg>, <seg type="word">ὧ<supplied reason="lost">ν</supplied>
                        </seg> ἐστι μείζων <expan>μὲν</expan></seg>
                    <seg n="12" type="line" facs="z142v1_12"><unclear>ἁ</unclear> ΘΑ, ἐλάσσων δὲ ἁ
                        ΘΕ, καὶ ἁ <seg type="unclearword">ἐλα</seg></seg>
                    <seg n="13" type="line" facs="z142v1_13"><seg type="wordend"
                            >χί<unclear>στ</unclear>α</seg>
                        <seg type="word">
                            <supplied reason="lost">ἴ</supplied><unclear>σ</unclear>α</seg> τᾶι
                        ὑπεροχᾶι, ἐντὶ δὲ</seg>
                    <seg n="14" type="line" facs="z142v1_14"><expan>καὶ</expan> ἄλλαι τινὲς γραμμαὶ
                            <seg type="word">ἀπ<unclear>ὸ</unclear>
                        </seg>
                        <choice>
                            <abbr>τ</abbr>
                            <expan>τοῦ</expan>
                        </choice></seg>
                    <seg n="15" type="line" facs="z142v1_15">Θ ποτὶ τὰν περιφέρειαν τοῦ</seg>
                    <seg n="16" type="line" facs="z142v1_16">κύκλου <seg type="word">
                                ποτι<unclear>π</unclear>ίπ<unclear>τ</unclear>ουσ<unclear>α</unclear>ι
                        </seg> τῶι <supplied reason="lost">μὲν</supplied></seg>
                    <seg n="17" type="line" facs="z142v1_17"><seg type="word">πλ<supplied
                                reason="lost">ή</supplied>θει</seg> ἴσαι <seg type="word">τ<supplied
                                reason="lost">αύται</supplied>ς</seg>, τῶ δὲ <seg
                            type="suppliedword">μ<unclear>ε</unclear></seg></seg>
                </seg>
                <seg n="147r2" type="folio">
                    <seg n="1" type="line" facs="z147r2_01"><seg type="wordend"><supplied
                                reason="lost">γ</supplied>έθει</seg> ἑκάστα ἴσα τᾶ μεγίστα,</seg>
                    <seg n="2" type="line" facs="z147r2_02"><expan>καὶ</expan> ἀναγέγραπται ἀπὸ
                        πασᾶν</seg>
                    <seg n="3" type="line" facs="z147r2_03">ὁμοῖοι τομέες, ἀπό τε τᾶν τῶι <seg
                            type="clearword">ἴ</seg></seg>
                    <seg n="4" type="line" facs="z147r2_04"><seg type="wordend">σω</seg> ἀλλαλᾶν
                        ὑπερεχουσᾶν <expan>καὶ</expan></seg>
                    <seg n="5" type="line" facs="z147r2_05">ἀπὸ τᾶν ἰσᾶν ἀλλάλαις τε καὶ</seg>
                    <seg n="6" type="line" facs="z147r2_06">τᾶ μεγίστα· οἱ ἄρα τομέες οἱ <seg
                            type="suppliedword">ἀ</seg></seg>
                    <seg n="7" type="line" facs="z147r2_07"><seg type="wordend">
                            <supplied reason="lost">πὸ</supplied>
                        </seg> τᾶν ἰσᾶν τᾶ μεγίστα <seg type="clearword">ἐλάσ</seg></seg>
                    <seg n="8" type="line" facs="z147r2_08"><seg type="wordend">σονές</seg> ἐντι ἢ
                        τριπλάσιοι τῶν</seg>
                    <seg n="9" type="line" facs="z147r2_09"><seg type="word">
                            <unclear>τ</unclear>ομέων</seg> τῶν ἀπὸ τᾶν τῶι ἴσωι</seg>
                    <seg n="10" type="line" facs="z147r2_10">ἀλλαλᾶν ὑπερεχουσᾶν· <choice>
                            <abbr>δέδεικτ</abbr>
                            <expan>δέδεικται</expan>
                        </choice></seg>
                    <seg n="11" type="line" facs="z147r2_11">γὰρ τοῦτο. <expan>εἰσὶν</expan> δὲ οἱ
                        μὲν τομέες οἱ</seg>
                    <seg n="12" type="line" facs="z147r2_12">ἀπὸ τᾶν ἰσᾶν ἀλλήλαις τε καὶ τᾶ</seg>
                    <seg n="13" type="line" facs="z147r2_13">μεγίστα ἴσοι τῶι ΑΖΗΙ κύκλωι, οἱ</seg>
                    <seg n="14" type="line" facs="z147r2_14">δὲ <seg type="word">
                            <supplied reason="lost">τ</supplied>ομ<unclear>έε</unclear><supplied
                                reason="lost">ς</supplied>
                        </seg>
                        <supplied reason="lost">οἱ</supplied> ἀπὸ τᾶν τῶι ἴσωι <seg
                            type="unclearword">ἀλ</seg></seg>
                    <seg n="15" type="line" facs="z147r2_15"><seg type="wordend"
                                >λα<unclear>λ</unclear>ᾶ<unclear>ν</unclear></seg>
                        <seg type="word">
                            <supplied reason="lost">ὑπ</supplied>ερεχουσᾶν</seg> ἴσοι τῶι</seg>
                    <seg n="16" type="line" facs="z147r2_16">προγεγραμμένωι σχήματι· <seg
                            type="clearword">ἐ</seg></seg>
                    <seg n="17" type="line" facs="z147r2_17"><seg type="wordend">λάσσων</seg> ἄρα ὁ
                        ΑΖΗΙΚ <seg type="word">κύκλ<supplied reason="lost">ος</supplied>
                        </seg></seg>
                </seg>
                <seg n="142v2" type="folio">
                    <seg n="1" type="line" facs="z142v2_01"><supplied reason="lost">τοῦ</supplied>
                        περιγεγραμμένου <seg type="suppliedword"><supplied reason="lost"
                            >σχ</supplied>άμα</seg>
                    </seg>
                    <seg n="2" type="line" facs="z142v2_02"><seg type="wordend">τος</seg> ἢ
                        τριπλασίων. τοῦ δὲ Ϟ <seg type="clearword">κύ</seg></seg>
                    <seg n="3" type="line" facs="z142v2_03"><seg type="wordend">κλου</seg>
                        τριπλασίων· ἐλάσσων <expan>ἄρα</expan></seg>
                    <seg n="4" type="line" facs="z142v2_04">ὁ Ϟ κύκλος τοῦ <seg type="clearword"
                            >περιγεγραμμέ</seg></seg>
                    <seg n="5" type="line" facs="z142v2_05"><seg type="wordend">νου</seg> σχάματος.
                        οὐκ ἔστι δέ, ἀλλὰ</seg>
                    <seg n="6" type="line" facs="z142v2_06">μείζων· οὐκ ἄρα ἐστὶ τὸ <sic>τὸ</sic>
                        <seg type="clearword">περιεχό</seg></seg>
                    <seg n="7" type="line" facs="z142v2_07"><seg type="wordend">μενον</seg> χωρίον
                        ὑπό τε τᾶς ΑΒΓ</seg>
                    <seg n="8" type="line" facs="z142v2_08">ΔΕΘ ἕλικος καὶ τᾶς ΑΘ <seg
                            type="clearword">ἐλάσ</seg></seg>
                    <seg n="9" type="line" facs="z142v2_09"><seg type="wordend">σων</seg> τοῦ
                            <supplied reason="lost">Ϟ</supplied> χωρίου. <choice>
                            <abbr>ἑξ</abbr>
                            <expan>ἑξῆς</expan>
                        </choice> τὸ <choice>
                            <abbr>ΣΧΑ</abbr>
                            <expan>ΣΧΑΜΑ</expan>
                        </choice></seg>
                </seg>
                <seg n="142v2" type="folio">
                    <seg n="10" type="line" facs="z142v2_10">Οὐδὲ τοίνυν μεῖζον. <seg type="word">
                            <supplied reason="lost">ἔ</supplied>στω</seg> γάρ, εἰ <seg
                            type="suppliedword">δυ</seg></seg>
                    <seg n="11" type="line" facs="z142v2_11"><seg type="wordend">να<supplied
                                reason="lost">τό</supplied>ν</seg>, μεῖζον. ἔστι δὴ πάλιν <seg
                            type="clearword">δυ</seg></seg>
                    <seg n="12" type="line" facs="z142v2_12"><seg type="wordend">νατὸν</seg>
                        <seg type="word">εἰ<supplied reason="lost">ς</supplied>
                        </seg> τὸ χωρίον τὸ <seg type="clearword">περιεχό</seg></seg>
                    <seg n="13" type="line" facs="z142v2_13"><seg type="wordend">μενον</seg> ὑπὸ τᾶς
                        ΑΒΓΔΘ ἕλικος</seg>
                    <seg n="14" type="line" facs="z142v2_14">καὶ τᾶς <supplied reason="lost"
                        >ΑΘ</supplied> εὐθείας ἐγγράψαι</seg>
                    <seg n="15" type="line" facs="z142v2_15">σχᾶμα, ὥστε τὸ <seg type="word"
                                >εἰρ<supplied reason="lost">ημέν</supplied>ο<supplied reason="lost"
                                >ν</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">χωρ</supplied>ίο<supplied reason="lost"
                            >ν</supplied>
                        </seg></seg>
                    <seg n="16" type="line" facs="z142v2_16">τοῦ <seg type="word">ἐγγρα<supplied
                                reason="lost">φέν</supplied>το<supplied reason="lost">ς</supplied>
                        </seg>
                        <supplied reason="lost">σχάματος</supplied>
                        <seg type="suppliedword">
                            <supplied reason="lost">μεῖ</supplied>
                        </seg></seg>
                    <seg n="17" type="line" facs="z142v2_17">
                        <seg type="wordend">
                            <supplied reason="lost">ζον</supplied>
                        </seg>
                        <supplied reason="lost">εἶμεν</supplied>
                        <seg type="word">
                            <supplied reason="lost">ἐλάσσ</supplied>ον<supplied reason="lost"
                            >ι</supplied>
                        </seg>
                        <supplied reason="lost">ἢ</supplied>
                        <supplied reason="lost">ὧ</supplied>
                        <seg type="word">
                            <supplied reason="lost">ὑπ</supplied>ερ<supplied reason="lost"
                                >έ</supplied>χ<supplied reason="lost">ει</supplied>
                        </seg>
                    </seg>
                    <seg n="18" type="line" facs="z142v2_18"><supplied reason="lost">τὸ</supplied>
                        <supplied reason="lost">εἰρημένον</supplied>
                        <supplied reason="lost">χωρίον</supplied>
                        <supplied reason="lost">τοῦ</supplied>
                        <supplied reason="lost">Ϟ</supplied>
                        <seg type="word">
                            <supplied reason="lost">κ</supplied>ύκλου</seg>.</seg>
                </seg>
            </ab>
        </body>
    </text>
