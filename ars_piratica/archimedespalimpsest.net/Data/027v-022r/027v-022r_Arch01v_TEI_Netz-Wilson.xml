    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="27v1" type="folio">
                    <seg n="1" type="line" facs="z27v1_01"><seg type="word"
                        >τ<unclear>ὰν</unclear></seg> Β συναμφοτέρου τᾶς ΑΒ ΒΕ </seg>
                    <seg n="2" type="line" facs="z27v1_02">καὶ Δ συναμφοτέρου τᾶς ΓΒ ΒΔ.</seg>
                    <seg n="3" type="line" facs="z27v1_03">ἔστι δὲ καὶ ὡς ἁ <seg type="word"
                                >Δ<supplied reason="lost">Ε</supplied></seg>
                        <seg type="word"><supplied reason="lost"
                        >π</supplied>ρὸ<unclear>ς</unclear></seg> ΕΒ, <seg type="word"
                            >οὕ<unclear>τ</unclear>ως</seg>
                    </seg>
                    <seg n="4" type="line" facs="z27v1_04"><supplied reason="lost">ἅ</supplied> τε
                            <seg type="word">Α<unclear>Γ</unclear></seg> πρὸς <seg type="word"
                                ><supplied reason="lost">Γ</supplied>Β,</seg>
                        <seg type="word"><unclear>ἐ</unclear>π<unclear>ε</unclear>ὶ</seg> καὶ κατὰ
                            <seg type="clearword">σύν</seg></seg>
                    <seg n="5" type="line" facs="z27v1_05"><seg type="wordend">θεσιν,</seg> καὶ ἁ Γ
                        τᾶς ΓΔ πρὸς <seg type="word">τὰ<unclear>ν</unclear></seg> Γ</seg>
                    <seg n="6" type="line" facs="z27v1_06">τᾶς ΔΒ <expan>καὶ</expan> ἁ Β τᾶς ΔΕ πρὸς
                        τὰν <supplied reason="lost">Β</supplied>
                    </seg>
                    <seg n="7" type="line" facs="z27v1_07">τᾶς ΕΒ· ὥστε καὶ ἁ <seg type="word"
                                >σ<supplied reason="lost">υ</supplied>γκειμένα</seg>
                    </seg>
                    <seg n="8" type="line" facs="z27v1_08">ἔκ τε τᾶς ΑΓ <expan>καὶ</expan> Γ τᾶς ΓΔ
                        καὶ Β <seg type="word">τ<supplied reason="lost"
                            >ᾶ</supplied><unclear>ς</unclear></seg>
                    </seg>
                    <seg n="9" type="line" facs="z27v1_09">ΔΕ πρὸς τὰν συγκειμέναν εἴς τε τᾶς </seg>
                    <seg n="10" type="line" facs="z27v1_10">ΓΒ καὶ Γ τᾶς ΒΔ καὶ Β τᾶς <seg
                            type="word"><supplied reason="lost">Ε</supplied>Β.</seg>
                        <seg type="clearword">ἀ</seg></seg>
                    <seg n="11" type="line" facs="z27v1_11"><seg type="wordend">νομοίως</seg> οὖν
                            <seg type="word">πάλι<supplied reason="lost">ν</supplied></seg> τῶν
                        λόγων <seg type="clearword">τε</seg></seg>
                    <seg n="12" type="line" facs="z27v1_12"><seg type="wordend">ταγμένων,</seg>
                        τουτέστιν ἐν <seg type="unclearword">τεταραγ</seg></seg>
                    <seg n="13" type="line" facs="z27v1_13"><seg type="wordend"
                            >μέν<unclear>α</unclear>ι</seg> ἀναλογίαι, δι' ἴσου τὸν <supplied
                            reason="lost">αὐτὸν</supplied>
                    </seg>
                    <seg n="14" type="line" facs="z27v1_14">ἕξει λόγον ἁ ΕΟ <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice> ΕΒ, ὃν ἁ ΑΓ </seg>
                    <seg n="15" type="line" facs="z27v1_15">μετὰ τᾶς Γ τᾶς <seg type="word"
                                >Γ<unclear>Δ</unclear></seg> καὶ Β τᾶς <sic>δὲ</sic></seg>
                    <seg n="16" type="line" facs="z27v1_16">πρὸς διπλασίαν συναμφοτέρου </seg>
                    <seg n="17" type="line" facs="z27v1_17">τᾶς ΑΒ ΒΕ μετὰ τᾶς Δ <seg
                            type="clearword">συναμφο</seg></seg>
                    <seg n="18" type="line" facs="z27v1_18"><seg type="wordend">τέρου</seg> τᾶς ΓΒ
                        ΒΔ· ὅλα οὖν ἁ ΟΒ </seg>
                    <seg n="19" type="line" facs="z27v1_19">πρὸς ΒΕ τὸν αὐτὸν ἔχει λόγον, </seg>
                    <seg n="20" type="line" facs="z27v1_20">ὃν ἁ ἴσα τᾶ τε Γ τᾶς ΑΒ μετὰ τᾶς </seg>
                    <seg n="21" type="line" facs="z27v1_21">Ϛ τᾶς ΓΒ καὶ τᾶ Γ τᾶς <seg type="word"
                                ><unclear>Β</unclear>Δ</seg> πρὸς</seg>
                </seg>
                <seg n="22r1" type="folio">
                    <seg n="1" type="line" facs="z22r1_01">τὰν Β συναμφοτέρου τᾶς ΑΒ ΒΕ </seg>
                    <seg n="2" type="line" facs="z22r1_02">μετὰ τᾶς Δ συναμφοτέρου τᾶς</seg>
                    <seg n="3" type="line" facs="z22r1_03">ΓΒ ΒΔ. <expan>καὶ</expan> ἐπεὶ αἵ τε ΕΔ
                        ΔΓ ΓΑ ἐν τῶι</seg>
                    <seg n="4" type="line" facs="z22r1_04">αὐτῶι λόγωι ἐντὶ καὶ <seg
                            type="clearword">συναμφό</seg></seg>
                    <seg n="5" type="line" facs="z22r1_05"><seg type="wordend">τερος</seg> ἑκάστα
                        τῶν ΕΒ ΒΔ ΔΒ ΒΓ ΓΒ </seg>
                    <seg n="6" type="line" facs="z22r1_06">ΒΑ <choice>
                            <abbr>εστ</abbr>
                            <expan>ἔσται</expan>
                        </choice> καὶ ὡς ἁ ΕΔ πρὸς ΔΑ, <seg type="clearword">οὕ</seg></seg>
                    <seg n="7" type="line" facs="z22r1_07"><seg type="wordend">τως</seg>
                        συναμφότερος ἁ ΕΒ ΒΔ <choice>
                            <abbr>πρ</abbr>
                            <expan>πρὸς</expan>
                        </choice>
                    </seg>
                    <seg n="8" type="line" facs="z22r1_08">συναμφότερον τὰν ΔΒ ΒΓ <seg
                            type="clearword">με</seg></seg>
                    <seg n="9" type="line" facs="z22r1_09"><seg type="wordend">τὰ</seg> τᾶς
                        συναμφοτέρου τᾶς ΓΒ</seg>
                    <seg n="10" type="line" facs="z22r1_10">ΒΑ. καὶ συνθέντι ἄρα ἐστὶν ὡς </seg>
                    <seg n="11" type="line" facs="z22r1_11">ἁ Ε πρὸς ΑΔ, οὕτως <seg type="clearword"
                            >συναμφότε</seg></seg>
                    <seg n="12" type="line" facs="z22r1_12"><seg type="wordend">ρος</seg> ἁ ΕΒ ΒΔ
                        μετὰ <seg type="clearword">συναμφοτέ</seg></seg>
                    <seg n="13" type="line" facs="z22r1_13"><seg type="wordend">ρου</seg> τᾶς ΔΒ ΒΓ
                            <expan>καὶ</expan> συναμφοτέρου</seg>
                    <seg n="14" type="line" facs="z22r1_14">τᾶς ΓΒΔ, ὅ ἐστι συναμφότερος </seg>
                    <seg n="15" type="line" facs="z22r1_15"><supplied reason="lost">ἁ</supplied>
                        <seg type="word">ΕΒ<supplied reason="lost">Α</supplied></seg> μετὰ τᾶς Β
                            <seg type="clearword">συναμφοτέ</seg></seg>
                    <seg n="16" type="line" facs="z22r1_16"><seg type="wordend">ρου</seg> τᾶς ΑΒΓ
                        πρὸς <seg type="word">
                            <choice>
                                <abbr>συναμφότερ<unclear>ο</unclear></abbr>
                                <expan>συναμφότερ<unclear>ον</unclear></expan>
                            </choice>
                        </seg>
                    </seg>
                    <seg n="17" type="line" facs="z22r1_17"><seg type="word"
                                ><unclear>τ</unclear>ὰ<supplied reason="lost">ν</supplied></seg> ΒΔ
                        ΒΑ μετὰ τᾶς Β τᾶς ΒΓ·</seg>
                </seg>
                <seg n="27v2" type="folio">
                    <seg n="1" type="line" facs="z27v2_01"><supplied reason="lost">ὥστε</supplied>
                        <seg type="word">
                            <supplied reason="lost">κ</supplied>
                            <unclear>α</unclear>
                            <supplied reason="lost">ὶ</supplied>
                        </seg>
                        <unclear>ἁ</unclear> Β <seg type="word"
                            ><unclear>π</unclear>ρ<unclear>ὸ</unclear>ς</seg> τὰν Β τὸν <seg
                            type="clearword">αὐ</seg></seg>
                    <seg n="2" type="line" facs="z27v2_02"><seg type="wordend">τὸν</seg> ἕξει <seg
                            type="word">λόγο<unclear>ν</unclear>,</seg>
