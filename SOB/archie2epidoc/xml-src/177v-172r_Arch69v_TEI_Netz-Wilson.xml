    <text xml:lang="grc-c">
        <body>
            <ab>
                <seg n="177v1" type="folio">
                    <seg n="1" type="line" facs="z177v1_01">πλῆθος διὰ τὸ <sic>
                            <seg type="word">ει<unclear>σ</unclear>χεν</seg>
                            <seg type="word"><unclear>α</unclear><supplied reason="lost"
                                    >υ</supplied>το<unclear>ς</unclear></seg>
                        </sic>
                        <seg type="word">εἶν<unclear>αι</unclear></seg></seg>
                    <seg n="2" type="line" facs="z177v1_02">εἰς ἕτερον τόπου <seg type="word"
                                ><supplied reason="lost">τ</supplied>οῦ</seg> ἴσου καὶ <seg
                            type="clearword">ἰσο</seg></seg>
                    <seg n="3" type="line" facs="z177v1_03"><seg type="wordend">γωνίου</seg>
                        σχάματος <choice>
                            <abbr>μετατιθεμέν</abbr>
                            <expan>μετατιθεμένου</expan>
                        </choice></seg>
                    <seg n="4" type="line" facs="z177v1_04">καὶ <seg type="word">ἑ<supplied
                                reason="lost">τ</supplied>έραν</seg> θέσιν <seg type="word"
                                >λαμβ<unclear>ά</unclear>νοντος</seg>. <seg type="unclearword">
                            <unclear>ὅ</unclear>
                        </seg></seg>
                    <seg n="5" type="line" facs="z177v1_05"><seg type="wordend">
                            <unclear>τε</unclear>
                        </seg> δὲ καὶ δύο σχήματα συνάμφω</seg>
                    <seg n="6" type="line" facs="z177v1_06">ἑνὶ σχήματι ἴσων ὄντων καὶ <seg
                            type="clearword">ὁμοί</seg></seg>
                    <seg n="7" type="line" facs="z177v1_07"><seg type="wordend">ων</seg>
                        <seg type="word">τῶ<supplied reason="lost">ι</supplied></seg> ἑνὶ σχήματι ἢ
                        καὶ δύο <seg type="clearword">σχη</seg></seg>
                    <seg n="8" type="line" facs="z177v1_08"><seg type="wordend">μάτων</seg> συνάμφω
                        ἴσων τε καὶ <seg type="clearword">ὅμοι</seg></seg>
                    <seg n="9" type="line" facs="z177v1_09"><seg type="wordend">ον</seg> ὄντων δυσὶ
                        σχήμασι συνάμφω</seg>
                    <seg n="10" type="line" facs="z177v1_10">πλείονα <seg type="word"
                                >σχήμα<unclear>τ</unclear>α</seg> συνίσταται <seg type="clearword"
                        >ἐ</seg></seg>
                    <seg n="11" type="line" facs="z177v1_11"><seg type="wordend">κτὸς</seg>
                        <seg type="word">μ<unclear>ετ</unclear>αθέσεως</seg>. <seg type="clearword"
                            >προγραφόμε</seg></seg>
                    <seg n="12" type="line" facs="z177v1_12"><seg type="wordend">νον</seg> οὖν τι
                        θεώρημα εἰς αὐτὸ <seg type="suppliedword">συ<supplied reason="lost"
                            >ν</supplied>τεῖ</seg></seg>
                    <seg n="13" type="line" facs="z177v1_13">
                        <seg type="wordend">νον.</seg>
                    </seg>
                </seg>
                <seg n="177v1" type="folio">
                    <seg n="13" type="line" facs="z177v1_13">ἔστω γὰρ <seg type="suppliedword"
                                >παραλληλ<unclear>ό</unclear><supplied reason="lost"
                        >γρα</supplied>μ</seg></seg>
                    <seg n="14" type="line" facs="z177v1_14"><seg type="wordend">μον</seg>
                        ὀρθογώνιον τὸ <supplied reason="lost">Ζ</supplied>Γ, καὶ <sic>
                            <seg type="word">δεδι<supplied reason="lost"
                                    >κ</supplied><unclear>ά</unclear><supplied reason="lost"
                                >σ</supplied>θω</seg>
                        </sic></seg>
                    <seg n="15" type="line" facs="z177v1_15">ἡ ΕΖ <seg type="word">τῶ<supplied
                                reason="lost">ι</supplied></seg> Κ, καὶ <seg type="word"
                                ><unclear>ἐ</unclear><supplied reason="lost"
                                >π</supplied><unclear>ε</unclear>ζεύχθω<supplied reason="lost"
                                >σ</supplied>α<unclear>ν</unclear></seg></seg>
                    <seg n="16" type="line" facs="z177v1_16">ἀπὸ τῶν ΓΕ αἱ ΓΚ ΒΕ δεικτέον <seg
                            type="unclearword"><unclear>μ</unclear>εί</seg></seg>
                    <seg n="17" type="line" facs="z177v1_17"><seg type="wordend">ζων</seg>
                        <expan>ἐστὶν</expan> ἡ ΓΒ τῆς ΒΗ <seg type="suppliedword">ἐκβεβλήσ</seg></seg>
                    <seg n="18" type="line" facs="z177v1_18"><seg type="wordend">θ<supplied
                                reason="lost">ω</supplied>σαν</seg> αἱ ΓΚ ΒΖ καὶ <seg
                            type="suppliedword">συμπιπτέ</seg></seg>
                    <seg n="19" type="line" facs="z177v1_19">
                        <seg type="wordend">
                            <supplied reason="lost">τωσαν</supplied>
                        </seg>
                        <seg type="word">
                            <supplied reason="lost">κατ</supplied>
                            <unclear>ὰ</unclear>
                        </seg> τὸ Δ καὶ <seg type="word"><unclear>ἐ</unclear><supplied reason="lost"
                                >π</supplied>εζεύχθω</seg></seg>
                </seg>
                <seg n="172r1" type="folio">
                    <seg n="1" type="line" facs="z172r1_01"><supplied reason="lost">ἡ</supplied>
                        <supplied reason="lost">ΓΗ.</supplied>
                        <supplied reason="lost">ἐπεὶ</supplied>
                        <supplied reason="lost">ἴση</supplied>
                        <supplied reason="lost">
                            <expan>ἐστὶν</expan>
                        </supplied> ἡ ΕΚ τῆι ΚΖ, ἴση <expan>ἐστὶν</expan></seg>
                    <seg n="2" type="line" facs="z172r1_02">καὶ ἡ ΓΕ, <choice>
                            <abbr>τουτ</abbr>
                            <expan>τουτέστιν</expan>
                        </choice>
                        <unclear>ἡ</unclear> ΒΖ, τῆι ΖΔ· <seg type="word">ὥ<supplied reason="lost"
                                >στ</supplied><unclear>ε</unclear></seg></seg>
                    <seg n="3" type="line" facs="z172r1_03"><seg type="word">μείζω<supplied
                                reason="lost">ν</supplied></seg> ἡ <unclear>Γ</unclear>Ζ <seg
                            type="word"><supplied reason="lost">τ</supplied>ῆς</seg>
                        <unclear>Ζ</unclear>Δ· καὶ <seg type="word">γωνί<supplied reason="lost"
                            >α</supplied></seg>
                        <supplied reason="lost">
                            <expan>ἄρα</expan>
                        </supplied></seg>
                    <seg n="4" type="line" facs="z172r1_04"><unclear>ἡ</unclear> ὑπὸ <seg
                            type="word"><unclear>τ</unclear>ῶν</seg> ΖΔΓ τῆς ὑπὸ τῶν ΖΓΔ</seg>
                    <seg n="5" type="line" facs="z172r1_05"><seg type="word"
                            >μ<unclear>εί</unclear>ζων</seg>
                        <expan>ἐστίν</expan>. <seg type="word">ἴσα<unclear>ι</unclear></seg> δέ
                            <expan>εἰσιν</expan> αἱ ὑπὸ ΗΒΔ ΒΓΖ·</seg>
                    <seg n="6" type="line" facs="z172r1_06"><seg type="word"
                                ><unclear>ἡ</unclear>μ<unclear>ί</unclear>σει<supplied reason="lost"
                                >α</supplied></seg>
                        <seg type="word"><supplied reason="lost">γ</supplied>ὰρ</seg>
                        <seg type="word">ὀ<unclear>ρ</unclear>θῆς</seg> ἑκατέρα· <seg
                            type="clearword">μεί</seg></seg>
                    <seg n="7" type="line" facs="z172r1_07"><seg type="wordend">ζων</seg> ἄρα καὶ ἡ
                        ὑπὸ <seg type="word"><unclear>τ</unclear>ῶν</seg> Γ<supplied reason="lost"
                        >Ζ</supplied>Β· ἡ <expan>γὰρ</expan></seg>
                    <seg n="8" type="line" facs="z172r1_08"><sic>
                            <supplied reason="lost">
                                <expan>γὰρ</expan>
                            </supplied>
                        </sic> ὑπὸ ΓΗΒ ἴση <expan>ἐστὶ</expan> δυσὶ ταῖς <seg type="word"
                                >ἐν<supplied reason="lost">τ</supplied><unclear>ὸς</unclear></seg>
                        <unclear>καὶ</unclear></seg>
                    <seg n="9" type="line" facs="z172r1_09">ἀπεναντίον ταῖς <seg type="word"
                                ><unclear>ὑ</unclear>πὸ</seg> ΗΒΔ ΗΔΒ,</seg>
                    <seg n="10" type="line" facs="z172r1_10">τῆς ὑπὸ τῶν ΗΓΒ· ὥστε μείζων</seg>
                    <seg n="11" type="line" facs="z172r1_11"><expan>ἐστὶν</expan> ἡ
                        <unclear>Γ</unclear>Β τῆς ΒΗ. ἐὰν <expan>ἄρα</expan> δίχα <seg
                            type="clearword">τμη</seg></seg>
                    <seg n="12" type="line" facs="z172r1_12"><seg type="wordend">θῆι</seg> ἡ ΓΗ κατὰ
                            <supplied reason="lost">Χ</supplied>, ἔσται <sic><seg type="word"
                                    >ἅμ<unclear>α</unclear></seg> λεία</sic>
                        <choice>
                            <abbr>μ</abbr>
                            <expan>μὲν</expan>
                        </choice></seg>
                    <seg n="13" type="line" facs="z172r1_13">ἡ ὑπὸ ΓΧΒ· ἐπεὶ γὰρ ἴση ἡ ΓΧ τῆι ΧΒ,</seg>
                    <seg n="14" type="line" facs="z172r1_14"><supplied reason="lost">καὶ</supplied>
                        κοινὴ ἡ ΧΒ, δύο <seg type="word"><supplied reason="lost"
                        >δ</supplied>υσὶν</seg> ἴσαι· καὶ</seg>
                    <seg n="15" type="line" facs="z172r1_15"><sic>
                            <seg type="word">βά<unclear>σ</unclear><supplied reason="lost"
                                    >ε</supplied><unclear>ι</unclear>ς</seg>
                        </sic> ἡ ΓΒ τῆς ΒΗ μείζων· καὶ</seg>
                </seg>
                <seg n="177v2" type="folio">
