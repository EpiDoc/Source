            <space dim="horizontal" extent="3" unit="character"/> Caesar </item>
         <item id="EncEx-2">Valeria 
            <space id="space1" dim="horizontal" extent="3" unit="character"/>
            <certainty target="space1" locus="#gi" degree="low"/> Atticilla </item>
         <item id="EncEx-3">senatus 
            <space id="space2" dim="horizontal" extent="3" unit="character" precision="circa"/>
             populusque </item>
         <item id="EncEx-4">dona 
