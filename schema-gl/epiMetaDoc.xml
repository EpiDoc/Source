                <elementSpec ident="typog" mode="add">
                    <desc>Individual examples of traditional typographic editorial conventions.</desc>
                    <classes>
                        <memberOf key="model.graphicLike"/>
                        <memberOf key="att.global"/>
                    </classes>
                    <content xmlns:rng="http://relaxng.org/ns/structure/1.0"
                        xmlns:sch="http://www.ascc.net/xml/schematron">
                        <rng:zeroOrMore>
                            <rng:text/>
                            <rng:ref name="model.gLike"/>
                            <rng:ref name="hi"/>
                        </rng:zeroOrMore>
                        <sch:pattern xmlns:sch="http://www.ascc.net/xml/schematron"
                            name="typog only in typogBox">
                            <sch:rule context="typog">
                                <sch:assert test="ancestor::topogBox"> ERROR: the 'typog' element
                                    must be a descendant of a 'typogBox'. </sch:assert>
                            </sch:rule>
                        </sch:pattern>
                    </content>
                </elementSpec>
                <elementSpec ident="typogBox" mode="add">
                    <desc>Contains one or more typographic editorial conventions, possibly with one
                        or more source references. </desc>
                    <classes>
