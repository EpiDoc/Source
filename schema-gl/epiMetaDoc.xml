		<body>
			<schemaSpec start="TEI div epiEntry" ns="http://www.stoa.org/epidoc/ns/gl-1.0"
				ident="epiMetaDoc" xml:lang="en">
				<!-- *********** -->
				<!-- TEI MODULES -->
				<!-- *********** -->
				<!-- required -->
				<moduleRef key="core"/>
				<moduleRef key="tei"/>
				<moduleRef key="header"/>
				<moduleRef key="textstructure"/>
				<!-- for schema creation -->
				<moduleRef key="tagdocs"/>
				<!-- for <link> & <linkGrp> and <seg> & <ab> -->
				<moduleRef key="linking"/>
				<!-- for <figure> and <figDesc> -->
				<moduleRef key="figures"/>
				<!-- *************** -->
				<!-- ELEMENT CHANGES -->
				<!-- *************** -->
				<elementSpec module="core" ident="name" mode="change">
					<exemplum>
						<eg></eg>
					</exemplum>
				</elementSpec>
					<!-- a <div> can include a series of <epiEntry>s instead of a series of <div>s -->
				<elementSpec module="textstructure" ident="div" mode="change">
					<desc>contains a subdivision of the body of the EpiDoc guidelines.</desc>
					<content>
						<rng:zeroOrMore>
							<rng:choice>
								<rng:ref name="model.divWrapper"/>
								<rng:ref name="model.global"/>
							</rng:choice>
						</rng:zeroOrMore>
						<rng:choice>
							<rng:choice>
								<rng:oneOrMore>
									<rng:ref name="div"/>
								</rng:oneOrMore>
								<rng:oneOrMore>
									<rng:ref name="epiEntry"/>
								</rng:oneOrMore>
							</rng:choice>
							<rng:group>
								<rng:oneOrMore>
									<rng:ref name="macro.component"/>
									<rng:zeroOrMore>
										<rng:ref name="model.global"/>
									</rng:zeroOrMore>
								</rng:oneOrMore>
								<rng:choice>
									<rng:zeroOrMore>
										<rng:ref name="div"/>
									</rng:zeroOrMore>
									<rng:zeroOrMore>
										<rng:ref name="epiEntry"/>
									</rng:zeroOrMore>
								</rng:choice>
							</rng:group>
						</rng:choice>
						<rng:zeroOrMore>
							<rng:choice>
								<rng:ref name="model.divWrapper"/>
								<rng:ref name="model.divWrapper.bottom"/>
							</rng:choice>
						</rng:zeroOrMore>
					</content>
				</elementSpec>
				<!-- remove part= from <ab> -->
				<elementSpec module="linking" ident="ab" mode="change">
					<attList>
						<attDef ident="part" usage="mwa" mode="delete"/>
					</attList>
				</elementSpec>
				<!-- remove calendar=, exact=, and precision= from <date> and <dateRange> -->
				<elementSpec module="core" ident="dateRange" mode="change">
					<attList>
						<attDef ident="calendar" mode="delete"/>
						<attDef ident="exact" mode="delete"/>
					</attList>
				</elementSpec>
				<elementSpec module="core" ident="date" mode="change">
					<attList>
						<attDef ident="calendar" mode="delete"/>
						<attDef ident="precision" mode="delete"/>
					</attList>
				</elementSpec>
				<!-- constrain content of <gi> properly, and alter -->
				<!-- list of possible schemes to suit our purposes -->
				<elementSpec module="tagdocs" ident="gi" mode="change">
					<content>
						<rng:data type="Name"/>
					</content>
					<attList>
						<attDef ident="scheme" mode="change">
							<datatype>
								<rng:ref name="data.enumerated"/>
							</datatype>
							<defaultVal>TEI</defaultVal>
							<valList type="open">
								<valItem ident="TEI">
									<gloss>this element is part of the unextended TEI scheme.</gloss>
								</valItem>
								<valItem ident="EpiDoc">
									<gloss>this element is an EpiDoc extension to the TEI scheme.</gloss>
								</valItem>
							</valList>
						</attDef>
					</attList>
				</elementSpec>
				<!-- same for <att> -->
				<elementSpec module="tagdocs" ident="att" mode="change">
					<content>
						<rng:data type="Name"/>
					</content>
					<attList>
						<attDef ident="scheme" mode="change">
							<datatype>
								<rng:ref name="data.enumerated"/>
							</datatype>
							<defaultVal>TEI</defaultVal>
							<valList type="open">
								<valItem ident="TEI">
									<gloss>this element is part of the unextended TEI scheme.</gloss>
								</valItem>
								<valItem ident="EpiDoc">
									<gloss>this element is an EpiDoc extension to the TEI scheme.</gloss>
								</valItem>
							</valList>
						</attDef>
					</attList>
				</elementSpec>
				<!-- add a required type= to <listBibl> -->
				<elementSpec module="core" ident="listBibl" mode="change">
					<attList>
						<attDef mode="add" ident="type" usage="req">
							<datatype>
								<rng:ref name="data.enumerated"/>
							</datatype>
							<valList type="closed">
								<valItem ident="appCrit"/>
								<valItem ident="commentary"/>
								<valItem ident="history"/>
								<valItem ident="physDesc"/>
								<valItem ident="images"/>
							</valList>
						</attDef>
					</attList>
				</elementSpec>
				<!-- make xml:id= required, and change some -->
				<!-- explanations and example for <egXML> -->
				<elementSpec module="tagdocs" ident="egXML" mode="change">
					<gloss>contains a single well-formed XML example demonstrating the use of some XML element
						or attribute.</gloss>
					<desc>Within the epidoc guidelines, the <att>xml:id</att> attribute is required such that
						the links will work.</desc>
					<attList>
						<attDef mode="change" ident="xml:id" usage="req"/>
					</attList>
					<exemplum>
						<egXML xmlns="http://www.tei-c.org/ns/Examples">
						  <egXML xml:id="EncEx-1" xmlns="http://www.tei-c.org/ns/Examples">
                <expan>co<supplied reason="abbreviation">n</supplied>s<supplied reason="abbreviation">ul</supplied></expan>
              </egXML>
            </egXML>
					</exemplum>
				</elementSpec>
				<!-- Give an EpiDoc example in <exemplum>s reference doc -->
				<elementSpec module="tagdocs" ident="exemplum" mode="change">
					<desc>contains a single XML example demonstrating the use of some XML element or attribute
						with optional paragraphs of commentary.</desc>
					<exemplum>
						<egXML xmlns="http://www.tei-c.org/ns/Examples">
              <exemplum>
                <egXML xmlns="http://www.tei-c.org/ns/Examples" xml:id="EncEx-2">
                	<gap reason="lost" extent="3" unit="character"/>
                </egXML>
              	<p>This example indicates the certain loss of exactly 3 characters.</p>
              </exemplum>
            </egXML>
					</exemplum>
				</elementSpec>
				<!-- use TEI's cRefPattern as a phrase-level element for holding
			    the regular expressions CHETC will use to transform the typographic
			    representation into EpiDoc XML. -->
				<elementSpec module="header" ident="cRefPattern" mode="change">
					<altIdent>regex</altIdent>
					<gloss>Defines regular expressions for epidoc XML fragments.</gloss>
					<desc>Specifies an search pattern and replacement expression for transforming the
						traditional typographic conventions against the EpiDoc XML markup. In addition, may
						contain prose commentary explaining the regular expressions.</desc>
					<attList>
						<attDef mode="change" ident="xml:id" usage="req"/>
						<attDef mode="change" ident="matchPattern" usage="req">
							<desc>specifies a regular expression intended to match a conventional typographic
								representation</desc>
						</attDef>
						<attDef mode="change" ident="replacementPattern" usage="req">
							<desc>specifies a replacement pattern which, once subpattern substitution has been
								performed, provides the epidoc XML fragment.</desc>
							<valDesc>Should be the skeleton of epidoc XML markup, with references to groups in the
									<att>matchPattern</att>.</valDesc>
						</attDef>
					</attList>
					<exemplum>
						<egXML xmlns="http://www.tei-c.org/ns/Examples">
              <regex xml:id="M1" matchPattern="(\S+)\((-\s*)+\)" replacementPattern="&amp;lt;abbr&gt;$1&amp;lt;/abbr&gt;"/>
              <regex xml:id="R1" matchPattern="\[([.\s])\]" replacementPattern="&amp;lt;gap reason='lost' extent='%g1%len1' unit='character'/&gt;">
                Note the use of a notation like '%g1%len2', which allows the
                chetc program to measure the number of occurences of the symbol in
                the second capturing group inside the first capturing group.</regex>
            </egXML>
					</exemplum>
					<remarks>
						<p>Note that becase the search expression and replacement expression are expressed in
							attribute values, the character <code>&lt;</code> must be escaped as
								<code>&amp;lt;</code>. Also, whichever character that is used to delimit the
								<att>matchPattern</att> or <att>replacementPattern</att> attributes (either
								<code>&apos;</code> or <code>&quot;</code>) must be escaped (either
								<code>&amp;apos;</code> or <code>&amp;quot;</code>).</p>
					</remarks>
				</elementSpec>
				<!-- make <revisionDesc> simpler -->
				<elementSpec module="header" ident="revisionDesc" mode="change">
					<content>
						<rng:zeroOrMore>
							<rng:ref name="change"/>
						</rng:zeroOrMore>
					</content>
					<remarks>
						<p>The content model is more restrictive than vanilla TEI, in that it does not permit
								<gi>list</gi>; it is more permissive than vanilla TEI, in that it does permit 0
								<gi>change</gi>s (i.e., no content at all). This is to make working with templates
							easier.</p>
					</remarks>
				</elementSpec>
				<!-- add rend= attribute (which is no longer in att.global) to <hi>,
          and give it a controlled vocabulaly -->
				<elementSpec module="core" ident="hi" mode="change">
					<attList>
						<attDef ident="rend" mode="add">
							<gloss>Idicates the rendition of the epigraphic conventions.</gloss>
							<datatype>
								<rng:ref name="data.enumerated"/>
							</datatype>
							<valList type="closed">
								<valItem ident="apex">
									<gloss>apex</gloss>
								</valItem>
								<valItem ident="intraline">
									<gloss>intraline</gloss>
								</valItem>
								<valItem ident="italic">
									<gloss>italic</gloss>
								</valItem>
								<valItem ident="ligature">
									<gloss>ligature</gloss>
								</valItem>
								<valItem ident="normal">
									<gloss>normal</gloss>
								</valItem>
								<valItem ident="reversed">
									<gloss>reversed</gloss>
								</valItem>
								<valItem ident="strong">
									<gloss>strong</gloss>
								</valItem>
								<valItem ident="superscript">
									<gloss>superscript</gloss>
								</valItem>
								<valItem ident="supraline">
									<gloss>supraline</gloss>
								</valItem>
								<valItem ident="tall">
									<gloss>tall</gloss>
								</valItem>
								<valItem ident="underline">
									<gloss>underline</gloss>
								</valItem>
							</valList>
						</attDef>
					</attList>
					<exemplum>
						<egXML xmlns="http://www.tei-c.org/ns/Examples">
							Amaran<hi rend="tall">t</hi>ho Agathemeris soror pro meri<hi rend="tall">t</hi>is
							<hi rend="tall">t</hi>ribu<unclear reason="damage" cert="high">i</unclear><unclear
								reason="damage" cert="high">t</unclear>
            </egXML>
					</exemplum>
				</elementSpec>
				<!-- remove type= from <abbr> -->
				<elementSpec module="core" ident="abbr" mode="change">
					<attList>
						<attDef ident="type" usage="opt" mode="delete"/>
					</attList>
					<exemplum>
						<egXML xmlns="http://www.tei-c.org/ns/Examples">
              <abbr>ZHLA</abbr>
            </egXML>
					</exemplum>
				</elementSpec>
				<!-- ************** -->
				<!-- ADDED ELEMENTS -->
				<!-- ************** -->
				<elementSpec ident="typog" mode="add">
					<desc>Individual examples of traditional typographic editorial conventions. The attribute
						id is required.</desc>
					<classes>
						<memberOf key="model.graphicLike"/>
					</classes>
					<content xmlns:rng="http://relaxng.org/ns/structure/1.0">
						<rng:zeroOrMore>
							<rng:text/>
							<rng:ref name="model.gLike"/>
							<rng:ref name="hi"/>
						</rng:zeroOrMore>
						<sch:pattern name="typog only in typogBox">
							<sch:rule context="typog">
								<sch:assert test="ancestor::topogBox">ERROR: the 'typog' element must be a
									descendant of a 'typogBox'.</sch:assert>
							</sch:rule>
						</sch:pattern>
					</content>
					<attList>
						<!-- make xml:id= required -->
						<attDef mode="change" ident="xml:id" usage="req"/>
					</attList>
					<exemplum>
						<egXML xmlns="http://www.tei-c.org/ns/Examples">
              <typog id="TypoEx-1">co(n)s(ul)</typog>
            </egXML>
					</exemplum>
				</elementSpec>
				<!--  -->
				<elementSpec ident="typogBox" mode="add">
					<desc>Contains examples of one or more traditional typographic notations for a particular
						epigraphic feature, along with bibliographic citations for those notations.</desc>
					<content>
						<rng:text/>
						<rng:oneOrMore>
							<rng:ref name="biblItem"/>
						</rng:oneOrMore>
						<rng:zeroOrMore>
							<rng:choice>
								<rng:ref name="model.pLike"/>
								<rng:ref name="model.listLike"/>
							</rng:choice>
						</rng:zeroOrMore>
					</content>
					<exemplum>
						<egXML xmlns="http://www.tei-c.org/ns/Examples">
              <typogBox id="abbreviationsunderstood-typog-1">
                <biblItem>
                  <title level="a" type="abbreviated">
                     <xref href="bibliography.xml#panciera">Panciera</xref>
                  </title>
                  <abbr expan="page">p.</abbr>
                  <biblScope type="pages">17</biblScope>
                </biblItem>
                <list type="ordered" lang="lat">
                  <item><typog id="TypoEx-1">co(n)s(ul)</typog></item>
                  <item><typog id="TypoEx-2">Aug(ustus)</typog></item>
                </list>
              </typogBox>
            </egXML>
					</exemplum>
				</elementSpec>
				<!--  -->
				<elementSpec ident="listResp" mode="add">
					<desc>Contains statements of responsibility for individuals.</desc>
					<content>
						<rng:oneOrMore>
							<rng:ref name="respStmt"/>
						</rng:oneOrMore>
					</content>
				</elementSpec>
				<!--  -->
				<elementSpec ident="epiEntry" mode="add">
					<gloss>EpiDoc entry</gloss>
					<desc>Contains the description of a particular epigraphic feature, examples of traditional
						editorial conventions, epidoc XML representations, and regular expressions to change the
						former to the latter.</desc>
					<content xmlns:rng="http://relaxng.org/ns/structure/1.0">
						<rng:zeroOrMore>
							<rng:ref name="model.pLike"/>
						</rng:zeroOrMore>
						<rng:optional>
							<rng:oneOrMore>
								<rng:ref name="typogBox"/>
							</rng:oneOrMore>
							<rng:oneOrMore>
								<rng:ref name="regex"/>
							</rng:oneOrMore>
						</rng:optional>
						<rng:oneOrMore>
							<rng:choice>
								<rng:ref name="exemplum"/>
								<rng:ref name="egXML"/>
							</rng:choice>
						</rng:oneOrMore>
						<rng:optional>
							<rng:choice>
								<rng:ref name="linkGrp"/>
								<rng:ref name="link"/>
							</rng:choice>
						</rng:optional>
						<rng:zeroOrMore>
							<rng:ref name="note"/>
						</rng:zeroOrMore>
						<rng:ref name="respList"/>
						<sch:pattern
							name="No 'ptr' elements in main epigraphic entry 'linkGrp' (it's only there for CHETC anyway)">
							<sch:rule context="epiEntry/linkGrp">
								<sch:report test="./ptr">ERROR: no 'ptr's allowed in the main 'linkGrp' child of
									'epiEntry'</sch:report>
							</sch:rule>
						</sch:pattern>
					</content>
					<attList>
						<attDef mode="change" ident="xml:id" usage="req"/>
					</attList>
				</elementSpec>
